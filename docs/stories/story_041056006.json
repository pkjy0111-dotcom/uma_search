      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "今日は授業もトレーニングもお休み…… なので街にでも出かけて羽を伸ばそうと思っていたのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
