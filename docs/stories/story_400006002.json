      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "『プロジェクトL'Arc』のメンバーと ともにトレーニングを積み、 世界一の栄冠を手にしましょう！",
