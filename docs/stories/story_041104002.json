      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "昨日見た、カツラギエースの走りと、 一片の曇りもないギラギラとしたあの眼。そして――",
          "flag_type": "",
          "flag": "",
