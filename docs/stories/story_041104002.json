      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "大きく息を吐く。ちょっとした言葉で揺らいでしまうような 自分とは違う、カツラギエースの姿。",
          "flag_type": "",
          "flag": "",
