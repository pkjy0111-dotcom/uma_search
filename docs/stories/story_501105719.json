        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーニング前、ネオユニヴァースは プラネタリウムに来ていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
