      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1105,
          "speaker_name": "ネオユニヴァース",
          "text": "……ボイジャー。 違う惑星も、体感したい……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
