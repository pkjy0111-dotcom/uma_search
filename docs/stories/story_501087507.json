      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "そうとわかれば、トレーナーさん。 今日もトレーニング、がんばらないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
