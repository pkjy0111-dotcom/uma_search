      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "…………。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "何かまずい内容だった？",
              "flag_type": "",
              "flag": ""
