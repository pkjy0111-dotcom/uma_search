      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そう言って手紙を読み始めた アストンマーチャンだったが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
