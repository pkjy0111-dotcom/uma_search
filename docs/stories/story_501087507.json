      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "おお、これは。マーちゃんてば 世界のマスコットに大前進では？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
