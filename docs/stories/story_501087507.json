    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "違うんです、トレーナーさん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "『いっぱいがんばるマーちゃんのこと、 何度も思い出してます』って手紙に……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 24,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "『失敗して落ち込んだりすると、 マーちゃんが走っている姿を思い出します』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 24,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "『そうすると、同じぐらいがんばって 前に進まないとって思えるから……』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 24,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "『マーちゃんの走る姿は私のお守りです――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "肌身離さず、お守りマーちゃん。 ……悪くありませんね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "それにしても不思議です、トレーナーさん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "マーちゃんはただ、マーちゃんのことを 覚えててほしかっただけなのに……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "それが記憶に残るってことじゃない？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "記憶に残った姿が、その人の心を 動かし続けることもあるのではと伝えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "記憶の中のマーちゃんが、 そのヒトの心を……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "……そうですね。それも 世界的マスコットの使命かもしれません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
