      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "何が書いてあるんでしょう。やはり マーちゃんのラブリーさについてでしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
