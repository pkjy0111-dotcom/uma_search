      "frame_id": 17,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "",
          "text": "これから彼女との日々を歩むうえで、 コミュニケーションを円滑にすべく、 空いた時間で勉強しているというわけだ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
