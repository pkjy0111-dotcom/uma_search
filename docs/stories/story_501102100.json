    "12": {
      "frame_id": 12,
      "background_id": 37,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "初日となる今日は、気合いを入れて早起きし、 トレーナー室へと向かっていた。 その道すがら――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "えっと、『エザルタツィオーネ』は興奮――",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 37,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "買ったばかりの音楽用語辞典を片手に、 勉強をしていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "『トレーナー。 最初はモデラートで構わないかい？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "『それからこの4小節目の メッゾ・フォルテをフォルテッシモに したいんだ。そっちの方が響くだろう？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……サウンズオブアースは言葉の節々に 音楽用語を引用することがとても多い。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
