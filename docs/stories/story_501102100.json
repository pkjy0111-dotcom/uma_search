      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "――サウンズオブアースと トゥインクル・シリーズへ 挑戦する3年間が、いよいよ始まる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
