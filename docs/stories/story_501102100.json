      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90001,
          "speaker_name": "",
          "text": "かすかにだが、たしかに聞こえる。 学園のどこかで、 ヴァイオリンの音が響いている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
