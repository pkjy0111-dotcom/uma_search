        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "翌日、トレーナー室にて。 ……ひと晩中、あれこれ考え続けた謝罪の言葉を反芻する。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "（彼女には……申し訳ないけど……）",
              "flag_type": "",
              "flag": ""
