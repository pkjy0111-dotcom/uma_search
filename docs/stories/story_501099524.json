      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――ふむ、向こうに、 手ごろな高さの山が見えますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "26": {
      "frame_id": 26,
      "background_id": 112,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "あれが樽前山だとして―― 見ている人は、まずなにを思うかな……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
