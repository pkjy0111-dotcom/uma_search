      "frame_id": 24,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "ありがとうございます！　となると…… 紹介の最後に、近くのお土産屋さんについて 軽く説明を入れて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
