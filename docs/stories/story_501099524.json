      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "ウマ娘とヒトの体力の違いも考慮しなきゃ。 ――トレーナーさん。まだお時間ありますか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 29,
              "loop_ends_at_frame": -1,
              "text": "大丈夫だけど",
