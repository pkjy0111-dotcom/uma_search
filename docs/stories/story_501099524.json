      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "……やっぱり『登ってみたい』ですかね？ でもその場合は、きちんと登山の準備をして 行くように、動画の中で言わないと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
