      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そしてホッコータルマエに先導され、 ひいひい言いながら、山を登ることに なったのだった――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
