      "frame_id": 29,
      "background_id": 112,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――では、ちょっとだけ登っちゃいましょう！ 一応、最低限の装備は持参してきましたので。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 30,
              "loop_ends_at_frame": -1,
              "text": "……ええ！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
