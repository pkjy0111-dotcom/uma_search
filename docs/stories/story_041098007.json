      "frame_id": 18,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "あれもこれもって、手を広げていったら…… いつの間にか準備することがたくさんできちゃってたの！",
