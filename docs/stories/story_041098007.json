      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "ほら、私って結構ガツガツ前でてくタイプでしょう？　気づいたら 『風水アドバイザー』から『実行委員長その2』みたいになってて。",
          "flag_type": "",
          "flag": "",
          "choices": []
