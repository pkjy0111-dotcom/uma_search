    "14": {
      "frame_id": 14,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "んー、少しね！　次の会議に持っていきたい資料があったから 準備してたの。気づいたら夜明けで、タルマエに怒られちゃった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "寝不足は運気の低下を招くから、普段は気をつけているんだけど…… 夢中になりすぎちゃってたみたい！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "今日はゆっくり休むんだよ",
              "flag_type": "",
              "flag": ""
