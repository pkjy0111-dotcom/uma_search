      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そのため各国で、議論も盛んに行われてきた。 その中のひとつの主張として、 クラシック級ウマ娘が有利という見方もある。",
          "flag_type": "",
          "flag": "",
          "choices": []
