          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "――……何か？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "ラーメン屋にいるなんて珍しいな",
