      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "……だからじゃないかな。 具が違いますよね？　1番人気メニューと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
