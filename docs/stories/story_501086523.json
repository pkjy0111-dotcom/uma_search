      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "――これが、貴方のラーメンへの愛？ そう。がっかりね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 97,
      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "な、なにがお気に障ったんでしょう……？ 具の香味野菜も上品で、 フレンチのオードブルみたいなのに……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
