      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "姫の中の姫、ベストオブ姫を決める戦い。\r 『エリザベス女王杯』しかないんですの！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "『エリザベス女王杯』か……",
              "flag_type": "",
