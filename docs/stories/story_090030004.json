      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "……でも、そんな…… キングさん、あれほど白拍子役に思い入れしてらしたのに……！",
          "flag_type": "",
          "flag": "",
          "choices": []
