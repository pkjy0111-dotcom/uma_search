      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90730,
          "speaker_name": "奉納劇の講師",
          "text": "悔しいでしょうけれど……無理をしてケガが悪化してはいけません。 ……ごめんなさい。私も監督不行き届きでした。",
          "flag_type": "",
          "flag": "",
          "choices": []
