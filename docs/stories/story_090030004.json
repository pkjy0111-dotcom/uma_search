      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90730,
          "speaker_name": "奉納劇の講師",
          "text": "――ですので、キングヘイローさんは白拍子を降板すべきでしょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
