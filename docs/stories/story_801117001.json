      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "ええ。すぐ削れて、 緩んじゃうんっすよねぇ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
