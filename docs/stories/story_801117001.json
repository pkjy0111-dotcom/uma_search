      "frame_id": 5,
      "background_id": 319,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "ウォームアップ中に、落鉄したみたいで……。 一昨日、打ち直したばかりなのに。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "一昨日！？二日でこんなに摩耗を！？",
