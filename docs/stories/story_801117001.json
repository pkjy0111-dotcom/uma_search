      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "……うーん、ないなぁ……。 どこで落鉄したっすかねぇ……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "もしかしてこれ、君の蹄鉄？",
              "flag_type": "",
              "flag": "male_trainer"
