      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "スぺ君、グラス君……君たちの刻んできた\r 蹄鉄の歴史、しかと見させてもらった。",
          "flag_type": "",
          "flag": "",
          "choices": []
