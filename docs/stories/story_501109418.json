      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "目が覚めると、シーザリオが心配そうに 彼女を覗き込んでいた。長時間の昼寝なんて、 疲れているのではないかと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
