      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして『ヴィクトリアマイル』当日。 ラインクラフトは、1番のティアラウマ娘に なると、同期たちに宣言するのだった。",
          "flag_type": "",
          "flag": "",
