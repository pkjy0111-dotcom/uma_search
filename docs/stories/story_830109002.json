      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "！　これ、いいかもしれません！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 30,
              "loop_ends_at_frame": -1,
              "text": "え？何が？",
              "flag_type": "",
              "flag": ""
