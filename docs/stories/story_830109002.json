      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "ありがとうございます！　準備するものも 多いですし、協力者も必要になりますが、 なんとかなりそうです！　私……頑張ります！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
