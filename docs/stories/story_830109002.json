      "frame_id": 28,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "……全部が勝ち……射的……お祭り……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
