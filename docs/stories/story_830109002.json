      "frame_id": 31,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "よ～っし！　やるぞ～！　用意ができたら、 トレーナーさんも誘いますね！ チヨノ・オー！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
