      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "君が何を祈ったのかは知らないが、\r 神頼みしたくなることでもあったんだろ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
