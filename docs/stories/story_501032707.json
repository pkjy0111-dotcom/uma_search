      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "そもそも、そんなことが起きたとすれば、\r 調子がどうのという問題では――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
