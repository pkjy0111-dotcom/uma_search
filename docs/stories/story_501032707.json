      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "まぁ、もちろん何の脈略もない冗談だが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
