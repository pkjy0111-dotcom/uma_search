      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "だがまぁ、用が済んだのなら\r 帰ろうじゃないか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "疲れてる？",
