      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "いいや、ただ少し調子が出ないだけさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
