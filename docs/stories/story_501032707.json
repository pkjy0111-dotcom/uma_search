      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……どうやら本当に調子が悪いらしい。\r なんとか元気にしてあげたいが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
