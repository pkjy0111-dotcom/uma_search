      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "……はぁ、どうでもいい話だったか。",
          "flag_type": "",
          "flag": "",
          "choices": []
