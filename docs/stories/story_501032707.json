      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "私の中にあるマクロファージがサボって、\r アポトーシス小体を放置しているのかもね。",
