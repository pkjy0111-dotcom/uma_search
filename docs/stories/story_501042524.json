      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90730,
          "speaker_name": "テレビのレポーター",
          "text": "『――いや～、今日もおいしそうな 駅弁が目白押しでしたね！ それではみなさん、また来週！』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "そろそろ始めるか",
              "flag_type": "",
