      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "それに……ファンの方に楽しんでいただく、 という趣旨に、便風を感じたのですよ。",
