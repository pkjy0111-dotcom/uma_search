      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "ええ。私にとっては、凪に帆を立てるような思いですが…… せっかく吹いた風ですから、花の心で誘われてみようかと。",
          "flag_type": "",
          "flag": "",
          "choices": []
