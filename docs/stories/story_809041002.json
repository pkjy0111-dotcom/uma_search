          "character_id": 9041,
          "speaker_name": "ゴドルフィンバルブ",
          "text": "そちらのトレーナーさん。 少しいい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "なんでしょうか？",
