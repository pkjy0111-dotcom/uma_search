      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "なんでいなんでい！　辛気臭え面しやがって！ そんなんじゃ祭りも湿気っちまうぜ？",
