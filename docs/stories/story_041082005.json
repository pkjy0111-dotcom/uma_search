    "2": {
      "frame_id": 2,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "はぁっ、はぁっ、はぁっ……ふうぅ～～！ よし、走り込み完了です！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "お疲れ様！じゃあ次は――",
              "flag_type": "",
              "flag": ""
            }
          ]
