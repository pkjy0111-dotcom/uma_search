    },
    "3": {
      "frame_id": 3,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "……っ、とと……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "フライト！大丈夫？",
              "flag_type": "",
              "flag": ""
            }
          ]
