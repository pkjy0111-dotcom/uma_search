        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "それはノースフライトの担当トレーナーになって しばらく経った、ある日のことだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
