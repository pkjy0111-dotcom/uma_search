      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "いえ、そんなことありません！ 確かに珍しいコーデではありますが、校則違反でもありませんし。",
