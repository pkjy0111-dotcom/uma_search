      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "『獲れないのでは、という意見は多数。 が、彼女ならば常識を覆せるはずだ。 そんな声も確かに聞こえてくる！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
