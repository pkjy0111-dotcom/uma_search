      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "あはは。この『週刊ぱかっとチャンネル』、 ずいぶんアタシをひいきにしてくれてるな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
