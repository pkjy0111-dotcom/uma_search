      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "『ミスターシービーはダービーウマ娘に なれるのか。空前絶後の期待感で レース場は浮ついている――』",
          "flag_type": "",
          "flag": "",
          "choices": []
