      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "クラシック戦線に入ってからは特に、 ミスターシービーへの期待は 異様なほどに膨れていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
