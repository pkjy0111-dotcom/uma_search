    "11": {
      "frame_id": 11,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "私のほうも、目立たないのに慣れちゃって。 誰かがせっかく見てくれようとしても、は、恥ずかしくて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
