      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "えっ、あ、はい……！　知ってたんですね……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
