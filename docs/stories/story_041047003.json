      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "せめて見せても恥ずかしくないようになろうって、焦って、 焦りすぎて……またケガしたりして。",
          "flag_type": "",
          "flag": "",
          "choices": []
