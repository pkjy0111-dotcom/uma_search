    "3": {
      "frame_id": 3,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "不思議そうにしているゼンノロブロイに、 担当教官から聞いたことを話すと納得したようだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "種目別競技大会とは、トレセン学園で行われる校内レース。 このレースに焦点を合わせ、体を作ってくる生徒もいる。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "頑張って！当日は応援に行くから！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "はい……ありがとう、ございます……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "……あ、いえっ、嫌とかそういうのではなくて！ ごめんなさい、ただ……私、目立たないタイプなので。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "だから……せっかく観に来てもらっても、 気づかれないかもとか……いろいろ考えちゃって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "……教官さんとお話ししたってことは、トレーナーさんも…… もう知ってますよね。最近の私が本当にダメなこと。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "ごめん、勝手に……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "いえ！　気にかけてくださって、ありがとうございます。 たしかに最近、行き詰まっていたところはありました……えへへ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "私、昔から特に取り柄もなくて。好きなものは本くらいで、 ずっと……すっごく、地味なんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
