        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "翌日、図書室で2人、資料探しをしていた時……ふと、 先日、ゼンノロブロイの担当教官から聞いた話を思い出した。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "そういえば、種目別競技大会に出るんだよな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "そういえば、種目別競技大会に出るんだよね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
