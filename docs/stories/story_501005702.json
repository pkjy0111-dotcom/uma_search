    "11": {
      "frame_id": 11,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "頬を染めて楽し気に展覧会場を歩く\r フジキセキを、見守りながら進んでいった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そうしてしばらく展覧会を楽しみ、\r 時間も閉館間近に差し掛かった頃――",
          "flag_type": "",
          "flag": "",
          "choices": []
