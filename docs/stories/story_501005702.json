      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "当時はストーリーがほとんどなくて、\r 見た目の華やかさが重要視されていたんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
