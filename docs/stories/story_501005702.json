      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "ああっ！　見て、トレーナーさん！\r あれ、すごく有名な衣装なんだけど――",
          "flag_type": "",
          "flag": "",
          "choices": []
