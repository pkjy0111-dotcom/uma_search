    "10": {
      "frame_id": 10,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "普段は人をエスコートしてばかりの\r フジキセキが、こんなに夢中になる姿は 滅多に見られない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "（今日は思いっきり楽しんでほしいな）",
              "flag_type": "",
              "flag": ""
