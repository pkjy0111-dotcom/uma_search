    "30": {
      "frame_id": 30,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 90001,
          "speaker_name": "",
          "text": "雨の中の彼女を見ていると、濡れることは 受け入れるべき当然のことのようで、 否定的な言葉はなにも出てこなかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "31": {
      "frame_id": 31,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "いずれは疾風となろうとも、 今は和風の如く、 共に雨の中を歩みましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "32": {
      "frame_id": 32,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "――ほら、とても心地いい。 呼吸をして、大きく手を伸ばして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "33": {
      "frame_id": 33,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "浄化の雨と恵風を、 この身にまとうように。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "34": {
      "frame_id": 34,
