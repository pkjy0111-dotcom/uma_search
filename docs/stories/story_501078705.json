      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 90001,
          "speaker_name": "",
          "text": "先程と同じ質問を投げかけて、 ヤマニンゼファーは外に出た。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
