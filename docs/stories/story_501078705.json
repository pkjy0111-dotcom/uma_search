      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "ええ。どんどん、ぐらぐら。 凄風に背中を叩かれた時とは まったく違っていて……体が少し痛いような。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 35,
              "loop_ends_at_frame": -1,
              "text": "だ、大丈夫？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "35": {
      "frame_id": 35,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
