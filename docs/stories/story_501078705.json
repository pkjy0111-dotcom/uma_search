    "29": {
      "frame_id": 29,
      "background_id": 37,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "私は、好きです。 しとしとと、ざあざあと。 音の中に溶けていくようで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
