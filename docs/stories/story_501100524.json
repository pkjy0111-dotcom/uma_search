      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "いつも穏やかで、柔らかな空気を まとうワンダーアキュート。 だが、その目は本気の光を宿していて――",
          "flag_type": "",
          "flag": "chance_branch_root",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "……わかった、限界を目指そう！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "倒れない程度にしよう",
              "flag_type": "",
              "flag": ""
