      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "レースも、これからどんどん 増えていくじゃろ？　本番で 情けない真似はしたくないからねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": []
