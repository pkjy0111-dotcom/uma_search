      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "他のウマ娘さんにも、トレーナーさんにも、 本気で立ち向かっていきたいと思うんだよ。 ……あたしのこの脚でね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
