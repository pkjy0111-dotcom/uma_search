    "11": {
      "frame_id": 11,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "いえいえ、気にしないでください。 いつも倒れてる私が言うのもなんですけど、えへへ～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
