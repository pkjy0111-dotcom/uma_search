      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "えっと、保冷剤のストックは……うん。まだある。 タオルも大丈夫。あとは診察まで少し時間があるから――",
          "flag_type": "",
          "flag": "",
          "choices": []
