      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ツルマルツヨシの作ってくれたおじやを食べる。 体調を崩した事さえ忘れるほど食欲をそそる香りに箸が止まらない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "美味しい……それに、すごく食べやすい",
