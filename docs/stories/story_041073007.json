    "12": {
      "frame_id": 12,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "一応あとでお医者さん来ますからね。 症状は比較的軽そうですけれど、念のためってことで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "あ、そうだトレーナーさん。 食欲とかはありそうですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "少しなら食べられそう",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "じゃあ、今のうちに軽く食べちゃいましょうか。 おじや作ってあるんで、温めてきます。",
