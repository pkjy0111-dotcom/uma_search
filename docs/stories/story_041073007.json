      "frame_id": 9,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "あっ……トレーナーさん、まだ体は起こさないほうがいいですよ。 そのまま横になっていてください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ツルマルツヨシに担がれて合宿所の保健室に連れてこられたようだ。 まだ少し、頭がぼーっとしている。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "……なんか、ごめん",
              "flag_type": "",
              "flag": ""
            }
          ]
