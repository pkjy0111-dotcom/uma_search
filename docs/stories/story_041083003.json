      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "――次、レッグランジ。――異存は、ないか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "だ、大丈夫！でも――",
