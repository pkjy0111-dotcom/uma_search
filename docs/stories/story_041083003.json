      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "シンボリクリスエスにはすでに伝えてあるとのことだが…… 様子が気になり、会いに行くことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
