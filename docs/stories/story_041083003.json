      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "サボりたいわけでも、能力的に限界が来ているわけでもない。 絶対に無理をしたくない理由があるのだろう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "――ここまで厳密に管理するのは、なぜ？",
              "flag_type": "",
              "flag": ""
