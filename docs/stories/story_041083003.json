      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "このラインがあるから――彼女は、トレーナーが組んでくれた メニューを、その通りにこなせなかったのだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
