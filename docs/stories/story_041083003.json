    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "シンボリクリスエスは、体にかかる負荷が ある一定のラインを越えないように、綿密にメニューを組んでいる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
