      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "『いつでも、\r アナタのことを見守っています……』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
