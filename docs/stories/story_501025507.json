    "3": {
      "frame_id": 3,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "その手紙……\r 気づいたら、そこに置いてありました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "トレーナーさん宛て……？\r それとも――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "確認してみるよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "封を開けてみるとマンハッタンカフェへの\r ファンレターだとわかったが――なぜだろう、 内容にはどこか違和感を覚えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ファンレターにしては\r かなり奇妙というか、なんというか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "どうしましたか……？\r 何か、おかしなことでも……？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "いや、そういうわけじゃ……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "マンハッタンカフェ宛てのファンレターなら\r 彼女が読むべきだろうか……そう思い、 渡すことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
