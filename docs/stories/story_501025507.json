      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "応援してくれるのは、\r 何も、目に見える観客だけじゃない……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
