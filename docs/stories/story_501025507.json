      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "念のため、トレーニング中は\r 鍵をかけておいたはずなのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "（いつからここに……？）",
              "flag_type": "",
              "flag": ""
            }
