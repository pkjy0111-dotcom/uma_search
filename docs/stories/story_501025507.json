      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "『朝、立ちのぼる霧の中から――\r 昼、ざわめく木々の影から―― 夜、静かな闇夜の中から――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
