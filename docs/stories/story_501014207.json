      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『有馬記念』。かつての仲良し3人組が今、\r ウマ娘界を代表する三強として、激突する！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
