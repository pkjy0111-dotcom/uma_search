    "4": {
      "frame_id": 4,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "わたしも、もっといっぱいたよられる先輩に なりたいっ！　だから―― キングちゃんに、いろいろ教えてほしいの！",
          "flag_type": "",
          "flag": "",
          "choices": []
