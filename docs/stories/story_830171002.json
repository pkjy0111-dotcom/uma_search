      "frame_id": 7,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "優しくて、カッコよくて、 とーってもたよりになるから！ わたし、キングちゃんみたいになりたいんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
