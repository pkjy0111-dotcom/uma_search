    },
    "6": {
      "frame_id": 6,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "わーい！　ありがとう、キングちゃん！！ キングちゃんが教えてくれるんなら、 絶対大丈夫だよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
