    "5": {
      "frame_id": 5,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "私に頼るとは、わかってるじゃない！ そういうことなら、 このキングに任せておきなさい！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
