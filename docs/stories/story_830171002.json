      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "私みたいにね……ふふっ、いいわ！ 手取り足取り、鍛えてあげる！ しっかりついて来るのよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
