      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "この前ね、テイオーちゃんとツルちゃんが 困ってた後輩を助けてあげてたの！ でも、わたしはうまくできなかったんだぁ。",
          "flag_type": "",
          "flag": "",
