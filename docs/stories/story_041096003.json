          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "なんと。頼みとは……ワシと将棋を一局指したいと申すか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ああ！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "うん！",
              "flag_type": "",
              "flag": "female_trainer"
