    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ノーリーズンは、本当に食わせ者なのかどうか。 盤上の戦である将棋を指せば、きっと見えるはずだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "ほほ～～～～～～～～ぅ？",
          "flag_type": "",
          "flag": "",
          "choices": []
