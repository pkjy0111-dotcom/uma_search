      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "いかがした、トレーナー。教室まで足を運ぶとは…… あ、やはり昨日の情報が欲しくなったか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "いや、少し付き合ってもらえないかな",
