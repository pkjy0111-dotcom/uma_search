    "3": {
      "frame_id": 3,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "うん？　ここではアレか？ よいぞ、では移動するかのう～！",
