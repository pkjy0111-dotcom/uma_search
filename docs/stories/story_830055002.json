      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1020,
          "speaker_name": "セイウンスカイ",
          "text": "新情報が出れば、容疑者は変わってくるかも。\r ……みなさん、どうします？",
