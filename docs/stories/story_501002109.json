          "next_frame": 3,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "あ……雪……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "好きなの？",
