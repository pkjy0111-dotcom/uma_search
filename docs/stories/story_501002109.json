    },
    "9": {
      "frame_id": 9,
      "background_id": 5,
      "sub_background_id": 4310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "――子どもの頃、静かな外気に誘われて\r 雪原を走ってみたことがあるんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 5,
      "sub_background_id": 4310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "だんだん自分の鼓動しか聞こえなくなって、\r 静かで、真っ白で、綺麗で……衝撃的でした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
