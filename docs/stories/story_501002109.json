      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "それに……雪は音を吸うので。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "外へ出てみる？",
              "flag_type": "",
              "flag": ""
