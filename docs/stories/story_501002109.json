      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "ふふ、やっぱり、空気が澄んでいますね。\r 懐かしいな……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 5,
      "sub_background_id": 4310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "雪の中で走るのは、心地がいいんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "よく走ってたんだね",
              "flag_type": "",
              "flag": ""
            }
