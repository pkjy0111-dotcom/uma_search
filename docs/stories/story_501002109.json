        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 5,
      "sub_background_id": 4310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "はい。たしか……走ることが好きに\r なったのも、雪がきっかけだったような。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "（彼女が、走り出したきっかけ……！）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 5,
      "sub_background_id": 4310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『その話、もっと聞かせてほしい』。\r そう頼むと、彼女は少しずつ語り始めた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
