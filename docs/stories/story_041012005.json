      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "そんで、栄養と同じくらい大切なのが運動だ。\r 最近、どうも気になってたんだよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
