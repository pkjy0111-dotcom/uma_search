      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "トレ公、アンタ、ちゃんと運動してるか？　まさか運動は\r アタシたちに任せて、ラク決め込んでるんじゃないだろうね？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "適度にはやってるつもりだけど……",
