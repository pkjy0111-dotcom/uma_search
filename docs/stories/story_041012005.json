    "6": {
      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "健康ってのは、そういうもんじゃないだろ。定番でいいやと思う\r 気持ちを捨てて、ガンガン栄養の幅を広げていかないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
