      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "……すみません。 私がもっと器用にできたら――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "問題ない。そのために作ったメニューだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "そこを重点的にトレーニングしていこう！",
              "flag_type": "",
              "flag": ""
