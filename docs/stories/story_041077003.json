      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "あの、すごいですっ。本当に……その、すごいです！ 何度か走りを見ただけで、ここまで……ありがとうございます！",
