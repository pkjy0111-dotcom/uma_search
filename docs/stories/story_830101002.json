          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "とてもいい夢を見たんだ……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "とてもいい夢を見たの……",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
