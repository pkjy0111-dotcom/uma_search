      "background_id": 50,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "そしてその欲望があらわになった時、 成就を求める想いはいや増していく。",
