      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "なら、君たちはどうだい？ 眠るっていっても数分のことだよ？ ちょっとしたお昼寝みたいなものだよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
