      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "い、いいいいいえっ！　遠慮しますっ！ 夜っ、眠れなくなっちゃうのでっ！！",
