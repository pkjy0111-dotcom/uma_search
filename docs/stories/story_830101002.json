      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "何が欲しいのか！　何を成したいのか！ そのためにどんな力を求めるのか！",
