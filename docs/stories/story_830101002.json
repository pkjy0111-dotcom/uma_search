    "8": {
      "frame_id": 8,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "……ふぅン。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "おや？　ちょうどいいところに。 そこのトレーナー、 1つ実験に付き合ってくれないか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "このドリンクを飲むとね、心地いい眠りと 素晴らしい夢が訪れるんだよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "疲労回復に効果絶大！ なぁに、対価は見た夢の内容を ちょこっと教えてくれるだけでいいよ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "確かに、最近疲れてるかも……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "それは重畳！　さぁさぁ、遠慮なく グイっといってくれたまえ！　グイっと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
