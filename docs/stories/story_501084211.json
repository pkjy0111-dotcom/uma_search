      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『――もしタニノギムレットが、 有馬記念を走ることがあったとしたら』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
