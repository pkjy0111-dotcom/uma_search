      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "俺は自らが望む限り、 あらゆる場所に遍在する。 距離などは、あってないようなものだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
