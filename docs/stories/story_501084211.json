    },
    "11": {
      "frame_id": 11,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90316,
          "speaker_name": "観客B",
          "text": "これで勝ったりしたら、秋シニア三冠……！ 今年『安田記念』勝ってるのに、 もうめちゃくちゃでしょ！　正に“破壊神”！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
