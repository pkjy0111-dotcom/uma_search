    "12": {
      "frame_id": 12,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "――ったく、毎度ハデに楽しませてくれるな！ 距離上等のイカれ具合、アタシは好きだぜ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
