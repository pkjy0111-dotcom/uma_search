      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『シンボリクリスエスたち世代のライバルと、 全身全霊で競う機会があったとしたら――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
