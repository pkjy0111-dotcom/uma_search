    "10": {
      "frame_id": 10,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90310,
          "speaker_name": "観客A",
          "text": "うお～、ようやく実感湧いてきた！ マジでギムレットが今年の『有馬記念』を 走るんだなぁ……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
