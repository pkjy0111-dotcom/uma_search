      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな一抹の不安を胸に、 暴走するゴールドシップを追って パリの街を東奔西走しているうちに――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
