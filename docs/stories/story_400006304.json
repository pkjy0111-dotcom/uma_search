    "6": {
      "frame_id": 6,
      "background_id": 243,
      "sub_background_id": 3110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "てことで、まずはルーヴルまで競走じゃーい！ サモトラケさん家のニケちゃんに チュゥしてもらうもんねーっ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "まずい、追いかけなくては――！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "気紛れなゴールドシップのことだ、 まさか完全に観光気分になって レースを忘れてしまっているんじゃ……？",
          "flag_type": "",
          "flag": "",
          "choices": []
