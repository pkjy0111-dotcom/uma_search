      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "完全にゴールドシップを見失って 独り迷子になってしまった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
