    },
    "5": {
      "frame_id": 5,
      "background_id": 243,
      "sub_background_id": 3110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "無理。ワクワクのウキウキなんだもん。 こうなったらもう アタシにもアタシを止められねー。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
