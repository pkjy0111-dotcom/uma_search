      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "セーヌ川、相棒のアオバスズメダイも 連れてきてやりたかったぜ―― くっ、海水魚でさえなけりゃぁな……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ちょ、ちょっと落ち着いて……",
              "flag_type": "",
              "flag": ""
