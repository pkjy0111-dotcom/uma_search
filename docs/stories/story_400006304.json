      "background_id": 243,
      "sub_background_id": 3110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "うぉっしゃらーい！！！！ ありおりはべり来たぜパリ！！ われいまはしるはしるせりーーッ！！！",
          "flag_type": "",
          "flag": "",
          "choices": []
