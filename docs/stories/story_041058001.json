    "7": {
      "frame_id": 7,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "――は、あ、脚！　机の、脚……！\r そうでした、わわわ、私、私私なんてことを……っ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
