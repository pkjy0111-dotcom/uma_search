    },
    "13": {
      "frame_id": 13,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "ごめんなさい、出来ないくせに挑戦しちゃってごめんなさい！\r 通りすがりのトレーナーさんに迷惑かけて、ごめんなさいぃ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
