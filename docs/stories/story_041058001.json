    "10": {
      "frame_id": 10,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "わ……私、ドジでグズで……な、なんにもできないんです……。\r それで、だから、その……なにか役に立てることないかなって……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "オペラオーさんの机が、ガタガタしてるって聞いたので……\r こっそり、直そうと思ったんです……けど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "そしたら……っ、力みすぎて折っちゃい、まして……！\r 慌てたら脚、手から飛んでっちゃいましてぇ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
