      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "だ、大丈夫、ですかぁ……？\r ごめんなさい、ごめんなさい、ごめんなさいぃ……！",
