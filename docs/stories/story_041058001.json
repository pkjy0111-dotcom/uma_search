    "9": {
      "frame_id": 9,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "脚が1本折れている机があった。\r 半べその彼女をなんとかなだめ、よくよく話を聞いてみると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
