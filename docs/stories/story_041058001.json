      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "ど、どどどどどど、どうしましょう～～……っ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
