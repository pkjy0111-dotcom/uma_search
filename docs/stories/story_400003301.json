      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "上手く歌ったつもりでも、 心がこもっていなければ意味がないと。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "歌に厳しい方だったんですね",
              "flag_type": "",
              "flag": ""
