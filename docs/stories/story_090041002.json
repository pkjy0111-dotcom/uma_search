    "8": {
      "frame_id": 8,
      "background_id": 321,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1004,
          "speaker_name": "マルゼンスキー",
          "text": "……ふふ、始まったわね～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
