      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "……ん？　どうしたシービー、微妙な顔して。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 321,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "……なんか、窮屈だなって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 321,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "書いてて気持ちよくなかった…… 確かに字はそれなりに書けたけど、しっくりこない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
