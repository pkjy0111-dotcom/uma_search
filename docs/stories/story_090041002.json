        {
          "next_frame": 3,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "はは、お見通しってか！ けどいいぜ。練習の成果、見せてやる！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
