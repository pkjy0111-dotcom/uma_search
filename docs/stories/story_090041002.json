      "frame_id": 4,
      "background_id": 321,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "まぁな！　あとは本番までに詰めてこうぜ！ パフォーマンスはどうするかとか、アレンジする部分とか――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
