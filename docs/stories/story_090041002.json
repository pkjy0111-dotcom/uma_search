      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1004,
          "speaker_name": "マルゼンスキー",
          "text": "ちょっと～！　パーペキじゃない！ 文字も綺麗に書けてるし、初めてなのにバッチグーよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
