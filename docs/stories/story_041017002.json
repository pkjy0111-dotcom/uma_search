      "frame_id": 17,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "常に大局的な視点を持ち、万事万端を己が糧とする。\r 高みに至るには、必要不可欠な姿勢だ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "すごいな……",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "すごいね……",
              "flag_type": "female_trainer",
