      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "おや、君は……奇遇だね。 向かいの席は、空いているかな？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "どうぞ",
