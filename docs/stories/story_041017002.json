    "16": {
      "frame_id": 16,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "無論、食事だけでなく、日々のあらゆる場面において、\r 意味を追求する事を心がけているよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
