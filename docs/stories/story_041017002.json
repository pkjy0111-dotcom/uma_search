    "15": {
      "frame_id": 15,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "『現在の自分にとって、最適な食事とは』 ……考え出すときりが無い、重要なテーマだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
