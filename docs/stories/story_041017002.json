    "13": {
      "frame_id": 13,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "こういった事は、常に頭の中を巡っているものでね……。\r つい長々と語ってしまった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "いつもそんなに気を配っているのか？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "いつもそんなに気を配っているの？",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
            }
