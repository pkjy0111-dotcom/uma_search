    "9": {
      "frame_id": 9,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "そうだな、丁度カフェテリアに居るのだし、\r 甘味のひとつでも摂取してはどうだ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
