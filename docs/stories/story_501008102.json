    "5": {
      "frame_id": 5,
      "background_id": 39,
      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "い、いちいち細かいことはいいんだよ！\r こういうのは勢いだろ、勢い。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "じゃあ願い事はない？",
              "flag_type": "",
              "flag": ""
