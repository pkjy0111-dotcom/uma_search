      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "よーし、ここはひとつ頼むぜ、トレーナー。\r 俺をもう1段、カッコよくしてくれ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "カッコよくって、どんな風に？",
