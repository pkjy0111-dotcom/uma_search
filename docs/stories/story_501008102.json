      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "逆にすがるのは――神の方だぜ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "どういう意味？",
              "flag_type": "",
              "flag": ""
