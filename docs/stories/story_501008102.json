      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "いや、あるにはあるけど……神様なんざ\r 叶えてくれるわけねーだろ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
