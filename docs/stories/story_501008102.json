      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "ん？　だけど、よく考えたら\r 叶えられるヤツがここにいるな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
