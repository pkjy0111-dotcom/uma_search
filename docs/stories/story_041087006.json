      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "トレーナーさん？　そろそろ帰る時間です。 いったい何を見て――",
