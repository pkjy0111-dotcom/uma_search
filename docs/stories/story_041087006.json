      "frame_id": 8,
      "background_id": 145,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "大丈夫、大丈夫。たくさん強くなって、活躍しましょう。 そうしたら、たくさんのヒトがマーちゃんを目に映します。",
          "flag_type": "",
          "flag": "",
          "choices": []
