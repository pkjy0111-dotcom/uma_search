      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――正直、グッズ化そのものは難しくはないだろう。 だがここに並ぶほどとなると……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
