      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "んん～？　あわてんぼうのトレーナーさん、めずらしいですね～。 ……ふむ。",
