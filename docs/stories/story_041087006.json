      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "本当に悲しいのは忘れられることです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 145,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "忘れさせない。ずっと覚えていてもらう。 その願いは、なんとしても叶えてあげたい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "ん？あれは……",
              "flag_type": "",
              "flag": ""
            }
          ]
