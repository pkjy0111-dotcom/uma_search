      "frame_id": 9,
      "background_id": 145,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "映したらガツンッです。マーちゃんのラブリーさに 観客席のみなさんもお茶の間のみなさんも、ハートをガシッです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 145,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "言葉も空気もフワフワとして柔らかい雰囲気だが、 その想いはどこまでも一途で強い。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
