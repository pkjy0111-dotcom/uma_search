      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "昔、この店に来たことがあることと、 当時食べたさくらんぼパイを売っているかを 店主に尋ねた。",
          "flag_type": "",
          "flag": "",
          "choices": []
