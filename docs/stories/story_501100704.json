      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 0,
          "speaker_name": "ケーキ屋の店主",
          "text": "ええ、材料を仕入れていないもので。 せっかく来ていただいて悪いんですが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
