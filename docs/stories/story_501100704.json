      "frame_id": 11,
      "background_id": 191,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "ケーキ屋の店主",
          "text": "レシピもいくつか受け継いではいますが、 さくらんぼパイは――今は出していませんね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "そうなんですか？",
