      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 0,
          "speaker_name": "ケーキ屋の店主",
          "text": "さくらんぼパイですか……多分、 祖母がやっていた頃のメニューですね。 実はこのお店、祖母から受け継いだもので。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
