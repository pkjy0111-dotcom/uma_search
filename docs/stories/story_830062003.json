    "6": {
      "frame_id": 6,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "合わせ練習、楽しかったですね～！\r みなさんのダンス、かっこよかったです！",
          "flag_type": "",
          "flag": "",
          "choices": []
