      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "そうだね。せっかくだし、\r 『Never Looking Back』で合わせて みよっか。ダンスチームも呼んでくるね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
