      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "……なんだかすごく、ドキドキしたわ。\r 客席からは私たちがあんなふうに見えるのね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
