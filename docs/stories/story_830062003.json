      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "ねえ、この衣装で動きづらいところが\r ないか、確認してみない？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
