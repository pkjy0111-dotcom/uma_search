    },
    "7": {
      "frame_id": 7,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1002,
          "speaker_name": "サイレンススズカ",
          "text": "ええ……鏡に映った一面の青……。\r みんなが踊ると、青と白が波のように揺れて。",
          "flag_type": "",
          "flag": "",
          "choices": []
