      "frame_id": 8,
      "background_id": 245,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "おい、エイの裏側みてーな顔して 突っ立ってねーでもっと ゴルシちゃん様を崇め褒め愛でまくれよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "すごい！歴史に名を刻むレベルですごい！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
