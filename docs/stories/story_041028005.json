      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "あははっ、大袈裟だな～。",
          "flag_type": "",
          "flag": "",
          "choices": []
