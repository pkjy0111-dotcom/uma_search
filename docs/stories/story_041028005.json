      "frame_id": 9,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "とりあえずその荷物は置いて……始めよっか！\r おいしいおいしい、昆布とり～！",
