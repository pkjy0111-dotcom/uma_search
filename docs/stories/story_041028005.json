      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "魚介もとっていいって！\r トレーナーさんは、鳥に取られないよう見張っててね。",
          "flag_type": "",
          "flag": "",
          "choices": []
