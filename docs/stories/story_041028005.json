      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "うーん、気持ちいい！\r 絶好の昆布とり日和だね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
