      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "それにしても……トレーナーさん、大荷物！\r なにをそんなに持ってきたの？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "大量のお水に、火打金に……\r あっ、食べられる野草図鑑もある！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "前回、島に取り残されたから……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "あははっ。そうだったねー。\r トレーナーさん、ここに倒れてたんだった！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "あの時は、びっくりしたな～。\r 目を開けなかったら、どうしようかと思ったよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "あの時は君が女神に見えたよ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "あの時は貴方が女神に見えたよ",
              "flag_type": "",
              "flag": "female_trainer"
