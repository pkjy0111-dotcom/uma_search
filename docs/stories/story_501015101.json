      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おおおっ、ここはどこだ？　ボクは誰だ？\r まさか……見知らぬ世界へ転生した！？",
