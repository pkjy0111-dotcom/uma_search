      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "異世界転生！　なるほど、よくわからないが\r いかにも“覇王”らしい響き！",
