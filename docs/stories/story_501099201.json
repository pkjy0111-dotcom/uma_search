      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――ありがとうございます。 でも、大丈夫です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
