      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "SNSでは、彼女を応援するファンや苫小牧の 人たちの声がたくさんある。これを見せれば、 ホッコータルマエの緊張も解消されるはずだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……応援している人の声を、見てみないか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……応援している人の声を、見てみない？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "4": {
