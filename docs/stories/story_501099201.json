      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "苫小牧の人たちに…… 強い私の姿を見せて、安心させたいから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
