      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "緊張は、してます。 半端な走りになったらどうしようって、 怖い気持ちもあります。それでも――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "決めていたんです。 この『レパードステークス』は、 自分の力でがんばろうって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
