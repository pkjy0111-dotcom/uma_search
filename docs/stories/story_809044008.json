      "background_id": 68,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "青い瞳のウマ娘",
          "text": "似たような感じで、私たちも色々と活動中 なんです。引退を考えてる子たち向けに、 こういう道もあるよってセミナーを開いたり。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "じゃあ……もしかして、君たちは……？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "じゃあ……もしかして、貴方たちは……？",
              "flag_type": "",
              "flag": "female_trainer"
            }
