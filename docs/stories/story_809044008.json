      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "いえいえ、お気になさらず。 私も、皆さんの活動を応援したいですから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "すみません、彼女たちの活動って……？",
