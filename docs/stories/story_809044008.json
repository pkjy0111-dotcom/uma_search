      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "私は配信活動を通して、ウマ娘たちに レース以外のスポーツを楽しむって道を 示したいと考えてますが――",
          "flag_type": "",
          "flag": "",
          "choices": []
