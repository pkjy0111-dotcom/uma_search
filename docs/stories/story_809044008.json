      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "えっと。ざっくりまとめると、 『引退ウマ娘たち向けの支援活動』ですね！",
