      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "店員A",
          "text": "も、申し訳ございませんっ。 すぐにお持ちしますので！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
