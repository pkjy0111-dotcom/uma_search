      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "お兄さん、土下座はいいじゃないっすか。 店員さんちゃんと謝ってるし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
