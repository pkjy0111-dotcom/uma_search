      "frame_id": 7,
      "background_id": 97,
      "sub_background_id": 101,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "みんなの前で土下座させることの方が、 よっぽど無礼だと思いますよ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
