    "6": {
      "frame_id": 6,
      "background_id": 97,
      "sub_background_id": 101,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "客A",
          "text": "あん？　俺はな、親切に教えてやってんだ！ 客商売の礼儀ってやつをよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
