      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "客A",
          "text": "その前に土下座だ、土下座しろ！ それが礼儀ってもんだろうが！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "（そこまで要求しなくても……）",
