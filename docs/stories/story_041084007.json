      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "欠けたページの一片が、ぴたりと嵌った。 本日はこれにて去るとしよう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "これからの予定は決まってるのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "これからの予定は決まってるの？",
              "flag_type": "",
              "flag": "female_trainer"
