    },
    "7": {
      "frame_id": 7,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女が追い求めるのは――『瞬間』。 それは夏合宿中でも変わらないようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
