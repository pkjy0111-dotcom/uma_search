      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9042,
          "speaker_name": "バイアリーターク",
          "text": "よくこの舞台にたどり着き、走り切った。 お前たちの走りは、我々の期待を 充分に満たすものだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
