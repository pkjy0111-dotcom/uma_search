      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "（ううん、よくない。もらった恩を返さない……。父さんも\r 父さんのお弟子さんたちも、そんなことだけは死んでもしない）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
