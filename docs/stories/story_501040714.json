      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "けど、ただでさえアタシ、モデルの仕事で\r 他の子より練習時間短いのにっ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
