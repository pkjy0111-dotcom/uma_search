      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "なにもせず休むだけなんて、\r 焦りでおかしくなりそうだよ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
