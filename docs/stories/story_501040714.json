      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "疲れてる自覚はあったのに、練習してないと\r 前に進めてない気がして、不安でさ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
