      "frame_id": 3,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "あ、でも大丈夫、練習は続けられるから！\r 医者が大げさに言っただけだと思うし。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "今は安静にしてほしい",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "気合いでケガを乗り切ろう！",
              "flag_type": "",
              "flag": ""
