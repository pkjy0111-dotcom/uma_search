      "frame_id": 23,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ヤマニンゼファーというウマ娘を見てきた自分が、 今、トレーナーとしてできることは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 24,
              "loop_ends_at_frame": -1,
              "text": "もっと自由になろう、ゼファー",
