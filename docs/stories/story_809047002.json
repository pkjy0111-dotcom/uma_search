      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 9047,
          "speaker_name": "スピードシンボリ",
          "text": "シリウスもまた、可愛らしかったなぁ！ 今よりはだいぶ素直で、無邪気で…… いや、今もある種、とても素直ではあるかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
