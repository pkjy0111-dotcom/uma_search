      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "ふふ、勿体ないお言葉です。",
          "flag_type": "",
          "flag": "",
