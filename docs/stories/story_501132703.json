      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1132,
          "speaker_name": "ラヴズオンリーユー",
          "text": "歯医者さんも無理ッ、もう音が怖いの！！ あんな怖いドリルで歯を、 け、削られるなんて～～！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "虫歯がなければドリルは使わないよ",
