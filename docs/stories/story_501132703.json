      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "昨日、書類の整理をしていた時――書棚の 隙間から、ラヴズオンリーユー宛のハガキが 落ちてきた。歯科の定期健診のご案内だ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "消印は二か月前……さては隠してたな？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 122,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1132,
          "speaker_name": "ラヴズオンリーユー",
          "text": "うう……今になって見つかるなんて……！ ソファの隙間に隠せばよかったぁ～！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "そもそも隠しちゃダメです",
              "flag_type": "",
              "flag": ""
