          "character_id": 1132,
          "speaker_name": "ラヴズオンリーユー",
          "text": "――どうしてぇっ！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ふっふっふ……作戦だよ",
