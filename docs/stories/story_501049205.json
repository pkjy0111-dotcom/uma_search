      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『私はあなたに 生きていてほしいと思っています』",
          "flag_type": "",
          "flag": "",
          "choices": []
