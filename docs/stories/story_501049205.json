      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『手紙を書くの、久々な気がします。 年賀状以外で手紙を渡すなんて、 いつぶりのことでしょうか』",
          "flag_type": "",
          "flag": "",
          "choices": []
