    "7": {
      "frame_id": 7,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『私はあなたと賭けをしました。 あなたに未来の希望を抱かせると』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
