    "3": {
      "frame_id": 3,
      "background_id": 121,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『今回、あなたと面と向かって話すと うまく言葉にできそうになく。 筆をとりました。ご容赦ください』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 121,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『あと言葉の間違いがあっても 許してください。あなたの授業を卒業して 久しいので、忘れている内容もあるのです』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 121,
      "sub_background_id": 110,
