    "8": {
      "frame_id": 8,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『しかし、ご存じのとおり夏を過ぎても。 秋が深まろうとも。私への評価は 目標には至っていないと思います』",
          "flag_type": "",
          "flag": "",
          "choices": []
