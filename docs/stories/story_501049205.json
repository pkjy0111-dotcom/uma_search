      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『さて、本題ですが』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
