      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9003,
          "speaker_name": "乙名史記者",
          "text": "はい、わかっております。あくまで彼女の\r 調子だけを扱わせていただきますので。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
