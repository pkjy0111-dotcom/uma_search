      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9003,
          "speaker_name": "乙名史記者",
          "text": "ええ、記者ですので最低限の知識は\r 持ち合わせております。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
