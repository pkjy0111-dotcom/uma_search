      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "どうやら彼女――雑誌記者の乙名史さんは\r 取材に来たようだが、それにしても……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "ウマ娘への知識が深いんですね",
              "flag_type": "",
              "flag": ""
