      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "中堅トレーナーA",
          "text": "あはは……参ったなぁ。\r まだ公表してないので、オフレコで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
