      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9003,
          "speaker_name": "乙名史記者",
          "text": "あ、<username>トレーナー。\r お疲れ様です。",
          "flag_type": "",
          "flag": "",
