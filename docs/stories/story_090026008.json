      "frame_id": 17,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『今年のミューズはオーディションではなくオファー形式』 というサプライズは、大きな話題を呼んでいた……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "18": {
      "frame_id": 18,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "ハァイ、みんな☆　来てくれてありがとう！ あの時は本当に、本当～に、お世話になったわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
