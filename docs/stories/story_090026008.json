      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "っ！？　褒められてんだか脅されてんだか…… よくわかんねーけど、わかったよ！",
