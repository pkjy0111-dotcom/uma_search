      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "おお～……！　深ぇじゃねーかっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
