      "frame_id": 23,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "そんなアナタたちにアタシの勝負服がプロポーズしたがっている。 どうか、『今年のミューズになって欲しい』って……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
