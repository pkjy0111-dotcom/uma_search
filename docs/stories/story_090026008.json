      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "アナタたちに恋して、グレードアップした勝負服が待っているわ。 エンゲージ、してくれる？　さぁ、返事を聞かせてちょうだい――！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
