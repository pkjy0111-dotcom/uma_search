      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――それから、しばらく経ち…… ついに『ビューティードリームカップ』が開催された。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
