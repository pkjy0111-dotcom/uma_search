      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "だって、チャンスを掴めるかって自分次第だもの。 その時が来るって信じて、全力でトレーニングして、待ってたわ。",
