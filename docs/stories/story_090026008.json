    "2": {
      "frame_id": 2,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "――待たせたわね☆　注文の多いポニーちゃん♪ さぁご覧なさい、これがニューデザインよっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
