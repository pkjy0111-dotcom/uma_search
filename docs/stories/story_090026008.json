      "frame_id": 21,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "だけど……それこそが、本気の証だった。 そう！　『夢』も『好き』も、本気だから苦しいのっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
