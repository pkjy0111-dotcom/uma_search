      "frame_id": 20,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90032,
          "speaker_name": "ビューティー安心沢",
          "text": "勝利を、勝負服を求めて、何度ダメでも立ち上がる姿。 『どうしてそこまでするの？』って、胸がチクリとしたわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
