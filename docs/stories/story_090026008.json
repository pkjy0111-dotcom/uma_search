    "13": {
      "frame_id": 13,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "ウオッカも、ですよ？　きっかけ1つで戻って来た、あなたも。 とっくに夢に絡めとられているんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
