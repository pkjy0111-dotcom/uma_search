    "11": {
      "frame_id": 11,
      "background_id": 33,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女はどうやら、自分の作ったスケジュールに絶対の自信があり、\r その通りに進行することにこだわっているらしい。だが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "予定外のことが起こったらどうするんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "予定外のことが起こったらどうするの？",
              "flag_type": "",
              "flag": "female_trainer"
