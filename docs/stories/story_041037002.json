      "background_id": 33,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "いえっ……ふぅ。……支障はありません。\r 休憩が短いのはスケジュールで決めていたことなので。",
          "flag_type": "",
          "flag": "",
          "choices": []
