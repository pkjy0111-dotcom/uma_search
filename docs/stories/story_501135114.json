      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "もっとこう…… 魂の輪郭が似ている、とでもいうべきか――",
          "flag_type": "",
          "flag": "",
