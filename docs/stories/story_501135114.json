      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "ま、よくわからんがそんな感じだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "そ、そういうのもあるんだな……",
              "flag_type": "",
              "flag": "male_trainer"
