          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "――ふぅ。 いい青だな……――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "よし、いたいた",
