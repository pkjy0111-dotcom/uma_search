    },
    "8": {
      "frame_id": 8,
      "background_id": 111,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "で、何か掴んだのか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "君が求めるべき強さを、具体化してみた",
              "flag_type": "",
              "flag": "male_trainer"
