    "7": {
      "frame_id": 7,
      "background_id": 111,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "ほう。 ……聞かせてもらおうかな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
