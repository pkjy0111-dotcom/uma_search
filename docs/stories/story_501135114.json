      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "お～、トレーナーか。 あんたも空、見にきたのか？",
