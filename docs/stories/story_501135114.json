      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "今日は比較的早く彼女を見つけられた。 少しずつではあるが、彼女のいる場所の 目星をつけるのが早くなっている気がする。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "ブレイクスルーについて、少し話したい",
