      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "自分は、ステイゴールドの実力を高める ブレイクスルーを求め、ある調査をしていた。",
