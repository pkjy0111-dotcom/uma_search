      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "わかった、夕食は君に任せてみよう。 遠慮なく尽くしたまえ！",
