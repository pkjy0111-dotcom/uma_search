    "16": {
      "frame_id": 16,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "これまでの実験を経て、尽くすという行為が私への アディクションとなっている可能性も否定できないな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
