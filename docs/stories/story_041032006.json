    "17": {
      "frame_id": 17,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ククッ、いいだろう。 私の時間が奪われるわけでもなし、悪くない提案だ。",
