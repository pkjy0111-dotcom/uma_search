    "8": {
      "frame_id": 8,
      "background_id": 220,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1114,
          "speaker_name": "ブエナビスタ",
          "text": "次に行ったのはカフェですよね。 廊下も捜しながら、行ってみましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 223,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "廊下もくまなく捜した。 カフェテリア中も捜させてもらった。 だが、見つからない……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "ここまで捜してもないなら、もう……",
