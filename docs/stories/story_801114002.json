      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1114,
          "speaker_name": "ブエナビスタ",
          "text": "なにか、捜しものですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "あ……実は、万年筆を落として",
              "flag_type": "",
              "flag": ""
