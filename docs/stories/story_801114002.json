      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ひとまずもう1度、最後に使った場所からの 動線を辿ってみることにしたのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "ないね……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
