      "frame_id": 7,
      "background_id": 220,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1114,
          "speaker_name": "ブエナビスタ",
          "text": "そうですね……それなら、 ここで万年筆を使って、ポケットに しまったところまでは確かみたいです。",
          "flag_type": "",
          "flag": "",
          "choices": []
