    "4": {
      "frame_id": 4,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーナー試験に合格した時に―― 自分へのご褒美として、夢の証として、 かなりフンパツして購入した宝物だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1114,
          "speaker_name": "ブエナビスタ",
          "text": "そんな大切なものを……！？ それは一大事です。私も、一緒に捜します！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
