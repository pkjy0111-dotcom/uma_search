      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "とんでもなく強かったです。\r これがスターウマ娘なんですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
