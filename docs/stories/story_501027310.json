      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "なんて強い……。そして、なんて魅せる\r レースなんだ。わざと？　それとも才能？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "お疲れ様、ライアン",
              "flag_type": "",
              "flag": ""
