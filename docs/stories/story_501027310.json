      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "あ、トレーナーさん。ありがとうございます。\r そして……すみません。",
