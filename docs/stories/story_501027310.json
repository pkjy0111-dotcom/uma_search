      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "はぁ、はぁ……ダメだ……。いけるかと\r 思っても、オグリさんはさらに……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
