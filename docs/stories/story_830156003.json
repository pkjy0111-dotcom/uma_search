      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "確かに、ちょっと傷んでるねぇ。 ……もしかしたら気候のせいかしら。 ここんところ、不安定だったからねぇ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
