      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "うぅ、あんなズバズバ呼ばれるとなんか頭に残っちゃって……！ ごめんね～。",
          "flag_type": "",
          "flag": "",
          "choices": []
