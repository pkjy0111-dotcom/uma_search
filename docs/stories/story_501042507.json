      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "名づけて、自家製フルーツの ハンティングケーキ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
