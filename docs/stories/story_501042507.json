      "frame_id": 15,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 0,
          "speaker_name": "ファンの店主",
          "text": "あ……そう、ですね、1度食べれば ハマってくれる方もいるんですが…… 正直、人気というほどではなくて。",
          "flag_type": "",
          "flag": "",
          "choices": []
