      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "例えば、そうね……ケーキの上に盛る フルーツを、お客さんに好きに選んで 自分で収穫してもらうとか！",
          "flag_type": "",
          "flag": "",
          "choices": []
