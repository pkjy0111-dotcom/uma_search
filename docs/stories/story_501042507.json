      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "ンン～～～～デリシャス！！ 最高にジューシーな味わいね！ これ、とっても人気なんじゃない！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
