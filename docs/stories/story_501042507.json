      "frame_id": 16,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "あら……それはあまりにもったいないわ！！ だったらもっと大々的に、 話題性のある売り方をしてはどうかしら！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
