      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 0,
          "speaker_name": "ファンの店主",
          "text": "パールさんはやっぱりすごいです！ いつもエキセントリックな発想で、面白い 挑戦をされている……改めて、尊敬します！",
          "flag_type": "",
          "flag": "",
