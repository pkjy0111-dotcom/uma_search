      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "フフ、わざわざ筆をとってまで私に想いを 伝えてくれるなんて……その熱いパッション、 しっかりと受け止めなくてはね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "エクセレント！ 私に触発されて、喫茶店の経営に チャレンジすることにしたんですって！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "『パールさんが挑戦し続ける姿を見て、 自分も何かを成し遂げたいと思いました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "『開店当初は大変でしたが、やっと軌道に 乗ってきました。よければパールさんも 食べに来てください』ですって……素敵だわ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "パールの頑張りが、勇気を与えたんだ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "フフ、そうね……嬉しいわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "それじゃ、この人のお店に行ってみましょう！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "えっ、今から！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "ご招待ありがとう、食べに来たわよ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 0,
          "speaker_name": "ファンの店主",
          "text": "えっ……ええええ！？ パ、パールさ……い、いらっしゃいませぇ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "このお店のイチオシは何かしら？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "ファンの店主",
          "text": "は、はいっ！　ええと、当店は自家製の フルーツにこだわってまして……それを ふんだんに盛ったケーキがオススメです！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 0,
          "speaker_name": "ファンの店主",
          "text": "お店のすぐ裏に畑があるので、 毎朝獲れたてのフルーツで作っております！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 148,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "素敵ね！　それをいただくわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
