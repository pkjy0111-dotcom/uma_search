      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1046,
          "speaker_name": "スマートファルコン",
          "text": "せっかくだし、一緒にスイーツ食べたい！\r どうかな？　ブルボンちゃん♪",
