      "frame_id": 13,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "では、手始めのオペレーションとして――\r 正面店で販売中の全42品完食を提案します。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "そ、そんなに食べたら体重が……！",
