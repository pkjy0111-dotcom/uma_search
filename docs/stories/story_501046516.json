      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "本日は、より優れた品を吟味するため、\r 専門店が集まるこちらへ足を運びました。",
          "flag_type": "",
          "flag": "",
          "choices": []
