      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1046,
          "speaker_name": "スマートファルコン",
          "text": "ってことは、リサーチだ！\r ファル子もそのために来たんだよ～☆",
