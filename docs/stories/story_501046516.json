      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1046,
          "speaker_name": "スマートファルコン",
          "text": "やっぱりブルボンちゃんだ！\r ここにいるってことは、スイーツがお目当て？",
