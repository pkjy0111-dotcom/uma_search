        {
          "next_frame": 6,
          "character_id": 90103,
          "speaker_name": "通りすがりのウマ娘A",
          "text": "わ！　このバイオリン弾いてたのって、 ダイイチルビーさんだったんだ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
