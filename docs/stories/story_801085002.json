      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90104,
          "speaker_name": "通りすがりのウマ娘B",
          "text": "やば～、なんか耳からゴージャスになれそ…… は～、さっすが『華麗なる一族』……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
