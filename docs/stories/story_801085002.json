      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "話しながらも行き過ぎていったウマ娘たち。 その足音が遠ざかってもなお、 しばらくの間聴きほれていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
