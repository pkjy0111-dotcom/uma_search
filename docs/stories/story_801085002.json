        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ある日、空き教室前の廊下を 歩き過ぎようとした時のこと――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "（これは……バイオリンの音？）",
              "flag_type": "",
              "flag": ""
