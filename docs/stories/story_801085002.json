    },
    "2": {
      "frame_id": 2,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "立ち止まり、耳を澄まさずにはいられない―― そんな美しく繊細なバイオリンの音色が、 鼓膜をシン、と震わせた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "音楽に関してはまったくの素人な自分でも、 直感的に理解させられる。 すさまじいまでの練度だ……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "いったい誰が……！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
