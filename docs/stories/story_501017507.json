      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "私は、私を走らせてくれるファンの想いを、\r 余すことなく抱えてゆきたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
