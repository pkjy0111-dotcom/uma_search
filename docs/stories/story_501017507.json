      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ありがとう。\r じっくり読ませてもらうよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
