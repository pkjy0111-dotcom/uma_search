      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "その間に、これらを読ませてもらえないか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "もちろん！",
