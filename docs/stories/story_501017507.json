      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "それが、私の――\r ウマ娘の、流儀というものだからな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
