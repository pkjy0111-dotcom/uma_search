      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……トゥインクル・シリーズを走るウマ娘は、\r レースを通して、ファンに様々な夢を見せる。",
          "flag_type": "",
          "flag": "",
          "choices": []
