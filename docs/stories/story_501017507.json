      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "一方で私たちは、ファンの応援があることで、\r 自分たちの夢に向けて走ることができる……。",
          "flag_type": "",
          "flag": "",
          "choices": []
