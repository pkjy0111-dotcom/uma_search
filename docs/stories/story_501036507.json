      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "オレがオレのファンをどう思っていようと、 オレの勝手だろ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
