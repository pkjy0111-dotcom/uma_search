      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "1通のハガキが床に落ち、 意図せずその文面が目に飛び込んできた。 そこには……。",
          "flag_type": "",
          "flag": "",
          "choices": []
