      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーニングを終えてから、 早速ファンレターを保管用の箱に移動させる。 すると――",
          "flag_type": "",
          "flag": "",
          "choices": []
