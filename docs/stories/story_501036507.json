      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "その代わり、相手がどう思っていようと 構わねェ……限りなく対等じゃねェか、なァ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
