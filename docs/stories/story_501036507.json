      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "それは、そうかもしれない。 だがそれなら――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "なら、手紙は保管しておく",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
