    },
    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "ア゛ァ？ だから、いらねェ世話だって――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "君のトレーナーとして、対等だろ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "貴方のトレーナーとして、対等でしょ？",
