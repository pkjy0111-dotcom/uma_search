      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "オイ、プロならてめェもプロの仕事しろ。 私情を持ち出すンじゃねェ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
