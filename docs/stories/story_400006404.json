      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "誰よりも強くなれると 佐岳メイに認めてもらえた時…… きっとその時が『凱旋門賞』を獲れる日だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
