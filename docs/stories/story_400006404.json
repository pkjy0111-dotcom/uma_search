      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "同じ目的を持つ者同士での切磋琢磨は、 お互いをより高めてくれる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
