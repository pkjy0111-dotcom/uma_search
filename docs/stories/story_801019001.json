      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "激エモかはわからないが、\r トレーニング中の<chrname>の話を 聞かせてあげた。",
          "flag_type": "",
          "flag": "",
          "choices": []
