      "frame_id": 12,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ハァハァ、燃料感謝……！　今日は出直して、\r 明日こそはウマ娘ちゃんを愛でてみせますよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
