      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "えへへ、お参りが効いたのかな。\r 早速いいことがありました！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
