      "dialogue": [
        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "最近調子が上がらないサクラチヨノオーと\r 神社にやってきた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
