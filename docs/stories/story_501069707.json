      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "まあ、それはとても楽しそうですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
