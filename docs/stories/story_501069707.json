    "16": {
      "frame_id": 16,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "肉球に絵の具を塗って、\r アートを作るみたいです……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
