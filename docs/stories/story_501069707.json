      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "大変です！　犬カフェでイベントを\r やってるみたいなんですが、その内容が――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
