      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "はいっ！\r 想像しただけでかわいいが溢れてきます！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
