      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1091,
          "speaker_name": "ヴィブロス",
          "text": "パパもママも、お姉ちゃんたちもすっごく優しくて、 いっぱい甘やかしてくれて……大好きなんだ。",
          "flag_type": "",
          "flag": "",
