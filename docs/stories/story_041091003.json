          "character_id": 1091,
          "speaker_name": "ヴィブロス",
          "text": "……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "って、ヴィブロス！？",
