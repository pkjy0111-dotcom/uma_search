      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "公園にて……ふと見かけたのは伝説級の 名ウマ娘、“初代三冠”セントライト。 ……と――",
          "flag_type": "",
          "flag": "",
          "choices": []
