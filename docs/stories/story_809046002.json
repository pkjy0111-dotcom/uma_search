    "3": {
      "frame_id": 3,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『ねーこれどこ行くの？』『わかんない！』 『でも楽しそうだし、僕もついてこ～！』 『うにゃ～ん』『クルッポー、ポッポー……』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "（……ブレーメンの音楽隊……？）",
              "flag_type": "",
              "flag": ""
