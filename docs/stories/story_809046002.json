      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9046,
          "speaker_name": "セントライト",
          "text": "卿も、加わられる？",
          "flag_type": "",
          "flag": "",
          "choices": []
