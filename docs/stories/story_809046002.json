    },
    "4": {
      "frame_id": 4,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女の後ろには、なぜか、列ができていた。 無邪気に笑う子どもたちや野良猫、野鳥…… たった今、リスらしき存在も加わった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "（リス、いたんだ、この公園……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
