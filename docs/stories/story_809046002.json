      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ゆったりとそう尋ねられ…… 自分でもどうかと思うのだが、 気づけば列に加わってしまっていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
