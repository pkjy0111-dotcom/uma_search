      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "さあ、飲むがよい！ ワシお手製――出汁コーラじゃ！",
