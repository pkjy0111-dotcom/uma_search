      "frame_id": 8,
      "background_id": 139,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "ずーっと思っておったんじゃ……日の本が誇る食文化と、コーラを 掛け合わせたら、とんでもないものが生まれるのではないかとな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
