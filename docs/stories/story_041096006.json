      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "にゃーっはっはっは！　ここで傾かずしていつ傾く！？ ほれ、トレーナー！　はよう宴に参加しようぞ！",
          "flag_type": "",
          "flag": "",
          "choices": []
