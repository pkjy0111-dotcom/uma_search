      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "だが、それでもこの両手を鳴らし続けよう！ ベニッッッシモ！！！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "やあ、アースも来ていたんだな！",
              "flag_type": "",
