      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "アモーレ！ 知らなかったよ、君もコンチェルトを嗜む人だったとは！",
