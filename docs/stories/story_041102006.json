          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "君のトレーナーとして音楽に触れようとね",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "貴方のトレーナーとして音楽に触れようとね",
              "flag_type": "",
              "flag": "female_trainer"
