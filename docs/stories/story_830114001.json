      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "いえ。では、失礼いたします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
