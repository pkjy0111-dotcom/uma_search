      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90640,
          "speaker_name": "アナウンサー",
          "text": "『これについて、ファンの間ではさまざまな 憶測が飛び交い話題になっていますが…… スタジオの皆さんはどうお考えでしょうか？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
