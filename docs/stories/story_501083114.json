    "4": {
      "frame_id": 4,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90730,
          "speaker_name": "コメンテーターA",
          "text": "『過密なローテーションでしたからね～。 秋に向けた充電期間に入ったのでは？』",
          "flag_type": "",
          "flag": "",
          "choices": []
