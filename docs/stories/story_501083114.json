      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……正直、シンボリクリスエスよりも タニノギムレットのほうが今後を 期待されていると言ってもいい状況だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
