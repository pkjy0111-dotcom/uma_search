      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『日本ダービー』以降、タニノギムレットの 注目度は上昇し続けており、人々は彼女の 今後に熱い眼差しを向けている……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "（……『熱狂』、か）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
