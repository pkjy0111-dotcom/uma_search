    "5": {
      "frame_id": 5,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90730,
          "speaker_name": "コメンテーターA",
          "text": "『とはいえ、あれだけの強い走りを我々に 見せてくれたギムレットのことです。 秋以降も大活躍は、間違いなしでしょう！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90720,
          "speaker_name": "コメンテーターB",
          "text": "『それに彼女には、他のウマ娘にない ユニークな魅力がありますからね！ コアなファンも多く、かくいう私も――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
