      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "トレーナー……ヤベぇな、アイツ……。\r 開き直って、全部1番を目指してきやがった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
