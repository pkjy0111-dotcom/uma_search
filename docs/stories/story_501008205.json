    "10": {
      "frame_id": 10,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "正直に言いなさい。どう？　アタシに\r 勝ちたくて勝ちたくて、仕方ないんでしょ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
