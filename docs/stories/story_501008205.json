    },
    "11": {
      "frame_id": 11,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "ああ、勝ちたいね。春のテメエは口だけの\r ガキだったが――今は違う。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
