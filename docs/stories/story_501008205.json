      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "どんなトレーニング積んできたのか\r 知らねえが、相手にとって不足はねえ。",
