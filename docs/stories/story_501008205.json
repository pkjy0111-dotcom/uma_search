    },
    "12": {
      "frame_id": 12,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "ったく、何がそこまでさせたんだか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
