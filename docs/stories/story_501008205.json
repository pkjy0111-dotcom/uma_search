      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "速さも、スタミナも、競り合いの強さも、\r ハートも！　覚悟してなさいよ、ウオッカ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
