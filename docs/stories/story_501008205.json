      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ウオッカ！　どうやら今日は逃げずに\r 来たようね！",
          "flag_type": "",
          "flag": "",
          "choices": []
