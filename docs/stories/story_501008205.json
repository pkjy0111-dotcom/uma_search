      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "コイツと走りたい！　負けたくない！\r 2度と袖になんかされたくない！",
