    "13": {
      "frame_id": 13,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "こうまでさせたのは――アンタでしょ。\r 置いていかれてたまるもんですか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
