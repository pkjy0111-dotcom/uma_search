      "frame_id": 15,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "そしてアタシが1番勝ちたいのは――\r アンタよ。",
