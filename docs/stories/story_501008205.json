      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "はぁ、はぁ……。当然よ。なにもかも\r 1番じゃなきゃ、気が済まないわ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
