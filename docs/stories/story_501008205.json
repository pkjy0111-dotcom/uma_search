    "14": {
      "frame_id": 14,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "悔しがらせたのはアンタ！\r 目覚めさせたのはアンタ！ 本気にさせたのはアンタ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
