    "12": {
      "frame_id": 12,
      "background_id": 11,
      "sub_background_id": 6130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "皆さんに思い切り遊んでいただきたく、 等身大のコースをご用意いたしました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
