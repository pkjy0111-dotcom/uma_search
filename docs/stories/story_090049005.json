      "sub_background_id": 0,
      "cg": "story_still_090049005_00",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "特別なゲームを用意しているマスもありますので、 楽しみにしていて下さいね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 174,
      "sub_background_id": 0,
      "cg": "story_still_090049005_00",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "我々運営係はガイド役を務めます。プレイ中、 どうぞお気軽にお声かけ下さい。そして『ゲームマスター』は――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "18": {
      "frame_id": 18,
      "background_id": 174,
      "sub_background_id": 0,
      "cg": "story_still_090049005_00",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "余が務める。存分に遊興に耽るがよい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
      "background_id": 174,
      "sub_background_id": 0,
      "cg": "story_still_090049005_00",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "それでは皆さん、いってらっしゃい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "20": {
      "frame_id": 20,
      "background_id": 7,
      "sub_background_id": 17130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "皆さん、順調に進んでいらっしゃいますねぇ……。 さて、そろそろ最初のスペシャルポイントですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
