      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 90103,
          "speaker_name": "参加者のウマ娘A",
          "text": "1、2、3っと！　『ここでスペシャルマス』？ ええと、教室へ入れ……。",
