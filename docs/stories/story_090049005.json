      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 40,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "アンタ、ポーカーは初めてか？　いいぜ、教えてやろう。 何、怖がることはない。ビギナーズラックってのも結構あるもんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
