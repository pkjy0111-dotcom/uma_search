      "frame_id": 35,
      "background_id": 6,
      "sub_background_id": 18130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 36,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "これを私とアンタで互いに選んで飲む。もちろん美味かったほうが 勝ちだ。これを5ゲーム中、3本先取で勝利。簡単だろう？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "36": {
      "frame_id": 36,
      "background_id": 6,
      "sub_background_id": 18130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 37,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "さあ――選んでくれ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "37": {
      "frame_id": 37,
      "background_id": 6,
      "sub_background_id": 18130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 38,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "4ゲーム目、アンタのが美味かったら勝ちだ。 せーの――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "38": {
      "frame_id": 38,
      "background_id": 6,
      "sub_background_id": 18130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 39,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "んぐっ……ハァァーーーー酸っ……ぺぇ……。 ……見事だぜお嬢さん。プレゼント、持っていきな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
