      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "カフェテリアの入り口を見な！　『スタート地点』があんだろ？ こっから学園全体使って、ドカーンとすごろくで遊ぼうぜ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
