      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "なぜか突然演奏を始めたサウンズオブアースは、 1曲弾ききると、戸惑うこちらに対し優雅に一礼した。",
          "flag_type": "",
          "flag": "",
          "choices": []
