    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "今の演奏はどうだったかな？　感想を教えてくれるかい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "なんだか荒々しくて、激しい曲……かな？",
