      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "しかし、鋭い末脚を持つ子もいる。彼女の得意な差しで 様子を伺うのが安牌か？　どうにも決め手に欠ける……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "（というか……なぜヴァイオリンを……？）",
              "flag_type": "",
              "flag": ""
