          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "追込の子が多いから、先行にすべきか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "追込の子が多いから、先行にすべき？",
