      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "デビューした後は必然的に、 この同期たちと争う機会が多くなる。 データをまとめておいて、損はないのだが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "（“記憶”だと、どうなっているんだろう）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
