      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "自分はトレーナー室で、ステイゴールドと デビュー時期が同じになるウマ娘…… いわゆる『同期』のデータをまとめていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
