        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "最近調子が上がらないヤエノムテキと、\r 神社にやって来た。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
