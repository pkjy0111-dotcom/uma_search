    "23": {
      "frame_id": 23,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "！　それなら問題ありません……！\r 3人でケーキのオーダー、承りました。",
