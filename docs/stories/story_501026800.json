      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90230,
          "speaker_name": "お店のスタッフA",
          "text": "遅くなってすみません！\r お手伝いありがとうございました！ あとは自分がやりますので上がってください！",
          "flag_type": "",
          "flag": "",
          "choices": []
