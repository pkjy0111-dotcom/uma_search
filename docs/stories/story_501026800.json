      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "了解です。ミッションコンプリート。\r お疲れ様でした。",
