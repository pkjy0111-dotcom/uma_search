    "13": {
      "frame_id": 13,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90730,
          "speaker_name": "女性A",
          "text": "甘めでいろんな味が楽しめる方がいいかな～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
