    "12": {
      "frame_id": 12,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "はい。お好みの味をお知らせください。",
          "flag_type": "",
          "flag": "",
          "choices": []
