    "16": {
      "frame_id": 16,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90730,
          "speaker_name": "女性A",
          "text": "え～、お姉さん詳し～い！！\r じゃあそのお店に行ってみま～す！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ミホノブルボンは食レポが得意だったとは。\r 意外な発見だ。全店試食したのだろうか？ なんだかバレンタインへの熱意を感じる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
