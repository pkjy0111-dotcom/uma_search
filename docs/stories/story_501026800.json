    "25": {
      "frame_id": 25,
      "background_id": 68,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "1月なのに、どこもバレンタイン一色ですね。\r 色んなチョコレートが味わえて嬉しいですが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
