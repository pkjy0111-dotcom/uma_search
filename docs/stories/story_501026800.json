      "frame_id": 26,
      "background_id": 68,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "この時期になると私のところにも、\r チョコスイーツの作り方を教えてほしいと 友人たちがやってきます。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 27,
              "loop_ends_at_frame": -1,
              "text": "もうバレンタインの準備をしているのか",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 27,
              "loop_ends_at_frame": -1,
              "text": "もうバレンタインの準備をしているんだ",
              "flag_type": "",
              "flag": "female_trainer"
            }
