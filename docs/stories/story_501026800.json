    "14": {
      "frame_id": 14,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "……サーチ完了。あちらのお店を提案します。\r ガナッシュがなめらかで、溶けた際に ミルクの風味が口いっぱいに広がります。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "その隣のお店はフレーバーが多彩です。\r フルーツ果汁を使ったジューシーで 香り高いトリュフはここだけです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
