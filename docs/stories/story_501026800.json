      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90730,
          "speaker_name": "女性A",
          "text": "すみませ～ん、バレンタイン限定のチョコで\r トリュフってありますか～？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
