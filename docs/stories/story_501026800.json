      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "ブルボンさん、お疲れ様です。\r 今日は本当にありがとうございました。",
          "flag_type": "",
          "flag": "chance_branch_leaf",
          "choices": []
        }
      ]
