    "21": {
      "frame_id": 21,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "ところで、このあとお時間はございますか？\r 今日のお礼にケーキをご馳走させてください。 ぜひ、トレーナーさんもご一緒に。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "22": {
      "frame_id": 22,
      "background_id": 101,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "ケーキ……？　……いえ、お礼は不要です。\r アルバイト代はもういただきましたから……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 23,
              "loop_ends_at_frame": -1,
              "text": "お礼は抜きで、一緒に食べようか",
