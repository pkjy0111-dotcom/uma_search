      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そして……何もかもが不透明な中、 ゴールドシップ社長のワンマンぶりにより、 屋台は開店となったのだったが――",
          "flag_type": "",
          "flag": "",
          "choices": []
