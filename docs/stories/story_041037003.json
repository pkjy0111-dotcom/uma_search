      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "……っ？\r ここは……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "目が覚めた？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
