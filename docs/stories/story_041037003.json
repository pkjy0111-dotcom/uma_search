      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1037,
          "speaker_name": "幼いフラッシュ",
          "text": "『美味しい～っ！　パパ、すごい！\r パパのケーキはどうしてこんなに美味しいの！？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『そうだね……レシピ通りに作ったからだろう。\r ただのレシピじゃない、何度も試行錯誤した完璧なレシピだ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『素材選びから焼き時間、粉砂糖の扱いまで全てキッチリと\r 決められている。狂いなく全てを決められた通りに作るんだよ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『1gの誤差も許さない――それが美味しさの理由であり、\r パティシエとしての私の誇りだ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『わかったかい、フラッシュ？\r 私たちの――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
