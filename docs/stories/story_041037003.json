      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "……倒れたことは少し予定外でした。\r ですが今からの修正は不要です。",
          "flag_type": "",
          "flag": "",
          "choices": []
