      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "……あれ。キミ、学園のトレーナー？\r どうしたの、こんな早くに。",
