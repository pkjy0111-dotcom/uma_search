    "9": {
      "frame_id": 9,
      "background_id": 33,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "流星群が来ていたのかな。暗い街の上に\r 流れ星がたくさん見えた。そうするうち――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
