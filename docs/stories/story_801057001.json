      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "一応、内緒にしておいてね、\r 怒られるのは退屈だから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
