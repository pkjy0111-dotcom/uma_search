        {
          "next_frame": 4,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "お散歩！ すごくいい夜だったし。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "この時間は、校則違反じゃなかったか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "この時間は、校則違反じゃなかった？",
