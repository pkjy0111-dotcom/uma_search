      "frame_id": 10,
      "background_id": 33,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "お腹が鳴ったんだ！　爪も痛くって、 もう朝だった。それで戻ってきたら、 キミに見つかったってわけ！　あはは！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "ということは、ひと晩中！？",
              "flag_type": "",
              "flag": ""
            }
          ]
