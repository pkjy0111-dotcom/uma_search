      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "と、そんなことより、 早く朝ご飯を食べたいんだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
