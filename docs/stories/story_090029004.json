    "11": {
      "frame_id": 11,
      "background_id": 104,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "別れがあるからこそ、その存在は深く魂に刻まれるのだ。 別れがあるからこそ、記憶は燦然と輝き、愛おしいものになる……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
