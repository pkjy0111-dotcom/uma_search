      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "旅館ランクボーナスも得られるので、 これらを指針に掘削を進めていきましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
