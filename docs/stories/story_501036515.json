      "frame_id": 12,
      "background_id": 112,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "観光に来たわけじゃねェンだぞ。 まだ高地に体も慣らしてねェだろ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "もちろん無理はしないよ",
              "flag_type": "",
              "flag": ""
            }
