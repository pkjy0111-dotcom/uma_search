      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "……オイ、さっきからスマホ片手に うろちょろ……。目障り。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "ちょっと景色を見たくて",
