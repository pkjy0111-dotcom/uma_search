    },
    "13": {
      "frame_id": 13,
      "background_id": 112,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女には言わなかったが、トレーニング場所 周辺で電波の通るところを確認していた。 何かあった時のために必要だと思っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
