      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "物量はかなりあり、中々にハードだが……\r エアグルーヴがやるにしては、 基礎的なものが多いように見受けられた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "これは、エアグルーヴ用のメニューなのか？",
              "flag_type": "male_trainer",
