      "frame_id": 11,
      "background_id": 22,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "いやいや、私とて年頃のウマ娘だ。\r 分析とは別に、直情的な想いもある。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
