      "frame_id": 12,
      "background_id": 22,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "実はこのチョコレートは全て、\r 生徒会室の前に置かれていたものだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
