      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "“皇帝”など、\r おあつらえ向きの肩書だと思わんかね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "達観してるなぁ",
              "flag_type": "",
              "flag": ""
