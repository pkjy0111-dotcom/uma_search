      "frame_id": 15,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "残す、かぁ……ライスが今感じてることも、 いつかの未来で、誰かに伝えられたらな……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
