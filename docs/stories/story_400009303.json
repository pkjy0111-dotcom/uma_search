    "16": {
      "frame_id": 16,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "自分を不幸だと思っていても、がんばる姿を 応援してくれる人が必ずいるっていうこと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "みんなを幸せにしただけ自分も幸せになれた。 そんなウマ娘がいたんだよっていうお話を。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
