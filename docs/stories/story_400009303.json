      "frame_id": 20,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "自分も彼女のように、後の時代に残せる 何かがあればとは思うのだが、ね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "君が持ってる物が、半分答えだと思うけど",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "貴方が持ってる物が、半分答えだと思うけど",
