      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "彼女の存在はきっと、長く人々の心に残る。 あの笑顔が運ぶのは、普遍的な希望だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
