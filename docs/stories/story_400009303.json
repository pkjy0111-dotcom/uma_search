      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "――というわけでね。 すっかりライス君の笑顔に絆されてしまった。",
