      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ふんっ……なに満足してるの。\r これくらいの結果、当然のことでしょ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
