      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "あの子、ぽやっとして見えるでしょう？\r でも、本当はすーっごく芯の強い子なんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 31,
              "loop_ends_at_frame": -1,
              "text": "芯の……？",
              "flag_type": "",
              "flag": ""
