    "18": {
      "frame_id": 18,
      "background_id": 154,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "それに、なんといっても走る姿が美しくて～！\r 優雅で、華麗で、理想的で。毎回、ぽや～んといたしますの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
      "background_id": 154,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "わかるわかるっ！\r マックイーンの走りって、本当にきれいなんだよね～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "20": {
      "frame_id": 20,
