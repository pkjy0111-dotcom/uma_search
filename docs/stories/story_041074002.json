      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "うふふ～。\r わたくし、ライアンお姉さまの走りも大好きですわよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
