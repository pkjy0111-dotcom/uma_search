        {
          "next_frame": 3,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "――というわけで、ご紹介いたしますわね～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
