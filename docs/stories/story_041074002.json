      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "いえいえっ、ブライトに連れられてきたんですよね？\r なら、きっとそこには意味があると思いますっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
