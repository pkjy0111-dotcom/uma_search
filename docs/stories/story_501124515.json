    "12": {
      "frame_id": 12,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "私が上手いこと明るい雰囲気を作れば、 きっと後輩たちもいい話が聞けたって 喜んでくれると思うんだよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
