      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "悪戯好きのウインディがいるからな。 間違っても後輩たちに噛みつかないよう、 厳重に見張らなければ。",
          "flag_type": "",
          "flag": "",
          "choices": []
