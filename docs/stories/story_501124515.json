      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "アヤベはちょっとぶっきらぼうだけど、 言ってることは間違ってないから――",
