      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "アイネスフウジンは、キラキラした目で\r ミスターシービーと話をしている。 なんというか――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "後輩っぽい……",
              "flag_type": "",
              "flag": ""
            }
          ]
