      "frame_id": 11,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "あれ、そうなんだ。\r それは困っちゃうね。",
          "flag_type": "",
          "flag": "",
          "choices": []
