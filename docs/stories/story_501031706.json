      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "普段はそこまでしないからさ、\r 欲しいものがどこで手に入るかわからなくて ウロウロしてたところなんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
