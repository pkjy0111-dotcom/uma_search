      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "もう夕方ですけど、大丈夫ですか？\r 早いお店は、もうすぐ閉まっちゃいますよ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
