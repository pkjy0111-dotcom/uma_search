      "frame_id": 8,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "うん、買い物に来ててね。\r たまにはしっかり自炊するのもいいかなって、 食材からこだわってみようと思ったんだけど。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
