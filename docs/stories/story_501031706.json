      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "あの、よかったらお手伝いします！\r 後輩として、バックアップは任せてください！",
