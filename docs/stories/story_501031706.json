      "frame_id": 7,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "あ、こんなに立ち話してていいのかな。\r なにか用事があったんじゃ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
