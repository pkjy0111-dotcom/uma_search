    "6": {
      "frame_id": 6,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "えっ？　そんなの当たり前なの！\r シービーさんは尊敬する先輩だもん、 ちゃんと敬わなきゃ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
