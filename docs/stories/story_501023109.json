      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ああいや、カレーはクリスマス料理\r ではないと理解しているよ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
