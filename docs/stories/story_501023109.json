      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "だがさすがにカレーはないか。ふふっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "カレー？",
