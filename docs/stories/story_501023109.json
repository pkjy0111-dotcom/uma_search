      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ローストチキンにクリスマスケーキ。\r ああ、あっちにはピザもある！",
