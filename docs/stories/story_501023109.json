      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "私は母に頼んで、フルーツケーキを。\r ブライアンは私に頼んで、カレーを。",
