      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ただ我が家のクリスマスは、\r それぞれの好物が並べられてね。",
