      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ひとつは、URAが管轄する トゥインクル・シリーズ。 俗に、『中央』と呼ばれる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
