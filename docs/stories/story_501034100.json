      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "これより語られるは波乱万丈物語！ 一世一代の大喧嘩道！ トゥインクル・シリーズの大祭り！",
          "flag_type": "",
          "flag": "",
          "choices": []
