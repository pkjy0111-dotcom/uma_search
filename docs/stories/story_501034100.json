    "11": {
      "frame_id": 11,
      "background_id": 114,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そしてもうひとつは『地方ウマ娘全国協会』、 通称NAUが管轄するレース。",
          "flag_type": "",
          "flag": "",
          "choices": []
