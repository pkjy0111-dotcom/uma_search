      "frame_id": 14,
      "background_id": 114,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "しかし、自分がイナリワンの走りに惚れ込み、 スカウトしたいと思って交流を重ねる内に、 彼女も中央のレースに魅力を感じてくれて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
