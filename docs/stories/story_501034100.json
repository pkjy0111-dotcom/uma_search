    "12": {
      "frame_id": 12,
      "background_id": 114,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "大井や笠松、船橋などのレース場で行われ、 ダートレースを主としているこちらは、 『中央』に対して、『地方』と呼ばれている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 114,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……イナリワンは、元は中央ではなく 大井にあるチーム、つまり地方で デビューするはずのウマ娘だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
