      "sub_background_id": 2131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "最後の最後まで―― 目ん玉かっ開いて見ててくんなあ！",
