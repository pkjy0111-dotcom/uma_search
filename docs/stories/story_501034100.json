      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――いよいよ今日から、イナリワンとの トゥインクル・シリーズへの挑戦が 本格的に始まる……！",
          "flag_type": "",
          "flag": "",
          "choices": []
