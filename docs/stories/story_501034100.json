      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "下町生まれの下町育ち！ 大井印の粋なウマ娘！ 江戸っ子イナリワンたあ、あたしのことよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
