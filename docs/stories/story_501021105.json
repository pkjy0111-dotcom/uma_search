      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "オグリのレースっぷりは、妙に人気あるんや。\r 理由は……ウチもなんとなく わかるんやけどな。",
          "flag_type": "",
          "flag": "",
          "choices": []
