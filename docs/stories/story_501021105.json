      "background_id": 40,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "やけに熱い展開になるんや。抜きつ抜かれつ、\r どうなるかわからんみたいな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
