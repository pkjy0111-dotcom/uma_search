      "background_id": 40,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "アイツの走りには華があんねん。特に\r 最後の直線、ギリギリの勝負になった時……。",
          "flag_type": "",
          "flag": "",
          "choices": []
