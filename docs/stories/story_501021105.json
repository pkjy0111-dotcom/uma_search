      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "あ、トレーナー。実はな、ウチの弟が\r 近所の子とケンカしたらしいねん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
