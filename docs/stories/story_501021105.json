      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "まったく、しょーもない……。\r でも、なんでアイツの走りはあんなに……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "どうしたんだ、タマモ？",
