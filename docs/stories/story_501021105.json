      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90215,
          "speaker_name": "近所の男の子",
          "text": "へへーん、1番強いのはオグリキャップや！\r なんたって走りがちゃうねん！ つぎはオグリキャップの時代やで。",
          "flag_type": "",
          "flag": "",
