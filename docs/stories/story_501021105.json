      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "でもアイツはなんで、あんなおもろいレース できるんやろな……。",
