    "7": {
      "frame_id": 7,
      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……いつから見られていたのだろう、不覚だった。 トレーナーでありながら、ウマ娘に心配をかけてしまうなんて――",
          "flag_type": "",
          "flag": "",
          "choices": []
