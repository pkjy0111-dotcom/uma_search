      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ワンダーアキュートの穏やかな促しに、 言うつもりのなかった言葉がぽろり、とこぼれる。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
