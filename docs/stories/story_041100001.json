      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "急に声かけてごめんなさいね。 あなたが泣きそうな顔してるもんだから気になっちゃって。",
          "flag_type": "",
          "flag": "",
          "choices": []
