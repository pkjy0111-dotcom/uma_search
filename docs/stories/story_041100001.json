      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ああほら、そんな迷子さんみたいな顔せんで。 お腹でも減ってるんかね？　ぽりぽりさん食べるかい？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
