      "frame_id": 9,
      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ぽりぽりさんというのはこの大根のぬか漬けのことだろうか。 いつもなら遠慮するところだが、今は慰めがありがたかった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "…………いただきます",
