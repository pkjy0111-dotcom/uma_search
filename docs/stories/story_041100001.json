      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ふふ、おいしいと思えるならまだ大丈夫かしらねぇ。 さ……なにがあったか話してごらん？　ゆっくりでええよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
