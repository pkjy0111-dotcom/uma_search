      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "後日、タニノギムレットは語った。 自分は人々の記憶に己の走りを焼きつけた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
