          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "これ、どうぞ。香り、気に入ってたよな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 15,
