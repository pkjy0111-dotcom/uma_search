      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "トレーナーさん……。やっぱり肩がズシリと 重いですね。覚悟はしてましたけど……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
