      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "……はい。あたしもテイオーさんみたく！ あんなにカッコよくはできなくても、 あたしなりの走りで。",
          "flag_type": "",
          "flag": "",
          "choices": []
