      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "いいかキタサン、油断すんなよ。 世界の猛者どものスパートに エジキにされねぇようにな！",
          "flag_type": "",
          "flag": "",
          "choices": []
