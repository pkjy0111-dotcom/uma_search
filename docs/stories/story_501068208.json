    "4": {
      "frame_id": 4,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "はい、ウオッカさん！　差されそうに なっても、根性で粘ります！",
