      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "重圧に立ち向かうキタサンブラックを 激励しようと、歴代のウイナーたちが 駆けつけてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
