      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "はい、光栄です……！　喜んで、 お役目を果たさせてもらいます！",
