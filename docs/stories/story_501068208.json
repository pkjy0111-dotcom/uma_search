      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "キタちゃん、頑張ってね。カイチョーに 憧れて、このレースを勝ったボクみたいに。",
