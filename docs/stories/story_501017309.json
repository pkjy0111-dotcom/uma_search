      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ありがとう、エアグルーヴ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
