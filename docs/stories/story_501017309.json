    "10": {
      "frame_id": 10,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ふふっ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
