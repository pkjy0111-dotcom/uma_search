      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "無論、次は『菊花賞』だ。\r 次の冠を、なんとしてでも獲得するぞ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "よし、挑もう！",
