      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "では、やはり――",
          "flag_type": "",
          "flag": "",
          "choices": []
