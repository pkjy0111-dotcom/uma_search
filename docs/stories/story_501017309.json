      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "ダービー制覇、おめでとうございます……！\r 素晴らしいレースでした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
