      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "勉強に悩み、友人関係に悩み、己の素質に悩み…… さまざまな日々が、あの学園内では送られています。",
          "flag_type": "",
          "flag": "",
          "choices": []
