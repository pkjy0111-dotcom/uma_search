      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "アイドル活動に熱心だったり、モデル業も行っていたり、 ずっと生徒会室にいたり、占いばかりしていたり――",
          "flag_type": "",
          "flag": "",
          "choices": []
