      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1132,
          "speaker_name": "ラヴズオンリーユー",
          "text": "あ。あっちに提灯あるよ！　昨日の縁日の 名残りかな？　わびさび～って感じ♡",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
