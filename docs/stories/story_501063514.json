      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "イクノディクタスの表情もまた、 シンボリルドルフに劣らない真剣さだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
