      "frame_id": 10,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "こうなれば私も大会現地まで赴き、 結末を見届けなければなりませんね。 助言者の責任として。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
