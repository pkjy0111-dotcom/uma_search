    },
    "8": {
      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "幸せが逃げちゃうかも、と笑ってみる。 一瞬不安げな顔をしたものの、こちらの 笑顔を見て、冗談だと受け取ったようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
