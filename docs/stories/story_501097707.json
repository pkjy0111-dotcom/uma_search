      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1097,
          "speaker_name": "スティルインラブ",
          "text": "で、ですが…… たくさん、お祈りしましたから。 きっと、よい方向へと向かっていくはずです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "空元気のような笑顔をする、 スティルインラブ。元気づけようと、 少しだけからかうように――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ため息ついちゃったからなぁ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
