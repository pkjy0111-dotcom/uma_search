      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1097,
          "speaker_name": "スティルインラブ",
          "text": "どうにも……目に見えない不安が、 常に体の中を……這いずり回って いるようで……不安なのです……。",
          "flag_type": "",
          "flag": "",
          "choices": []
