    "5": {
      "frame_id": 5,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1097,
          "speaker_name": "スティルインラブ",
          "text": "それに、調子を崩してから…… トレーナーさんにもご迷惑を おかけしていて……申し訳なくて……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "スティル……",
