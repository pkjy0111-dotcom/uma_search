    "9": {
      "frame_id": 9,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……見てわかるだろう？\r 私の髪は……その、少しだけ膨張しやすくてね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "なるほど……？",
              "flag_type": "",
              "flag": ""
            }
