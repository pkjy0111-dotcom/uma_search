      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "せっかくだから、新製品でも出ていないかと探しに来たんだ。\r 同じ製品でも、日々の研究によって成分が見直されることもあるし。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ずいぶん気を使ってるんだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ずいぶん気を使ってるんだね",
              "flag_type": "",
              "flag": "female_trainer"
