      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "な、なぜここにっ……いや、買い物に決まってるか。\r しかしなんたる偶然……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "何を見てたんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "何を見てたの？",
              "flag_type": "",
              "flag": "female_trainer"
