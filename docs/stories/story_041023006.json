      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "となればやはり先ほどのバオバブ種子油のものを……\r だが、浸透率を考えると成分が今ひとつ……むぅ……。",
          "flag_type": "",
