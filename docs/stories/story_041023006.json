      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "まあ……この通り、髪の悩みは尽きないからな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "この通り？",
              "flag_type": "",
              "flag": ""
