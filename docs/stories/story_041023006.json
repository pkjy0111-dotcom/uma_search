    "6": {
      "frame_id": 6,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……ヘアオイルを色々と見比べていたところだ。\r ちょうど今使用しているものが切れそうだったのでね。",
