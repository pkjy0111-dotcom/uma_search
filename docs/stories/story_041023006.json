    "10": {
      "frame_id": 10,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "確かに彼女の髪はふわふわしているが、特別目立つ印象はない。\r そこまで気遣うものなのだろうかと疑問に思っていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……なあ君。まさか……私の髪が、何もせずに\r 元々この状態なのだと勘違いしてはいないか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "え？",
              "flag_type": "",
              "flag": ""
            }
