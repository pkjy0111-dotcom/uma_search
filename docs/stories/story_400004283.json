      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "――執事のコスチュームを販売している 店舗を、都内で確認しました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
