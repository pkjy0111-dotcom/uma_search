      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "しかしこれでは、我欲のみで『神馬』に臨むのと同じ。 真に大事なのは儀式にこめられた祈りに寄り添うことだった……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
