      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90620,
          "speaker_name": "カメラマン",
          "text": "渓流の岩の上でイナリさんの弾けるような ワイルドさを表現するとか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
