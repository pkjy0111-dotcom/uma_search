      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90620,
          "speaker_name": "カメラマン",
          "text": "ですね！　この辺りはイナリさんの勝負服に 合いそうなスポットが多いですからね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
