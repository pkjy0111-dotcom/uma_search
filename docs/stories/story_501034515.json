      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "ま、あたしにとっちゃ和室―― 畳は、昔馴染みたいなもんだからな。 気合いも入るってもんよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "昔馴染み？",
              "flag_type": "",
              "flag": ""
            }
