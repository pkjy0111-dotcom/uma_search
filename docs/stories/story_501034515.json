    },
    "15": {
      "frame_id": 15,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "ああ、そうさ。ずっと一緒にやって来た仲だ。 ガキの頃から、ずうううっとな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
