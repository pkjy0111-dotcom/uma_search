      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "Merry Kris S. ――トレーナー。朝早くに――すまない。 少し……相談したいことがある。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "い、今なんて？",
              "flag_type": "",
              "flag": ""
            }
          ]
