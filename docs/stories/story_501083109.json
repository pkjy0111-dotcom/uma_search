    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "流暢な発音では誤魔化し切れない 強烈な違和感があったような――",
          "flag_type": "",
          "flag": "",
          "choices": []
