      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "そこで――何人かに相談し―― “武器”を得た。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "それが、メリークリスエス",
              "flag_type": "",
              "flag": ""
