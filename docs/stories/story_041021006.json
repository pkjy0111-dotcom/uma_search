    "4": {
      "frame_id": 4,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "なんでもええ！\r あんな、ウチな、アウェイで1人めっちゃ困っとってん！",
