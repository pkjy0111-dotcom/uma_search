      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "予算はあんまかけられへんのやけど……なにがええかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
