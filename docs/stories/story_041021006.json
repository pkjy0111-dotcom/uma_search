      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "こないな高級店で買い物したことないやろ？\r どこ入ればええかわからんくて、途方に暮れとったんや！",
          "flag_type": "",
          "flag": "",
          "choices": []
