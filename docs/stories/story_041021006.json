    "9": {
      "frame_id": 9,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "先ほど彼女はここの店を高級と言っていたが、\r 本来は学生でも入れる価格帯のお店だ。それが高いとなると――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "ちょっといいお菓子はどうだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "ちょっといいお菓子はどう？",
              "flag_type": "",
              "flag": "female_trainer"
