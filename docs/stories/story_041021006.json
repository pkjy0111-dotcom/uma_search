    "6": {
      "frame_id": 6,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "頼む！　ウチの両親の結婚記念日のプレゼント、\r 一緒に探してくれへんか！？",
          "flag_type": "",
          "flag": "",
          "choices": []
