      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ここからロンシャンまではおよそ1時間。 かなり余裕をもって出発したのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
