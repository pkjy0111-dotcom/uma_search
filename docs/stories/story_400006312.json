      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "なら、そっちは かなり混乱してるんじゃないか？　大丈夫か？",
