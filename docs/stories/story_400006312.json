      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90127,
          "speaker_name": "サングラスのウマ娘",
          "text": "タップ……この音、やっぱりまだバスの中？ もしかして渋滞はまってる？ ――実は、私たちもなんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
