    },
    "6": {
      "frame_id": 6,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "――Darn it! なんでこんなに動かないんだよ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "乗った車はひどい渋滞にはまっていた。 刻一刻と時間が過ぎてゆく。到着の遅れは、 そのまま彼女の消耗へと繋がってしまう……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "……明日はレースだっていうのに…… You're kidding me!",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "（何とか落ち着かせないと……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "レースまでのベストな過ごし方は 最早難しいが、せめてベターで臨みたい。 ――と、電話が鳴った。",
          "flag_type": "",
          "flag": "",
          "choices": []
