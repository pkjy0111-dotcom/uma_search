    },
    "4": {
      "frame_id": 4,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "ハハハッ、いよいよだな『凱旋門賞』！ 思わず脚も踊るってものさ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 37,
