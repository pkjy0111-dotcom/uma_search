          "flag": "",
          "choices": [
            {
              "next_frame": 31,
              "loop_ends_at_frame": -1,
              "text": "（でも、彼女は道を切り開いた）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
