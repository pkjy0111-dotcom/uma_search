      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "勝負服に袖を通す、ということは…… その重責を身に纏うこと、なのだろうな。",
