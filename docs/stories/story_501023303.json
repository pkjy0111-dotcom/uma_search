      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "心配は無用さ。せっかく実戦を通して\r 今まで見逃していた欠点に気づけたんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
