      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "時間は1秒でも惜しくてね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "（……ナリタブライアンに勝利する方程式）",
              "flag_type": "",
              "flag": ""
