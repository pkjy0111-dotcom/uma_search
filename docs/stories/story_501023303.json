    "3": {
      "frame_id": 3,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "もう1度、映像の再生を頼む。\r 他にも洗い出すべき点がありそうだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "ところで、休まなくて平気？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "初めてのレースとライブを経験したばかり。\r 彼女の体にも疲労が溜まっているはずだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
