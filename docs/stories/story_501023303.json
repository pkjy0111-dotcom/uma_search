      "frame_id": 2,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ふむ。今回の位置取り、多少甘かったようだ。\r レース中、前ばかり見てしまう弊害だな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
