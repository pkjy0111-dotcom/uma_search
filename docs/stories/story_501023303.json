      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "なら改善するまでだよ。何せ、私が欲する\r 勝利の方程式は彼女に対抗しうるもの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
