      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……海中から、\r ずぶ濡れのヤエノムテキが上がってきた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
