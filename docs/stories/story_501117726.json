        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ウインバリアシオンが示すほうを見てみると、 ウマ娘たちをモチーフにしたぬいぐるみが 入った、クレーンゲームがあった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "もしかして、ぱかプチに興味ある？",
              "flag_type": "",
              "flag": ""
