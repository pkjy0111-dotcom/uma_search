      "background_id": 36,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "そうなんです！　おかげで、小さい頃から バレエに夢中になる子も多いんです。",
