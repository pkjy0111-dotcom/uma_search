      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "だが、様々なライバルと出会って……\r 私自身も、誰にも負けたくないと思った。",
          "flag_type": "",
          "flag": "",
          "choices": []
