      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "勝ってみんなに、\r 喜んでほしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
