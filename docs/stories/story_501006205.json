      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "キミと出会った日のこと、\r ともにトレーニングを積んだ日のこと……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
