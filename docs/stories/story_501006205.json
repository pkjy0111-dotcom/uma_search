    "7": {
      "frame_id": 7,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "最初はただ、故郷の人たちのためだけだった。\r みんなの期待に応えたかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
