      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "私を応援してよかったと、\r みんなに思ってもらえる走りをしたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
