    "6": {
      "frame_id": 6,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女の穏やかな語り口から、\r 様々な想いが伝わってくる……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
