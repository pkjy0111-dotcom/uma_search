        {
          "next_frame": 23,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "ふふっ……ええ、なんなりと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
