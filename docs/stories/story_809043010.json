      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "未来ある後輩の成長が嬉しいんだよ、私は。",
