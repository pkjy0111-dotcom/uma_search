      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "君が普通じゃなかったのは、どんな時でも 夢とロマンを捨てなかったことだな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
