      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "ふふっ、そう驚くなって。 思ったことを言っただけさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 237,
