      "frame_id": 11,
      "background_id": 161,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "はいはいっ！　新年を慶ぶための行事なんだったら、 たのしい催しはいくらあってもいいわよね？",
          "flag_type": "",
          "flag": "",
          "choices": []
