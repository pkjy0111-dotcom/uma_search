      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ええ、本日は特別メニューです。 目標は全力で楽しみ、仲間との絆を深めること。",
          "flag_type": "",
          "flag": "",
          "choices": []
