      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "ナカヤマの手紙",
          "text": "『見に来てくれなかった、と 恨み言を言うつもりはありません。 近況を報告するつもりも、それほど』",
          "flag_type": "",
          "flag": "",
          "choices": []
