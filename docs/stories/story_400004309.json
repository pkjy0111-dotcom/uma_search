        {
          "next_frame": 2,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "さぁてバクシンオー！ 今日の仕事は責任超重大っスよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
