      "frame_id": 14,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "う～ん……隠れたアツさもあるのかも しれないっスけど、こんなヘンテコな本を 学園に置いておくのは風紀的に――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
