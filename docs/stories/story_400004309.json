      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "生徒が物語からどんな教訓を読み取るか…… それも立派なお勉強ではないでしょーか！",
          "flag_type": "",
          "flag": "",
          "choices": []
