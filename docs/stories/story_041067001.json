      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ふふ、お手柔らかにね……って言いたいところだけど～。\r キタちゃん、レースで優しくしてくれたことなんてないよね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
