    "8": {
      "frame_id": 8,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "うん！　走る時はいつだって真剣だよ！\r 特にダイヤちゃん相手の時は！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
