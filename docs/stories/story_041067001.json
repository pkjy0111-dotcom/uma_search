      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "あっ、あなたもトレーナーさんですよね？　ずいぶん熱心に\r 見てくれたようですけど、ダイヤちゃん、すごくなかったですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
