    "10": {
      "frame_id": 10,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "サトノダイヤモンドの才能はどれほどなのか？\r いよいよレースの火蓋が切られた。",
          "flag_type": "",
          "flag": "",
          "choices": []
