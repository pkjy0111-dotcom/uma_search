      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――高嶺の花。大資産家の令嬢。普通のトレーナーが熱意だけで\r 近寄ってはいけない存在に思えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
