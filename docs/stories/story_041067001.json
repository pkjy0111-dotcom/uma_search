      "frame_id": 32,
      "background_id": 74,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "小さい頃から速かったんですけど、最近ますます切れ味が増してて！\r もう、あたしとしては激推しワッショイですっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
