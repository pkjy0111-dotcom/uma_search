      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その走りは素晴らしいものだった。勝負所をしっかりと読み、\r そこに持ち前の瞬発力を正確に合わせる……。",
          "flag_type": "",
          "flag": "",
