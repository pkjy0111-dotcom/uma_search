      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "幼い頃、オークスウマ娘の走りとその言葉に 彼女は感銘を受けた。以来、自分の走りを 誰かに繋げていくことを夢見ている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレセン学園には、ティアラウマ娘の先輩も 在籍している。ラインクラフトはいつも、 目を輝かせて、憧れの背中を見つめていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だが、ライバルは多い。同期のシーザリオや エアメサイア、デアリングハートもまた、 ティアラ路線を目指している。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
