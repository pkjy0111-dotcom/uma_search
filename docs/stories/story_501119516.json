          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "……？　当然、全て食べますよ。 この体のために必要なことなので。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "全部は食べ過ぎだよ……",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 20,
