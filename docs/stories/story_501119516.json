      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ドリームジャーニーが料理を食べ始める前に 1つ確認しておきたいことがあった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "えっと……これ全部は食べないよな？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "えっと……これ全部は食べないよね？",
              "flag_type": "",
              "flag": "female_trainer"
