      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "摂取を優先すべき栄養素など、おおむね 理解できました。あとは、味ですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
