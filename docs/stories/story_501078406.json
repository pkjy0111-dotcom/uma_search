      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "無理でもやる。無理だからやる。 最初から、道は険しいとわかっていた。 だから、立ち止まったりしないと語る。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
