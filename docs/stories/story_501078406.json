      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "全治6か月――しかしトウカイテイオーは すでにリハビリを行っており、 『菊花賞』に絶対間に合わせてみせると宣言。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 5,
      "sub_background_id": 410,
