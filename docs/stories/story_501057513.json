    "2": {
      "frame_id": 2,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "うんうん、やっぱこの勝負服いいね。 どんな動きもポーズもしっかり馴染む！",
          "flag_type": "",
          "flag": "",
          "choices": []
