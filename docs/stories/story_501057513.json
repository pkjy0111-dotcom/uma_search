      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "シンボリルドルフの言葉に、勝負服の デザインを決めた時のことを思い出した――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
