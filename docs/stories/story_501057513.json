    },
    "3": {
      "frame_id": 3,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "それはなによりだ。 雨過天晴、一時はデザインが固まらず、 どうなることかと心配していたが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
