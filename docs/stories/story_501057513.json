      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "えっと、なんていうか……派手だね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "華々しくも仰々しい――成程。 世間がミスターシービーというウマ娘に抱く 期待を形にしたもの、といったところか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "ええ？ けどそれ、アタシっぽさからズレてない？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "まだ原案段階だしすり合わせをすればいいよ",
              "flag_type": "",
              "flag": ""
