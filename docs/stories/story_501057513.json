      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "へー。 勝負服の原案、届いたんだ。どんなの？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "これなんだけど",
              "flag_type": "",
              "flag": ""
