      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ところでシービー、 君からデザインの希望は出さなかったのか？ 出していれば認識のズレも防げたと思うが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 223,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "うーん……勝負服を作りますって言われても、 なんだかピンとこなくてさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
