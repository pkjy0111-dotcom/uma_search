      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "うーん……わかった。 じゃあ、あっちの席で考えよう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
