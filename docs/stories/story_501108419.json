      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "トレーナーの手記",
          "text": "そこで、彼女をよく知るエアグルーヴに 意見を仰いだ。エアグルーヴは、 幼い頃の出来事を教えてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
