          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "――おや……これはこれは。トレーナーさんではないですか。 またお会いできるとは。何か、ご用事でも？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 34,
              "loop_ends_at_frame": -1,
              "text": "……今日はこれを持ってきたんだ",
              "flag_type": "",
              "flag": ""
            }
