      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9003,
          "speaker_name": "乙名史記者",
          "text": "私、『月刊トゥインクル』の記者――\r 乙名史　悦子【おとなし　えつこ】 と申します。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
