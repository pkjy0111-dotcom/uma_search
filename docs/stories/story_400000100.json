      "frame_id": 7,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9003,
          "speaker_name": "乙名史記者",
          "text": "……トレーニングを拝見し、脱帽しました。\r ぜひ、お2人の活躍を記事で後押ししたい。 そのご相談をと、お待ちしていた次第です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "礼儀正しく、誠実そうな人だ。\r 丁寧な記事で、<chrname>の ファンの支持を集めてくれるかもしれない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "こちらこそ、ぜひお願いします",
              "flag_type": "",
              "flag": ""
