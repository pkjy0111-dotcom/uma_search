      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "視線の主は、見覚えのない人物だった。 ファンか見学者だろうと思ったのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
