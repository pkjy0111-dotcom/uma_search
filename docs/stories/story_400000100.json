      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9003,
          "speaker_name": "？？？",
          "text": "それはよかったです！ 実は、お話したいことがございまして。",
          "flag_type": "",
          "flag": "",
