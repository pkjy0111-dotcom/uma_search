      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーニングを終える頃になっても 依然として、彼女はこちらを見ていた。 ……何か話したいことがあるのだろうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……あの、どうかしましたか？",
              "flag_type": "",
              "flag": ""
