      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9003,
          "speaker_name": "？？？",
          "text": "あ……申し訳ありません。 練習の邪魔をしてしまったでしょうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "いえ、もう終わりましたから",
              "flag_type": "",
              "flag": ""
