      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "中央は、ジョーダンさんをイメージしたアイシングドーナツです。\r ネイルと、賑やかな学園生活をモチーフに作製しました。",
          "flag_type": "",
          "flag": "",
          "choices": []
