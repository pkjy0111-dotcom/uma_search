    "7": {
      "frame_id": 7,
      "background_id": 90,
      "sub_background_id": 13000,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "記憶をたどるだけではなく、視覚情報から、\r モチーフへ落とし込むべき要素を探し出せればと考えています。",
          "flag_type": "",
          "flag": "",
          "choices": []
