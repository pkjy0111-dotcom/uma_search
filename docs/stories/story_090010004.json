      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "左側はバクシンオーさんをイメージしたものです。模範であろうと\r する姿からヒントを得、健康によい野菜ケーキを作製しました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
