      "frame_id": 5,
      "background_id": 90,
      "sub_background_id": 13000,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "バクシンオーさん、ジョーダンさん、フラワーさんの\r 生活を体験して、得たイメージから作製したお菓子です。",
          "flag_type": "",
          "flag": "",
          "choices": []
