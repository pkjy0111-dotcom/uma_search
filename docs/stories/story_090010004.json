    "6": {
      "frame_id": 6,
      "background_id": 90,
      "sub_background_id": 13000,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "両親がたまに、こういった方法で思考を整理し、 新作を開発していたので、それを真似ました。",
          "flag_type": "",
          "flag": "",
          "choices": []
