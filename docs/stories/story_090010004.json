      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "なるほど。オーダーを受理しました。 詳細の説明を願えますか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
