      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "くっ……故郷を滅ぼし、我が父母まで……！ 邪龍よ、あなただけは絶対に許さない！",
          "flag_type": "",
          "flag": "",
          "choices": []
