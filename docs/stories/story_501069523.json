      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "ヤエノさんの体幹がしっかりしているのは\r 武術の影響ですよね。私もやってみようかな。",
          "flag_type": "",
          "flag": "",
          "choices": []
