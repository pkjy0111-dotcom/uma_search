      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "チヨノオーさんのお父上は力士でしたよね？\r 一緒に稽古した経験はないのですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
