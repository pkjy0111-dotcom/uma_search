      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "であれば、まずはそれらを\r 試してみることをお勧めします。 理解度が段違いだと思いますので。",
          "flag_type": "",
          "flag": "",
          "choices": []
