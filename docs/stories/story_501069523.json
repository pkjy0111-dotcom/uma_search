      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "子どもの頃ですけど、四股や鉄砲、\r すり足や股割りのやり方は習いました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "それは非常に貴重な経験だと思います。\r 正しい型を何度も見ているでしょうし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
