      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "日本とはずいぶんトレーニング環境が 違うのだと実感いたしまして……。",
