      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "視察団の乗った車が見えなくなる。 ほっと肩の荷を下ろすが……隣の サトノダイヤモンドは険しい表情をしていた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "どうかしたのか？ダイヤ",
