      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90720,
          "speaker_name": "フランス視察団",
          "text": "――本日はありがとうございました。 日本遠征時のよき参考になりそうです！ それでは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
