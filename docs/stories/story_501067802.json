      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "トレーニングの必修カリキュラムには、 水泳もございます。プールの距離は50m――",
