      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "寮は栗東寮と美浦寮に分かれております。 食事や清掃などは寮の職員が担当し――",
          "flag_type": "",
          "flag": "",
          "choices": []
