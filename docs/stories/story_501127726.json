      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ですが！　あのぬいぐるみがあれば、 親しみを持って私を見てくれるはず！",
