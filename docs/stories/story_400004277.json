      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "……完璧にスイッチが入ってやがる。 こうなるとこいつら、ぜってー引かねェぞ。",
