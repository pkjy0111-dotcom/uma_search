      "frame_id": 10,
      "background_id": 140,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "安心して！　と～っても美味しくなるのは、 さっき私とタキオンの舌で実験済みだよ！ だから2人も試してみて！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
