      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "するとあら不思議！　美味しいカップ麺が、 さらに美味しくなっちゃいま～す！",
          "flag_type": "",
          "flag": "",
          "choices": []
