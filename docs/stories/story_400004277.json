      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ククッ、そしてこのカップ麺に 特製調味料を1滴たらす……！",
