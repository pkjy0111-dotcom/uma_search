      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "……その調味料、 どう見ても怪しすぎるんですが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
