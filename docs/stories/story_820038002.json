      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "大勢の生徒を従え、たびたび\r コースを占拠しているだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
