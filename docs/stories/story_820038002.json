      "frame_id": 20,
      "background_id": 2,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "つまり、優等生のアンタと違って――\r アイツらに『次』なんてねぇんだよ。",
