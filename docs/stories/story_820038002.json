      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "大抵のヤツはな、\r 生まれながらに何かしらの問題がある。",
