      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "ハッ、なるほど！\r どんどんこぼれてくってワケだ！",
