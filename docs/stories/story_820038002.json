    "19": {
      "frame_id": 19,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "そして、そういう問題は突発的に\r 降りかかってくる。レースがあろうと トレーニングがあろうと、否応なくな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
