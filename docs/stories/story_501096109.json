    "16": {
      "frame_id": 16,
      "background_id": 29,
      "sub_background_id": 6111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "例えば、ローストビーフやローストチキン。 ビーフシチューやピザ、ポテトサラダ…… あたりが日本では用意されがちだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
