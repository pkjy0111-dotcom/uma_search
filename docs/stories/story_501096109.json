      "frame_id": 13,
      "background_id": 29,
      "sub_background_id": 6111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "いざ買い出しじゃー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
