    "14": {
      "frame_id": 14,
      "background_id": 29,
      "sub_background_id": 6111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "いや、助力に感謝するぞ！ そなたがいれば百人力よ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 29,
      "sub_background_id": 6111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "して、まずは食事か――ええと？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "基本的に、洋食が一般的かな",
              "flag_type": "",
              "flag": ""
