      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "あとは、トップスピードに押し上げるタイミングを もう少し早めてもいいかもしれないが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "どうかな、君の感覚は",
