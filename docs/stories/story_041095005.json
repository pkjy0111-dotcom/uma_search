    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1095,
          "speaker_name": "ビリーヴ",
          "text": "……もう少しタイミングを早めても、粘れるとは思います。 ただ、ギリギリです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
