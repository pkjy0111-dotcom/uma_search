      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "それに～……村を出ンのはちょっと寂しいでがんすし、 1人でやってけンのかも心配ですし……えへへ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
