      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90730,
          "speaker_name": "クラブのコーチ",
          "text": "――すごい、また新記録だよユキノさん！ やっぱりあなたには、オグリみたいに中央で戦える才能がある！",
          "flag_type": "",
          "flag": "",
          "choices": []
