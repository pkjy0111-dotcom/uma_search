      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90730,
          "speaker_name": "クラブのコーチ",
          "text": "う～ん、そっかぁ……まだ小学生だものねぇ。 指導者としては、早めにいい環境へ送り出してあげたいけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
