      "frame_id": 5,
      "background_id": 204,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "はわわ……みなさんまたそったなことぉ～。 上京なンて、あたしにはまだ早ぇかったンですって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
