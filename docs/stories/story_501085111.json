      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "初めてこの家に訪れた時は、その視線から 感じる重圧に、足が竦みそうだったが…… 今では、胸を張って受け入れることができる。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "（きっと、貢献できたはずだから）",
              "flag_type": "",
              "flag": ""
