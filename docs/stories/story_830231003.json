      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "『ぼ、ボク……！？　そんな……こんなにも 美しいウマ娘が、ボク以外にも……！？』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "（か、カオスな劇だ……！？）",
