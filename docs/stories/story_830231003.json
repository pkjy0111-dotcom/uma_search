      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "素晴らしい！　君は音響の経験があるのかい？ 本番も、この調子で頼むよ！",
