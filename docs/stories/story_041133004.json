    "8": {
      "frame_id": 8,
      "background_id": 324,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "と、トレーナーさん……トレーナーさんっ。 ちょっと、すご――すごかったですねっ……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "あ、ああ。すごかったな……！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "う、うん。すごかったね……！",
              "flag_type": "",
              "flag": "female_trainer"
