      "frame_id": 12,
      "background_id": 324,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "……誰一人として、いませんでした。 過去を“なぞって”、歴史を打ち立てた人は。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "ああ……そうだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "うん……そうだね",
