      "frame_id": 23,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "微睡の誘惑に使命感と食欲で立ち向かう、\r という訳だな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 24,
              "loop_ends_at_frame": -1,
              "text": "寝起き、あまりよくないのか？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 24,
              "loop_ends_at_frame": -1,
              "text": "寝起き、あまりよくないの？",
