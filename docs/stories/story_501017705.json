    },
    "18": {
      "frame_id": 18,
      "background_id": 33,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ただ、彼女たちからも評判なので\r 君にも事前に忠告しておくが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
