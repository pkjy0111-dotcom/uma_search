      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "支障の無い様対策は講じているが、\r 性質自体は如何ともし難い。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
