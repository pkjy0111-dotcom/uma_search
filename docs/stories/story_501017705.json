      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "事程左様に、私は起床に難有りだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
