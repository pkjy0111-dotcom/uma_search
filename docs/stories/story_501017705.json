    "22": {
      "frame_id": 22,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "『弁当を作る』という予定が詰まっている――\r そう自覚しておく事で、 より強く起床が促される。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
