      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "――はは、さすがはトレーナー君。\r 察しがいい事この上ない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
