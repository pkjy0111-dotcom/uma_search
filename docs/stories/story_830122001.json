      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1048,
          "speaker_name": "トーセンジョーダン",
          "text": "マ？　にしては距離遠くね？ あ、『ケンタイキ』的な！？",
