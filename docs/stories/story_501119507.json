      "frame_id": 9,
      "background_id": 136,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 0,
          "speaker_name": "子ども",
          "text": "わあ、ドリームジャーニーさんだ！ ぼくの絵、見てくれてたんだ！？",
          "flag_type": "",
