      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "ええ、驚きました。貴方は鋭い観察眼を お持ちなのですね。自由で大胆な表現も、 オリジナリティがあって素晴らしいです。",
          "flag_type": "",
          "flag": "",
          "choices": []
