          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "ああ！“最強”のダービーウマ娘を一緒に！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
