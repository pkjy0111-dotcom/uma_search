      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "――そこの資料を図書室に戻しておけばいいんですね。 なら、私が返却しておきますよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
