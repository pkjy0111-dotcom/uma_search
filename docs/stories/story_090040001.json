    "11": {
      "frame_id": 11,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "はい。帰りがけに図書室に寄って――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "――嗚呼、我らウマ娘だけでなく、師さえも走るこの季節……。 駆けよ踊れよ、ただひたすらに。時に翻弄される身の虚しさよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "だが安心したまえ！　この覇王が来た！ 宴は間もなく開かれる！　約束が君たちの彩りとならんことを！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "ふふっ、こんにちは、オペラオーちゃん！ 何かの告知に来たんですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "いいや、まだその時ではない……トップロードさん、アヤベさん。 “世紀末覇王”伝説のカバレッタに、さらなる火をくべよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "え、ええっと……！？　何かお話がある……みたいなことですか？ わかりました！　カフェテリア、行きましょうか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
