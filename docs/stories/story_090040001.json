      "frame_id": 9,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "これくらいお安いご用です！ ほら、生徒会室でみなさんが待ってますよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
