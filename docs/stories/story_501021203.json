      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "まんまか！　ちゃうわ！\r “栗東の秘密兵器”や！",
