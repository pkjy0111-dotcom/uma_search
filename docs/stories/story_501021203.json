      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "『本当に強くなるのは来年。だから\r 菊花賞には出さない方がいい』ってな。",
          "flag_type": "",
          "flag": "",
          "choices": []
