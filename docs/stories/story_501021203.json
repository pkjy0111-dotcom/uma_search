      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "フフーン、ウチの将来性を認めたヤツが\r いるっちゅーこったな。さらには こんなことも言われとる。",
          "flag_type": "",
          "flag": "",
          "choices": []
