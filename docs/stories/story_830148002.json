      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90625,
          "speaker_name": "カメラマンA",
          "text": "あっ、これ、お世話になってる式場のパンフ。 興味があるなら、見学に行ったらどうかしら？",
          "flag_type": "",
          "flag": "",
          "choices": []
