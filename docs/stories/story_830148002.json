      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ありがとうございます！ 参考にさせていただきますね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 130,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "この式場、ブライダルフェアやってるんだ。 でも、これって普通は カップルで見に行くものよね……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "それ、結婚式場のパンフ？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
