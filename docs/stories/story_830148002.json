    },
    "7": {
      "frame_id": 7,
      "background_id": 130,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "あ、こんにちは。実はですね、 ファンの方の結婚式にお呼ばれしまして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
