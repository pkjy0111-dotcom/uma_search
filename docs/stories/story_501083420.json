      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "敗北したタニノギムレットだが、その走りで 観客を熱狂させた。シンボリクリスエスは、 彼女のような熱狂は作れないと感じる。",
          "flag_type": "",
          "flag": "",
          "choices": []
