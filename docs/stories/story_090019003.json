      "frame_id": 20,
      "background_id": 206,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "それは――――申し訳ありません。 おふたりに追いつけるよう、努めます。",
