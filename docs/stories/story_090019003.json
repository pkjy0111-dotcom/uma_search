      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "――よし。ほな今日は、頭から通しで見ていって、 現状のウチらの課題を確認してこか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
