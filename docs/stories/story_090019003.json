      "frame_id": 19,
      "background_id": 206,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "てか、ヤエノはここのパート得意やと思っとったんやけど…… なんか調子悪いんか？　声も勢いももう少し出せると思うで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
