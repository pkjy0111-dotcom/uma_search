      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……お二方の激情の表現には倣うべき部分が多いですね。 この部分は私が最も遅れているので、尽力しなければ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
