      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "まあアルダンの場合、普段デカい声出すイメージあれへんしな。 とはいえ、最初と比べて声出てきたと思うし、イケるイケる！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
