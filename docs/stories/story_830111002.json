      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "来られなくなっちゃった子たちの代役 頼まれたんだ！　さ～て、ボクの演技で お客さんたちを釘づけにするぞ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
