      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そうして劇は開演し、順調に進んでいった。 ……だが、ちょうど物語が 佳境に差し掛かった時――",
          "flag_type": "",
          "flag": "",
          "choices": []
