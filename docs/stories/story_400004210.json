      "frame_id": 9,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "それでないと髪がもわ～ん！　ってバクハツ してしまうんですの……こればかりは、 時間をかけるしかないんですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
