      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1059,
          "speaker_name": "メジロドーベル",
          "text": "まあ……そうだね。 ずっと同じもの、家から送ってもらってる。",
