      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1059,
          "speaker_name": "メジロドーベル",
          "text": "けど、ブライトの髪、 ふわふわしててアタシは好きだよ。",
