      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "まあ！　わたくしはドーベルのような サラサラの髪に憧れますわ。 やはり、ヘアオイルは特注のものを？",
          "flag_type": "",
          "flag": "",
          "choices": []
