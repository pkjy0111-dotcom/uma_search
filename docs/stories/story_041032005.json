      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "発汗しているね。\r 体温も……ふむ、見た感じ上昇しているようだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "え……え！？",
