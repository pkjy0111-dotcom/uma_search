      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "君の潜在意識にいる私は\r 『人の話を聞かず勝手に実験をするウマ娘』、ということになるな。",
          "flag_type": "",
          "flag": "",
          "choices": []
