      "background_id": 33,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "固執すンのも危険だがな。\r 今回は確信あるし、当然組みなおす。じゃ。",
          "flag_type": "",
          "flag": "",
          "choices": []
