      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その後、具体的な数式の話をされたが、\r 専門外だった……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
