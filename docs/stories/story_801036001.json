      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "あー、もういいわ。やっぱこの方面での\r アプローチをやめる。今決めた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "式を組みなおしたらどうかと伝える",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "根気よく続ければ結果が出ると伝える",
              "flag_type": "",
              "flag": ""
