      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "アア？　ハナからそのつもりだ。\r 『この方面』はやめるっつったろ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
