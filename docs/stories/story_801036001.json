      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "自分の方程式を立証できねェで、\r ハンパに終わらせるわけねェだろ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
