      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "あっ……そうですね。 そろそろバレンタインですし。",
          "flag_type": "",
          "flag": "",
          "choices": []
