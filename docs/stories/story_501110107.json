      "frame_id": 9,
      "background_id": 312,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――しかし最近は、 シーザリオの活躍に憧れて、クラブに 新規入会する子どもたちが急増していた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
