      "background_id": 312,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "毎年、クラブ・イリリアの子どもたちに、 バレンタインチョコを 手作りしてあげているシーザリオ。",
          "flag_type": "",
          "flag": "",
          "choices": []
