      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "心が温まる一方で、多くの子どもたちに チョコを手作りできるか、悩ましくもあった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
