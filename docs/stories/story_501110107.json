      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "できることなら、手作りで気持ちを 届けたいのですが……時間と、数が。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "俺が手伝ったら、少しは楽になる？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 12,
