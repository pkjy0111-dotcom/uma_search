      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "しゅ、『祝福』……！？ かしこまりました。騎士に二言はありません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
