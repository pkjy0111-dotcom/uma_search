      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "我が喉に潤いを！ 我が心に未知なる雫を！ 外界より誕生せしモクテルを頼む！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
