      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "他の人が作ったモクテルを飲みたい、 ということでしょうか。 ……私でよければ、承ります。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
