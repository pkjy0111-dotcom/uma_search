      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……理の鎖など無用。 今こそ、己を解き放つがいい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
