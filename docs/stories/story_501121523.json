      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "えっ？　ど、どういうこと……？ 外界……他の世界で誕生？　ええと……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
