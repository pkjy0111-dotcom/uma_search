      "frame_id": 7,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "（でも、『祝福』の味って……？ お祝いだから甘い？　それじゃ安直かしら）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
