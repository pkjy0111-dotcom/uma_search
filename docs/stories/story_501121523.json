      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "（己を解き放つ……自由にやれ、ってこと？ なら、私のイメージは――！）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
