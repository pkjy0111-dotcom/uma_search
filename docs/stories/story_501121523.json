    "5": {
      "frame_id": 5,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "騎士の厚情に感謝を。 では――『祝福』のモクテルを。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
