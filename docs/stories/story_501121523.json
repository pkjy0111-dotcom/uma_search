      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "幾度の困難があろうとも…… “王”から授かりし、称賛の言葉があれば！ どんな苦労も報われる！",
          "flag_type": "",
          "flag": "",
          "choices": []
