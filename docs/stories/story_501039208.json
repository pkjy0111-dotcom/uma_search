      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "今回出走する『宝塚記念』には\r カワカミプリンセスの1つ下の世代、 ウオッカも出走を表明した。",
          "flag_type": "",
          "flag": "",
          "choices": []
