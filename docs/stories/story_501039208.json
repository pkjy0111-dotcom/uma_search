      "frame_id": 11,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "燃えてきましたわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
