      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "ですが新主人公は決して諦めずに\r 迎え撃つ！　そしてその結果……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
