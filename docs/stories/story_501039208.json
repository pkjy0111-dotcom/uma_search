      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "やべっ、本当だ！\r 長居してすみません！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
