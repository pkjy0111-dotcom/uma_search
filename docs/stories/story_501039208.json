      "frame_id": 2,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "やっぱ自分の生き様を貫いてこその\r カッコよさですよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "いや、マジで先輩の感謝祭の宣言、\r めちゃくちゃシビれましたよ！ 改めて心が決まったっていうか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "（彼女はウオッカ。今年クラシック級で\r ティアラ路線から三冠路線に途中で変更し、 ダービーを勝利したウマ娘だ）",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "そろそろパドックの時間が始まるよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 25,
