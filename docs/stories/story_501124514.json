    "5": {
      "frame_id": 5,
      "background_id": 274,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "大丈夫！　気にしてないよ。 いいカメラだね。写真が好きなの？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
