      "background_id": 274,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "学生コンテスト？　へぇ～、面白そう！ そういうことなら私の写真を使ってよ！ 遠慮は要らないからさ！",
          "flag_type": "",
          "flag": "",
          "choices": []
