      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90350,
          "speaker_name": "カメラを持った少女",
          "text": "あ、はい……。すみません、 貴方の走りがあまりに綺麗で、気づいたら シャッターを切っていたと言いますか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
