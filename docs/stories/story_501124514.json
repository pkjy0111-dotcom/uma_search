    "7": {
      "frame_id": 7,
      "background_id": 274,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90350,
          "speaker_name": "カメラを持った少女",
          "text": "あの……撮ったデータはちゃんと消します！ 学生コンテストに出すのは、 別の写真にしますから……！",
          "flag_type": "",
          "flag": "",
          "choices": []
