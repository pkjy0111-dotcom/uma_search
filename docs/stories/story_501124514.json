      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90350,
          "speaker_name": "カメラを持った少女",
          "text": "あっ……！　ご、ごめんなさい！ 撮影許可も取らずに……。 ――っ、私ったらなんてことを……！",
          "flag_type": "",
          "flag": "",
          "choices": []
