      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90104,
          "speaker_name": "U.A.F.出場ウマ娘B",
          "text": "『ケガで引退しちゃったアタシだけど…… 覚えていてくれた人がいたんだな～って』",
          "flag_type": "",
          "flag": "",
          "choices": []
