      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『ワクワクアスリート系ウマチューバー、 ソノンエルフィーです！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 26,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『今回の動画も“U.A.F.”特別企画ッ！ 決勝大会出場選手の“今”を紐解く、 アスリートフューチャー！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 26,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『お話をうかがうのは こちらのアスリートウマ娘！ よろしくお願いしますッ！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 26,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『は～い、よろしく～。 にしてもまさか、マジで試合直後に インタビューとは驚きました、ハハハ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 26,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『こんな間近でMMAの試合を見たのは 初めてだったので、興奮しました！ 見事な勝利でしたね！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 26,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『いや～、前に同じように インタビューを受けてすぐくらいかな？ ちょっと身の回りに変化があって――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『――これがその、寄せ書きですか！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『そ。地方で走ってた頃から あたしを知ってるって人たちが、 わざわざ送ってくれたんですよ～』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『いやー……見てください、これ。 今も変わらず応援してますとか、 ずっと探してましたとか……』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『今日も皆さん、 あたしの試合を観に来てくれていて……――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『この人たちと私を結びつけてくれた U.A.F.には感謝してます。 だからこそ優勝、狙っていくつもりですよ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 25,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『いいですね～～ッ！！ バチバチの勝負をしましょう！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
