      "frame_id": 6,
      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "……思えばそのときから、 ドトウとオペラオーの因縁も 始まっていたんだよな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
