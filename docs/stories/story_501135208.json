      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "あのときのオペラオーの輝きは、 時代の頂点に相応しいものだったよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
