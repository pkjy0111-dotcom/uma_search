      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "……別世界の私とオペラオーは、 『宝塚記念』を、2回走っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
