    "5": {
      "frame_id": 5,
      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "そして『宝塚記念』で 最も強いと目されていたグラスを破り、 己の時代の訪れを高らかに宣言したんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
