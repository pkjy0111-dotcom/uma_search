      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "1回目の年、あいつは覇王の時代を築かんと、 圧倒的な強さを示しつつあった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
