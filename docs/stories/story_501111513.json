      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "数日前、最近注目のウマ娘として、 エアメサイアに雑誌のインタビューの 声がかかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
