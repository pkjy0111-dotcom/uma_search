      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そこでエアメサイアの要望もあり、 過去の記事を参考に質問事項を列挙し、 リハーサルすることになったのだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "最初に簡単な自己紹介をお願いします",
              "flag_type": "",
              "flag": ""
