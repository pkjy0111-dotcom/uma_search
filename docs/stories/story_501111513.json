      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "私の受け答えもよくなかったのでしょう。 次からはもう少し話を広げて、 情緒豊かに答えるようにしてみます。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
