      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90967,
          "speaker_name": "ダイヤの父親",
          "text": "したがって、ある試験を受けてもらおう。\r 君には……いや、2人には。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ある……試験？",
              "flag_type": "",
              "flag": ""
