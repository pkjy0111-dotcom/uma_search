      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90967,
          "speaker_name": "ダイヤの父親",
          "text": "気持ちはわかるが、さすがに実力も見ずに、\r 認めるわけにはいかない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
