      "frame_id": 13,
      "background_id": 31,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "お願いします……！　あなたのお力を、未来を、ダイヤに分けて\r くださいませんか？　みんなもわかってくれます。ですから！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
