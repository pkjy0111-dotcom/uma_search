      "background_id": 31,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "でも私、信じているんです！　家柄に眩むこともなく、真っ直ぐ\r 走りを見てくれたあなたとでなければ輝けないって。ですから……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
