    "11": {
      "frame_id": 11,
      "background_id": 31,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "マックイーンさんは……とても強いです……。しかも2400mなんて\r 未知の要素ばかりですし、簡単には認めていただけないでしょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
