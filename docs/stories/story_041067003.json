      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "あのっ、おいやでしょうか？\r 私と一緒に、マックイーンさんに挑むのは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
