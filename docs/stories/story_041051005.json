    "29": {
      "frame_id": 29,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "食事をしながら、トレーニングやウマ娘との付き合い方など、\r また色々と勉強させてもらった。礼をしたいのだが――腹が、重い。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 30,
              "loop_ends_at_frame": -1,
              "text": "お2人とも、今日は、ありが……うぷ……",
              "flag_type": "",
              "flag": ""
            }
          ]
