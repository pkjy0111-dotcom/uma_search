      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 90420,
          "speaker_name": "先輩トレーナーA",
          "text": "大丈夫ー？　君ってそんなに少食だったっけ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
