      "frame_id": 15,
      "background_id": 112,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "どこまでも続くような草原の中で、 穏やかに暮らす人々がいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
