      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "旅人である私を歓迎し、テントへと 案内してくれた。料理をふるまって、 寝床を整えて、歌を聞かせてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
