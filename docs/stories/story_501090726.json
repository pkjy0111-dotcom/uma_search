      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1090,
          "speaker_name": "ヴィルシーナ",
          "text": "シュヴァルも口には出さなかったけれど、 ちらちらと視線を向けていたものですから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
