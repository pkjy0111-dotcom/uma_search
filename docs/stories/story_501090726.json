      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1090,
          "speaker_name": "ヴィルシーナ",
          "text": "ヴィブロスったらぱかプチを見つけた途端、 『カワイイ！』って大はしゃぎして。",
