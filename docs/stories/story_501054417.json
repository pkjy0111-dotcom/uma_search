      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "主演をやり遂げて自信を得た俳優が、 ビコーペガサスを激励する。熱い血潮が ある限り、私たちは何度でも立ち上がれる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
