      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ビコーペガサスはいつだって負けない。 その背中には夢の翼があるからと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
