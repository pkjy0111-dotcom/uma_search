      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "（皆さんの前で、『神馬駆』の先駆けを堂々と務めよう。 私らしく、己を偽らずに――）",
          "flag_type": "",
          "flag": "",
          "choices": []
