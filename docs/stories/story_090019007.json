      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 90645,
          "speaker_name": "女性アナウンサー",
          "text": "――間もなく最初の儀式である『想撚』が始まるようです。 『蹄板』を焚き上げた炎の前に、『神馬』が姿を現しました。",
          "flag_type": "",
          "flag": "",
          "choices": []
