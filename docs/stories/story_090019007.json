      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "過去から未来へ、災い払いて想いを護り、繋ぐ。その種火、 切欠となるのは、『神馬』による苛烈なまでに力強き祈りだ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
