      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "私だからこそできる祈りが、なんであるかを理解した……！",
          "flag_type": "",
          "flag": "",
          "choices": []
