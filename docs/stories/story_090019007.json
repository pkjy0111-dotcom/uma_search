      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "――皆さんと一丸となって『神馬駆』について改めて学び、私は、 私の寄り添うべき想いを、寄り添いたいと願う祈りを知った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
