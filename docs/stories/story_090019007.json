      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90645,
          "speaker_name": "女性アナウンサー",
          "text": "――晩秋、囃子響きたる中、帳が下り……ここ『駿大祭』の 会場はにわかに神秘的な空気に包まれているように思えます。",
          "flag_type": "",
          "flag": "",
          "choices": []
