      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "咆哮を上げ炎と共に『駆場』を舞い巡るのは、その荒々しきで ウマ娘たちに降りかかる厄を振り払わんとするもの……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
