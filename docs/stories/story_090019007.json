      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "今こそ私はなる……！　ウマ娘の想いを燃え上がらせ、 不幸から彼女たちを護る、炎の『神馬』に！",
          "flag_type": "",
          "flag": "",
          "choices": []
