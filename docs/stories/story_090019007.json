      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "『想撚』は、過去のウマ娘の想いを炎に見立て、 それを再び燃え上がらせるための儀式であり――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
