      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "お願いいたします。 さらに強い、まことの烈風となるためにも。",
          "flag_type": "",
          "flag": "",
          "choices": []
