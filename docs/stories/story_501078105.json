      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ヤマニンゼファーはいつの間に、 ここまで成長していたのだろう。",
