      "frame_id": 13,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "正直――驚かされた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
