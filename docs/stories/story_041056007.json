      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "あ、小川ですね。こんなところに川があったなんて。\r へぇ～風流ですね～。",
