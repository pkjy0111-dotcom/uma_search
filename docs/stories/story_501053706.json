      "frame_id": 11,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90103,
          "speaker_name": "バンブーの幼馴染",
          "text": "……昔の私は、バンブーが求めるような 存在には、なってあげられなかったからさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
