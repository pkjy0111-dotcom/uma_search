      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "確か、いきなり引っ越しちゃったっスよね？ ショックだったんスよ！？ ひとことも言ってくれなかったから！",
          "flag_type": "",
          "flag": "",
          "choices": []
