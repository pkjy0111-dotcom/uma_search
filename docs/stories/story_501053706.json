      "background_id": 176,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1053,
          "speaker_name": "幼いバンブー",
          "text": "――そんでそんで！　父さんは来年の決勝で もう1度会おう！　ってその人と 誓いあったそうなんス！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 176,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1053,
          "speaker_name": "幼いバンブー",
          "text": "いいなあ～！　アタシも、 そんなライバルと熱い戦いしてみたいっス！",
          "flag_type": "",
          "flag": "",
          "choices": []
