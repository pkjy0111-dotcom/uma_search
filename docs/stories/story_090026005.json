      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "ヒシアマさんは、やっぱり少しも迷わないんですね。 すごいです。いつでもまっすぐで、ほんとうに……すごい。",
          "flag_type": "",
          "flag": "",
          "choices": []
