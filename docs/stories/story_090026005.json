      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "強いやつの背中が近づくほど、自分が強くなってるのを 実感できるだろう？　これほど嬉しいことはないね！",
          "flag_type": "",
          "flag": "",
          "choices": []
