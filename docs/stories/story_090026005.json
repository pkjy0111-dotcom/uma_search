      "frame_id": 14,
      "background_id": 9,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "きっと『がんばれる』のって、苦しいのをのりこえた先に、 それが吹き飛ぶような、大きな喜びや夢があるからだと思うんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
