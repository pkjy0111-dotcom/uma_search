    "13": {
      "frame_id": 13,
      "background_id": 9,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "いえ、すなおな気持ちですよ。ヒシアマさんにとっては、 当たり前すぎて気づけないのかもしれませんけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
