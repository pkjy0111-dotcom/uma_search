      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "だけど、わかっていても、みんなすぐ『忘れてしまう』んですよね。 どんなに好きなことでも、本気でかなえたいことでも。",
          "flag_type": "",
          "flag": "",
          "choices": []
