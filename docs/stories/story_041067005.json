    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "今年は私がリーダーになったんですよ♪\r 責任重大ですっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "成功させるためには、冷静で気の回る副リーダーさん……\r つまり――トレーナーさん！　あなたが必要なのです！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "もちろん協力するよ",
              "flag_type": "",
              "flag": ""
            }
