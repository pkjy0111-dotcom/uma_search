    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "わぁ、ありがとうございます！　みなさまに喜んでいただけるよう\r 学園中を華やか～にいたしましょうね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
