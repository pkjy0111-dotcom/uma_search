      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "――その名の通り、有志が集まって校内に花を植えたり\r 飾ったりする、毎年恒例の校内奉仕活動ですっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
