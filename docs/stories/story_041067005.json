      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "あら、ご存じありませんでしたか？\r えっと、『校内・花たくさん運動』というのは……。",
          "flag_type": "",
          "flag": "",
          "choices": []
