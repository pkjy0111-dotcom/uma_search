      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "おおっ！？ 急にどうしたんだい、マスター。",
          "flag_type": "",
          "flag": "",
          "choices": []
