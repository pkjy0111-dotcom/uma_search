      "background_id": 97,
      "sub_background_id": 101,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "ハハッ、ありがとな！ そんじゃ、遠慮なくいただくよ。 期待しといてくれ。",
          "flag_type": "",
          "flag": "",
          "choices": []
