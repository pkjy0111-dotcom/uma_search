      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90721,
          "speaker_name": "店主",
          "text": "『菊花賞』、出るんだろ？ 『皐月賞』からすっかりあんたのファンでさ！ 頑張れよ、応援してっから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
