      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90720,
          "speaker_name": "テレビの音声",
          "text": "『――決してまぐれではない強さを 示していましたからね。 私も、菊花賞での走りを期待していて――』",
          "flag_type": "",
          "flag": "",
