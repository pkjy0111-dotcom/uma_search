      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "をやるのは、やめておきましょうか。\r これは私の手で成し遂げること。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
