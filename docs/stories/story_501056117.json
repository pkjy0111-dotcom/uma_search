    },
    "22": {
      "frame_id": 22,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "わかりました。なら……これは願いじゃなく\r 別のものにします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "23": {
      "frame_id": 23,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "考えたら、神様の前でできるのは、\r お願いだけじゃありませんものね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "24": {
      "frame_id": 24,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "見よう見まねですけど――アレを！\r 今日こそ、ふさわしい日です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "25": {
      "frame_id": 25,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "誓詞奏上！　此れの神床に鎮まり坐す\r シラオキ神よ。我が誓い、受け取り給え。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "26": {
      "frame_id": 26,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "絶対に――『有馬記念』、勝ちます！　私に\r 誇らしさをくれた人たちに幸を届けるために。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
