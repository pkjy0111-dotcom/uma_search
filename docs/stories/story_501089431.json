      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "大盛り上がりだった『菊花賞』からの帰り道。 シュヴァルグランは、胸の内を打ち明ける。",
          "flag_type": "",
          "flag": "",
          "choices": []
