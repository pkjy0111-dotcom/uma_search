      "frame_id": 9,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "これで完璧です。 マーちゃん、安心してがんばれます。",
          "flag_type": "",
          "flag": "chance_branch_root",
          "choices": []
        }
      ]
