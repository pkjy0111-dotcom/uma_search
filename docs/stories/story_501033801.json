      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ひと通り確認したが、悪い箇所はない。 本人がこれに納得いっていない原因は、 単に経験不足によるものなのか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
