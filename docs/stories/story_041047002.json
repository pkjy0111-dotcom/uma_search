      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "あと1歩がなかなか届かなくても、 全身全霊でその差を縮めようと食い下がっていくウマ娘だったり。",
          "flag_type": "",
          "flag": "",
          "choices": []
