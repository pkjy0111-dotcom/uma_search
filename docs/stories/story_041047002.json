      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "今日は、そのクリスエスさんが走るはずなので…… 観に行ってみませんか、トレーナーさん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
