      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "憧れの先輩なんです。強くて、速くて、カッコよくって…… もう、デビューも済ませてます。",
          "flag_type": "",
          "flag": "",
          "choices": []
