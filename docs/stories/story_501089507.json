      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1089,
          "speaker_name": "シュヴァルグラン",
          "text": "内容は、すごくシンプルなお便りです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
