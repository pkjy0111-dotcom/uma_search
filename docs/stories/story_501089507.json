      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『シュヴァルグランさん 上手く言葉にできなかったのですが、 一言だけ……これからも、応援します』",
          "flag_type": "",
          "flag": "",
          "choices": []
