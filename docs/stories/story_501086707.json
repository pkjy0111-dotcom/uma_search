      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "あら、姉様の……。 貴方もお詣りですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ああ。彼女はしないようだから、代わりに",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ええ。彼女はしないようだから、代わりに",
              "flag_type": "",
              "flag": "female_trainer"
