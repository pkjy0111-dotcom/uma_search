      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "――はぁ、はぁ……！　気持ちいい……けど、 そろそろ限界かな……！",
