      "frame_id": 13,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "大丈夫です。2人で、ともに 風になりきればよいだけですから――",
