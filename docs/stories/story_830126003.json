      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "はぁ、はぁ、ふぅ……。 私はまだ、心に風が吹いているみたいです――",
