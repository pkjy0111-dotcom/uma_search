    "5": {
      "frame_id": 5,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "――涼風。そよ風。好風……。 次は、どんな風が来るかしら――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1093,
          "speaker_name": "？？？",
          "text": "――ゼファー？　ゼファー……だよね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "あら、ミラクルさん。 風に誘われて、いらっしゃったのですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "ああ、ごめん。邪魔したかな。 ランニング中、遠くにキミの姿が見えて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "あんまり気持ちよさそうに走ってたから、 つい……引き寄せられちゃって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "まあ。ふふふ……清風に乗って、 跳ねていたから、かもしれませんね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "あなたをここに誘ったのは、きっと時つ風。 ……一緒に、跳ね回ってみませんか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……いいの？　おれがいると、 その『風』を感じる邪魔にならない？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
