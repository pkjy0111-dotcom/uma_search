      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……そっか。ふふ―― じゃあ、ぜひ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
