    },
    "2": {
      "frame_id": 2,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1078,
          "speaker_name": "？？？",
          "text": "あら……わざわざありがとうございます。凱風な方ですね。 あの、しなとをご存じありませんか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "……なんて？",
              "flag_type": "",
              "flag": ""
