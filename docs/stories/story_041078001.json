      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1078,
          "speaker_name": "？？？",
          "text": "なので、しなとを探していたのですけれど、 どこにあるのかわからなくて。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ごめん。ちょっと何を言ってるのか……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1078,
          "speaker_name": "？？？",
          "text": "ああ、そうなのですね。それは梅雨の雨風ですね。 お時間をとらせてすみませんでした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
