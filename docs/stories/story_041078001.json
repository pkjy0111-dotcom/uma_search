      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "大きな木の傍に屈みこんで足元を見つめている ヤマニンゼファーの視線の先には、1羽の野鳥がいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
