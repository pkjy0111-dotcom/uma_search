    },
    "6": {
      "frame_id": 6,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1078,
          "speaker_name": "？？？",
          "text": "少々、急ぎますので、これで失礼します。 ありがとうございました、凱風な方。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "一体、何だったんだろう……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "あのう、気にしなくてもいいと思いますよ。 ヤマニンゼファーさんはいつもあんな感じなんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "なんだか不思議な子で、悪い子じゃないと思うけど、 ちょっと合わないというか、合わせる気がないというか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
