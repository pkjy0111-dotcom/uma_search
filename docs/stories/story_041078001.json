      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "……大丈夫ですか、だるまさん。 もう少しだけ、我慢してくださいね。",
