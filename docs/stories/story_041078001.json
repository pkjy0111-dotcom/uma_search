        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ある日の昼下がり、学園の校門前できょろきょろと 何かを探しているウマ娘を見かけた――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "どうかした？何か探し物？",
              "flag_type": "",
              "flag": ""
