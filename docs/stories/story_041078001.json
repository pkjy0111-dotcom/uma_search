      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "とにかくマイペースな子なので、あまり気にしちゃうと、 振り回されて疲れちゃいますよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう告げてウマ娘たちは去っていった。確かに不思議な子だったし、 言ってることもよくわからなかった。でも――",
          "flag_type": "",
          "flag": "",
          "choices": []
