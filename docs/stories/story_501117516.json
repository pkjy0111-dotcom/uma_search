      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90200,
          "speaker_name": "魚屋さん",
          "text": "たとえば、この青森で採れたヤマトシジミ！ どれも大粒でねぇ、 口の中に広がるうま味がたまらねえんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
