      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90200,
          "speaker_name": "魚屋さん",
          "text": "そうなんだよ！ 珍しく全国各地のしじみが入ってねぇ。 産地によって味も違うよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
