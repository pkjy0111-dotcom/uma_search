      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "しじみ、大好きなんです。 こんなにいろんなしじみが揃うなんて。 せっかくだし……全部食べようかな。",
          "flag_type": "",
          "flag": "",
          "choices": []
