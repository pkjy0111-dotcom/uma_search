    "4": {
      "frame_id": 4,
      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "走っている途中に泥がはねたのか―― ……顔に何かがついたみたいでね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
