    },
    "5": {
      "frame_id": 5,
      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "まあ、どのようなボクであっても 麗しいことに変わりはないが！",
