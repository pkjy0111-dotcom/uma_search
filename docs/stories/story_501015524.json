    },
    "13": {
      "frame_id": 13,
      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おやおや、恥じらいを隠さなくてもいいよ。 じっっっくりと、ボクの顔を見つめてごらん？",
          "flag_type": "",
          "flag": "",
          "choices": []
