      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "特に何かがついているような印象はないが、 とりあえず鏡を取りに行くことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
