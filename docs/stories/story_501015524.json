      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "はぁ……次のレースでは担架を用意しよう。 ボクを見て、気を失う子もいるかもしれない。",
          "flag_type": "",
          "flag": "",
          "choices": []
