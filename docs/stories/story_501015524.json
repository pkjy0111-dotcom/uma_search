      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ふっ……最近レースで大喝采を浴びるたびに 今までにない視線を感じていたが……。",
          "flag_type": "",
          "flag": "chance_branch_leaf",
          "choices": []
        }
