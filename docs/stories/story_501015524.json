      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "トレーナー君。鏡を持ってきてくれないか？ 美しいボクについて確認したいことがある。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "いいけど、なぜ？",
