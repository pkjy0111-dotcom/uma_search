      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "それも仕方のないこと……。 こんなにも壮麗なオーラは無視できない……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……そうだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……そうだね",
              "flag_type": "",
              "flag": "female_trainer"
