    "10": {
      "frame_id": 10,
      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "見てごらん、トレーナー君。 前に比べてより精悍に、 風格漂う顔になっているんだ。",
          "flag_type": "",
          "flag": "chance_branch_root",
          "choices": []
