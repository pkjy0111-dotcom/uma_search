      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90410,
          "speaker_name": "ベテラントレーナーA",
          "text": "こういうのはどう？　順番を決めて、 1人につき1日だけクリスエスさんの仮トレーナーになるの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
