      "frame_id": 11,
      "background_id": 74,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90425,
          "speaker_name": "中堅トレーナーA",
          "text": "抜け駆けしないでくださいよ、先輩！ クリスエスさん、俺の話も1回聞いてみてほしいんだけど……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
