      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "観客席の前を通り過ぎ、あっという間に小さくなっていく背中。 彼女が果たすという『使命』がなんなのか――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "（……知りたい）",
              "flag_type": "",
              "flag": ""
