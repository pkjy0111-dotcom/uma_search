      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90410,
          "speaker_name": "ベテラントレーナーA",
          "text": "クリスエスさん！　素質を感じさせる、素晴らしい走りね。 特に直線での力強い伸び！　ぜひ私と契約を――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
