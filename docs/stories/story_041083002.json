      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 22,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
