      "frame_id": 16,
      "background_id": 89,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "だが、美味しいごちそうのことを考えていたら、\r いつの間にか当日になってしまっていて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
