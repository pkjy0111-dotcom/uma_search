      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1050,
          "speaker_name": "ナリタタイシン",
          "text": "違いますけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
