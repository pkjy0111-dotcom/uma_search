      "frame_id": 21,
      "background_id": 157,
      "sub_background_id": 6310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "お気持ちは素晴らしいですが、やや具体性を欠きますね。\r ふむ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
