      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1050,
          "speaker_name": "ナリタタイシン",
          "text": "そんなことある？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
