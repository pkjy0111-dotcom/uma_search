    "5": {
      "frame_id": 5,
      "background_id": 40,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90605,
          "speaker_name": "記者A",
          "text": "あ、お疲れ様です！ お世話になっております！ すみません、お騒がせしてしまい……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "いえ。特別取材か何かですか？",
              "flag_type": "",
              "flag": ""
