      "frame_id": 7,
      "background_id": 40,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90605,
          "speaker_name": "記者A",
          "text": "あ、学園側から許可はいただいております！ お邪魔にはならないようにいたしますので、 なにとぞご容赦いただけますと……！",
          "flag_type": "",
          "flag": "",
          "choices": []
