      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "『皐月賞』の後に話した、新メニュー…… 私は、模擬レースを増やすことを提案したい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
