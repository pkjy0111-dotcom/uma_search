      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "レースに入れ込んでしまうのは、 経験が少ないのも要因の1つと考えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
