      "sub_background_id": 411,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そういえば、そんな話をしていたウマ娘が いたような気がする。そう、あれはいつかの 『日本ダービー』のレース後だった……――",
          "flag_type": "",
          "flag": "",
          "choices": []
