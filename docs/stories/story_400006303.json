      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "日本のファン",
          "text": "やっぱり、日本のウマ娘は…… 世界に通用するんだなあ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 24,
      "sub_background_id": 133,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "元担任からの手紙",
          "text": "『あなたは、日本人全員の夢を叶えてくれた。 夢を見る、皆の心臓を動かした』",
          "flag_type": "",
          "flag": "",
          "choices": []
