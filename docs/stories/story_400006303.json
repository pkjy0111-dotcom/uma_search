    "8": {
      "frame_id": 8,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "元担任からの手紙",
          "text": "『前に、お話ししましたね。 結果を見たいという欲求は、 別名、希望というのだと』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 0,
          "speaker_name": "元担任からの手紙",
          "text": "『あなたは、希望のその先を―― 夢が息づくさまを、私に見せてくれた』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
