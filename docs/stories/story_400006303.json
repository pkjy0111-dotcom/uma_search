    "10": {
      "frame_id": 10,
      "background_id": 245,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 0,
          "speaker_name": "元担任からの手紙",
          "text": "『……という文章を、実は今、 凱旋門賞前日の、 空港のロビーで書いています』",
