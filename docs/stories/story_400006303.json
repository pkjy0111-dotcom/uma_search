      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "……先生ッ！？ な、なんでいんだアンタ！？ ちゃんと医者に許可とったんだろうな！？",
