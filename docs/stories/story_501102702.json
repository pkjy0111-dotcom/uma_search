      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "これは職人向けの専門書だよ。 ヴァイオリンについて詳しく書いてある。",
