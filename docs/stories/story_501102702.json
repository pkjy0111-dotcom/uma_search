    "14": {
      "frame_id": 14,
      "background_id": 148,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "スィ。繊細なお姫様だからね。 弓の毛替えや駒の反り直しをさぼって、 美しい歌声を台無しにしては失礼だろう？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "毛替え？反り直しって？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 148,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "ふふっ、興味を持ってくれると思ったよ。 ちょっとそこで待っていてくれ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
