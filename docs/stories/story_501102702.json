      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "この店で定期的に見てもらっているんだ。 いつもは予約を取るんだが、 ちょうど空きがあったからついでにね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "楽器もメンテナンスが必要なのか",
              "flag_type": "",
              "flag": "male_trainer"
