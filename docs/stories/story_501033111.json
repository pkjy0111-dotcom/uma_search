      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『これは手紙なのか、それとも日記なのか、 考えてみればよくわからないけれど』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 37,
      "sub_background_id": 530,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『どちらでもいいことにして、とにかく、 書き残しておくことにしました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 37,
      "sub_background_id": 530,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『いつかあなたと会えた時、なるべく たくさんのことを話せるように』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 37,
      "sub_background_id": 530,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『“私”が生きた時間を、 私に命をくれたあなたに、 きちんと分け合えるように』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 4,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『昨晩はいつもの4人で、 天体観測に行きました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 4,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『いつもの4人、だなんて書いてしまうのは、 なんだかまだ少し、複雑な気分だけれど』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 4,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『でもいい加減、そう呼ばないというのも 難しくなってきたと、正直、私も思います』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『星を見ながら、しばらくの間オペラオーは 勝手な名前をつけたり、勝手な伝承を 高らかに語り出したりでうるさかったけど』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『（一応、全部訂正はしておきました）』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『天体観測に行こうと言い出した張本人の 割には、やっぱりすぐ眠たくなったようで』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『倒れかけたところをドトウが支えに行って、 結局2人で寄り添って眠っていました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『だからまた――以前の海での時のように また、トップロードさんだけが起きていて』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『星の話を、たくさん、しました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『あまりに飽きずに聞いてくれるものだから、 つい、少し、話しすぎてしまったようで』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
