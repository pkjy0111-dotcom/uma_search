      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『カレンさんに関しては、もう少し きちんとお礼をした方がいいのだろうと、 書きながら思いました』",
          "flag_type": "",
          "flag": "",
          "choices": []
