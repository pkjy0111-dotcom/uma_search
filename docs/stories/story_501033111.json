      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『その笑顔がやけに、 嬉しそうにも見えたからで』",
          "flag_type": "",
          "flag": "",
          "choices": []
