      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『けれども、笑われたというのに、 思っていたより怒る気になれなかったのは』",
          "flag_type": "",
          "flag": "",
          "choices": []
