      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1033,
          "speaker_name": "アドマイヤベガ",
          "text": "『翌朝、いつになく声が嗄れていたのを、 カレンさんに笑われました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
