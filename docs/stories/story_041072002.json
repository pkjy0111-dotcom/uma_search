      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "私は叩き直さねばならないのです！\r 溢るる暴に、内なる『烈火』に惑う己の未熟さを！",
          "flag_type": "",
          "flag": "",
