    "2": {
      "frame_id": 2,
      "background_id": 33,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "レース前日にここで見たヤエノムテキと、\r レース本番で見たヤエノムテキ――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "（なぜああも違っていたんだろう……？）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "3": {
      "frame_id": 3,
