      "frame_id": 8,
      "background_id": 12,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "開店と同時に最速で購入する。確かに、\r シンプル且つ合理的です。検証します。",
          "flag_type": "",
          "flag": "",
          "choices": []
