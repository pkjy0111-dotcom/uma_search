      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "『食べた感想を聞いてみたい』と言われ、\r それを達成すべく商品購入を試みたところ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
