      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "私のスケジュールと搬入タイミングを照合。\r ――照合完了。開店前待機、可能です。",
