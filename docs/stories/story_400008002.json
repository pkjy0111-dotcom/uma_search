      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "それぞれの試食会までに獲得した 『お料理Pt』に応じて、『試食会』での ウマ娘たちの満足度も変化します。",
          "flag_type": "",
          "flag": "",
          "choices": []
