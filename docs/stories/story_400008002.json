      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "育てた野菜で料理を作り、 みんなで味見する『試食会』が、 計4回にわたり開かれることになります。",
          "flag_type": "",
          "flag": "",
          "choices": []
