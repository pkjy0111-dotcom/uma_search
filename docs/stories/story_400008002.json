      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "大満足してもらえれば、みんなの料理への 意欲も高まり、作った料理の効果も変化！ ウマ娘たちも大きく成長することができます！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
