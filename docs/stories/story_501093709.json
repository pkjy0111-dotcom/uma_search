      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "頑張ったつもりじゃダメなんだ。 あと一歩、勝ち切るためには――",
