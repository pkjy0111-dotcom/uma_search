      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "みんなの期待に、応えてみせます。 今度こそ！",
          "flag_type": "",
          "flag": "",
          "choices": []
