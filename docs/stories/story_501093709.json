      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "悔しいなぁ……ファンのみんなも トレーナーさんも、おれを精いっぱい 支えてくれているのに……。",
          "flag_type": "",
          "flag": "",
          "choices": []
