      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "トレーナーさん…… はい、そうですね！　足りるまで……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
