      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "決まってます！　頑張るだけ！ おれが、前を向けばいいだけです！",
