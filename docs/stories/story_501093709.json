      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……おれ、頑張ります！ 明日からもっともっと、自分を甘やかさずに トレーニングしますね！",
          "flag_type": "",
          "flag": "",
          "choices": []
