      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "足りなかったのは、おれ。 おれの頑張りだけです……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "じゃあ、足りるまで一緒に頑張ろう！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "なら、このあとはどうする？",
