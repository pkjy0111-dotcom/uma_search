      "frame_id": 3,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1105,
          "speaker_name": "ネオユニヴァース",
          "text": "ネオユニヴァースは“ILLN”を拾ったよ。 N36°58' - 42°30' W87°30' - 91°30'だね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90103,
          "speaker_name": "クラスメイトのウマ娘",
          "text": "へ？　え、っと……このピース、どこのだろ。 とりあえずありがと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "どうやら2人は立体パズル型の地球儀を 落としてしまったようだ。よく見ると、 ピースは国や州の形になっている。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "俺も手伝うよ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "私も手伝うよ",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
