    "2": {
      "frame_id": 2,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90103,
          "speaker_name": "クラスメイトのウマ娘",
          "text": "も～、あたしのバカ……全部バラバラだし。 てか何で教材の地球儀が立体パズルなの……？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
