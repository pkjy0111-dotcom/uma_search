      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90103,
          "speaker_name": "クラスメイトのウマ娘",
          "text": "ありがとうございます！ 正直、全然わからなくて…… 国や州の正確な場所なんて、難しすぎて――",
          "flag_type": "",
          "flag": "",
          "choices": []
