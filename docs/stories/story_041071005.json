          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "トレーナーさん、準備できました。\r ラスト1本、お願いいたします。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "――それじゃ、用意、スタート！",
              "flag_type": "",
              "flag": ""
            }
