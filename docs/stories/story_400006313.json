      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90126,
          "speaker_name": "ヘアバンドのウマ娘",
          "text": "自分たちはそういうレースが観たいんだって。 だから、協力するんだって言ってた。",
