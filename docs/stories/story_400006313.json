      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90126,
          "speaker_name": "ヘアバンドのウマ娘",
          "text": "なんで親切にしてくれるのって言ったら…… 外国からのウマ娘にも、『凱旋門賞』を 全力で走ってもらいたいからだって。",
          "flag_type": "",
          "flag": "",
          "choices": []
