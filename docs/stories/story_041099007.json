      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "えっと……あ、これ柿ですよね、たしか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "大正解！さすが、詳しいな",
              "flag_type": "",
              "flag": "male_trainer"
