      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "えへへ。でもあまり見慣れてはなくて……今日見られて嬉しいです！ 北海道には柿の木って、自生しないので。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
