      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "ネイチャーウォッチング、ですか？ 私、この辺の自然にはあまり詳しくないんですが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "俺がエスコートするよ！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "私がエスコートするよ！",
              "flag_type": "",
              "flag": "female_trainer"
