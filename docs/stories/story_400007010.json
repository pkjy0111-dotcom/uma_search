      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "先輩トレーナーが今回の合同練習から、 監督役として参加をすることになり――",
          "flag_type": "",
          "flag": "",
          "choices": []
