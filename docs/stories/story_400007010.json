      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90054,
          "speaker_name": "ハット帽のトレーナー",
          "text": "……ああ。エルフィーが学園にいた頃、 私が担当トレーナーだったんだよ。",
