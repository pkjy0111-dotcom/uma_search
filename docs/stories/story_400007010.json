      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90054,
          "speaker_name": "ハット帽のトレーナー",
          "text": "『いや、突然申し訳ない。 こんなことを相談するのには、 少々、個人的な理由があるんだが……――』",
          "flag_type": "",
          "flag": "",
          "choices": []
