      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ダイワスカーレットの\r 調子が良くなるよう心から祈願した。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
