      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "でも……そうね。\r 少しだけ良くなった気がする……かも？",
          "flag_type": "chance_branch_leaf",
          "flag": "chance_branch_leaf",
          "choices": []
