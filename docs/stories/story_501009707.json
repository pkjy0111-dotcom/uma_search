      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "……よしっ、こんなもんかしら。\r 少しは効くといいんだけど……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "何を祈願してたの？",
              "flag_type": "",
              "flag": ""
