    "3": {
      "frame_id": 3,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "なんだか最近、体が重くってね。\r いろいろ試したけど治る気配もないし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
