      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "あら、殊勝な心掛けじゃない。",
          "flag_type": "",
          "flag": "",
          "choices": []
