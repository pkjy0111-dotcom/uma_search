      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ま、プラシーボ的ななにかでしょうけど。\r ひとまず少し走って、調子を確かめたいわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
