      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ふぅん、随分真剣ね……。\r ま、担当だし当然か……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "どう？ご利益はあった？",
