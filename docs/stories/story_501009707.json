      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ふふっ、バカね。\r そんなにすぐ効果が出るわけないじゃない。",
