      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ここって無病息災にご利益があるらしいから、\r 気休めにでもなればいいなって思って。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "なら俺も祈願してくるよ",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "なら私も祈願してくるね",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
