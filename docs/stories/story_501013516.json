        {
          "next_frame": 6,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "この誘惑に、立ち向かう術はありませんわ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "せっかくだし行こう。俺も付き合うよ",
              "flag_type": "male_trainer",
