      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "トレーナーさん、これは……夢ですわよね？ あの旗、なんて書いてあるように見えます？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "『喰らい尽くせッ、モンブラン食べ放題！』",
