      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "江戸ソーギョーって、激エモじゃん！ テンアゲよいちょまる～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
