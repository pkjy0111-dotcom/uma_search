      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "てかてか、江戸で流行ったのって チェック柄なん！？　ぎゃんかわすぎ！ ウチも青チェックとか着てぇ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
