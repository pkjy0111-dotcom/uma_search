      "background_id": 59,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "推し活ってやつかな？ 推しの真似したいって気持ちはわかるかも！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
