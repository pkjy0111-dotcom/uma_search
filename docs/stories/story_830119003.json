      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "旅館のスタッフ",
          "text": "江戸時代に確立された『つまみ細工』の 技術は、大いに流行しました。今日は この技を使ってかんざしを作ってみましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
