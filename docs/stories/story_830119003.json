      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ふふ、江戸時代の人々はまさに その『激エモ』の生活をしていたんですよ。 少し想像してみましょうか――",
          "flag_type": "",
          "flag": "",
          "choices": []
