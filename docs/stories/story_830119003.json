      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "こーんなちまちま作業で ゴーカなお花作ったりしてさー、 こんなん頭にさすとか激エモじゃん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
