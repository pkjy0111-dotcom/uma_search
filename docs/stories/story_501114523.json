      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1114,
          "speaker_name": "ブエナビスタ",
          "text": "……本当に、勝負強いんですね。 レイズばかりで、ヒヤヒヤしましたけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
