      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "でも……『私と走るのが楽しい』『かけっこが好きだ』と言って……\r アイツはいつも、私の背中を追ってきてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
