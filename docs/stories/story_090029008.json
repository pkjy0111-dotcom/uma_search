      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "だが皆で創り上げた物語が、酩酊を生んだあの刹那…… ただ度数の高い酒を創っても、これほどの余韻は生まない。",
          "flag_type": "",
          "flag": "",
          "choices": []
