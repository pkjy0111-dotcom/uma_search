      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "先導してくれるバブルガムフェローの言葉を信じ、 一歩ずつ、奥へ奥へと進んでいくと……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
