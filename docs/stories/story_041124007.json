      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "よかった。 じゃあ、私と探検しよう！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
