      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "『ライブしてるところをネットで見たけど、 とっても盛り上がってて、感動しちゃった！』",
          "flag_type": "",
          "flag": "",
          "choices": []
