    },
    "13": {
      "frame_id": 13,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "私生活のことまで気にしてないで、\r アンタはトレーニングだけに集中して。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "でも、君を支えるって言っただろう？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "でも、貴方を支えるって言ったよね？",
