    },
    "10": {
      "frame_id": 10,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "わかってるわよ。けどね、アタシは優等生なの。",
          "flag_type": "",
          "flag": "",
          "choices": []
