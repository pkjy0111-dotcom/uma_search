      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "……ふぅっ。朝からいろいろ頼まれごとの多い日だったわね……。\r さっさとトレーニングの準備に行かないと――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "疲れた顔してるな",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "疲れた顔してるね",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ひゃあ！？　ト、トレーナー！？　びっくりさせないでよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "……ったく、わざわざ呼びに来たわけ？\r 余計なお世話だっての！　今から行くとこだったんだから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "……それと、別に疲れた顔なんてしてないから。\r アンタの気のせいよ、気のせい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "頼まれごとなんて日常茶飯事なんだから、どうってことないわ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "無理はしないようにな？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "無理はしないようにね？",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
