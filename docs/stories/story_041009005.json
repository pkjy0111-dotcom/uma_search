    },
    "11": {
      "frame_id": 11,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "誰よりも頼りになって、誰の期待も裏切らない。\r 生活態度も勉強も走りも、誰にも負けない1番のウマ娘。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "それがダイワスカーレットなの！\r このくらいのこと、自分でどうにかするわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
