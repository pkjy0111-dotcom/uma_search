      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90435,
          "speaker_name": "同期トレーナーA",
          "text": "バブルガムフェローはその中でも、特に積極性があったらしくて。 噂じゃ『切り込み隊長』とか言われてたらしいぞ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
