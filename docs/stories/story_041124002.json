      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "同期トレーナーたちとの食事の席で、バブルガムフェローについて 聞いてみると、どうやらみんな、何かしら知っているようだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
