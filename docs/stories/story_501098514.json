    "3": {
      "frame_id": 3,
      "background_id": 37,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1067,
          "speaker_name": "気品のある声",
          "text": "夜の学園で連日泣いていたのは、 それだけ悲しいことがあったんですよね？ 遠慮せず話して、安らかに成仏してください！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "いや、誰！？幽霊ってなに！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 37,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1098,
          "speaker_name": "聞きなれた声",
          "text": "待った待ったーーーーー！ その人、私の知り合いかも！？ っていうか……トレーナーだよね！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "やっぱりトレーナーだった！ ビックリさせちゃってごめーん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "すみませんっ！　あたしたち幽霊を追ってて、 ちょうどそれっぽい影が見えたので、 てっきり……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "最近、夜にうちのクラスを訪れると、 幽霊のすすり泣く声がするって噂が 広まっているんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "結構な子たちが怖がっていて…… だからキタちゃんと一緒に、幽霊さんに 成仏してくださいってお願いに来たんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "なるほど……で、リッキーはなんでここに？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "私も同じだよ！ みんなの不安を解決してハッピーにするため☆ カワイイ後輩の力にもなりたかったし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
