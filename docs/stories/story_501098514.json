    "2": {
      "frame_id": 2,
      "background_id": 37,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1068,
          "speaker_name": "元気な声",
          "text": "ふう、捕まえましたよ幽霊さんっ！！ ああっ、けど後生ですから、 たたらないでくださいね……！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
