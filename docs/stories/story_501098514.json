      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "まあ、原因は幽霊とは限らないけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
