      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女は日本のウマ娘の海外遠征を 応援するチームを受け持っているらしく――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
