      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "――うん、今日は実にいい話が聞けた！ 本当にありがとう。",
