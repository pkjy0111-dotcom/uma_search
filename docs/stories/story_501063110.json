        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "イクノディクタスとの3年間が終わり、 時が少しだけ流れ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
