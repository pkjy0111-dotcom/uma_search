      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "イクノディクタスと自分の管理技術を、 今後の海外遠征に積極的に 活かしていきたいとのことだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
