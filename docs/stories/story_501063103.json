      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "『秋華賞』にむけて…… 気合いを入れなければ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
