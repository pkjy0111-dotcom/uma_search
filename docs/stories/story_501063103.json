      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『日本ダービー』以降、イクノディクタスの 気合いの入り方が少し変わったように思える。",
          "flag_type": "",
          "flag": "",
          "choices": []
