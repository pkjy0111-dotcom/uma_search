      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "世間では、少し前に休養を発表した アイネスフウジンを心配する声が 多く上がっていた……――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
