      "frame_id": 8,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "こういった機会こそ、マネジメントの技術が 最大限に活かされる時ですからね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
