      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "同室のマックイーンさんから 伺った話なのですが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "……アイネスさんの休養が発表されて以来、 ライアンさんは鬼気迫る勢いで トレーニングをされているようです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "……そうか",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "……そっか",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 64,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "私もライアンさんに倣って、 この合宿を最大限に活かし、 秋に備えたいと思っています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
