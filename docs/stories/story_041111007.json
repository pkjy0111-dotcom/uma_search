    "12": {
      "frame_id": 12,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "それらによる消耗は今後、確実に自分を追い込んでいき、 疲労に繋がっていくだろう……と予想できた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "（……何か方法を考えないとなあ）",
              "flag_type": "",
              "flag": ""
