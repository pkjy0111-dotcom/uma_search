      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "夏合宿が始まった。始まって早々、 エアメサイアの調子は上がり、トレーニングは順調に運んでいる。",
          "flag_type": "",
          "flag": "",
          "choices": []
