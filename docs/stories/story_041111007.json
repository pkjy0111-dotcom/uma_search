      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "今日も首尾よく終わり、あとはクールダウンを済ますだけ。 気になることがあるとすれば、それは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "今日はいい休養、取れそう？",
              "flag_type": "",
              "flag": ""
