      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "普段と違う環境に加え、いつもと違うスタッフとのやり取り、 合宿ならではのトレーニングメニューの考案――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
