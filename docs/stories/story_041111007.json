    },
    "4": {
      "frame_id": 4,
      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "夏合宿は寝起きする場所が変わり、食べるものも生活リズムも 変化する。そんな中、彼女がリラックスできているか気になった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "あ―― そのこと、なのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 181,
