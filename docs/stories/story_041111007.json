      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "……実はあまり、充実できるものに出会えていなくて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "この環境では、使用できるのは電子書籍くらいですので。 能動的な休養に取り組む、という意味ではあまり。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "そうか、何か見つけられるといいな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "そっか、何か見つけられるといいね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "はい。合宿期間はまだありますし、 何か見つけられたら、と思っております。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
