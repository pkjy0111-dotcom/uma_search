      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おやトレーナー君、怪訝な顔をしているね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "どうしてボクが、これほどご満悦なのか。\r その理由がわからないと見える。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "上機嫌な理由は……ボクがこの夏、\r とてつもなく大きな成果を1つ得たからさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "なんだったと思う？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "実力の向上？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "それは確かにあったね。とくに前世代との\r トレーニングでは成長著しかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "しかし成長以上に大きかったものがある。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "それは……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
