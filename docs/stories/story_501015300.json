    "11": {
      "frame_id": 11,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ふふふ、それは……\r “情報”だよ“情報”。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 61,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "トレーニングに同行し、前世代のリアルな\r 強みを、この目と耳で確かめることができた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 61,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "スぺ君の底力、グラス君の聡明さ。\r そういったものを事細かにね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
