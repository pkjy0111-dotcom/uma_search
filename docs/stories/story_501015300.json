      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "全てを見抜くボクの『覇王アイ』にかかれば、\r もはや彼女らの能力は丸裸も同然さ！",
          "flag_type": "",
          "flag": "",
          "choices": []
