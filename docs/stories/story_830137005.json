      "background_id": 136,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 9040,
          "speaker_name": "ダーレーアラビアン",
          "text": "過去から託された想いか……！ 君にとって大切な思い出なんだな！",
