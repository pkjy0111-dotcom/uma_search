      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "私もです！　この思い出の場所―― サトノ家の想いは、他の方にも 知ってもらいたいですから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "じゃあ、この場所は君の……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "じゃあ、この場所は貴方の……",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
