      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ここは――私とクラちゃんが トレセン学園に入学をした時、 お祝いをいただいた場所なんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
