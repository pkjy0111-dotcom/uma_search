      "frame_id": 8,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "サトノ家の人たちは、もう大喜びで！ 代々語り継がれる悲願を語り合って……。",
          "flag_type": "",
          "flag": "",
          "choices": []
