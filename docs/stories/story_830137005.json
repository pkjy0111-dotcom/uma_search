      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 9040,
          "speaker_name": "ダーレーアラビアン",
          "text": "へえ！　ここが君の故郷か。 潮風が心地いい、いい町だな！",
