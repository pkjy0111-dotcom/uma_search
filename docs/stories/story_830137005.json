    "4": {
      "frame_id": 4,
      "background_id": 136,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9040,
          "speaker_name": "ダーレーアラビアン",
          "text": "誰かの大切な風景を巡る旅がしたくてね。 彼女たちに声をかけたら――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 136,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "大歓迎です！　1人でも多くの方に見せたい 風景がありますから！ なので……トレーナーさんも、ぜひっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
