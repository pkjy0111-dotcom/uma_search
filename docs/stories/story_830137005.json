    "9": {
      "frame_id": 9,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "いつか、サトノ家の誰もが成し得なかった。 そんな歴史を切り拓く―― そう、想いを託されたんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
