      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9040,
          "speaker_name": "ダーレーアラビアン",
          "text": "けど、これも太陽の導きだな。 よかったら、俺たちと一緒に旅をしないか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "俺“たち”……？",
              "flag_type": "",
              "flag": ""
