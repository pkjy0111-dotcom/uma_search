      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "そうなんですっ！　苫小牧は、 私の大好きな故郷なんです！",
