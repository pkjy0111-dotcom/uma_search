      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9040,
          "speaker_name": "？？？",
          "text": "おや、子羊くんじゃないか！ 間違えて、君まで転送してしまったようだな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
