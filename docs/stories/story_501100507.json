    "3": {
      "frame_id": 3,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "温かい気持ちと敬意を込めて、 ワンダーアキュート――と。これでよし！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "何してたんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "何してたの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ファンレターにちょいとお返事をね。 ふふっ、なんと海外から届いたんだよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "なんでも日本に来た時に、偶然 あたしの出たレースを観てくれたんだって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "『粘り強く挑む姿勢に勇気をもらった……』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "『またいつか日本でレースを観るのを目標に、 日々の仕事をがんばっていきます――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "驚きよねぇ。あたしの走りが、 遠い海の向こうの人にも届いてたんよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "昔のあたしは、ジムの練習生さんをリングの 下から応援するくらいしかできんかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "だけどトレセン学園へ来て、 トレーナーさんと沢山トレーニングして――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "精一杯レースに出てたら、知らない間に 知らない誰かを励ますことができてた……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "なにより嬉しかったのは、 この人がもう1度あたしに会うのを 目標にしてくれたことなんよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "がんばる誰かを支えたいって走ってきたけど、 こんな届きかたもあるのねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "より長く、より多く走っていかないとな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "より長く、より多く走っていかないとね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
