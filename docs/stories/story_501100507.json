      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "あらまあ、本当だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "せっかく日本に来ても、あたしが元気に 走れていないんじゃあ、台無しだものね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
