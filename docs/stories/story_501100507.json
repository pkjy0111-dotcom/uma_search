      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "A journey of a thousand miles begins with a single step.",
          "flag_type": "",
          "flag": "",
          "choices": []
