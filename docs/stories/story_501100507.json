      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "お互いに千里の道を1歩ずつ進んで、 笑顔でもう1度会えるようにしないと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
