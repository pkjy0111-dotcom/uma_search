      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "それらを読んでいると、理論的に\r 成立していない部分が気になってしまうんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
