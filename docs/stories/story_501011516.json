      "frame_id": 3,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "あ、トレーナーさん。ちょうど、\r お昼ご飯の量で悩んでいまして～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
