        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ある日の昼下がり。グラスワンダーが、\r 何やら難しい顔をしている……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
