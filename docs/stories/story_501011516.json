      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "面白かったんですよ～。スぺちゃんなんて、\r ずっと両手にお茶菓子を握っていて……ふふ♪",
          "flag_type": "",
          "flag": "",
          "choices": []
