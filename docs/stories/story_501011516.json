      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "はい。つい先ほどまで、クラスの方々と\r 一緒に野点を行っていたんですが――",
          "flag_type": "",
          "flag": "",
          "choices": []
