      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "……ともあれ、どういたしましょう？\r 『身から出た錆』ではありますが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
