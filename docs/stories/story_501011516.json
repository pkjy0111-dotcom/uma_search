      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "うう～ん。\r 流石にこれ以上食べるのは……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "どうしたの？",
              "flag_type": "",
              "flag": ""
