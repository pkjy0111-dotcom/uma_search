      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "その……お恥ずかしながら、\r 既にお腹がいっぱいなんです……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "間食でもしたのか？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "間食でもしたの？",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
