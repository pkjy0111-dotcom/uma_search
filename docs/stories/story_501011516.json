      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "ふふ。でも野点、楽しかったんですよ～。\r 今度はトレーナーさんも一緒にしましょう？",
