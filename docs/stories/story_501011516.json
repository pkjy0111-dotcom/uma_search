      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "みなさんと点てるお茶が美味しくって、\r ついついお茶菓子も食べすぎてしまいまして。",
          "flag_type": "",
          "flag": "",
          "choices": []
