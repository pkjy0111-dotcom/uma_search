      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "まあ……そうですね。\r 本日は節制することにいたします。",
