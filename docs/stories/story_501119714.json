      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "自重トレーニングもいいかもしれません。 いずれにしろ、ここで立ち止まるのは 致命的でしょう？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "今は安静にすべきだよ",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……無理だと判断したらすぐに止めるよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
