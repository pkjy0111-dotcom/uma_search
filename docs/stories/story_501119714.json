      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ここで無理をすれば、レース人生に 響くかもしれない。そんな無理は 絶対にさせられないと、説得した。",
          "flag_type": "",
          "flag": "",
          "choices": []
