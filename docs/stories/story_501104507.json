      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "『私もトゥインクル・シリーズを目指して いましたが、周囲から田舎出身では 無理だと言われ、諦めかけていました』",
          "flag_type": "",
          "flag": "",
