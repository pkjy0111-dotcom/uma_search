      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "『壁に立ち向かう勇気をくれたエースさんと、 いつかターフで会えることを願っています。 本当に、ありがとう！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
