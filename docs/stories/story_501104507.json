        {
          "next_frame": 4,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "なになに――『カツラギエースさんへ。 私は、エースさんの大ファンです――』",
          "flag_type": "",
          "flag": "",
          "choices": []
