      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "『でも、力強い貴方の走りがそれは偏見だと、 私も夢を見ていいのだと教えてくれました！』",
          "flag_type": "",
          "flag": "",
          "choices": []
