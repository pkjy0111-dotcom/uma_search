      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "あうっ、すみません……！ たくさん食べてほしくて、つい！",
