    "3": {
      "frame_id": 3,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "一応ね、マックイーンやアルダンに お茶会のマナーを見てもらってたんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
