      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "今度のファンイベントの準備だよ！ “キチンとした”お茶会をやるからさ～。",
          "flag_type": "",
          "flag": "",
          "choices": []
