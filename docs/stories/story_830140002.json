    },
    "4": {
      "frame_id": 4,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "とはいえ、テイオーさんは所作も完璧なので ほぼほぼ私の練習の場なんですけれど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "ニッシシ♪　まあね！ テイオー様にできないことはないのだ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "さすがテイオーさん……！ 私も早く、自信を持っておもてなし できるようにならないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "あ、そうだ！　お客さん役をやってくれない？ こういうのって雰囲気も大事だと思うからさ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "そういうことなら協力するよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ふふ、感謝いたします。それでは改めて、 おもてなしの練習を始めましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "どうぞ、紅茶がお気に召したようで 何よりです。またお申しつけください。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "おかわりありがとう！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "お代わりはホストの仕事―― 問題ありませんわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "チヨノオーさんも、ケーキスタンドの準備が できましたね。ただほんの少しばかり、 お皿のお菓子が多いかもしれません。",
          "flag_type": "",
          "flag": "",
          "choices": []
