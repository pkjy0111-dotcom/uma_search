      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "みんなに愛される マスコットになるためですから。 マーちゃんはがんばりますよ、むん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
