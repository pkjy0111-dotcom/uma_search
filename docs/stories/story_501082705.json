      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "えっと、確かハト……じゃなくて カラスだったかな。マーちゃんには いろんなお友だちがいるみたいで。",
