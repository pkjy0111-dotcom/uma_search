      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "明けましておめでとうございます、 トレーナー。いますか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ライトオ？",
