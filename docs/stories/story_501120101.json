    "4": {
      "frame_id": 4,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "寝癖すごいですね。写真いいですか？ デュランダルにも見せたい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "絶対ダメ！",
              "flag_type": "",
              "flag": ""
            }
