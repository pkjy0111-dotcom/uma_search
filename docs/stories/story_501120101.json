      "frame_id": 5,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "チッ。 こちら帰省土産です、どうぞ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "『娘が大変お世話になっています。 ほんの気持ちです。違法性はありません。 どうぞお納めください』と、父が。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……怪しいものでも入っているのだろうか。 それとも単に説明が丁寧なだけなのか。 とりあえずありがたくいただこう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "休み明けでもよかったのに",
              "flag_type": "",
              "flag": ""
