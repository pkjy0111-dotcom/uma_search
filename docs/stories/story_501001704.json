      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "トレーナーさんが傍にいてくれるおかげで\r その不安がなくなってきた気がするんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
