      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "改めて、すごい人なんだなあって思います。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "お母さん代わりだと思ってくれていいよ",
