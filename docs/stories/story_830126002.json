      "frame_id": 17,
      "background_id": 68,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "あはは、なりきらなくたっていいから。 いくらなんでも、感情移入しすぎ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
