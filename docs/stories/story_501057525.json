    "6": {
      "frame_id": 6,
      "background_id": 111,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90957,
          "speaker_name": "シービーの父親",
          "text": "伊達に長年トレーナーをしてきたわけじゃ ないからな。秘密の穴場だよ、ここは。",
