      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90957,
          "speaker_name": "シービーの父親",
          "text": "……トレーナーさん。今日のレースを見て、 なんだか無性にほっとしたんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
