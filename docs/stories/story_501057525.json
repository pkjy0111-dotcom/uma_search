        {
          "next_frame": 9,
          "character_id": 90957,
          "speaker_name": "シービーの父親",
          "text": "だってシービーは、晴れやかな顔で レースを走っていましたから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "えっ……？",
