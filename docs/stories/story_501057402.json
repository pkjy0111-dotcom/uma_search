      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな自分には、何か 大事なものが欠けているのではないかと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
