      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その日の帰り道、ミスターシービーは語る。 自分は、他のウマ娘のように、 誰かのために走ろうとは思えない――",
          "flag_type": "",
          "flag": "",
          "choices": []
