      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "委員会の子から連絡のようです。 少し失礼しますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
