      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "随分と長い時間、 熱心にお参りされていましたね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "少しでもよくなればと思って……",
              "flag_type": "",
              "flag": ""
            }
          ]
