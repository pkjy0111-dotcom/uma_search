      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "すみません、遠征支援委員会で トラブルがありまして。急遽、 私が出向かなければならなくなりました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "気にしないで行ってきて",
