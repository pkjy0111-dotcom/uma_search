      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "お願いごとは終わりましたか？ お疲れ様です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 39,
