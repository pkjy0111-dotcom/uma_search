      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "……どうかしましたか？ でしたら、手配のし直しを――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
