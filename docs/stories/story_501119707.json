      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "そうですね……ん？",
          "flag_type": "",
          "flag": "chance_branch_leaf",
          "choices": []
        }
