      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "……それは急を要しますね。 私が直接伺いましょう。では、後ほど。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
