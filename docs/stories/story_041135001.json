    },
    "7": {
      "frame_id": 7,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……突如吊り橋が崩落し、帰り道を失って、途方に暮れる。 このまま救助を待つか考えていると――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "（…………ハーモニカの音？）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 128,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "音に導かれ奥へ進むと、ひとりのウマ娘が演奏をしていた。 その光景はとても神秘的で、思わず聴き入ってしまう……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
