      "frame_id": 9,
      "background_id": 128,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1135,
          "speaker_name": "？？？",
          "text": "――――……よう。あんたも、旅の途中かい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "あ、いや……俺は、その――",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "あ、いや……私は、その――",
              "flag_type": "",
              "flag": "female_trainer"
