      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1135,
          "speaker_name": "？？？",
          "text": "私が来たルートでよけりゃ、教えてやるよ。 ……とはいえ、そっちでも今日中に人里に戻るのは無理かな。",
          "flag_type": "",
          "flag": "",
          "choices": []
