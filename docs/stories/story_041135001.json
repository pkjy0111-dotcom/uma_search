    "10": {
      "frame_id": 10,
      "background_id": 128,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1135,
          "speaker_name": "？？？",
          "text": "――で、帰れなくなったと。ハハッ、そりゃ災難だったな。 ……ま、命があって何よりじゃないか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "たしかに、不幸中の幸いか……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "たしかに、不幸中の幸いだね……",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
