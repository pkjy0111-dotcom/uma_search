      "background_id": 376,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1135,
          "speaker_name": "？？？",
          "text": "……せっかくだ。少し振り返ってみるか。 ここまで来た、この旅の……始まりを。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
