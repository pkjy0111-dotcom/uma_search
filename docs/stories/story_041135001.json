      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1135,
          "speaker_name": "？？？",
          "text": "にしても、あんたと私の見たいものは同じらしい。 ……これも何かの縁だ、一緒に秘境まで行くか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "……でも、帰り道が",
