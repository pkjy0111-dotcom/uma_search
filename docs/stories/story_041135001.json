    "4": {
      "frame_id": 4,
      "background_id": 21,
      "sub_background_id": 30,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――ある日のこと。『人気のない秘境で、絶景を眺めたい』。 そんな強烈な欲求が突然、自分の中で湧いてきた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 129,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……そして今、衝動のままに、秘境へ続く道を歩いている。 年季の入った吊り橋をちょうど渡り終えると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "（バキッ……バキバキバキメキッ！！！）",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "わぁっ！？吊り橋が……！",
              "flag_type": "",
              "flag": ""
            }
