      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "……文字には、 書く方の想いがこもるものなのですね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "文字？",
