    },
    "2": {
      "frame_id": 2,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "へェ！　わざわざ手紙か。 気合い入ってんじゃねえか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『――力強く、そしてワイルドに 相手に食らいつくジャングルポケットさん』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『あなたの迫力を目の前で感じたくて、 この前初めてレース場に行きました。 そうしたら、レースの後――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『ポッケさん、戻ってくる時に こっちに向けて拳を突きつけてくれて！ 胸がドキドキしちゃいました！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "拳……？　おー！ こいつ、あん時の！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "俺のタオルを肩にかけててくれたんだ！ ずーっと俺のことだけ見ててさ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ファンサービスしてあげたんだ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "仲間たちとさ、同じ目ぇしてたからな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "なにがあろうと、一生ついていく…… そんな、覚悟が決まった目だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "……応えてやりてぇじゃねえか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『私って弱虫で。少しでも失敗したら 諦めて……自分でも嫌になるくらいでした』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『だけど、“最強”を目指すあなたを見て…… 絶対に諦めない闘志を感じて。 カッコいいなぁ、って心から思ったんです』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『もし嫌なことがあっても、 私も諦めず立ち向かい続けます。 少しでも、ポッケさんみたいになりたいから』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『私が勝手に思ってるだけ、ですけど…… ずっと、私はあなたの舎弟です！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
