      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "けどよ、ダンナ。こうして 景色のいい場所で、いいもん食うと、 やっぱりぜんぜん違うもんだろ？",
          "flag_type": "",
          "flag": "male_trainer",
          "choices": [
            {
              "next_frame": 30,
              "loop_ends_at_frame": -1,
              "text": "それは確かに……",
