      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "飯1つとってもよ、投げやりじゃいけねえ。 手前自身に対する甲斐性を 忘れねえようにしねえとな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
