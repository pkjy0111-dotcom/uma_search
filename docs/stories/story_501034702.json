      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "ああ、いや。すまねえなトレーナーさん。 あたし、悪さをしちまったよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
