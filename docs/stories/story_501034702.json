      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "あのくじだがな……実は、最初っから おむすび屋のくじしか入れていなかったのよ！",
