      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "……なにしろ浮世ってのは世知辛いもんさ。 そうそう思う通りに渡っていけねえ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
