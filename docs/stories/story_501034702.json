      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "だからこそよ。どんな時も手前の心だけは 涼やかにしとくのさ。どんなことが 起きたって、どんと構えて楽しめるようによ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
