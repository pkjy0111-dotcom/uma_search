      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "迷わず頷くと、なぜかイナリワンは 忍び笑いを漏らし始めた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
