        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "イナリワンとのお出かけ中、 小腹も空いてきたときのこと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
