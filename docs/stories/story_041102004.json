      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "……おっと。プレスティッシモはやめよう。 重要なパートだ。メノ・モッソを心掛けなければ。",
          "flag_type": "",
          "flag": "",
          "choices": []
