      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "春には春の花、夏には夏の花――\r 季節の移ろいを直に感じられますから。",
          "flag_type": "",
          "flag": "",
          "choices": []
