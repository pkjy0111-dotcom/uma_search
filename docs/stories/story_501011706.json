      "dialogue": [
        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "グラスワンダーとのお出かけからの帰り道、\r 商店街に寄ることになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
