      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "まあ、賛成です。\r 生活にお花があるのって素敵ですよね～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
