      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "普段から長い時間を過ごす部屋です。\r どんな花がいいか、慎重に考えなければ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
