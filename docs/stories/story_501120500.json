      "frame_id": 3,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "だな、お前左回りのコースだと けっこー外に膨らんじまうもんなぁ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
