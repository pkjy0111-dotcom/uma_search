      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "ああ、実は私もそんな気がしていた。 まだまだ下手だが、前よりマシになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
