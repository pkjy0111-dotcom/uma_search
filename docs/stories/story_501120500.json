      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 20,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "なるほど、よくわかった。もういいです。 体勢がキツそうなので無理しないでください。",
          "flag_type": "",
          "flag": "",
          "choices": []
