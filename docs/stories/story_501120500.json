      "frame_id": 5,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "トレーナーの教え方がいいからだろう。 すごくわかりやすいんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
