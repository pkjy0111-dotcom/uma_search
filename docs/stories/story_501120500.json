      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "あの人、『いい曲がり方』を 実演してくれたんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "は？　実演って、ウマ娘の走り方をか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 20,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "そう、ただしスローモーションで。 さすがに速度は真似できないから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "こ、こう！胴の角度をキープして……くっ！",
              "flag_type": "",
              "flag": ""
