      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1094,
          "speaker_name": "ジャングルポケット",
          "text": "でも、さっきはいい感じに抑えられてたぜ！ 他にも技術的なとこっつーの？ 最近なんか変わったよなー。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
