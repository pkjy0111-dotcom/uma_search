      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 9048,
          "speaker_name": "ハイセイコー",
          "text": "『あまいココロで　走っていくのよ うけとめてね　ハニーハート』……♪",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
