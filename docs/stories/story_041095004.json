    "4": {
      "frame_id": 4,
      "background_id": 263,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 92173,
          "speaker_name": "ビリーヴの祖父",
          "text": "……乗れ。おれの仕事なら、好きに見ろ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ありがとうございます……！",
              "flag_type": "",
              "flag": ""
            }
