      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "今ウマトックで大ブーム中！ 『面白カワイイ目標宣言！』まで！ 3・2・1――",
