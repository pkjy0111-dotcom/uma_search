      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "緊張を和らげる気遣いは、 今後も不要かもしれない。 この心の強さは、やはり……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "（出自も関係してるんだろうな）",
              "flag_type": "",
              "flag": ""
