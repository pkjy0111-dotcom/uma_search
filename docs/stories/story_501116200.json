    "7": {
      "frame_id": 7,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ジェンティルドンナの家は、 実力主義……幼少の頃から、ストレスに 晒される場面は多かったと推測できる。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ところで、ご家族は……",
              "flag_type": "",
              "flag": ""
            }
