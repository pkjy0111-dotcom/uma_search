        {
          "next_frame": 6,
          "character_id": 1116,
          "speaker_name": "ジェンティルドンナ",
          "text": "お気遣い、どうも。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "（……さすがの強さだな）",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "（……さすがの強さだね）",
