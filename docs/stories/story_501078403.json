      "frame_id": 3,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "もう1人は、とてつもない爆逃げをくり返し、 速さの底が見えないダイタクヘリオスだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
