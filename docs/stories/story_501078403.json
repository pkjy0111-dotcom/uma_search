      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ヤマニンゼファーは、越えねばならない 2つの壁の高さに愕然とする。視野が広がり、 相手を知ったことで、気負ってしまったのだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
