      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "――そう、ですね。やはり、争いごとは―― みなさんの、和やかな時間を―― お守りできれば、と思います。",
          "flag_type": "",
          "flag": "",
          "choices": []
