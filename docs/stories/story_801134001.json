      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "よかったら――私の分を、どうぞ。 向こうのテーブルに、ありますから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
