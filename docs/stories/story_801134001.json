      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "あの―― もし、よろしければ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "私が、予約していたスペースも、使って―― 一緒に、レッスンしませんか？ 共に、踊るのも――きっと、よいものです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 11,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "ありゃ！　あたしの一日の終わりを飾る…… サラダが、ラストひとつ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 11,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "サラダ狙いのウマ娘",
          "text": "あたし、ダイエット中なの…… グラン、絶対に譲らないからっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 11,
