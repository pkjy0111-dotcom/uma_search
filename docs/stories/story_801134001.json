      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "はぁ？　アンタの勘違いなんじゃないの！？ 予約してたのは、ア・タ・シ！ ついでに、次のレースで勝つのもね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
