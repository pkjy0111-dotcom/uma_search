      "frame_id": 11,
      "background_id": 11,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "え、いいの？ でもそれだと、ブーケさんの分が……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
