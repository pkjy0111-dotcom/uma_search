      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "お？　勝負するってこと？ いいね、いいね！　楽しそう！ だったら、なんマイルで――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
