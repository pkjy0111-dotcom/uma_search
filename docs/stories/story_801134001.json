      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "んだとぉ！？ 上等だ！　今ここで、 どっちが上か、はっきりと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
