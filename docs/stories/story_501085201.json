      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "皆様の研究、分析は万全のはずですが。 他に気になることでもございましたか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "そうじゃなくて……",
              "flag_type": "",
              "flag": ""
