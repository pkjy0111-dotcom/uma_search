      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "今日はここで実験をしようかと思ってね。\r なーに難しいことじゃない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
