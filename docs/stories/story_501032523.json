      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "いやぁ、いい天気だねぇ。\r それじゃあさっそく始めようか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ここはどこ？",
