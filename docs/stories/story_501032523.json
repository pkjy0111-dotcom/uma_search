      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "アグネスタキオンに言われるまま\r 砂浜を走った。",
          "flag_type": "",
          "flag": "",
          "choices": []
