      "frame_id": 4,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "とりあえず、君の限界を教えてくれ。\r 本当の実験はそこからだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
