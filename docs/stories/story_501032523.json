      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "目を開けると、ウニはすぐ横にあった。\r どうやら寸前のところでアグネスタキオンが 手を引いてくれたらしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
