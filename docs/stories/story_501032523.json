    "5": {
      "frame_id": 5,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "さぁ、私はゆっくり君について行くから\r 君は全力で走ってごらん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
