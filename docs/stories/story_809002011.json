      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "ここで出会ったのも何かの縁だっ！ 君も来るといい！！　はっはっは！",
