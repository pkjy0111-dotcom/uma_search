      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "はい。なるべくたくさんの神様に、 日ごろ学園を見守ってくださっている お礼を伝えたいので。",
          "flag_type": "",
          "flag": "",
          "choices": []
