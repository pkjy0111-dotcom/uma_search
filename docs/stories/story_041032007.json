      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "測定器の準備はいいかい？\r 1周ごとに酸素濃度を記録するから、忘れずに頼むよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
