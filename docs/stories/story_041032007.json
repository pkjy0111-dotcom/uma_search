      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "まずは道の傾斜を利用して、心肺機能を活性化するぞ！",
          "flag_type": "",
          "flag": "",
          "choices": []
