      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "まさか飲んでみるだとか言わないだろうね。",
          "flag_type": "",
          "flag": "",
          "choices": [
