      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "ゴルシさんなりの意気込み、\r なのでしょうか。",
