      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "山があり、谷があり、\r 川があって、やがて海へと続いていく。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
