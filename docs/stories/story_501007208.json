      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "……人生ってのは、\r レースみたいに一直線じゃねえ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
