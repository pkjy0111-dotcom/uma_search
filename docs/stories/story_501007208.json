    },
    "11": {
      "frame_id": 11,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "けど、アタシはその山脈を越えてみせる。\r オマエらを越えて、大海へ出てやるぜ……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
