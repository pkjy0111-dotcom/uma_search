      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "貴方とは幾度も戦い、\r 切磋琢磨してきました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
