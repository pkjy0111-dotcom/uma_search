      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "よお、おったまげたぜ。\r オマエら2人がいるとはなあ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
