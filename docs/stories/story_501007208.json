      "frame_id": 10,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "今日のレースを地形でたとえるなら、\r きっとでけえ山――いや、 日本アルプス並みの山脈なんだろうな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
