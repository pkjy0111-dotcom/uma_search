    "18": {
      "frame_id": 18,
      "background_id": 39,
      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "とか言ってるけどアンタ本当に ギムレットさんの言葉わかってるわけ？ どーせ知ったかぶりしてるんでしょ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
