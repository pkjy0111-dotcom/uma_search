    "19": {
      "frame_id": 19,
      "background_id": 39,
      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "っ、確かに全部はわかんねーけど、 でも魂は伝わってくるんだ！！ それが先輩の言葉のスゲーところだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
