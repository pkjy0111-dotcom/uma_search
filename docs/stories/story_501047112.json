      "frame_id": 6,
      "background_id": 65,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "ですから、まだ完全にないと 決まったわけではありません……！ 気を落とされずとも、大丈夫です……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "ん？うん……",
              "flag_type": "",
              "flag": ""
            }
