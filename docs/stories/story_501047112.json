      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "それより今は温泉、ですよねっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
