      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "いえ。やっぱり、どう探しても…… これから行く温泉には、 英雄の出てくる逸話はないみたいで……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
