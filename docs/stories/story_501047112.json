    "4": {
      "frame_id": 4,
      "background_id": 65,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "商店街の福引で当てた温泉へと向かう 電車内でも、ゼンノロブロイの興味は 普段通り『英雄』へと向けられていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 65,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "……まあ、宿の人に聞いたら教えてくれたり パンフレットにひっそり書かれていたり、 そういうケースもありますからね……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
