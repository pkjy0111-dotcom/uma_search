    "20": {
      "frame_id": 20,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "だからボクは前夜祭としてオペラを上演し、\r レースをさらに盛り上げようと考えているわけさ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "前夜祭は前日にやるものじゃ……？",
              "flag_type": "",
              "flag": ""
