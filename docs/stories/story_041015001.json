      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "恥ずかしがらずにボクの手を取ってくれ！\r さあさあ！！",
