    },
    "14": {
      "frame_id": 14,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ふふっ、目的はわかっているよ。\r ボクのオペラを鑑賞しに来たんだろう？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "なんといっても、脚本ボク！　演出ボク！　主演ボク！\r 豪華キャストで送る、史上最高の超大作オペラだからね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "さあさあ、そこのベンチに腰をかけてくれたまえ。\r ……コホン、では始めるよ――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "ち、ちょっと待ってくれ！",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "ち、ちょっと待って！",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
