      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "常識に縛られるなんて、全くもってナンセンスなことだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
