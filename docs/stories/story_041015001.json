      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "想像しただけで、頭の中に天使たちのファンファーレが\r 鳴り響いてくるようだよ！",
