      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "あとは観客が訪れるのを待つだけなんだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
