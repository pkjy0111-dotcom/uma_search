      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おおっ！！　よく来てくれたね、そこの君っ！！",
          "flag_type": "",
          "flag": "",
