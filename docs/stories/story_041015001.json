    },
    "21": {
      "frame_id": 21,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おやおや、おかしなことを言うね。\r 前夜祭が前日でなければダメだと誰が決めたというんだい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "そもそも夜じゃないんだが……",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "そもそも夜じゃないけど……",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "22": {
      "frame_id": 22,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "はっはっはっはっ！　前夜祭が夜じゃなければダメだと\r 誰が決めたというんだい？",
