      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "いいかい、3日後にはボクの走る『選抜レース』がある。\r これは学園全体を巻き込む一大イベントだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
