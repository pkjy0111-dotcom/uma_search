      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "轟音を伴い、頭上を電車が駆け抜けていく。 耳をつんざくような激しい響きと、強い振動に包まれた――！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
