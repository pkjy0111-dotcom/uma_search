      "frame_id": 15,
      "background_id": 119,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "いつの間にか、タニノギムレットがいなくなっている……！ 代わりに、地面に1通の手紙が落ちていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
