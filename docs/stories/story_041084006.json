      "frame_id": 14,
      "background_id": 119,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "瞬く間の出来事だったが、音と衝撃がしばらく体に残っていた。 しばらくぼんやりした後、視線をもとに戻す――と。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "（あれ、いない……！？）",
              "flag_type": "",
              "flag": ""
