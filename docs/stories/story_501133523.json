      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "ハバネロと長ネギ、剣山に生けてたらな…… ピーンときたわけよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
