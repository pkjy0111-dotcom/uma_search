      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "せっかく手当てもしてもらったんだし、\r 意味のあるトレーニングにしないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
