      "frame_id": 18,
      "background_id": 186,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1116,
          "speaker_name": "ジェンティルドンナ",
          "text": "『特別な貴方を歓迎いたします』――",
          "flag_type": "",
          "flag": "",
          "choices": []
