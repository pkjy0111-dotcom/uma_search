      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "いえ、諦めるわけにはいきません。 眼鏡がなければどうなるか、その不便さは 貴方も身に染みているはず。",
          "flag_type": "",
          "flag": "",
          "choices": []
