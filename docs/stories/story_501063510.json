      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "『ありがとう、もう十分よ。これ以上は タイパが悪すぎるわ。引き上げて』",
