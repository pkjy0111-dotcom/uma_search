      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "（いつなんどきも、揺るがぬ意志。 刃のように研ぎ澄まされた、折れない信念）",
          "flag_type": "",
          "flag": "",
          "choices": []
