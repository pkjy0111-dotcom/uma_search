    "9": {
      "frame_id": 9,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "……もしかすると、くるりんさんが 便風を運んでいる音かもしれません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
