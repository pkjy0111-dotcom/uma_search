    "10": {
      "frame_id": 10,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "きっと子どもたちのために、 ソリで夜空を駆け巡っているのですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "でも、この寒さはこたえそうです…… せめて小春凪を願いましょう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "トナカイは寒さに強いから大丈夫だよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "ええ、そうですね。 くるりんさんはもふもふで―― しかも、屈強な方ですから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
