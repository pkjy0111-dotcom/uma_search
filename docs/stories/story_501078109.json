    "8": {
      "frame_id": 8,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "……あ、聞こえますかトレーナーさん。 季節外れの饗の風……。 ここまで響いていますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
