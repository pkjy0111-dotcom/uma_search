      "frame_id": 13,
      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "きっと、切り裂くような鋭い逆風も 乗り越えてしまうでしょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
