      "background_id": 111,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "この辺りも、冬枯れが広がって…… いつ雪嵐に変わってもおかしくありませんね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
