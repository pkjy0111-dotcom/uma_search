      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "この朔風……はーーっ……。 まさに肌を刺すような感覚ですね。",
