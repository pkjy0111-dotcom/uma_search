      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "いいえ、大丈夫ではありません！\r 栄養の乱れは筋肉の乱れ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
