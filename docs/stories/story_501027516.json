    "4": {
      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "今日は筋肉の休息日にしようと決めてたのに、\r ついつい普段通りの量のご飯をとっちゃって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "炭水化物は大事なエネルギー源。だから、\r 運動する日はたくさん摂取するんですけど――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "今日みたいな休息日に摂りすぎると、\r かえって逆効果なんです……！",
          "flag_type": "male_trainer",
          "flag": "male_trainer",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "半分もらおうか？",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "自分でとったなら食べなきゃな",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "自分でとったなら食べなきゃね",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
