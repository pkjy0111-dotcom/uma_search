    "9": {
      "frame_id": 9,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "あっ。でも、トレーナーさんは\r 自分の食事もありますよね……？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "このくらいなら大丈夫だ",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "このくらいなら大丈夫だよ",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
