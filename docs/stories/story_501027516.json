      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "ううん、どうしよう。\r これじゃ少し食べ過ぎかな……？",
          "flag_type": "",
          "flag": "",
