      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "うっ。そ、そうですよね……。\r 残したりでもしたら、食材に失礼ですし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
