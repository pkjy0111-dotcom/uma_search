      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "よしっ。自分でよそったんだから、\r ちゃんと全部食べることにします。",
          "flag_type": "",
          "flag": "",
          "choices": []
