      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "食事の後、少し筋トレでもいかがでしょうか？\r 大丈夫、あたしが見ててあげますから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その後、腹八分目で食事を済ませた\r メジロライアンと共に筋トレを行った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
