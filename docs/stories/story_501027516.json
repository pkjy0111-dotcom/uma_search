      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "い、いいんですか？\r ありがとうございます！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
