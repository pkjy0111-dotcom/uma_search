      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "ふふふ……ちっちぇ子まで。 ほにいろンなファンの人がいますよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
