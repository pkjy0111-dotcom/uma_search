      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "どれも懐かしいなぁ……応援の声は 十人十色ですけど、全部ありがたいです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "あ、新しい手紙も届いているよ",
              "flag_type": "",
              "flag": ""
