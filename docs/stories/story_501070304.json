      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "……納得いかねぇって顔だな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
