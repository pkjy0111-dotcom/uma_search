      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "残した戦績を上塗りしてやるだけで、 鎮座まします王座の輝きはくすむ。",
