      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『ダイイチルビーには、母親の雪辱を果たし、 絶対にトリプルティアラを達成してほしい』 それがコラムの趣旨だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
