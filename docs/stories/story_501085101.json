      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "“華麗なる一族”の大ファンだという その記者は、あるウマ娘を引き合いに出し、 ファンだからこその懸念を語っていた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "（そのウマ娘の名は――）",
              "flag_type": "",
              "flag": ""
            }
