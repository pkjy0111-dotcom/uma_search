      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ひねくれているが誰よりも強く、 絶対に諦めない誰かから 『諦めない勇気』をもらったという少女。",
          "flag_type": "",
          "flag": "",
          "choices": []
