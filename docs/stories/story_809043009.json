      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "随分と早い到着なんだな。 研修は明日からだぞ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "パリのことを学ぼうと思いまして",
              "flag_type": "",
              "flag": ""
