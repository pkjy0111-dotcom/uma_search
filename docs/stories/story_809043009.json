      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "ふむ、そうだ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 243,
