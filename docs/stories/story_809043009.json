      "background_id": 243,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "君もなかなか真面目だな。 パリのことを本当に知ろうとしているんだね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
