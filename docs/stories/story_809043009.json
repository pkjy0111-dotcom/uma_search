    },
    "3": {
      "frame_id": 3,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "いい心がけだな！　現地の文化を知るか 知らないかで学習の深度に差が出るものだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "VRで見るのと、実際に体験するのでは また感じ方も変わってくるだろうし……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
