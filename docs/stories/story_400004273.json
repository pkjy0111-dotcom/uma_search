      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "おそらく、登頂中の発汗による水分枯渇、 および山頂における気温の低下が関係して いたのでしょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
