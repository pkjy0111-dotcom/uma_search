      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "走ることはアイネスフウジンの わがままなんかじゃない。 多くの人々に希望を与えているのだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
