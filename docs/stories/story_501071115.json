      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90640,
          "speaker_name": "アナウンサーA",
          "text": "『――ケガで皐月賞を回避しましたが、\r 去年のジュニア級王者と言ってもよい彼女が 今回のダービーで中心になるのでは――』",
          "flag_type": "",
          "flag": "",
          "choices": []
