      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90640,
          "speaker_name": "アナウンサーA",
          "text": "『――それでいうとヤエノムテキは、 対抗として最有力ではないでしょうかね。 皐月賞での走りは、そりゃもう見事で――』",
          "flag_type": "",
          "flag": "",
          "choices": []
