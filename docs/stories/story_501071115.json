    },
    "5": {
      "frame_id": 5,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90640,
          "speaker_name": "アナウンサーA",
          "text": "『――メジロアルダンは、姉ラモーヌの 威光に迫ることができるのか。ここもまた、 ポイントになるんじゃないでしょうか――』",
          "flag_type": "",
          "flag": "",
          "choices": []
