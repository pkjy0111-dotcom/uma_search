      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90105,
          "speaker_name": "ウマ娘A",
          "text": "ええ～、そんな！　どうして追いつけないの～！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
