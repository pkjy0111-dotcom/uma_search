      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90105,
          "speaker_name": "ウマ娘A",
          "text": "やっと見つけた～～！！　捜したよ、ゼファーさんっ！",
