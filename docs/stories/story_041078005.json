      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90107,
          "speaker_name": "ウマ娘C",
          "text": "――って、いない！？ おかしいな、本当に今の今までいたんですけど……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
