    "18": {
      "frame_id": 18,
      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90001,
          "speaker_name": "",
          "text": "流れる風のように、捕まえられないヤマニンゼファー。 次はどこに行ったのかと、屋上から周辺を見回していると――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 19,
              "loop_ends_at_frame": -1,
              "text": "い、いた――！！",
