      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "吹き渡る風の行く先は知らず、 晴天に嵐を起こしてはあからしまかと思いまして。",
