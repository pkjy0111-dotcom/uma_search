      "frame_id": 21,
      "background_id": 57,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "――それは、わざわざ申し訳ありません。 すでに凪とお伝えしたつもりでおりました。",
