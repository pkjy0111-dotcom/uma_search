      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90105,
          "speaker_name": "ウマ娘A",
          "text": "…………えっと？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
