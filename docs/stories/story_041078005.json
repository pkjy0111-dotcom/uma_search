      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "まあ、ずいぶんと青嵐なご様子ですね。 私に何かご用でしょうか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
