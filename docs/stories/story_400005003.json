      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "担当ウマ娘が無事デビューしたということで。 まずはみなさん、お疲れ様です！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
