      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな彼女に、トレーナーとしても、 どうにか称賛の気持ちを届けたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
