      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "強く、そして誰よりもひたむきな魂を燃やし、 夢を叶えてみせたヤマニンゼファー……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
