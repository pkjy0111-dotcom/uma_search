      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "称賛……ですか。ふふ、私からすれば、 それはトレーナーさんにこそ贈られるべき 恵風のように思いますが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
