      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ということで、なにか希望はないか 訊ねてみると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
