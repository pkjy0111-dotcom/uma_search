      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "さらには『天皇賞（秋）』までをも制し、 距離の壁すらものともしない まことの風となってみせた。",
          "flag_type": "",
          "flag": "",
          "choices": []
