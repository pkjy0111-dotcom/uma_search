    "11": {
      "frame_id": 11,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "大きいのはやはり、“華麗なる一族” ダイイチルビーさんの短距離転向ですね！ 年明けから重賞勝ちも決めて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "その後のレースでは、あの バンブーメモリーさんと競ったうえで 見事、勝利していますから！",
          "flag_type": "",
          "flag": "",
          "choices": []
