    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "ええ、『トウカイテイオー、ダービー勝利！』 みたいな特大ニュースと比べちゃうと さすがに、ってところはありますが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
