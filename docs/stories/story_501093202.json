      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "――なるほど！ サマースプリントに向けてのトレーニングは、 たいへん順調に進んでいると……！",
          "flag_type": "",
          "flag": "",
          "choices": []
