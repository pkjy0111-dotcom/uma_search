      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "いや、ありがとうございます！ 現在の短距離路線の盛り上がりを 後押しするような記事が書けそうです……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "現在の盛り上がり、ですか？",
              "flag_type": "",
              "flag": ""
