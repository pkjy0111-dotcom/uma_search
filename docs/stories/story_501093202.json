      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 0,
          "speaker_name": "緑のネクタイの記者",
          "text": "それでも！　渋いレースファンなんかからは 『今、短距離も面白い』って評判なんですよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
