      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "本番においても全く動じない この強靭な精神力と自己管理能力が、 彼女の強みであることを改めて理解する――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "まずは……無事に、走り切ろう",
              "flag_type": "",
              "flag": ""
