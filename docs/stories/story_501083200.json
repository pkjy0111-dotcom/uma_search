      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "本番のレースがシンボリクリスエスの体に 与える負担は、まだ未知数な部分が多い。 故に、言葉を選んだ。",
          "flag_type": "",
          "flag": "",
