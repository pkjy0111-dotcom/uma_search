      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "シンボリクリスエスは、いい意味で 普段のトレーニングと変わらない 落ち着きと緊張感を保っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
