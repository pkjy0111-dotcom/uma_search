    "22": {
      "frame_id": 22,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "と、トレーナー君！？",
