      "frame_id": 29,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ああ、だから制服で走ろう！\r 場所は河川敷がいいかな、まさに青春チックにね！",
          "flag_type": "",
          "flag": "",
          "choices": []
