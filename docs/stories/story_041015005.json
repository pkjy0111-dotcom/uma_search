      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "水に濡れたボクを堪能する、\r せっかくの機会を逃すなんて……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 26,
              "loop_ends_at_frame": -1,
              "text": "風邪をひいたら困るから",
              "flag_type": "",
              "flag": ""
