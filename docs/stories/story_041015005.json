      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ボクが“世紀末覇王”となり、\r 世界を照らすということだけをさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "28": {
      "frame_id": 28,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "では、今日のトレーニングに励むとしようか。\r まさかこのボクが、日課をサボるわけにはいかないからね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 29,
              "loop_ends_at_frame": -1,
              "text": "でもジャージはまだ濡れているんじゃ……",
              "flag_type": "",
              "flag": ""
