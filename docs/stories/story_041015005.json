      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "全く、勝手に部屋から追い立てて……。\r 本当に君はよかったのかい？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
