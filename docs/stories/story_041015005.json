    "23": {
      "frame_id": 23,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90001,
          "speaker_name": "",
          "text": "いつまでも部屋から出ようとしないテイエムオペラオーを、\r なんとかして部屋の外に追い出した。",
          "flag_type": "",
          "flag": "",
          "choices": []
