      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その日のトレーニングは室内で筋トレをする予定だったので、\r 自分たちには影響がないと思っていたのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
