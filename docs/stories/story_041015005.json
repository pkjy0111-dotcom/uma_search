      "frame_id": 26,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おやおや、そんな訪れない未来など心配しなくてもいいよ。\r 君が信じる未来はひとつだけでいい。",
          "flag_type": "",
          "flag": "",
          "choices": []
