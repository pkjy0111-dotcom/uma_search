      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "な゛っ！？　そ、その発想は、\r 今までありませんでしたわ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
