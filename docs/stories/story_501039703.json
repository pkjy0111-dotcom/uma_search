      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "この子を、立派な姫に……！\r 最善の方向に導くには、 何を教えればいいんですの……！？",
