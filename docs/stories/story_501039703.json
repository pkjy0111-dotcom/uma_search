      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "こんな時、キングさんなら\r どうするのかしら…… ふんぬぬぬぬぬぬぬ……！！",
