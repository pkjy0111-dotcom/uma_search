            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "君とキングヘイローみたいだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "貴方とキングヘイローみたいだね",
