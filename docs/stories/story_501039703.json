      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90220,
          "speaker_name": "野球帽のウマ娘",
          "text": "？　どうしたの姉ちゃん？\r 早く遊びに行こうよ！",
