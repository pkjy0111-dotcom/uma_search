      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "『フライトさんの姿は輝いて見えました。 私と貴方は違うのに、私も貴方のように 輝けるかもしれないと――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
