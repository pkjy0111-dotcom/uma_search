      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "『何をしても上手くいかず 落ち込む毎日でした。そんな中、 貴方のレースを観たんです』",
          "flag_type": "",
          "flag": "",
          "choices": []
