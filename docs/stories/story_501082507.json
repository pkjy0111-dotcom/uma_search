      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "『貴方の走りは、 そんな希望を持たせてくれた』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
