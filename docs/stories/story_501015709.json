      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "そんな、鏡に映る麗しいウマ娘を見る度に 驚くボクみたいな表情をして。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "いつも通りで安心した！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "てっきり、落ち込んでいるかと……",
