    "10": {
      "frame_id": 10,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "任せたまえ！　次に輝くのは滴る水ではない。 ボクの勇ましき走りさ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
