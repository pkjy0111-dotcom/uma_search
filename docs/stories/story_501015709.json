      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ふふっ、何を言い出すかと思えば。 ボクはいつも通り、誰よりも美しいさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
