      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "さあ！　トレーナー君、学園に戻ろう。 そしてボクの輝きを、磨こうじゃないか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "気合いを入れていこう",
              "flag_type": "",
              "flag": ""
            }
