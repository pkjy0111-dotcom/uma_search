      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "決まりだな。では皆、 体を冷やさないよう着替えてから集まろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
