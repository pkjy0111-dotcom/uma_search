      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "韋編三絶、共に高め合うとしよう。",
          "flag_type": "",
          "flag": "",
          "choices": []
