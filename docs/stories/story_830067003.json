    "3": {
      "frame_id": 3,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "あのっ、今日は体の調子がいいんですっ！ なので、このチャンスを逃さず、 走りのテクニックを限界まで探求したく！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ふむ……体を動かすトレーニングもいいが、 どうだろう？　読書尚友――皆で本に触れ、 知見を高め合うというのは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "実は新しい技法書が 図書室に入ったという話を聞いていてね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "どの提案も魅力的だが、 みんなで追加トレーニングをするなら……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "読書で走りの知識を深めようか",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "やっぱり併走かな！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "しっかりテクニックを磨いていこう",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
