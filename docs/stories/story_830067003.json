      "frame_id": 2,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "はいはいっ！　ボク併走トレーニングしたい！ せっかくカイチョーがいるんだもん、 一緒に走らないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
