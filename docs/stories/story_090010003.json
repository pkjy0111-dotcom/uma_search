    "5": {
      "frame_id": 5,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『……そうか、なるほど。――フラッシュ。 シュヴァルツヴェルダー・キルシュトルテを知っているね？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『？　はい。シュヴァルツヴァルト地方の黒い森をモチーフに、 特産品のサクランボの蒸留酒を使用した銘菓、ですよね？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『ああ。諸説あるが、クリームが森の積雪を、チョコチップが 落ち葉を、サクランボで産業を表現していると言われている』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『私はシュヴァルツヴェルダー出身ではないが、\r キルシュトルテを食べると、黒い森を感じられるんだ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90937,
          "speaker_name": "フラッシュの父親",
          "text": "『冷たい雪、うっそうとした森、サクランボ畑。これらがあの場所\r に古くからあり、愛されてきたのだと思うと、心がくすぐられる』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『……お父さん……。』",
          "flag_type": "",
          "flag": "",
          "choices": []
