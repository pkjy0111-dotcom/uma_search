    "4": {
      "frame_id": 4,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『……はい。万事順調です。 必ず素晴らしい結果を残し、コンテストを盛り上げてみせます』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
