      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だがエルコンドルパサーは主張する。 『凱旋門賞』勝利には、あと1年必要だと。 ……必ず、勝ってほしいからこそ。",
          "flag_type": "",
          "flag": "",
          "choices": []
