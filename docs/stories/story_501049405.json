      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "元担任に残った時間は、わずかだ。 知っている。だから力が足りないと わかっていても、負けるとわかっていても――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
