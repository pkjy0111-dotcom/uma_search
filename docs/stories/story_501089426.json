      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "レース後、負けて大いに悔しがる同期の ウマ娘たちを目にし、シュヴァルグランは 周囲との熱量の差を痛感する。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
