    "9": {
      "frame_id": 9,
      "background_id": 76,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "一世一代の大舞台に立つ、誇り高き装い。\r それを纏う喜びを分かち合うという意味でね。",
          "flag_type": "",
          "flag": "",
          "choices": []
