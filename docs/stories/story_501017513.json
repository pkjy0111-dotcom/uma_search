    "10": {
      "frame_id": 10,
      "background_id": 76,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……私も両親に送るべきかどうか、\r 少し考えていたんだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "送ればいいのに",
              "flag_type": "",
              "flag": ""
