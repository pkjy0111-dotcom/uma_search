      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……思うところがあってね。",
          "flag_type": "",
          "flag": "",
          "choices": []
