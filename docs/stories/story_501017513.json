    },
    "14": {
      "frame_id": 14,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "故に、子である私にも厳格で――\r 偉大な指導者になることを、望んでいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
