      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "そういえば君には、私の家族について\r あまり話したことはなかったな――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "私の両親は――熱烈峻厳。世界を良くすべく、\r 己の身をも投げ打つ者たちだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
