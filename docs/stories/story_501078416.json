      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ヤマニンゼファーは、彼女がこれまでの どの瞬間よりも強いと感じる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
