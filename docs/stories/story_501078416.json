      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "度重なるケガから復帰を遂げた…… トウカイテイオーと走るため。 トウカイテイオーは言う――",
          "flag_type": "",
          "flag": "",
          "choices": []
