      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 90001,
          "speaker_name": "",
          "text": "こうしてヤマニンゼファーとの二人三脚のリハビリが始まった！",
