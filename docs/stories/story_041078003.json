      "frame_id": 31,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 90001,
          "speaker_name": "",
          "text": "少しは信頼されているということだろうか。 もしそうなら――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 32,
              "loop_ends_at_frame": -1,
              "text": "リハビリに協力させてくれないか？",
              "flag_type": "",
              "flag": "male_trainer"
