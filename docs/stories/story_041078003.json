    "32": {
      "frame_id": 32,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "……どうしてそこまでしてくださるのかわかりませんが、 これも帆風に乗るのだと甘えさせてください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "33": {
      "frame_id": 33,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "ありがとうございます、トレーナーさん。 やっぱりあなたは凱風なのですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
