    "24": {
      "frame_id": 24,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "……わ、私はドトウではないっ！　ぽこっ！\r たった今この娘に取り憑いた妖怪…… じゃない、悪魔タヌキぽこっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "25": {
      "frame_id": 25,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "はっはっは！　これはまたずいぶん\r 可愛らしいタヌキさんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
