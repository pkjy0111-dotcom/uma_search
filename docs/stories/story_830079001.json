      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ほう、変身譚というわけか！\r はっはっは！　それも面白いじゃないか！",
          "flag_type": "",
          "flag": "",
          "choices": []
