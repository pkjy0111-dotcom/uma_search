      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1058,
          "speaker_name": "メイショウドトウ",
          "text": "…………。ぷしゅ～……！\r デビル・インストールぅ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
