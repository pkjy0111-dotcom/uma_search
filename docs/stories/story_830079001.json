      "frame_id": 23,
      "background_id": 69,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おや、どうしたんだねドトウ？\r 恥ずかしがらずに、こっちに来てごらん――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
