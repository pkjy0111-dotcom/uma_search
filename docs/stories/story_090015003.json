      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1059,
          "speaker_name": "メジロドーベル",
          "text": "『ドーベルが、友だちのピンチに気づいて、 手を取ってあげられたってだけで、あたしは誇らしいよ！』",
          "flag_type": "",
          "flag": "",
