    "7": {
      "frame_id": 7,
      "background_id": 151,
      "sub_background_id": 131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "『皐月賞を獲った後、“天駆けるウマ娘”なんて持ち上げられて、\r そのくせダービーでは大観衆に竦んで力が出しきれず――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 151,
      "sub_background_id": 131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "『菊花賞でも荒れたバ場に脚をとられ、勝ちきれなかった。けれど\r そんな私を、ファンの皆さんは有馬の出走者に選んでくれました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 151,
      "sub_background_id": 131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "『駿大祭の明かりの下で私は誓いました。\r “負けられない。絶対に、絶対に、絶対に”――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 151,
      "sub_background_id": 131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "『思うに、あれは。当時の私が、全てを懸けレースに臨む覚悟を\r 決めるにあたり、必要な儀式であったように思います』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 67,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1040,
          "speaker_name": "ゴールドシチー",
          "text": "儀式、か――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
