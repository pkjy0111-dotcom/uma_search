      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ターフを駆けるウマ娘にとって大事なものを持つ彼女と\r もう1度話をしたいと思い、学園を探し回っていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
