    },
    "5": {
      "frame_id": 5,
      "background_id": 5,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "今日もボクは光り輝いている！\r はっはっはっはっは！　はーっはっはっはっはっはっはっは！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
