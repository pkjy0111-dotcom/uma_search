      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "こうしてみると、その走りは力強い。\r 思い返せば、これまでのトレーニングも――",
          "flag_type": "",
          "flag": "",
          "choices": []
