    "3": {
      "frame_id": 3,
      "background_id": 5,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "嗚呼、木洩れ日よ！\r ボクと共にこの世界を照らそうじゃないか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
