    },
    "4": {
      "frame_id": 4,
      "background_id": 5,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女が求めるのは、太陽のような“覇王”の輝き。\r そしてその思いには強い信念がある。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
