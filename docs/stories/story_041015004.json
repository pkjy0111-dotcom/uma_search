      "frame_id": 6,
      "background_id": 5,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "また独特なトレーニングのために、他の場所へ行くのだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
