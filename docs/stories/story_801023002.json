      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……いや、これも無難だな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "悩み事？",
              "flag_type": "",
