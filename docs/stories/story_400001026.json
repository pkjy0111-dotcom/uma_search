      "dialogue": [
        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ある日、トレーナー室で\r 資料探しに苦戦していたところ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
