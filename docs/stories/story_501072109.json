      "background_id": 117,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1072,
          "speaker_name": "幼いヤエノムテキ",
          "text": "…………ぅ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
