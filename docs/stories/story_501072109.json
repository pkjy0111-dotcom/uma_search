      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "やあ、ヤエノのトレーナーさん。\r 私に用って、なにかな？",
