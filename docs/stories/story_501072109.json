      "background_id": 117,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90750,
          "speaker_name": "ヤエノムテキの祖父",
          "text": "ここ数日はな。\r だが、先週はどうだ。先月は？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
