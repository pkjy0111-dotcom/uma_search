      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "コミュメンさんたちも―― ラヴズさんへ、 愛を込めた花束を――贈りたいと。",
          "flag_type": "",
          "flag": "",
          "choices": []
