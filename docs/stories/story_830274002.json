      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "彼女には――いつも愛をもらってばかりで。 同じだけの愛を、返したいそうです。",
