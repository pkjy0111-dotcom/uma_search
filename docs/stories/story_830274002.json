      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 99111,
          "speaker_name": "三日坊主系女子",
          "text": "『さっきのコーナー、感激しちゃった！ それで……ブーケちゃん、 どうか、あたしたちを助けてくださいっ！』",
          "flag_type": "",
          "flag": "",
          "choices": []
