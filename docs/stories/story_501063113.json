      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（そしてこの『クラシック三冠』と対になる、 もう1つの至高が存在する――）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
