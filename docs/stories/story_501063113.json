      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "はろはろ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "あら、ライアンに、アイネスさん――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（メジロライアンさん。マックイーンさんと 同じくメジロ家のウマ娘で、筋肉に対して 並々ならぬ情熱を持つ、熱く頼りになる方）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（アイネスフウジンさん。明るく爽やかで、 走りも同じく風を切るかのような勢いがある。 そしてなにより……家族思いの優しい方）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "18": {
      "frame_id": 18,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（おふたりとマックイーンさん、 そして私は……レースにおける同期になる）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "2人もまだ食べる前だよね？ よかったら、一緒にどう？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "20": {
      "frame_id": 20,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "私は構いません。 マックイーンさんは――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "21": {
      "frame_id": 21,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "もちろん、構いませんわよ。 同期で食卓を囲むとしましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "22": {
      "frame_id": 22,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "――イクノちゃんはデビュー戦後のこととか、 なにかもう考えているの？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
