      "dialogue": [
        {
          "next_frame": 37,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（こちらもクラシック三冠と同様、 一生に一度のレースとなるが…… 異なる部分が多く存在する）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
