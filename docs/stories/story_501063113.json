    "8": {
      "frame_id": 8,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（メジロマックイーンさん。 幾多の名ウマ娘を世に輩出してきた 名門・メジロ家の方であり――）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（寮における私のルームメイト。 才気を宿しつつも、名門をその身に背負い 努力を重ねる、尊敬すべき友人だ）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "それでは早速参りましょうか。 イクノさんは、なにをお食べになるのですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "そうですね……。 今日のトレーニングを加味して、 消化に負担のないものを――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "さて、空いている席は……――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1027,
          "speaker_name": "？？？",
          "text": "――あれ、マックイーンとイクノちゃんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 11,
