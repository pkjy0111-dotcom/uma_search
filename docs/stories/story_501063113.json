      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "……ええ。メジロのウマ娘として 誇りをもって臨めるよう、努める所存です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
