      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（クラシック級でしか走ることのできない 一生に一度のレースにして、 世代の最高峰を決める栄光の戦場――）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
