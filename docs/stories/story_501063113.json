    "28": {
      "frame_id": 28,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "イクノさんなら必ず、 ティアラ路線を走ることができますわ。 自信を持って目指してくださいまし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
