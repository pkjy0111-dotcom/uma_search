      "frame_id": 7,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "もちろんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
