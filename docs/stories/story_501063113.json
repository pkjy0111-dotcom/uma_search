      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "……皆さんはやはり、クラシック三冠路線を 目指すおつもりなのでしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
