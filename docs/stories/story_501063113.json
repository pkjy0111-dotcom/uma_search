      "background_id": 24,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（『クラシック三冠』。それは、 『皐月賞』『日本ダービー』『菊花賞』の 3つのレースの総称）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
