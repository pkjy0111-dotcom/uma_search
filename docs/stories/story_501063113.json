      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "（その勝者はもれなく歴史に名を残し、 輝かしき存在として称賛される。 多くのウマ娘が憧れる、最上の誉れだ）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
