      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "ふふっ、失礼しました！ お2人の髪の手入れ方法が、 どうしても気になったもので。",
