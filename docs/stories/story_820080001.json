      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "ローレルさんに、スカイさん。お2人とも 柔らかそうな、しなやかな髪をしてますよね。",
