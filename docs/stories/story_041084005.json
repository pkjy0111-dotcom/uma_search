    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "アッハハハハハ！！　まるで此岸に現れたヴィーナスの如くッ！！ 光り輝く様相ッ！！　生の喜びに満ち満ちた面持ち！！！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "大いに結構ッ！！！　冬の微睡から目覚め、 春を謳歌せよ！！！　存分に生を堪能するがいいッ！！！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "ありがとう、目が覚めたよ",
              "flag_type": "",
              "flag": ""
            }
