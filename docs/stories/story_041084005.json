      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ならば、オマエに与えるのは刹那の閃光が相応しい。 ……そうだろう、隣人よ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
