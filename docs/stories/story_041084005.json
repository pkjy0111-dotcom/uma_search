      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……フ、当然のことだ。オマエに酩酊する義務があるのと同じく、 ワタシには酩酊させる義務があるのでな。",
          "flag_type": "",
          "flag": "",
          "choices": []
