      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "何の香りだろうと思って、目を開けると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
