      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "やっほー、トレちゃん。 遊びに来たよーん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
