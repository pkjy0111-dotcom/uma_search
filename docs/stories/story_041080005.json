      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "何が好きで、何が嫌いで、何に無関心なのかまで、 趣味、嗜好、哲学、思想――ミリレベルの情報だってばっちり！",
          "flag_type": "",
          "flag": "",
          "choices": []
