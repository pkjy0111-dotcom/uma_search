      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ふふ。私もまったく同じです。 それに……『愛』や『恋』の意味、だけじゃないんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
