      "frame_id": 9,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ふふっ、ありがとうございます。\r 大変光栄なお話です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
