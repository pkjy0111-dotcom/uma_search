      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ですが――申し訳ございません。お断りいたします。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "……！",
              "flag_type": "",
              "flag": ""
