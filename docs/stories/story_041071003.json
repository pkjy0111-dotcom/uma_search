      "sub_background_id": 30,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "過去の模擬レース等の結果を一覧しても、\r それが適性外のレースであることは火を見るより明らかだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
