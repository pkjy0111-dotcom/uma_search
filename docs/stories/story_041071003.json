      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "どなたからお話をいただいたとしても、\r 私は同様に断るつもりでおりましたので。",
