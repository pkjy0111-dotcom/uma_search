      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "今のオマエならば、牛頭の怪物をも打ち倒し、 迷宮を抜け出すことができるはずだ！",
