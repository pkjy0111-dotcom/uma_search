      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ハハハッ！！　そうだろう！！ 俺にも聞こえるぞ……オマエの膂力の隆盛が！",
          "flag_type": "",
          "flag": "",
          "choices": []
