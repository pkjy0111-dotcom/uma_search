      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "あ。トレーナー、先に休んでていいよ？\r 喉渇いてない？　私のことは気にせず、休憩とってね？",
          "flag_type": "",
          "flag": "",
          "choices": []
