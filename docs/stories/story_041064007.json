      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "落ち着いてと言うより先に走り去っていく。\r ……一体、どうしたのだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
