    "4": {
      "frame_id": 4,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "っていうか、ちょっと顔が赤いような……！？\r 日陰に入って、日陰に！　たしか濡れタオル、あったよね！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "パ、パーマー！？",
