      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90953,
          "speaker_name": "バンブーの父親",
          "text": "だが……あの日のお前は、 ただ楽しんでいるように見えたよ。 強い相手と勝負し、全力を出し切ることをな。",
          "flag_type": "",
          "flag": "",
          "choices": []
