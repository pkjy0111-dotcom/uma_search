      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90715,
          "speaker_name": "和菓子屋店主",
          "text": "無理して続ける理由もなくなっちまったし…… そろそろ、潮時かと思ってたんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
