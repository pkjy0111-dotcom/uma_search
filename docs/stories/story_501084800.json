    "3": {
      "frame_id": 3,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ハッ！　ひとときの安寧を……。 静寂を纏った瞳で真実を捉えてみるがいい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
