      "frame_id": 10,
      "background_id": 175,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "愛と美の女神を魅了しようと 人間が作り出した花の庭……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 175,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "定められた運命を辿るように、 従順に咲き誇る彩……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 175,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "トレーナー、 ここに立つワタシに、何を思う？",
          "flag_type": "",
          "flag": "",
          "choices": []
