      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "タニノギムレットに言われ、 落ち着いてドレスを見ると…… どうやら、そういう加工らしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "更に詳しく聞くと、タニノギムレットは 用意した古着のドレスを自ら破り、 この加工を施したようだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "――相応しいだろう？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "短く問われ、よく似合っていると頷く。 だが……どうしてわざわざ破いたのだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "元のデザインは気に入らなかった？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "フッ……わかるはずだ、トレーナー。 暗黒の先で、輝く残滓―― 破壊が生む、その価値が！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "……破壊の価値？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
