      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "朝の挨拶に来てくれた、タニノギムレット。 新しい衣装を手に入れたと 聞いていたのだが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "や、破けてる！？",
              "flag_type": "",
              "flag": ""
