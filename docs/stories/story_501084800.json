      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……フ。答えは聞くまでもないようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
