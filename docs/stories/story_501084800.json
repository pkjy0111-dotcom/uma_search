    "13": {
      "frame_id": 13,
      "background_id": 175,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "先ほどと同じように、 似合っているかを聞いているのだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
