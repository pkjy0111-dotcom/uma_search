    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……クッ。幼子のような顔だな。 ならば、ついてくるといい。 答えを、導こう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
