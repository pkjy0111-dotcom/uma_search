      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "秘密にしておくほうが、それらしいのではないか？\r どう思う、トレーナー。",
