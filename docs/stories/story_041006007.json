      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "すまない、トレーナー。\r トレーニングを開始する前に、受け取って欲しい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
