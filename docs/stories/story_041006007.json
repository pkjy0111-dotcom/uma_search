      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "オグリキャップが準備運動で走っている間に、\r アンケート用紙を記入した……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
