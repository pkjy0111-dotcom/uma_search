      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "知らないのか？\r 今日は夜から――……むっ、これはアレだな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
