    "11": {
      "frame_id": 11,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "助かった。\r 夜までには確認して、役立てたいと思う。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "役立てるって？",
              "flag_type": "",
              "flag": ""
