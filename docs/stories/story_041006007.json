      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "アンケートの内容は――",
          "flag_type": "",
          "flag": "",
          "choices": []
