      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "あとはトレーニングに集中し、\r 今日のメニューをしっかりとこなすだけだ。",
