      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "ありがとう。\r では、よろしく頼む。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
