      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "オグリキャップがバッグの中からゴソゴソと1枚の紙を取り出す。\r ――どうやら手書きのアンケート用紙のようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "私が体を温めている間にでも、答えて欲しい。\r なに、すぐに済むはずだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
