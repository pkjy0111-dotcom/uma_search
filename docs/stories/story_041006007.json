    "6": {
      "frame_id": 6,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『甘いものは好きか』『辛いものは好きか』\r 『食物アレルギーは？』と、食べ物に関するものばかりのようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "ほとんど『はい』か『いいえ』で答えられるように\r なっているので、手間はとらせない……と、思う。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "頼めるだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "構わないけど……",
              "flag_type": "",
              "flag": ""
            }
