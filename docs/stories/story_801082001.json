    "6": {
      "frame_id": 6,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ノースフライトがコースを去っていく。 他のウマ娘たちと比べると、ずいぶん 早い切り上げに思えるのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "（もしかして、調子を崩してたり……？）",
              "flag_type": "",
              "flag": ""
