      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "ありがとうございます、エアグルーヴさん！ ご紹介いただいたレース戦術の本、 とっても参考になりました。",
          "flag_type": "",
          "flag": "",
          "choices": []
