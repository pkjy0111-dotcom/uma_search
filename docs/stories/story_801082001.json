      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "！　ぜひ、お願いします！ コースごとの特徴について、 できればもっと学びを深めておきたくて。",
