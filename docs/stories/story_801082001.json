      "frame_id": 9,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "構わん。有用だったのなら何よりだ。 追加で勧められるものもあるが、どうする？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
