      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "あらら……ごめんね、ビコーちゃん。 またの機会に走ろうね、約束！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
