      "frame_id": 17,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "それとも、契約に違反するつもりか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
