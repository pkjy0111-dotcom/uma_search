      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90022,
          "speaker_name": "実況",
          "text": "先頭のウマ娘が最後の直線に差し掛かりました！\r ここで鋭く足を伸ばすのは、1番人気エアグルーヴ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90022,
          "speaker_name": "実況",
          "text": "届くか！　届くか！　ゴール板はもう目の前！！\r エアグルーヴ届くかっ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
