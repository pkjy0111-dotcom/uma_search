      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……エアグルーヴの多忙ぶりは気がかりだが、\r 下手に口出しをすると契約を打ち切られてしまいそうだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
