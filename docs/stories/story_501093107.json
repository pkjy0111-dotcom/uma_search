      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "幼少期から彼女の中で育まれてきてしまった、 『自分には何もない、何も持っていない』 という想いが、ここにも作用しているのか――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
