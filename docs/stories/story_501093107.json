    "10": {
      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女を応援したいと思うから。 丈夫な体で元気に走り続けてほしいから。 誰よりそう願う相手だから、行動した。",
          "flag_type": "",
          "flag": "",
          "choices": []
