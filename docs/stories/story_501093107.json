      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "でもそれを、彼女は 『トレーナーさんが優しいから』と受け取る。 ――要因が自分にあるとは、決して考えない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
