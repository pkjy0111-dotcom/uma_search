    "9": {
      "frame_id": 9,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "あははっ！　いや、踊るってより ありゃ振り回したってなもんで てんで上手くいかなかったけどさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "……なっ！　撮影も終わったしさ、 アタシと踊ってみないかい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "えっ！？でもダンスはよく知らなくて……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "11": {
      "frame_id": 11,
