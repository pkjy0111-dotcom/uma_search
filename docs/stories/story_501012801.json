      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1012,
          "speaker_name": "幼いヒシアマゾン",
          "text": "『2人だけの、別世界にいるみたいだ……！』",
          "flag_type": "",
          "flag": "",
