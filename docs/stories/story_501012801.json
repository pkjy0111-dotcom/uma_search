    "8": {
      "frame_id": 8,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "懐かしいねぇ！　その後、 兄姉を連れ出して踊って――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
