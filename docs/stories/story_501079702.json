      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1079,
          "speaker_name": "フリオーソ",
          "text": "ええ。打倒中央のために、 惜しみなく時間をかけるべきです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
