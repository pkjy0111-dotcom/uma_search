      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1079,
          "speaker_name": "フリオーソ",
          "text": "潮の香り……うん。 これなら妙案が出ますよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
