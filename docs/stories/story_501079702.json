      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "フリオーソは中央に来てから、 ずっと気持ちが張りつめっぱなしに見える。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ここはしっかりとガス抜きさせたほうがいい。 例えば船橋にいた頃のように 浜辺で過ごしたり――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……海なら落ち着いて話せそうじゃないか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……海なら落ち着いて話せそうじゃない？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1079,
          "speaker_name": "フリオーソ",
          "text": "なるほど……いい案が浮かびそうですね。 行きましょう、トレーナー。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
