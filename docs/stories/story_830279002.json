      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "おうおうマメちんよ～？ 『レースと勉学に励む生徒たるもの、余裕を 持った登校を！』とか言ってたの誰だっけ～？",
          "flag_type": "",
          "flag": "",
          "choices": []
