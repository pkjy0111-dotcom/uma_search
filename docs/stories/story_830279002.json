      "frame_id": 8,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ううっ……！　結果的に寝坊して しまったのは本官の失態であります。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
