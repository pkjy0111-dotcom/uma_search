      "background_id": 33,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "だがトレーナーの純粋な想いに\r あてられた彼女は、己の脚で夢の実現を 果たす決意を固め、弱点を克服した。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
