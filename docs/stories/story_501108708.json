      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "勝ち続ける意味は、そこにある。 歩みを止めずにいこう、トレーナー。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
