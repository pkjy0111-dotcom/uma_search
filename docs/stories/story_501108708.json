      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "……いや。今日の走りは “最強”と言えるものではなかった。",
          "flag_type": "",
          "flag": "",
