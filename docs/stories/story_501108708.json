      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "最強ならば、常勝は当然。 当たり前だと思わせる結果もまた、 最強を成す要素ということか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
