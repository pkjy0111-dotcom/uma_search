      "frame_id": 4,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "私が目指すのは、あの日見た圧倒的な強さだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "まだそこへ至れていない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "でも、勝ち癖をつける事はいい事だよ",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "誰よりも成長すれば、きっと届く！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 25,
