      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "“最強の降臨”を目撃した観客は熱狂し―― そして、畏怖する。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
