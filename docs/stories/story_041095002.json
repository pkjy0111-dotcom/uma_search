      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90416,
          "speaker_name": "ベテラントレーナーC",
          "text": "……トレーニングぶりは、申し分ないんだ。合宿で君も見ただろう。 あれほどひたむきな姿勢を持った子は、そういない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
