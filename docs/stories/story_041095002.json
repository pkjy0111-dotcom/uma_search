      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "と、答えつつも……やはりか、という思いで、 先日ベテラントレーナー陣から聞いた話を回想する。",
          "flag_type": "",
          "flag": "",
          "choices": []
