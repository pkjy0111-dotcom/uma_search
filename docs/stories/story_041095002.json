      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90415,
          "speaker_name": "ベテラントレーナーA",
          "text": "『彼女、王道の中距離路線で活躍した、バブルガムフェローとかを どこか彷彿とさせるような走りをしてるふうに見えてね』",
          "flag_type": "",
          "flag": "",
          "choices": []
