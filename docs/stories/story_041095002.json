      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90415,
          "speaker_name": "ベテラントレーナーA",
          "text": "ただ、ひたむきすぎるというか……そうだなぁ。……意見も、 ましてや文句でさえ、何も言わなすぎるんだよな、あの子は。",
          "flag_type": "",
          "flag": "",
          "choices": []
