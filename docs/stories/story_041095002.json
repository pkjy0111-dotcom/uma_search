      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90418,
          "speaker_name": "ベテラントレーナーB",
          "text": "実はね。私たちそれぞれ一度ずつ、あの子と仮契約を交わしたのよ。 けれど、どうにも……うまく導いてあげられなくてね。",
          "flag_type": "",
          "flag": "",
          "choices": []
