      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "そうだ……！ 『状況や観察だけで相手のことを当てる』 という、探偵の得意技さ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
