    "7": {
      "frame_id": 7,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "まず、注目すべきは2人のよく磨かれた靴。 汚れるような道は通ってきていない……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "と……考えるのは浅はかだ！ ブライアンは滅多に靴など磨かない。逆に 『酷く汚れたから磨いた』直後だとしたら？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "な、なるほど……！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "近くの川沿いに、水たまりがあったな。 2人はそこを通らざるを得なかったのだ…… おそらく、何者かに追われていたがゆえ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
