      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "クラシック三冠も夢ではない。\r デビューしたばかりだが、目標として 据え置いても問題ないだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
