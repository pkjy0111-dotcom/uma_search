      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "子どものころに、ママと駆けっこして 見つけた場所なんですよ～。 なので、あそこは特別なんです！",
          "flag_type": "",
          "flag": "",
          "choices": [
