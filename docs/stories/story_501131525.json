    "3": {
      "frame_id": 3,
      "background_id": 33,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "散歩コースはだいたい決まっていて、 ちょうどよい時間に学園に戻れるくらいの 距離なのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 33,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "こんなに気持ちのいい日なんですし、 今日は少し遠出しましょう！ あたし、いい場所を知っているんですっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 33,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "とっても特別な景色なんですから。 本当にキレイなので、 トレーナーさんも気に入りますよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "グランアレグリアがワクワクとした顔で 先導してくれる。お気に入りの場所を 教えてもらえたことが嬉しく――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女の言う『遠出』がどこまでなのか、 深く考えることはしなかった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "葉の音が喝采みたい！ なんだか、走りたくなっちゃいますね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "グ、グラン……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "はい？　どうしましたか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "もうかなり歩いたと思うんだけど……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "あははっ。まだまだですよ！ レースだったら、まだ序盤って感じです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "でも……少し、ペースをあげましょうか！ 夕暮れに見る景色が一番キレイなので、 見逃せませんしっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "えっ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "実は夜になると、空くらいしかまともに 見られるものがないんですよね。 なので、急ぎましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "グランアレグリアは、ぐんぐんと進む。 一方こちらは、足元がおぼつかなくなり、 立ち止まることが多くなった。だが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
