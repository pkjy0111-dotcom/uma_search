      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1131,
          "speaker_name": "グランアレグリア",
          "text": "もうすぐ森を抜けますね。ふふっ。 あの景色を一緒に見られるなんて、 嬉しいな～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
