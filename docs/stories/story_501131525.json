      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーニングのない日などは、 こうしてグランアレグリアと 散歩することがある。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
