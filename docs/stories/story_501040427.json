      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "レース後、マネージャーはゴールドシチーに 今のあなたは美しくない、空っぽだと告げた。 そして、走るのをやめるよう提案する。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
