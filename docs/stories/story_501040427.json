      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だが、トレーナーは本人が『やめたい』と 言うまでは走らせるべきだと反論した。 今やめたら、きっと後悔する――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
