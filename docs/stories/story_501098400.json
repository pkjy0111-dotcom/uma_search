      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ダートの上の世代には、ホッコータルマエと ワンダーアキュートがいる。 ゆくゆくは彼女たちと戦うことになるだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
