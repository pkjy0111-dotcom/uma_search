      "frame_id": 3,
      "background_id": 71,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "風水を活かした戦法でレースに勝てば、 きっと風水を知ってもらえると考え―― 彼女に適したダート路線を進むことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 71,
      "sub_background_id": 110,
