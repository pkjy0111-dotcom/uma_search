      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "もともと自覚はあったんです！ どうにかしたいなって、 常々、思ってはいたのですが……。",
