      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1137,
          "speaker_name": "キセキ",
          "text": "咲け、乱れろ、天空の環よ。一を刻み、零の先。 我――ここに、原初を記す。『六花・十六夜』！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ど、どうしたんだ？急に……",
