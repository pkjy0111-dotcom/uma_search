      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "アルバムの中の写真には小さな頃の彼女、 そしてスーツ姿の男性や 白衣を着た女性が映っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
