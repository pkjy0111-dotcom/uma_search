      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "完成した時は、ずっとるんるん気分でした。 まさに、私の思い描いていた通り――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "父みたいに真っ直ぐ！　母みたいにきらきら！ ……そんな勝負服でしたから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "お、お父さんとお母さん……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "私が最高速を目指すようになったのは、 両親の影響です。なぜなら……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "両親に会ってもらったほうが速い。 2人を呼びます。飛行機の手配を――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "わー！そこまでしなくていいから！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
