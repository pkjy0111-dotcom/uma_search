      "frame_id": 10,
      "background_id": 220,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "家族のアルバムです。 両親に相談したら、送ってくれました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
