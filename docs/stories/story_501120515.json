      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "後日、仕事の合間に休憩を取っていると、 カルストンライトオがやってきた。 今すぐ見てほしいものがあるという。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
