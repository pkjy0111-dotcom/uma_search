      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "もちろんです。 『この勝負服と最高速に辿り着きたい』 そう想いを込めてデザインしましたから。",
          "flag_type": "",
          "flag": "",
          "choices": []
