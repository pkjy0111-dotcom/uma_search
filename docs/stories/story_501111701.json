      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "書き綴るうちについ筆が乗ってしまいまして。 送るかどうか、少し迷ったのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
