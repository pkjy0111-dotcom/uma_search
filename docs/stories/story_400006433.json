      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "国のプライドを背負ったその気迫は、 併走トレーニングの走りですら 風を切る轟音と共に大地を揺らす。",
          "flag_type": "",
          "flag": "",
          "choices": []
