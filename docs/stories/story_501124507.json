        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーニング前、バブルガムフェローは ファンレターを熱心に読み込んでいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
