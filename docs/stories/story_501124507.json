      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1124,
          "speaker_name": "バブルガムフェロー",
          "text": "他のことに気を取られていられないよ。 さぁ、トレーニング頑張ろ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
