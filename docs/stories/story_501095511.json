      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "建設業者",
          "text": "なぁに、理事長とウチの仲だ。施設の修繕に 関しては、今後もガンガン頼ってくれよな！",
          "flag_type": "",
          "flag": "",
          "choices": []
