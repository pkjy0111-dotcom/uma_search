      "background_id": 91,
      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "建設業者",
          "text": "そもそも、仕方のねぇことだろう？ ここはウマ娘の学園なんだからよ！",
