      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "陳謝ッ！　いつもすまない……！ 時間に融通をつけてもらって！",
