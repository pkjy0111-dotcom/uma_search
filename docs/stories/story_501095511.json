      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "建設業者",
          "text": "がっはっは！　今回もハデにやったな！ こりゃあ腕がなるってもんだぜ！！",
