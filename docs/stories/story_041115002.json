      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90420,
          "speaker_name": "中堅トレーナー",
          "text": "――オルフェーヴル！　本当に、素晴らしい走りだった……！ 貴方なら必ず頂点に立てる……！　私と一緒に、挑戦してみない？",
          "flag_type": "",
          "flag": "",
          "choices": []
