    "12": {
      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そうこうしているうちに、自分よりもはるかに実績のある トレーナーたちが彼女に駆け寄って取り囲んでしまった……！",
          "flag_type": "",
          "flag": "",
          "choices": []
