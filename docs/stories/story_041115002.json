    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90415,
          "speaker_name": "ベテラントレーナー",
          "text": "君の素質は圧倒的だ！　過去のどんな名ウマ娘にも劣らない……！ 私と共に、輝かしい戦績を打ち立てようじゃないか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
