      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "あ、トレーナーさんっ。\r おはよう☆　よく眠ってたね～。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "なんでアケボノがここに……？",
