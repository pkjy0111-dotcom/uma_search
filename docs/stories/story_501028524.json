      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "ほら、もう夜遅いでしょ？　あたしは\r 気にせずたくさん食べてほしいんだけど……。",
