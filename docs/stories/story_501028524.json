      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1028,
          "speaker_name": "ヒシアケボノ",
          "text": "夜遅くまでがんばってるトレーナーさんに、\r ちゃんこの差し入れだよ～。 あたしのためにありがとう～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
