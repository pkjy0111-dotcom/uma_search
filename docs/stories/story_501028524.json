      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "確かに目の前には鍋がある。\r とても有難いと思いつつ――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
