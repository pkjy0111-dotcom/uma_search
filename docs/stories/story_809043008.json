      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "正確には『VRウマレーターの前』だ。",
          "flag_type": "",
          "flag": "",
