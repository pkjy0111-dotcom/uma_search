      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "さあ、締めはやはりここだろう！ ロンシャンレース場だ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
