      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "エルコンドルパサーはシューズを買おうとしたが、\r 欲しいシューズのイメージを店員に伝えられず困っていたという。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "つまり君に似合うシューズが欲しいんだな？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "つまり貴方に似合うシューズが欲しいんだ？",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
