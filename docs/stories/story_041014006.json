      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "でもトレーナーさんなら、エルの考えわかってくれますよね？\r だって、アタシのトレーナーさんデスからっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
