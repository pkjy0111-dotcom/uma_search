      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "ブエノ！　でも、うまく伝えられなくて。\r 頭、チンプンカンプン、ショボーンです。",
