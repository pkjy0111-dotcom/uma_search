      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "グラシアス！　感謝、感激、エルにっこり！\r トレーナーさんがいれば怖いものなしデス！",
