      "frame_id": 12,
      "background_id": 46,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "そして店員さんにも上手く説明できますよねっ？\r だって、抜群のコンビですからっ！！",
