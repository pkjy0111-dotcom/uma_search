      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "うぅ～、なんて言えばいいんでしょうか……？\r はぁ、こんな時トレーナーさんがいてくれたら――",
          "flag_type": "",
          "flag": "",
          "choices": [
