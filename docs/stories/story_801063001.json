    "22": {
      "frame_id": 22,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "もっとも、理由はそれだけではありません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
