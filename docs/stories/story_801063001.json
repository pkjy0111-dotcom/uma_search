      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "オリエンテーションは、\r 外部の方がお見えになられます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
