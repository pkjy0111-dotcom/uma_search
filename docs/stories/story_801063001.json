      "frame_id": 20,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "中にはいずれこの学園に来る\r ウマ娘もいるかもしれません……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
