      "frame_id": 18,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ええ。当然です。\r 熱がこもらないはずがありません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
