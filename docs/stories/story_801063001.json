      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "私はそんな子たちに、安全に\r 心ゆくまで楽しんでもらいたいのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
