      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "まさか、こんな場所でお目にかかるなんて……\r なにやら、幼い頃に時が戻ったような、 不思議な心地ですね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
