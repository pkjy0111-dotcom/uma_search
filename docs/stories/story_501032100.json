    "15": {
      "frame_id": 15,
      "background_id": 16,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "本来ならば一致団結し手と手を取り合う\r はずのトゥインクル・シリーズ挑戦初日は――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "やはり、アグネスタキオンのペースに\r 巻き込まれてしまったのだった。",
