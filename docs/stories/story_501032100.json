      "frame_id": 20,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "初めは操作しづらいかもしれないが、\r 走れば走るほどスムーズになる。 とりあえずまずはリラックスだ。いいね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
