      "frame_id": 21,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 90001,
          "speaker_name": "",
          "text": "拒否権はないらしい。普段見ている\r グラウンドが、まるで違って見える。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
