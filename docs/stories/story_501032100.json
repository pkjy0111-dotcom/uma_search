      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "",
          "text": "気づけば、スタート位置についていた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "うわ！？",
