      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……その反応を見るに、\r 出来は悪くないようだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "何も言えなくてすまない",
