      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "君の忌憚のない意見を是非聞きたい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "可愛い振り付け",
