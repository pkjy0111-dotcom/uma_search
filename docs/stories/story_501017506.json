      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……さて。\r ここからは新たな相談だが――",
          "flag_type": "",
          "flag": "",
          "choices": []
