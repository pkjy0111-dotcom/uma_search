      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ダンスの表現の幅を広げたいと考えている。\r そのために、今何を取り入れるべきだろうか？",
          "flag_type": "",
          "flag": "",
          "choices": []
