    "21": {
      "frame_id": 21,
      "background_id": 147,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1083,
          "speaker_name": "＜機械人形＞",
          "text": "悪魔よ、どうか頼まれてくださいませんか。 私と主を引き合わせてくだされば、なんでも差し上げましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
