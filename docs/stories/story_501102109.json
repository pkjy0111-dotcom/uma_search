      "frame_id": 11,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "君のために演奏会を開きたいと思っていてね。 その日は空いていると聞いたが、 私の招待を受けてくれるだろうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "もちろん！絶対に行くよ",
