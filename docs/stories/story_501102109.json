      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "差しだされたのは1枚のメッセージカード。 もうすぐクリスマスのため、 何かの招待状であることは察しがついた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
