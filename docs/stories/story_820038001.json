      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "気弱なウマ娘",
          "text": "うわぁ、どうしよう……\r シリウス先輩と取り巻きの不良たちだ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
