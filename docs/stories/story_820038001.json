      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "大勢の者を従え、不当にコースを\r 占拠していると生徒会に訴えが来ております。",
          "flag_type": "",
          "flag": "",
          "choices": []
