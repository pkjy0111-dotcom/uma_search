      "background_id": 9,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "つきましては、他の生徒に支障が出ぬよう\r 場所を空けてはいただけませんか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
