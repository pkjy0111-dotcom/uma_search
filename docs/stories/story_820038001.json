      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "シリウスシンボリが大勢の生徒に指示を\r 出している。その姿は教官さながらに 頼もしいが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
