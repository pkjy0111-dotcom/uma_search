      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "あたりを見回すと、コース外の\r 生徒はみな困った顔をしている。 どういうことだろう……？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
