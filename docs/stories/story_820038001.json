      "frame_id": 18,
      "background_id": 9,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "じゃ、規則のためならコイツらは\r 締め出されてもいいって？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
