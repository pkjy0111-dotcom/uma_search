      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "つまりな、個人の事情は全無視なのが\r この学園の『規則』。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
