    "21": {
      "frame_id": 21,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "どうにもならねぇ事情があって\r 仕方なく規則を破っていようと、 お構いなく、だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
