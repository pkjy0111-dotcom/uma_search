      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 0,
          "speaker_name": "真面目なウマ娘",
          "text": "困りましたね。これでは\r トレーニングをする場所が……。",
          "flag_type": "",
          "flag": "",
          "choices": []
