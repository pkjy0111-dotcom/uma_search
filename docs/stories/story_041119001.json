      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "快いお返事、ありがとうございます。 是非、忌憚なきご意見を……よろしくお願いいたします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
