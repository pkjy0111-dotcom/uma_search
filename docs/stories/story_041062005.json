      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "それにネイチャもすっごく優しくって。\r なんと最後に残った貴重な1本を私に譲ってくれたんですよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
