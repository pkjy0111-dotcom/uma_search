    "11": {
      "frame_id": 11,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "いやー、たまたま会った商店街のおばちゃんがお茶を淹れてくれて、\r これが貝ヒモに合うのなんのっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
