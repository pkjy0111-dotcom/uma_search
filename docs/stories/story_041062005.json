      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "それからはもうタコさん食べまくり！　そして粘りに粘った結果\r カニさんを2個も発見し、楽しくわけっこしてきたわけです♪",
          "flag_type": "",
          "flag": "",
          "choices": []
