      "background_id": 181,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "国の誇り……？ ――否。",
          "flag_type": "",
