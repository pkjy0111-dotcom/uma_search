      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "余が、国を誇るのだ。 この王を生み、育んだ至高の地として。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "……！？",
              "flag_type": "",
              "flag": ""
