      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "おお、オペラオー！ どうしてボクはオペラオーなんだい！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
