    "13": {
      "frame_id": 13,
      "background_id": 347,
      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "謎解きするつもりで、考えてみませんか？　ラモーヌさんの 言葉の意味。謎めいた言葉、いろいろあったでしょう？",
          "flag_type": "",
          "flag": "",
          "choices": []
