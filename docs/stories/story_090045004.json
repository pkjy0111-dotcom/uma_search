      "sub_background_id": 100,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "あ……ふふ、ありがとうございます。おかげで 少し肩の力が抜けた気がします。もっと前向きに――そうだ！",
          "flag_type": "",
          "flag": "",
