      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1112,
          "speaker_name": "デアリングハート",
          "text": "――眉間のシワ、取れたわね。 辛気臭いのはナシよ。ポジティブにいきましょ？",
