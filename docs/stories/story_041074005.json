      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "トレーナーさま。\r 今、少々よろしいでしょうか～？",
          "flag_type": "",
          "flag": "",
          "choices": []
