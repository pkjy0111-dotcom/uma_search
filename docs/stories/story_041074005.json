      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "もうすっかり夏ですわね～。\r 今度のお休みは、きっと絶好の遊園地日和になりますわ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
