      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "はい～。この間、\r お父さまがチケットを送ってくださって――",
