      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "ありがとうございます。\r ではでは……しばらく、お邪魔させていただきますわ～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
