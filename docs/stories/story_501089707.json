      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1089,
          "speaker_name": "シュヴァルグラン",
          "text": "でも……いいのかな……。 不調は自分のせいなのに…… 神さまに頼むなんて……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
