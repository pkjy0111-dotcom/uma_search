      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "それは、さっきやってイマイチだったでしょ？\r 大好きなお姉ちゃん1人で充分だって！",
