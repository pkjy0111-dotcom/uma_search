    "8": {
      "frame_id": 8,
      "background_id": 52,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "いやまあ、確かにガラじゃないんだけど……\r 応援って、なかなか侮れないもんじゃない？",
          "flag_type": "",
          "flag": "",
          "choices": []
