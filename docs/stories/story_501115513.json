      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "……ふふ。さすが、察しがよろしいですね。 ご推察の通り、オルの勝負服の装飾品ですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
