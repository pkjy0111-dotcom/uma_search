      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90230,
          "speaker_name": "宝石店の店員",
          "text": "お待たせいたしました。 どうぞ、ご確認ください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
