      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "デザインも素材も、厳選に厳選を重ねました。 “王”たるオルに相応しいものをと、 方々から候補を取り寄せてね。",
          "flag_type": "",
          "flag": "",
          "choices": []
