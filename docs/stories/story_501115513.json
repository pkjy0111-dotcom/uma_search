    "14": {
      "frame_id": 14,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "このお店とは、昔から懇意にしていましてね。 目利きも腕も、信頼できる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "宝石も金細工も、メンテナンスは重要です。 まして勝負服ですから……特殊な汚れは、 専門家に磨いていただくのが一番です。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "すごく丁寧に管理しているんだな……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "すごく丁寧に管理しているんだね……",
              "flag_type": "",
              "flag": "female_trainer"
