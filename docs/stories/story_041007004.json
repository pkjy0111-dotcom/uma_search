      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "レースの申し込みまではトレーナー無しでなんとか騙せてきたけど、\r さすがに今日ばっかりは無理っぽくってさ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "つまりトレーナーなら誰でもよかった……？",
