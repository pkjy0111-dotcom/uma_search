        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "模擬レースの観戦にやって来た。\r 次のレース開始を待っていると……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
