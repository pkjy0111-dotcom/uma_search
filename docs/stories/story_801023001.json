      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "しかし、多角的には視認できない。監視塔に\r 登らせてもらうか？　いや、さすがにな……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
