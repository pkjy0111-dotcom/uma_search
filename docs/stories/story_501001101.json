      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "改めて見ると、はみ出しそうになるほどの\r 豪快な筆運びに、彼女の『日本一』を目指す 想いの強さが感じられる気がする……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "一緒に日本一になろうな！",
              "flag_type": "male_trainer",
