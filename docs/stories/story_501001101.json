      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "えへへ、どうでしょうか？\r 私の新年の抱負、決意表明みたいなものです！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
