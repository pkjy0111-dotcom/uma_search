      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "なんと言っても今年はクラシック級に\r 挑戦するとっても大事な1年ですから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
