      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "その始まりに、目標は変わらないんだぞって\r 自分に言い聞かせる意味で書いてみました！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
