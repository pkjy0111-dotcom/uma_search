    "10": {
      "frame_id": 10,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "――よし。ワシも当然、豆大福に投票じゃ！ ここで我を通せんようでは、一軍の将の名折れじゃからのう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "なあに、見ておれ！　氷菓派なんぞ三日天下よ！ 数日後には逆転しておるじゃろうて！　にゃーっはっはっは！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
