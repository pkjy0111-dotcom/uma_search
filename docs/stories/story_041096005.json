      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "むむ？　なんじゃ？　この…… 『デザートメニュー投票』というのは？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ああ、この間からやってるんだよ",
