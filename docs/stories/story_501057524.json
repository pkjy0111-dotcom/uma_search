    "11": {
      "frame_id": 11,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『彼の担当ウマ娘は、天を駆けているようだ』 ――そんな逸話が語られるほどに。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
