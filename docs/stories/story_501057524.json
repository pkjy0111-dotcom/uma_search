    "12": {
      "frame_id": 12,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そして、母親は重賞を制するほどの名ウマ娘。 そんな2人が結ばれることになるとは。",
          "flag_type": "",
          "flag": "",
          "choices": []
