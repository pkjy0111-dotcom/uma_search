      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90857,
          "speaker_name": "シービーの母親",
          "text": "うちの子がお世話になってます。 まさか、こんな場所でお会いするなんて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
