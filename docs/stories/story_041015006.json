      "frame_id": 22,
      "background_id": 33,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ええっ！？　ボクとボクを戦わせるだって！？",
          "flag_type": "",
          "flag": "",
          "choices": []
