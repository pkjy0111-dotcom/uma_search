      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ビコーペガサスの目標は2つ。 『スプリンターズステークス』と 『マイルチャンピオンシップ』両方の勝利。",
          "flag_type": "",
          "flag": "",
          "choices": []
