      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "まさかこの土砂降りの雨の中、走りたいとでも言うのだろうか。\r いくらなんでも風邪をひいて――",
          "flag_type": "",
          "flag": "",
          "choices": []
