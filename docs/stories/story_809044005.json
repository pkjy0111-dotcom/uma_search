      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "次はぜひ、トレーナーさんの行きたい所へ お付き合いさせていただけませんか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "そうですね、でしたら――",
              "flag_type": "",
              "flag": ""
