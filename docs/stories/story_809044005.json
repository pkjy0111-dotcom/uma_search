      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "今日は相談に乗ってくださり、 ありがとうございます。おかげで あの子の気に入りそうなものが選べました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
