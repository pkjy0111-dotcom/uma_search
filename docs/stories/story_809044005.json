      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "1度見立ててあげたらとても気に入って くれて、それ以来……でしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
