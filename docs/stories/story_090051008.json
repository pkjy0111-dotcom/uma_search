      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1010,
          "speaker_name": "タイキシャトル",
          "text": "これは、我が家のバーベキューで食べる、ママ直伝の定番。 ファミリーで食べる、思い出の味デス。",
          "flag_type": "",
          "flag": "",
