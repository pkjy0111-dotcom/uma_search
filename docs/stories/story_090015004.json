      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "ケ？ でも、川に浮かぶ石も、台座もどこにもないデス！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
