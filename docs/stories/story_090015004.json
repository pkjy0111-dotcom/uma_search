      "frame_id": 5,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "こ、ここで私たちが挑戦するのは川に浮かぶ石の道を渡り、 台座に乗ったストーンを手に入れることです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
