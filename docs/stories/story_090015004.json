      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "それには実は仕掛けがありまして……！ スイッチを作動させると川から石と台座がせり上がってくるんです！",
          "flag_type": "",
          "flag": "",
          "choices": []
