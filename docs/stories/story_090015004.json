      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "原作の映画見つかんなくってねぇ。 今日はロブロイちゃんに頼りきりになっちゃうかもな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
