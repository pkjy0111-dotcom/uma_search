    "4": {
      "frame_id": 4,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "（そっか……お話の内容は私しか知らないんですよね。 ……し、しっかりしなきゃ……！）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
