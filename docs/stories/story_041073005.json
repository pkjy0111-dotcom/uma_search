      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "（雨とか芝とか水捌けとか。そういう話してもらったの、 いつだっけ？　むむむ、思い出せ～～！）",
          "flag_type": "",
          "flag": "",
          "choices": []
