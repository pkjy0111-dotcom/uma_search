      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "暗渠排水で乾きが早いなら、荒れてると思って 混みあう外側よりも、勝ち目がある場合も……ってことですね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
              "loop_ends_at_frame": -1,
              "text": "先入観で避ける相手が多ければハマるかもね",
