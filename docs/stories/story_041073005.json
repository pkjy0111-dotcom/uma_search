      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1073,
          "speaker_name": "ツルマルツヨシ",
          "text": "すぅ～っ、はぁ～っ……落ち着いてけ、ツヨシ～……！ すぅ～……よし！",
