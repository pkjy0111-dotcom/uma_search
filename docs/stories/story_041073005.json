      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "何やらさっきからずっとこの調子で、 課題と睨みあってはウンウンと唸っている。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "それ、何の授業の課題？",
              "flag_type": "",
              "flag": ""
            }
