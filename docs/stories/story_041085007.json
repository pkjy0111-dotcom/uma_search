      "sub_background_id": 2120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "目に見えるトレーニングの成果に密かにガッツポーズする。 トレーナーとして過不足なく業務が行えているのが嬉しい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
