      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90001,
          "speaker_name": "",
          "text": "しかし集中しているダイイチルビーには、 外の音など届いていないようだ。そして――",
          "flag_type": "",
