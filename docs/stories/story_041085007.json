    },
    "2": {
      "frame_id": 2,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "みっちり詰め込んだ高負荷のトレーニングメニュー。 それをフォローするためのアイシングや、マッサージの準備。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "お互い、やるべき事は共有しているため、 トレーニング自体は淡々と進む。それでも――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
