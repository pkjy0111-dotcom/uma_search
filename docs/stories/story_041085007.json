    "7": {
      "frame_id": 7,
      "background_id": 42,
      "sub_background_id": 2120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "本日も、ご佑助ありがとうございました。 それでは、着替えて参りますのでこれで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 42,
      "sub_background_id": 2120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーニングが終わった後も、自分にやれる事はまだ残っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "ねーねー、お祭り行くっしょ？ 今年もにんじん焼き出てるかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "お夕飯、控えめにしたほうがよかったかしら。 外でいただくと、つい食べ過ぎちゃうのよねえ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "合宿の日程も半ばを過ぎ、今日は近くで夏祭りが開催される。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "多くのウマ娘たちが楽しそうに外出していくが、 おそらくダイイチルビーは合宿所に残るだろう――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "……トレーナーさん？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "勉強の用意、しておいたよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "普段なら学習に使う時間――なるべく学園にいる時と 変わらない環境を用意してみた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "……合宿所にこのような静かな場所があったのですね。 それに、机や照明まで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
