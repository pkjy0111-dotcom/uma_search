      "frame_id": 16,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "……ご配慮、感謝いたします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
