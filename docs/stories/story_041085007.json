          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "――ふっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "いいぞ、タイム上がってる！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "いいよ、タイム上がってる！",
              "flag_type": "",
              "flag": "female_trainer"
