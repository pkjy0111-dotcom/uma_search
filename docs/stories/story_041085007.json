      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ようやくひと区切りついた様子に、準備していた言葉をかける。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
