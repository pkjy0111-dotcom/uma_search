      "sub_background_id": 2120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "…………。",
          "flag_type": "",
          "flag": "",
          "choices": []
