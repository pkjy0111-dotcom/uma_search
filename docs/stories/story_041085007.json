      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ドォン――と、遠くから花火の音が聞こえてくる。 そろそろ祭りも終わりの時間だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
