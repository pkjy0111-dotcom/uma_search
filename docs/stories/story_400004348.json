    },
    "12": {
      "frame_id": 12,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "その通りですね。いつかあの背に並び、\r そして前に出たいと思っています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
