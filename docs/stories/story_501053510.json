      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "――アタシが学園の風紀委員長になった時、 父さんが話してくれたんスよ。 風紀を守る者の心得について。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
