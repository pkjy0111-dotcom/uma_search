      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90953,
          "speaker_name": "バンブーの父親",
          "text": "風紀を守る人間ってのはな、まず…… 最高に、カッコよくなくちゃならねえ！",
