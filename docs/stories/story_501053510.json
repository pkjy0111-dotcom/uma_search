    "16": {
      "frame_id": 16,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "……いえ！　お気持ちは本当嬉しいっスけど、 コイツだけはアタシの力でやり切りたいんス！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
