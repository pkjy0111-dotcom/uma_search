      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "なんせ今日は…… 『1日64善』の日っスから！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "……い、1日1善じゃなくて？",
