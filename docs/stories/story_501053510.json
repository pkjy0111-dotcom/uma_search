    "12": {
      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "おう、苦労かけちまったなバンブー！ 後輩らに差し入れ配るの 手伝ってもらっちまってよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "いやいや！　こっちこそイナリ先輩の お役に立てて嬉しいっスよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
