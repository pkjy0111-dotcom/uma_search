      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "最初はそうだったっス！　そっから 1日ごとに倍に増やして、2、4、8！ ……16！　えーと……32？　32ッ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
