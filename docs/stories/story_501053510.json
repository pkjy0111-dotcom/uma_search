      "frame_id": 14,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "へへ、相っ変わらず頼もしいヤツだ。 聞いたぜ、ここ最近は特にあっちこっちで 助っ人をつとめてるってよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "世話になっといてなんだが、大変じゃねえか？ 他ならぬお前さんのことだ、 なんなら手を貸すぜ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
