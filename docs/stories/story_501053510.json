      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――止める間もなく行ってしまった彼女を、 なんとも心配な気持ちで見送った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
