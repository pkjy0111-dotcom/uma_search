      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "ととっ！　急がなきゃ間に合わないっスね！ それじゃ、失礼します！",
