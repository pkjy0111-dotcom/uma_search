      "frame_id": 8,
      "background_id": 4,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "えーとえーとえーっと……そんで！ 1週間目の64！　コイツをクリアできれば、 サイコーにカッコいいッ！　熱いっス！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "さすがに無茶じゃ――",
