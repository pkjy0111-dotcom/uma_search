      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90953,
          "speaker_name": "バンブーの父親",
          "text": "お～風紀委員長になったか！　よくやったな！ じゃあ、俺の教師経験から1つアドバイスだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
