      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――そして、夕方。コースにて、 バンブーメモリーの姿を見かけた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
