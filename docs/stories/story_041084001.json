      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ハァーッハッハッハッハァ～～ッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
