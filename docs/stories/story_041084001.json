      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90307,
          "speaker_name": "マスター",
          "text": "……すみませんね、お客さん。ああ見えて世話焼きなんです。 暗い顔したあなたのことが、放っておけなかったんでしょう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
              "loop_ends_at_frame": -1,
              "text": "（それで、わざわざ……）",
