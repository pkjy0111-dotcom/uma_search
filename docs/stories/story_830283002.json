      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "想い、作り上げるほどに理解が深まります。 この喜びもまた、推し活の醍醐味なのですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
