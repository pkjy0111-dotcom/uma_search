      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーナーは、彼女の悲痛な願いを 無下にすることはできなかった。",
          "flag_type": "",
          "flag": "",
