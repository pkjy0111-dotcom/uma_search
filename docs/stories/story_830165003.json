      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "物語はこうだ……。 何度も別れを繰り返した、 孤独な研究者――彼はうんざりしていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
