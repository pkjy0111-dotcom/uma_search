      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "何も、思いつかぬのだ…… あれほど書きたかった、『悲劇の別れ』が！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
