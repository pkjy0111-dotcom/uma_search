      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90730,
          "speaker_name": "同門からの手紙",
          "text": "『あの時は、酷いこと言ってごめんなさい。\r だいぶ遅くなったけど……私からも、 ヤエちゃんを送らせてください。』……。",
          "flag_type": "",
          "flag": "",
          "choices": []
