      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90730,
          "speaker_name": "同門からの手紙",
          "text": "『……師範代も昔言ってたね。 “来る者拒まず、去る者追わずでは不足。 来る者は迎え、去る者は送ってあげなさい”』",
          "flag_type": "",
          "flag": "",
          "choices": []
