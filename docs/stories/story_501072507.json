      "frame_id": 17,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90730,
          "speaker_name": "同門からの手紙",
          "text": "『だけど、ヤエちゃんが走ってる姿を テレビで見て、気づきました。ヤエちゃんは、 今も立派に“道”を進んでいる。』",
          "flag_type": "",
          "flag": "",
          "choices": []
