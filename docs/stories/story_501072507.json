      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90730,
          "speaker_name": "同門からの手紙",
          "text": "……『私は、ずっと怒ってた。\r ヤエちゃんだけ、私を置いて 武道から逃げたと思ったから。』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
