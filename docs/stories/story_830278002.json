      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "だ、誰がビビっているでありますか！ しょ、所詮は噂、幽霊などい、いるはずが――",
