      "background_id": 127,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1076,
          "speaker_name": "サクラローレル",
          "text": "ネットで調べてたら見つけちゃって！ ぜひ、直接見たいなって思ってたんですっ。",
