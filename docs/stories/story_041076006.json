      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1076,
          "speaker_name": "サクラローレル",
          "text": "まずは、このステンドグラスです！ ゴシック様式っていう建築様式の特徴なんですよ！",
