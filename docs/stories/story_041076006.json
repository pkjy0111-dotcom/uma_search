      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1076,
          "speaker_name": "サクラローレル",
          "text": "天井を高く、窓を大きくして、光がたくさん差し込むように―― そういう意図で発展した文化なんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "へぇ～！他には？",
