      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1076,
          "speaker_name": "サクラローレル",
          "text": "キレイな教会でしょう？　ここの建築様式、 フランスの要素も多く取り入れられてるんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
