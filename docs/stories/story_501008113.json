      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "まったく……おとなしく見てればいいのに、\r 直接乗り込んでくるなんて。",
