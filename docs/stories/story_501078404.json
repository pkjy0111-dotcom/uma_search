      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だが、トレーナーから自分にしかできない 励まし方があると教わる。そして……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
