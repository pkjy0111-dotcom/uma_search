      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "ん～？　そうデスか？　強くなれるのなら、\r それに越したことはないと思いますけど？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "ここぞという時まで取っておこう",
