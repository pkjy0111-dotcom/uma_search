      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "なので、いつもの倍の\r メニューを注文したのデス。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
