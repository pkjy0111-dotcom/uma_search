      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "これを使っていっぱい食べれば、\r 連戦連勝間違いなしなのでっ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "一気に使い切るのはもったいない",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "いっぱい食べて、強くなろう！",
              "flag_type": "",
              "flag": ""
            }
          ]
