      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "ひとさじかければ空をも飛べる！\r 我が家に伝わる秘伝のホットソース！",
          "flag_type": "",
          "flag": "",
          "choices": []
