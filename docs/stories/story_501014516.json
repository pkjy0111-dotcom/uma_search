      "frame_id": 10,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "なるほど、大事なレースの前に食べれば\r 万全の状態でレースに出られる という訳デスね！",
          "flag_type": "",
          "flag": "",
          "choices": []
