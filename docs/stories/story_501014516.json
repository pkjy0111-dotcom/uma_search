      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1014,
          "speaker_name": "エルコンドルパサー",
          "text": "ここぞという時と言いますとー……\r 大事なレースとかデスかね？",
