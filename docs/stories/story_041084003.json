      "frame_id": 24,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……ククッ！　ようこそ、我が巨大樹へ。 オマエならば、それを読み、必ず訪れると踏んでいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
