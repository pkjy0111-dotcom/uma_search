      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 37,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "それこそが俺の求めるワタシ！ ギムレットをもって『タニノギムレット』の存在意義は開始する！",
