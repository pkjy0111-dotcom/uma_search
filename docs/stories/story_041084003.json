    "27": {
      "frame_id": 27,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "まだ飲めぬ俺に、父は味を言葉で教えてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "28": {
      "frame_id": 28,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "『ギムレットとは刹那にて強烈に残り、 そして思いを馳せたくなる美酒だ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
