      "frame_id": 29,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "そしてこうも言った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
