    "25": {
      "frame_id": 25,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "さて、話を戻そうか。 そう、父からの教示とは、まさにその聖なるカクテル――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "26": {
      "frame_id": 26,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 90001,
          "speaker_name": "",
          "text": "と、タニノギムレットは手元のグラスを指し示した。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 27,
              "loop_ends_at_frame": -1,
              "text": "ギムレット――？",
              "flag_type": "",
              "flag": ""
            }
