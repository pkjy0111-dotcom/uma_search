      "frame_id": 35,
      "background_id": 237,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 36,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "速さと強さをもって、刹那にて眩く輝き！ 目に、脳に、心に、過去に未来に刻印せし神話だ……ッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
