      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "……～～～ッ！　ああ、ああ！　それこそだ、父さん！ この躍動する鼓動が聴こえるか、父さん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
