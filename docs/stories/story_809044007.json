      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "正直、助かりました。 ……少々お待ちいただけますでしょうか。",
