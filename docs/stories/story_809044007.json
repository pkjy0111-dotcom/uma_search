      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "あ、でも大丈夫です！ 行きましょうか、トレーナーさん。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "いえ、そういうことなら――",
