      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "あの番組、字幕表示のテンポも 色味もすごく工夫されていて、なので……。",
