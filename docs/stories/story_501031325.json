      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そしてそのウマ娘たちの姿に、 人々は想いを重ねる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
