      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "誇り高き栄誉を求め、幼き頃からの夢を求め。 ウマ娘たちは王座へと手を伸ばす。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
