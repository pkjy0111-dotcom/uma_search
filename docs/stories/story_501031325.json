      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "肌が粟立つ激闘。骨身に染みる熱。 我先にとゴールへ駆けこむ走者。",
          "flag_type": "",
          "flag": "",
          "choices": []
