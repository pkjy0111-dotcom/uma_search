      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『ひと口食べれば“運命”が鳴り響く』\r 言葉にできない美味しさだと評判なのですが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
