      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "実店舗を持たない、移動販売のみのクレープ屋です。\r 6か月前からウマ娘の間で話題になっていました。",
          "flag_type": "",
          "flag": "",
