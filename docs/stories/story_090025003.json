      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "投げたら取られ、避けても追われ、 背後に迫るあの高笑いが、昨夜は夢にまで出てくる始末！",
          "flag_type": "",
          "flag": "",
          "choices": []
