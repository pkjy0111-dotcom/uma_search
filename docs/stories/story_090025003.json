      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "んん？　どういうこと？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
