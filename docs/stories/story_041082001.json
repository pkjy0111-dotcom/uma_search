    "9": {
      "frame_id": 9,
      "background_id": 233,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "ふふっ。 はい、トレーナーさんがよければ、もちろん！",
