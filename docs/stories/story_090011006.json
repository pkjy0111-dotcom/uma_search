      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "大量リード……なるほど！\r 確かにその方がタンホイザさんの助けになりますね！",
