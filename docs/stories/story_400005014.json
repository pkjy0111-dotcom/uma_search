      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "そして、クラシック級の中でも三女神さまに 認められるほどの素質をもったウマ娘が 舞台に上がることを許される『WBC』！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
