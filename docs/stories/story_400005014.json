      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "去年は『グロウアップレース』、 そして今年は『WBC』…… これってすごいことなんじゃない？",
          "flag_type": "",
          "flag": "",
          "choices": []
