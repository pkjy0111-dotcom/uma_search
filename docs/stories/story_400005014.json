      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "ある意味、クラシックレースの舞台に 上がるのと同じくらい、すごいことかも。",
