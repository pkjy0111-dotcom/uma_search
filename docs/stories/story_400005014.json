      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "数多くのデビューを果たしたウマ娘の中で、 抜け出た新星のみが出走できる 『グロウアップレース』――",
          "flag_type": "",
          "flag": "",
          "choices": []
