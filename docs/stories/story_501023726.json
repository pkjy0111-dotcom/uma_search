      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "今は、彼女がぬいぐるみを持っている姿すら\r 想像できないが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
