      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "懐かしそうにぬいぐるみを眺める彼女を見て、\r あることを思いついた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "久しぶりに持ってみる？",
