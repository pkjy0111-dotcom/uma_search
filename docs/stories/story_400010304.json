      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "――なるほど、それで屋敷に。 姉はじきに、社交場から戻ると思います。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "休日もか……君は事情を知っているのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "休日もか……貴方は事情を知っているの？",
              "flag_type": "",
              "flag": "female_trainer"
