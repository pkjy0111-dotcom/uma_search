    },
    "8": {
      "frame_id": 8,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "スポーツ記者との会合や、レース界の重鎮が 集う社交場に積極的に参加するようになり――",
