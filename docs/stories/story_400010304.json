      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "他者のことなどほとんど気に留めない彼女が、 人脈作りに奔走していた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "（らしくない、どうしたんだろう……）",
              "flag_type": "",
              "flag": ""
