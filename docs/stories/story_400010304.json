      "background_id": 37,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして―― 次の日から、メジロラモーヌは変わった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
