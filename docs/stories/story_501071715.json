      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……そうですね。オーバーワークは\r ケガに繋がる危険がありますし、 走るのはやめておきます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
