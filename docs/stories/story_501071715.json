      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "トレーナーさん。もう少々走っていても\r よろしいでしょうか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
