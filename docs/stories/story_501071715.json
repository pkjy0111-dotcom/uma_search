      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "その代わり、クールダウンストレッチや\r アイシングを行っている間、 お化けがいないか探してみます。",
