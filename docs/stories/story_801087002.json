      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "――やや、ウワサをすれば辻写りチャンス。 ではではそれでは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
