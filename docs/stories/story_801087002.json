      "frame_id": 2,
      "background_id": 74,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "気のせいだろうか？　なんだかやけに 写りこむウマ娘が居るような――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 24,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "また別の日に写真を撮っていると……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "君、確かこの前も――",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "貴方、確かこの前も――",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 24,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "おや、気づかれてしまいましたか。 マーちゃん一分の不覚。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 24,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "マーちゃんつい、カメラがあると うずうずしてしまい……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 24,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "どうにも辻写り――みなさんのお手元に マーちゃんを残してほしくて、 写りに行ってしまうのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
