    "8": {
      "frame_id": 8,
      "background_id": 209,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90721,
          "speaker_name": "不良のリーダー",
          "text": "そうは言っても、お前はこっちで有名になりすぎたんだよ。 上のアニキも直属の俺らより、お前の度胸を買ってやがる。",
          "flag_type": "",
          "flag": "",
          "choices": []
