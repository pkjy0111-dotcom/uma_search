      "background_id": 209,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "とはいえまだこの街には知らない場所も多い。たまにはのんびり 散歩でもしてみようと、足を延ばしてみたのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "（この通りは長居すべきじゃないな……）",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "（この通りは長居すべきじゃないかな……）",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
