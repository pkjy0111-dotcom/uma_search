      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1049,
          "speaker_name": "ウマ娘の少女",
          "text": "……チッ、こっちはもう足洗ってんだ。 勝手に因縁つけてくんじゃねぇよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "（ウマ娘……！？）",
              "flag_type": "",
              "flag": ""
