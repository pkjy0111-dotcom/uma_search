      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "事業もご隆昌のことと伺っております。 なんでも、若者向けのレジャー施設を お建てになるそうですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
