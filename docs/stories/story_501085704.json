      "frame_id": 8,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "起業家の男性",
          "text": "ええ。ご存じでしたか。実は我々は、 新しい事業を展開しようと考えており……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
