      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "……よい情報を得られました。 そろそろ学園に戻りましょう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "そ、そうしてくれると助かる……！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
