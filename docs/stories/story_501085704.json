      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "我々の世代では、やはり『遊園地』、 『プール』、『野外アトラクション』等が 好まれる傾向にございます。",
          "flag_type": "",
          "flag": "",
          "choices": []
