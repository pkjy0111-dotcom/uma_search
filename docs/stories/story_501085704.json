      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "例えば遊園地ですと――",
          "flag_type": "",
          "flag": "",
          "choices": []
