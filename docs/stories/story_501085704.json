    "6": {
      "frame_id": 6,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "いつもお世話になっております。わざわざ ご足労を賜りまして、ありがとう存じます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
