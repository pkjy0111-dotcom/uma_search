    "13": {
      "frame_id": 13,
      "background_id": 34,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "私の経験則ではありますが、遊園地は 来場者同士の精神的距離を縮められる 場所ではないかと考えております。",
          "flag_type": "",
          "flag": "",
          "choices": []
