      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その数日後、彼女の協力者として 起業家のパーティーに同行することになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "起業家の男性",
          "text": "ああ、こちらにいらっしゃいましたか。 お越しになると聞いてぜひご挨拶をと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
