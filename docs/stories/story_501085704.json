      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "ですので『同行者と共に得られる体験』を 重視した施設を特にご用意されますと、 昨今の潮流に合うかと存じます。",
          "flag_type": "",
          "flag": "",
          "choices": []
