    },
    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90104,
          "speaker_name": "素直なウマ娘",
          "text": "あ、えっと、その……もともと短距離だったんだよね？\r なのに中距離も走れるようになったって！　本当、すごいよ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "やや興奮した面持ちの彼女を、\r ミホノブルボンはじっと見つめた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
