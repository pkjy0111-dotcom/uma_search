    "8": {
      "frame_id": 8,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "あなたのその声かけは、どういった意味合いを持っていますか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90104,
          "speaker_name": "素直なウマ娘",
          "text": "え？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
