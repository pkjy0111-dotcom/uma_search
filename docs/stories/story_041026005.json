    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "いえ、『会話』は歓迎します。\r どうしましたか？",
