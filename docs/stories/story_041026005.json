      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90104,
          "speaker_name": "素直なウマ娘",
          "text": "急にごめんね、いつもハードトレーニングしてて、\r がんばってるな～って気になってて……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
