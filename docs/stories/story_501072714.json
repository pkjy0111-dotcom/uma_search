      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "心頭滅却すれば火もまた涼しと言いますし、\r トレーニングを続けさせてください。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "治ってからしっかりトレーニングしよう",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "無理のない範囲でやろう",
              "flag_type": "",
              "flag": ""
