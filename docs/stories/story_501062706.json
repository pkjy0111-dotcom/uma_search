    "25": {
      "frame_id": 25,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "友だちに、親戚の子に、えーとそれから、 近所のおじいちゃんおばあちゃんとか！ ベテランなのです！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
