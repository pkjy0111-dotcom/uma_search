      "frame_id": 27,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "と、あれこれ帽子を物色する マチカネタンホイザを微笑ましく 見守りつつ、2人で帽子を選んだのだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
