        {
          "next_frame": 27,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "さー、見ていきますよ！\r まずはこのハンチング帽から――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
