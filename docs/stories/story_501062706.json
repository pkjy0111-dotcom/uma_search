      "frame_id": 28,
      "background_id": 37,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "いえいえ～。さすがにもう 迷子にはなりませんし、帽子が 飛んでいくことだってないですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
