      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして彼女は、アメリカへ向かった。 一番指導のスタイルが合う トレーナーとともに――！",
          "flag_type": "",
          "flag": "",
          "choices": []
