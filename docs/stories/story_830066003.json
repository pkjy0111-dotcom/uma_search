      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "う、うんっ。幽霊なら中身は関係ないし、\r 衣装も簡単にできると思うのっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
