      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そこには、自身に影を差す偉大な姉の眩き\r 蹄跡をかき消し、メジロアルダンとして 今に光跡を残したいという強い想いがあった。",
          "flag_type": "",
          "flag": "",
          "choices": []
