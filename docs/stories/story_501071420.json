    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その一方で、メジロアルダンは、\r サクラチヨノオーとヤエノムテキの クラシック級での台頭を認識していた。",
          "flag_type": "",
          "flag": "",
          "choices": []
