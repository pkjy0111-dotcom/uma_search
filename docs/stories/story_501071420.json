      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『日本ダービー』での勝つ確率を少しでも\r 高めるべく、前哨戦に『青葉賞』を設定し、 改めて気を引き締め進み始めるのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
