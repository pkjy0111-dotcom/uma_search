      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "互いの意地が鍔迫り合う、デッドヒート。 ……あの決戦の日、 どんな想いでレースに臨みましたか？",
          "flag_type": "",
          "flag": "",
          "choices": []
