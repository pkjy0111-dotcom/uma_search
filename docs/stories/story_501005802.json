      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "……そっか、そうかもしれないね。\r 私の目に映るトレーナーさんは、 もっと素敵だからね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 33,
              "loop_ends_at_frame": -1,
              "text": "えっ！？",
              "flag_type": "",
              "flag": ""
