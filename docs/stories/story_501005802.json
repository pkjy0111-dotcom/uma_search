    "27": {
      "frame_id": 27,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "なるほど、トレーナーさんは1日中、\r 私のことを考えていてくれたわけだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
