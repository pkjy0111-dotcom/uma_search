      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "思わず固まり無言になるこちらを見て、\r 悪戯っぽく笑うフジキセキだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
