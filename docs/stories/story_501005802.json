      "frame_id": 28,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "ふふっ、嬉しいな。\r お互い、いい時間を過ごせたなら満足だよ。 ――それと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
