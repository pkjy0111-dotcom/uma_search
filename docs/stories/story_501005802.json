      "frame_id": 31,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "おや、そうかい？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "32": {
      "frame_id": 32,
      "background_id": 29,
