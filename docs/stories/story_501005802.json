      "frame_id": 29,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "私の目に映るトレーナーさんのことも……\r わかって、もらえたかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
