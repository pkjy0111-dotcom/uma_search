      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "（なるほど。さすが世界で戦ってきた相手、 集中力も加速力も段違いだ。……こりゃ普通は勝てねぇな）",
          "flag_type": "",
          "flag": "",
