      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90645,
          "speaker_name": "アナウンサー",
          "text": "さあ、両者飛び出した！　先日の雨によりぬかるんだ足元。 だが世界と戦った“怪鳥”エルコンドルパサーの敵ではない！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
