      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "は～い。マーちゃん、行ってきま～す。",
          "flag_type": "",
          "flag": "",
          "choices": []
