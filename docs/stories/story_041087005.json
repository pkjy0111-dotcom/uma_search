      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "世界にはたくさんの命があります。 みんなみんなその在り方は違うのです。",
