      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "はっ、はっ……はぁ……ふー……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "あと1周！",
