      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "今年からは、クラシックでの挑戦だもの。\r 初日から気を引き締めていかないと。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "対策も練らないとね",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "休憩も大事だよ",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "しっかりデータを取るよ",
