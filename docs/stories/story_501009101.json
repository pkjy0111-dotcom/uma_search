      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "そしたら走り込みの後は、\r 対戦相手の研究といきましょうか。",
