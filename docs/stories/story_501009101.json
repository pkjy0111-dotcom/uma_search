      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "お年賀。一応アンタ、アタシの\r トレーナーだし。お世話になってるし。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "律儀だな……！",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "律儀だね……！",
