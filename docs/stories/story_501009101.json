      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "まあ、これで礼儀は尽くしたってことで。\r 早速走り初めといきましょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
