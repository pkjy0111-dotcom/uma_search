      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "え、なに感動してるの。\r 別に普通のことでしょ……？",
          "flag_type": "",
          "flag": "",
