      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "…………あんた、まったく疑っていないみたいだな。 というより、その目――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "正直、少しワクワクしているかも",
              "flag_type": "",
              "flag": ""
            }
