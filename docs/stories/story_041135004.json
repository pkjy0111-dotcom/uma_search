      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "その感覚が好きでね。 ……だから私は、トレセン学園にいるんだ。",
