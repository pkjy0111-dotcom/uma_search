    "12": {
      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "ハハッ。やっぱあんた、変わってるよ。ま、だからこそか。 ……とりあえず、今話せるのはこんな感じだな。満足したかい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……君は、記憶を辿ってどうするつもりだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……貴方は、記憶を辿ってどうするつもり？",
