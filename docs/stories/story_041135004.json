      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "……だからレースを走るとな、ときどき鮮明に見えるんだよ。 記憶の景色が、熱く、眩しく――",
          "flag_type": "",
          "flag": "",
          "choices": []
