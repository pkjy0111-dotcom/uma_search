    "11": {
      "frame_id": 11,
      "background_id": 156,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1050,
          "speaker_name": "ナリタタイシン",
          "text": "あのさ、もうチケットのことは一旦忘れなよ。\r 天気のことなんだから気を揉んだってしょうがないじゃん。",
          "flag_type": "",
          "flag": "",
          "choices": []
