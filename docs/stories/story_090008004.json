      "background_id": 156,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……チケットが治ったとしても、\r 今日中に我々と合流するのは厳しいかもな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
