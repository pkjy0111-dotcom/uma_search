      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "安全も保証されない……か。だが危ないと言っても、\r 無理を押して来てしまいそうだ。私にできることは――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
