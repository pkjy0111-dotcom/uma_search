      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "だが、それではチケットが――",
          "flag_type": "",
          "flag": "",
