      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "……トレセン学園に入る前から、そうなんです。\r トレーニングする私のこと、いつもたくさん支えてくれて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
