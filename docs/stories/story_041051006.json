    "10": {
      "frame_id": 10,
      "background_id": 28,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "お父さんたちが、そろそろ新しいものを買いなさいって、\r 仕送り、いつもより多くくれて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
