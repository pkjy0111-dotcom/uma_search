      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "は、はいっ。えっと、トレーナーさんもお買い物ですか……？\r 私は、トレーニング用のシューズを買いに来ましたっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
