      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "勘違いとは、なんのことだろう。それも聞けないまま、\r ニシノフラワーの背中を見送ることになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
