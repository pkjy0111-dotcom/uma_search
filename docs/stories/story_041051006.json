      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1051,
          "speaker_name": "ニシノフラワー",
          "text": "……っ。お父さん、お母さん！\r 私……もっと走りたい！　たくさんレースに出たい！",
