      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "さあさあみなさんにも福をおすそ分け！\r どうぞ、試していってくださいな！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
