    "9": {
      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "そうですか！　でも大丈夫なんです！\r なにしろ福が来まくりですから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
