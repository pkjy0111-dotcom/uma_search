      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "触れるだけで気分がよくなる木彫りの熊ッ！\r 撫でるだけで空が飛べる能面ッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
