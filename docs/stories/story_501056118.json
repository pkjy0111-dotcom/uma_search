    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "だって、ずーっと勝利続きなんですよ！？\r 福、福、福、どっちを向いても福！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
