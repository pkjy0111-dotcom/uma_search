      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "――おやトレーナーさん！\r アナタもよければどうぞっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "試しに、空を飛べる能面を撫でてみた。\r ……が、何も起きない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "空、飛べないけど",
              "flag_type": "",
              "flag": ""
