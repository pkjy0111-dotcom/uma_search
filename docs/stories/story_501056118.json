      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……マチカネフクキタルは\r 大風呂敷を広げて、往来する ウマ娘たちに開運グッズを宣伝していた。",
          "flag_type": "",
          "flag": "",
          "choices": []
