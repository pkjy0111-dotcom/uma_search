    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "これはもう来てますよね！？\r 確実に来ちゃってますよねえぇ！！！？",
