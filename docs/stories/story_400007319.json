      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "え……？　柔道の……？",
          "flag_type": "",
          "flag": "",
          "choices": []
