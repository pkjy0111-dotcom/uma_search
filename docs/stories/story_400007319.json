      "background_id": 98,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "く、クリスエスさん、待ってぇ……っ！！",
