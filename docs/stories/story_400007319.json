      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "いーや？　これは『柔道』の練習だ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
