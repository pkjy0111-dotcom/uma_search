      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "（……柔道の『柔らかさ』を実践するには、 まず、普段の心構えが重要。つまり――）",
          "flag_type": "",
          "flag": "",
          "choices": []
