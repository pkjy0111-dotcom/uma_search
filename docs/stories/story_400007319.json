      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "――柔軟剤。 いつもより――多めに――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
