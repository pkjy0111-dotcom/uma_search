      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "というのも、『柔よく剛を制す』 という言葉が、日本にはありまして……。",
