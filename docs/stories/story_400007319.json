    "5": {
      "frame_id": 5,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "『柔』を養うとなれば、まずはダンスだろ！？ 全身をしなやかにするには、もってこいさ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "な、なるほど……？　ただ、たしかに 『柔らかい』『道』と書きますけど、 それはダンスの柔軟性とは別というか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "Oops!　だとしたら、クリスエスに 間違ったことを教えちまったかもな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
