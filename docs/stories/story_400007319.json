      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "い、いろいろとツッコミを入れて しまいましたが……たしかに、 柔道に柔らかさは欠かせません。",
          "flag_type": "",
          "flag": "",
          "choices": []
