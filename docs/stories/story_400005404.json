      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ところが、桐生院トレーナーの話を聞くと、 ハッピーミークの成長度合いが他のウマ娘と 比べても遅く、悩んでいる様子だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
