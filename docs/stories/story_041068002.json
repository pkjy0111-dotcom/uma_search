      "background_id": 33,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "えええっ、トラックが動かない～！？　積んでるのは……\r 救援物資ですか！？　なら早く届けなきゃ。",
          "flag_type": "",
          "flag": "",
          "choices": []
