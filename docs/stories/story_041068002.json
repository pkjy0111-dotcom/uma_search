      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "模擬レースで負けたキタサンブラックをスカウトした。\r が……返ってきたのは純粋な疑問だった。",
          "flag_type": "",
          "flag": "",
