      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "えっ、お相撲さん、足ひねっちゃったんですか？\r 肩貸すくらいじゃ歩けないですよね……。",
          "flag_type": "",
          "flag": "",
          "choices": []
