      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "契約は保留になったが、\r 後日、話を聞くことになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
