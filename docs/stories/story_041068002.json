      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "だったら、あたしが運びますっ！　大丈夫、\r たった20kmの往復なら！　そのくらいへっちゃらですっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
