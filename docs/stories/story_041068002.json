      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "その～、負けちゃったあたしの、\r どこを気に入ってくれたんですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……目つきと顔つきがよかったから",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "ええっ？　つまり見た目ですか～？　うーん、よく見えたんなら\r 嬉しいですけど、できれば走りとかの方が……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "とにかく、あたしにはまだスカウトされる資格なんて\r ありませんから。見習いみたいなもんですし……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "あ、でもトレーナーってことは専門家ですよね？　だったら\r どうか走りの相談に乗ってもらえませんか？　お願いしますっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
