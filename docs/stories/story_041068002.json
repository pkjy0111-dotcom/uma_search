      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "が……約束の時間になってもキタサンブラックは現れなかった。\r 律儀な子のはずだが、これは……。",
          "flag_type": "",
          "flag": "",
          "choices": []
