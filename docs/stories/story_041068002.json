      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "わかりました！　なら、あたしが背負いますっ。\r 遠慮せず、おぶさってください！",
          "flag_type": "",
          "flag": "",
          "choices": []
