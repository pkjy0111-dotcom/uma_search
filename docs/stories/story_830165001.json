      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "混乱の時代に、全てをかけて変革を求めた人、 散った人……主人公たちのような境遇の人は 本当にいた。だからこそ、感慨深かったかな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
