    "9": {
      "frame_id": 9,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "特に物語中盤の慟哭……あれはいい！ 絶望に打ちひしがれる様は涙を誘う！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
