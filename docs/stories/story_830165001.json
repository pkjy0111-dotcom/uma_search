    },
    "5": {
      "frame_id": 5,
      "background_id": 164,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "考えても解決しないなら、 1度違う世界に没頭してみてはいかが？ いい気分転換になると思うの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
