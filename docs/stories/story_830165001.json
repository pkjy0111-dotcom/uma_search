    "10": {
      "frame_id": 10,
      "background_id": 113,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "1度地につくほどの苦しみを味わうからこそ、 僅かな希望が光の道となり主人公を照らす…… 嗚呼！　今の俺に必要な伝言だったとも！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 113,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "うんうん！　あの展開は熱かったね！ 悲劇は時代設定とも噛み合っていたし、 とても練られた脚本だと思ったよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
