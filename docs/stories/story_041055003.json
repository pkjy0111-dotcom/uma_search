      "frame_id": 33,
      "background_id": 19,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 1055,
          "speaker_name": "マーベラスサンデー",
          "text": "トレーナーが手伝ってくれるの！？　でもでも、どうやって！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 34,
              "loop_ends_at_frame": -1,
              "text": "トレーニングの指導をやらせてほしいんだ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
