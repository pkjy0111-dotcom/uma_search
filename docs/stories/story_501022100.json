      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "全力で楽しもう。素晴らしい夢を見よう。 抱えきれないほどの思い出を――持ち帰ろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
