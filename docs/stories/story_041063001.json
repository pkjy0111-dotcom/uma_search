      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "新人トレーナーである自分にとっては、 未来の担当ウマ娘を決めるための、重要な情報収集の場だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
