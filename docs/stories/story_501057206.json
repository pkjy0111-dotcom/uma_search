      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90010,
          "speaker_name": "解説",
          "text": "安定感と強さがずば抜けていますね！ 先行して好位置を確保、道中息を入れて 最後にしっかりスパートして勝ちきる。",
          "flag_type": "",
          "flag": "",
          "choices": []
