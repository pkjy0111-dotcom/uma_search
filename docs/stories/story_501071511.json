      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "アタシはですね、えーと……。",
          "flag_type": "",
          "flag": "",
          "choices": []
