      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "あ、いやそのぉ…… 少女漫画好きだってバレたら恥ずかし――",
          "flag_type": "",
          "flag": "",
          "choices": []
