      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "まさしくその通りですね。 逆に、気持ちは通じ合っているのに 運命に翻弄されるというのも、また王道。",
          "flag_type": "",
          "flag": "",
          "choices": []
