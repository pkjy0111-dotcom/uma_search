      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "よしよしよし！　では実証といこう。\r ……委員長君、先日の教室爆破の犯人は私さ！",
          "flag_type": "",
          "flag": "",
          "choices": []
