      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "むにゃ？　……あれ、私はいったい何を？\r 夜のビーチはいずこ？？",
