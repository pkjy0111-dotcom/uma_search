      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "あの夏。 精神さえ膿みそうなほど蒸し暑かった、あの日。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
