      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "眠りを知らない、原初の本能と出会った……あの夜のことだ。 そう、あれは新人トレーナーとして外部業務を手伝った帰り道――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "（すっかり遅くなってしまった……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
