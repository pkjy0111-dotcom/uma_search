      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ふと、どこか足早になっている自分に気がつく。大人になっても、 人気のない夜というのは、どこか心細く、不気味で……恐ろしい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "（変だな、暑いはずなのに……――寒い）",
              "flag_type": "",
              "flag": ""
