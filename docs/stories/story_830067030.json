      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "カイチョーみたいになりたいって想いが、 ボクをここまで連れてきたんだよ！",
