      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90600,
          "speaker_name": "記者A",
          "text": "相変わらずの素晴らしいご活躍ですね！ それこそ“追いたい背中”などあるのか 疑問なほどの！",
          "flag_type": "",
          "flag": "",
          "choices": []
