      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "今後も、私自身が“追いたい背中”で あり続けられるよう、 粉骨砕身、励んでいく所存です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
