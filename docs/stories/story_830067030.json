      "frame_id": 9,
      "background_id": 130,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1003,
          "speaker_name": "トウカイテイオー",
          "text": "そんなの、シンボリルドルフ会長に 決まってるじゃん！　カイチョーは 1番すごくてカッコいいウマ娘だもん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
