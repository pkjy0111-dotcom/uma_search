      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして、彼女はティアラウマ娘たちの 活躍を振り返り、大切なことを思い出す。",
          "flag_type": "",
          "flag": "",
          "choices": []
