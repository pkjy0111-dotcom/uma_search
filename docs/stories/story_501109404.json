      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ラインクラフトは、あなたみたいになりたい、 と憧れの気持ちを伝える。しかし、それは 他人の真似でしかないことを指摘された。",
          "flag_type": "",
          "flag": "",
          "choices": []
