      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "憧れたのは、自分だけの輝きを求めて 駆けるティアラウマ娘たちの背中。 自分も誰かの憧れになりたいと彼女は願う。",
          "flag_type": "",
          "flag": "",
          "choices": []
