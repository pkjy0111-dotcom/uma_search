      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "その子は優しくて、ふわふわしてて、 ちょっと不思議な力があるんです。 だから神話とか、似合うかなって……。",
          "flag_type": "",
          "flag": "",
          "choices": []
