      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "おっと、驚かせてしまったかな、青薔薇の君。 お詫びと言ってはなんだが、 何か探しているのなら手伝わせてもらおう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
