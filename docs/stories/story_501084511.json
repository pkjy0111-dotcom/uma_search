      "frame_id": 8,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "え……そんな……！　いいんですか？ ライスなんかの、ために……。",
          "flag_type": "",
          "flag": "",
          "choices": []
