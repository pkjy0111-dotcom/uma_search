      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "絵本の主人公……あ、じゃ、じゃなくてっ！ えと……公園で会う猫さんに、 お名前をつけたくて……！",
          "flag_type": "",
          "flag": "",
