    "12": {
      "frame_id": 12,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "わぁ……今日もいっぱい囲まれてる。すごいな……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "シーザリオもエアグルーヴに憧れてるのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "シーザリオもエアグルーヴに憧れてるの？",
