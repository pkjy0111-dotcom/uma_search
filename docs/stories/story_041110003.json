      "frame_id": 10,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "うちも、最後の伸びがちょーっと足りなくて……。 ハナ差ですよ、ハナ差！　もう悔しくって！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
