      "frame_id": 20,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "たとえ1人の夢は小さくとも、その夢が寄り集まれば 大きな夢となります。それは……とっても素敵なことで！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
