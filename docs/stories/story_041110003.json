      "frame_id": 11,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "わかったわかった。お前たちの走りも見にいこう。 次に繋がるよう、それぞれ対策を立てるぞ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
