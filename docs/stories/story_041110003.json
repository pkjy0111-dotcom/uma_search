      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "母のようにはいかないかもしれんが、私もお前に示したい 背中がある。遠慮せず頼ってくれ、ドゥラ。",
          "flag_type": "",
          "flag": "",
          "choices": []
