      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "グル姉……先生のようにはいかないなど、決して。 貴方の背中は、いつだって大きい。昔と、変わらず。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
