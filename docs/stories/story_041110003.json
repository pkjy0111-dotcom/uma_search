      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "あ、先輩いた！　聞いてくださいよ～。アタシってば またスタートで出遅れちゃって……。ミスしなければ勝ってたのに！",
          "flag_type": "",
          "flag": "",
          "choices": []
