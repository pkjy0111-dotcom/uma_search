      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "私の育てた子がレースを彩り、彼女の後継者が更なる蹄跡を刻む――",
