      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "大外を走ったウマ娘と内側を走ったウマ娘…… ……走る距離は違うでしょう？",
