      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ダービーは芝・2400mのレースです。 そんな端数の数字になりません……よね？",
