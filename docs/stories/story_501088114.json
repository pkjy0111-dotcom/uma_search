      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "ふふっ。ダイヤが気づかせてくれたことよ。 同じレースでもウマ娘ごとに、 実質的に走る距離は異なる。",
          "flag_type": "",
