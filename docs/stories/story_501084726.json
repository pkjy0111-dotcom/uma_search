      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "クク……！　ハーッハッハッハッハーー！！ どちらにせよ、ここに眠るは、 それほどの財宝というわけだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
