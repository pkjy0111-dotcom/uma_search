      "background_id": 36,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "いや……危険を顧みず、 理想の乙女を、求める人形に恋した王か……？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
