      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "どうやら彼女はクレーンゲームに…… ひいては、ぬいぐるみに興味があるらしい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "俺もやってみようかな",
