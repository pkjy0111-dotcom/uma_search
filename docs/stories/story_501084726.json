      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "タニノギムレットが示すほうを見てみると、 ウマ娘たちをモチーフにしたぬいぐるみが 入った、クレーンゲームがあった。",
          "flag_type": "",
          "flag": "",
