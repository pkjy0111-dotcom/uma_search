      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "答えはいつもレースの中に落ちてる。\r そしてそいつは――",
