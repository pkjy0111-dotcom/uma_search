      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ククッ……許すのだな？　トレーナー。 俺のために――ステュクス、あるいは アケローンの渡し守となる自分を。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
