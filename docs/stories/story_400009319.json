      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう言って彼女が見せてくれたのは…… ボトルを自由にジャグリングして、 お酒を提供する、バーテンダーの動画だった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "す、すごい……こんな技術が……！",
