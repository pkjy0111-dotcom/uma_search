    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "すでに、仮初めの器での練習は盤石。 あとは透き通る本物でやり遂げるのみ。",
          "flag_type": "",
          "flag": "",
          "choices": []
