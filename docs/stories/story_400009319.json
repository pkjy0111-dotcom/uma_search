    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "故に、トレーナー！　俺もまた、 この深淵を覗き込むことにした！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ギムレットならきっとできるよ！",
