      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ハァーッハッハァ！！　オマエにもわかるか！ この技術が！！　年月を捧げた狂気が！！",
          "flag_type": "",
          "flag": "",
          "choices": []
