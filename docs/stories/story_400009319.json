      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "さあ、用意してくれ。 ヒュドラへと挑む権利を！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "挑戦当日。タニノギムレットの眼前には、 様々なガラス製のボトルが並んでいた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "今日はここ、好きに使っていいから！",
              "flag_type": "",
              "flag": ""
            }
