      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "そうだ、これこそが俺の求めていた刺激！！ 黄金郷に至るを記した、地図の一片だ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
