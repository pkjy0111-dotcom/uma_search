    "10": {
      "frame_id": 10,
      "background_id": 139,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "俺は今より、冥府に挑む。アイアコスの 鍵を取り上げるなら、今しかないぞ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "まさか！君の挑戦を止めたりはしないよ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "まさか！貴方の挑戦を止めたりはしないよ",
              "flag_type": "",
              "flag": "female_trainer"
