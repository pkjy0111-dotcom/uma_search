      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "……ふんふんふん、なるほど……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "どうでしょうか……",
              "flag_type": "",
              "flag": ""
            }
