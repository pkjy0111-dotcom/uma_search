      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "トレーナー、なんだか疲れてるね？",
          "flag_type": "",
          "flag": "",
          "choices": []
