    "10": {
      "frame_id": 10,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "あは、やっぱり問題あるみたい！　特に寝具周りかなー。 あ、もちろん不潔ってわけじゃないよ！　掃除はバッチリ☆",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "ただ、風水的にみると、もっとこうしたほうが 運気が上がるんだけどなーっていうのが、チラホラあるね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 21,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "具体的に言うと、枕カバーをタオルで代用しているところとか。 便利なのはわかるんだけど、仕事運が下がっちゃうの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
