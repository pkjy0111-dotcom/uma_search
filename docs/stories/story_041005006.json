      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "だけどね、彼女の恐ろしさはそれだけじゃなかった――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "彼女を恐れていたのか",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
