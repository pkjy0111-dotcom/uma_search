      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "今日は大切な舞台。\r 友人――いや、かつてのライバルが初主演を務めるのさ。",
