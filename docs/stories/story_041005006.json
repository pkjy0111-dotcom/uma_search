      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "いやはや、君にはつい本音が出てしまうな。\r お言葉に甘えて、実は行きたいところがあるんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
