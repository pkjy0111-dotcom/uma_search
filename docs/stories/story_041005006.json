      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "児童劇団からの幼なじみでね。\r いっつも、主役の座を競っていたんだ。",
