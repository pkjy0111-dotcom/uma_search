      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "今宵、トレーナーさんをお招きするのは\r こちら、王冠クラシックホテルでございます。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "国内屈指の高級ホテルじゃないか……！",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "国内屈指の高級ホテルじゃない……！",
