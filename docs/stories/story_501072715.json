      "frame_id": 10,
      "background_id": 2,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "トレーナー殿、私はこれからバンブーさんと\r 物の怪を追いかけ回して参ります。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
