      "frame_id": 11,
      "background_id": 2,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "よくわからない方向へ話が進んでいるが、\r とにかくヤエノムテキはこの後も 走る気まんまんらしい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "正体を突き止めてきて",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "ここで待っていれば、そのうち通るのでは？",
              "flag_type": "",
              "flag": ""
