      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ちなみに、アグネスタキオンというウマ娘の\r 他に、怪しい者は見つからなかったらしい。",
