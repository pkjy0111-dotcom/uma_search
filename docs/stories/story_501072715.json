      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "押忍、必ず勝って参ります！",
          "flag_type": "",
          "flag": "",
          "choices": []
