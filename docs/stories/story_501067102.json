      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "明けましておめでとうございます。\r コーチ、先生、お久しぶりです。",
