      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90103,
          "speaker_name": "ダイヤの元コーチ",
          "text": "実は今の教え子に、私はあのダイヤさんの\r コーチをしていたと自慢してるのよ！ すごいすごいと、感心の嵐なんだから！",
          "flag_type": "",
          "flag": "",
          "choices": []
