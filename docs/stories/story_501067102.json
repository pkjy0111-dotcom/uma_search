      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90103,
          "speaker_name": "ダイヤの元コーチ",
          "text": "ダイヤさん、明けましておめでとう！\r トゥインクル・シリーズで大活躍じゃない！",
