      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "よし。 じゃあちょっと、走ってきますね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "とはいえ、オーバーワークはダメだよ？",
              "flag_type": "",
              "flag": ""
