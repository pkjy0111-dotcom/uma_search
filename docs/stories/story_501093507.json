    "2": {
      "frame_id": 2,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "…………ふふっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "すごく嬉しそうだね",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "はい！　嬉しくて、 もう何回も読み返しちゃってます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……おれ、こういう手紙には、 昔から救われていて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "子どもの頃、ずっと病院だったから、 同じクラスのみんなが、手紙を書いて くれたことがあったんです――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 121,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "『早く元気になってね！』『絶対良くなる！』 『みんな待ってるから』『元気になったら、 一緒に遊ぼうね』って……ふふっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 121,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "手紙って、素敵ですよね。 飾りのないそのままの気持ちが伝わってくる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "ファンレターも、同じです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "レース終盤、息が上がって、もうダメだって なったとき――ファンの方が送ってくれた 手紙が、おれを前に走らせてくれるんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "『あなたの走る姿に勇気をもらいました』 『これからも頑張って！』『勝つところを 楽しみにしています』……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "そんな言葉が頭をよぎって、思うんです―― 勝って、みんなに報いなきゃって！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "だから……ふふっ。ファンのみんなは、 おれに救われたって言ってくれるけど―― 救われてるのは、おれです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……うん。もっと頑張ろう。 もらったぶん、ちゃんと恩返ししなきゃ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "俺も、一緒に頑張るよ！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "私も、一緒に頑張るよ！",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "はい、ありがとうございます！　……おれ、 いつかあなたにも、恩返ししてみせますから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
