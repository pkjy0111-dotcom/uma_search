      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1026,
          "speaker_name": "ミホノブルボン",
          "text": "体温の上昇、心身のやわらぎ……\r ステータス『リラックス』を確認。",
