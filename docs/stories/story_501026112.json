      "frame_id": 12,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "2人で頑張ってきたんだから。\r そう付け加えると……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
