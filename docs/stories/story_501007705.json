      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "エ・サ・ニ・ア・キ・タ……だと！？\r 一大事じゃねぇか、オイ！！",
