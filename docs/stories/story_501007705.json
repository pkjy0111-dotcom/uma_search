      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "見ろ！　蔦に浸食された電柱だ！\r がんばれ電柱！　負けんな電柱！",
          "flag_type": "",
          "flag": "",
          "choices": []
