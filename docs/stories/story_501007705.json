          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "だから、バスん中。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "バスの中！？",
