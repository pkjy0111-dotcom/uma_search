      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "おう！　バスにはこの世の全てが\r 詰まってんだよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
