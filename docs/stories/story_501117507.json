      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "…………ええ、そうですよね。 才能を見せつけられるのは……つらいっす。",
          "flag_type": "",
          "flag": "",
          "choices": []
