      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90220,
          "speaker_name": "学生服を着た少女",
          "text": "はい。ここ、私の通学路で、前からよく すれ違ってたんです。……ある時、私が 泣いてたら、シオンさんが声をかけてくれて。",
          "flag_type": "",
          "flag": "",
          "choices": []
