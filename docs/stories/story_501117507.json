      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "わあ、ありがとうございます！ ……へへ、元気になってなによりっす。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "もしかして、2人は知り合い？",
              "flag_type": "",
              "flag": ""
