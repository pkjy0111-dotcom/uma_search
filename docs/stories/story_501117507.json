      "frame_id": 9,
      "background_id": 33,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90220,
          "speaker_name": "学生服を着た少女",
          "text": "才能がなきゃダメなんだって思ったら…… 悲しくて……でも、諦めきれなくて……。 あの子さえいなければなんて、考えて……！",
          "flag_type": "",
          "flag": "",
          "choices": []
