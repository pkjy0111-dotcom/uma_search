    "6": {
      "frame_id": 6,
      "background_id": 33,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1117,
          "speaker_name": "ウインバリアシオン",
          "text": "泣き顔が……とても、苦しそうで……。 つい、声をかけたんですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
