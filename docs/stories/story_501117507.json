    "8": {
      "frame_id": 8,
      "background_id": 33,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90220,
          "speaker_name": "学生服を着た少女",
          "text": "夜遅くまで自主トレして、 先輩にも教えてもらったのに……！ 才能がある同級生に敵わなかった！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
