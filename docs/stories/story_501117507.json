      "frame_id": 7,
      "background_id": 33,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90220,
          "speaker_name": "学生服を着た少女",
          "text": "うう、ひっく……今度の、部活の大会…… 絶対に、レギュラーになりたかったのに。 ベンチになっちゃったんです……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
