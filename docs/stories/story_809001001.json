      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "って、トレーナーさん！？\r あらら、お見苦しいところを……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "理事長室までひとっ走りしますよ！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "書類作成、お手伝いしますよ",
