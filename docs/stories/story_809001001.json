      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "あぁっ、秋川理事長の猫ちゃんにおやつ！\r でも今から理事長室に向かっていたら……。",
          "flag_type": "",
          "flag": "",
          "choices": []
