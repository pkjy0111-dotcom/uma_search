          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "じ、時間が足りない～……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "大丈夫ですか……？",
