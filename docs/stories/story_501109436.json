      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな時、彼女は意外な人物を見かける。 小さな頃にレースで観たオークスウマ娘と、 その赤ちゃんだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
