      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "しかしある日、彼女は食事中にいつの間にか 眠ってしまっていた。未来のシーザリオの 夢を見ながら。最近こういうことが多い……。",
          "flag_type": "",
          "flag": "",
          "choices": []
