    },
    "11": {
      "frame_id": 11,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "どうしたら、キミの美しさを出せるかな？\r 青い空と、白い花弁のコントラスト…… 本当にひと目惚れだったんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
