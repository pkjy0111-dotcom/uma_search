      "frame_id": 10,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "花は選べたし、あとは……対話、だよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
