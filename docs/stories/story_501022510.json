      "frame_id": 12,
      "background_id": 6,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "それでね、日本で会えた時は感動しちゃって！ 何度も桜のトンネルをくぐって、 花びらのシャワーを浴びたなぁ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
