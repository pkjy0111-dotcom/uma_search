      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "はーい♪\r ジャパニーズフラワーアレンジメント、 楽しみにしていたよ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "よろしくお願いします",
              "flag_type": "",
              "flag": ""
