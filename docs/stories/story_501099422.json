      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "日を跨いでも続く議論でホッコータルマエは、 自身がダートウマ娘として、あるいは アイドルとしてのみ見られていたと知り――",
          "flag_type": "",
          "flag": "",
          "choices": []
