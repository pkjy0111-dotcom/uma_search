      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "そして貴様は、そんなシーザリオに首ったけだと。 どうりで必死なわけだな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "……それで、どう思った？",
              "flag_type": "",
              "flag": ""
