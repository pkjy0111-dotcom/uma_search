      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "木陰のように、涼しく……\r 闇夜のように、なめらか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
