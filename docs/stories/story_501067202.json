        {
          "next_frame": 9,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "お話の途中に失礼いたしました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "ご両親から応援の電話？",
