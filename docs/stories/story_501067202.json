      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "そうですね。お父さまもお母さまも、 私に『勝て』と命じたことはございません。 親として優しく接してくれます。",
          "flag_type": "",
          "flag": "",
          "choices": []
