      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "レース前はいつも、私より 周りのみなさまの方が落ち着きませんね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
