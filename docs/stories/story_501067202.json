      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ええ。……うふふっ！ お父さまも、トレーナーさんと同じように 忘れ物確認をなさるんですもの。",
          "flag_type": "",
          "flag": "",
          "choices": []
