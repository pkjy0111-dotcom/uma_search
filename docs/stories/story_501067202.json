      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "だからこそ……私は明日のレースに勝ちたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
