      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "……私が『必ず勝利を届けます』と伝えても、 ケガのないように走ればよいと仰いました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "それも本心だと思うよ",
              "flag_type": "",
              "flag": ""
