      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "ふぅ～、気持ちいいですね！\r 足の裏がちょびっと砂でつぶつぶしますけど。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "そろそろ休憩にしようか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "そろそろ休憩にする？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "いえ、まだ大丈夫です～。今、ノリに乗ってる感じなので！\r やっぱりそばに海があるからですかね。へへへ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "あ、それに今日の夜は夏祭りですしっ。トレーニングは\r 今のうちにやっておくべし！　ちゃちゃちゃっと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "マチカネタンホイザとの夏合宿は順調だ。\r みっちりトレーニングをしつつ、適度に夏を楽しんでいる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "海の家でかき氷を食べたり、ビーチで砂の城を建ててみたり――\r 今夜の祭りもそのうちの1つだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "いい汗かいたー！　このままざぶんと海に飛びこみたいですが～、\r それはまた後でっ！　もう1本行ってきまーす！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーニングが終わり、マチカネタンホイザと別れて\r 合宿所へ帰ってきた。祭りが始まるまではしばらく時間がある。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "（仮眠でもしようかな）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "幸い、合宿所のロビーには誰もいない。\r これといった予定もないので、ソファーで少し眠ることにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "ぷはぁ～、ざぶざぶしたーっ！\r 本当に海は最高だねえ♪　きれいだし、気持ちいいし～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "ふふん、あともうちょっと泳いで――あ、じゃなかった！\r お祭り行く準備しなくちゃなんだった。やばしやばし！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
