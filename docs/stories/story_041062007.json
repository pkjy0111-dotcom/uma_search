      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "……んむむ？　もしかして寝てる？\r なんだろ、どしたのかな？　お昼寝かな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
