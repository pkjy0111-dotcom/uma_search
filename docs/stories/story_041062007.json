      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "これですか？　これはみんなから集めた元気の源たちです。\r トレーナーを元気にしようと思って！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 32,
              "loop_ends_at_frame": -1,
              "text": "どういうこと？",
              "flag_type": "",
              "flag": ""
            }
          ]
