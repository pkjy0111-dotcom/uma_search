      "dialogue": [
        {
          "next_frame": 38,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "トレーナーはよく私に元気だって言ってくれてましたけど、\r それはみんなからたくさんの応援や気持ちをもらってたからで――",
          "flag_type": "",
          "flag": "",
          "choices": []
