    },
    "32": {
      "frame_id": 32,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "えっと……トレーナー、寝ちゃってたじゃないですか？\r それを見たら心配になっちゃって。元気出してほしいなって思って。",
          "flag_type": "",
          "flag": "",
          "choices": []
