      "frame_id": 6,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "とびきり目を引く広告だもの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "そう言ってくれるな。 結果に基づいての人選に変わりはない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
