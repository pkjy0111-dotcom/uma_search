      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "覚えているか？　子どもの頃の、 全国レース大会『リトルスターカップ』…… 私たちは別のコースで走っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
