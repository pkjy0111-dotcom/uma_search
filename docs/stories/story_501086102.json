      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "それにしても……君と並んで表彰されるとは、 懐かしい気分だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
