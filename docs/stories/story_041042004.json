      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "あのおばあさんだけじゃないわ。オイルマネーで造られた 摩天楼群の裏街では、たくさんの人々があえいでいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
