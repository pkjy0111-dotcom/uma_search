      "background_id": 60,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "そう、この世界は――ハンデ戦よ。 持てるものが勝ち、持っていないものが負ける残酷なレース。",
          "flag_type": "",
          "flag": "",
          "choices": []
