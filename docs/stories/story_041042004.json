    "15": {
      "frame_id": 15,
      "background_id": 158,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90715,
          "speaker_name": "ガイドのおばあさん",
          "text": "私らは夢なんか見たって叶うわけがない。だからチャンスのある お嬢ちゃんには頑張ってほしいのさ。私たちの分もね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
