      "background_id": 113,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "あるいは産業のない国に生まれ、せっかく才能に恵まれても 活かすチャンスすら与えられない人もいたわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
