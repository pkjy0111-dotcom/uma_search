      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "レースではさまざまな不利に見舞われることがあるわ。 たとえば――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
