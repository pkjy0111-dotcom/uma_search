      "frame_id": 22,
      "background_id": 60,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "そしてたとえ持っている者でも、より持っている相手には負ける。 でも生まれ持ったもので決まるなんて全然ファビュラスじゃないわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
