    "16": {
      "frame_id": 16,
      "background_id": 60,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "託されはしたけれど……私が抱いた目標は違った。 活躍して、豪邸に住むことじゃない。",
