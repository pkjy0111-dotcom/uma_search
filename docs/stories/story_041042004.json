      "frame_id": 12,
      "background_id": 60,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "でも、あの話には――続きがあるの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 158,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "幼いパール",
          "text": "ねえ、おばあちゃんは今、こんなお家に住んでいるの？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 158,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90715,
          "speaker_name": "ガイドのおばあさん",
          "text": "まさか……。私は裏街の小さな借部屋住まいさ。 観光地なんていうのは、そんなものだよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
