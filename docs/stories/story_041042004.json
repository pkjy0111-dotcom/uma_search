      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "この目で見てきた、世界中の夢を見られない人々の “絶望を取り払う”ことよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
