    "11": {
      "frame_id": 11,
      "background_id": 158,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90715,
          "speaker_name": "ガイドのおばあさん",
          "text": "将来は頑張って、こういう場所に住めるようになるんだよ？ お嬢ちゃんなら、きっとできるさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
