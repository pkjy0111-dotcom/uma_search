          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "何かいいことあったのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "何かいいことあった？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
