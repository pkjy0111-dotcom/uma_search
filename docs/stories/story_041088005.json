    },
    "14": {
      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "エッグタルトの出店をやると聞いて、先んじて食品関係の業者を いくつかピックアップしておいたと伝えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "Great!　ありがとう、助かるわ……！ おかげでかなり時短できた。さっそく交渉を――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "トレーニングプランも調整したほうがいいな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "トレーニングプランも調整したほうがいいね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "16": {
      "frame_id": 16,
