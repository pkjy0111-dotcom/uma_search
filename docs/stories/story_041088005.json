    "2": {
      "frame_id": 2,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "ほら、そろそろオープンキャンパスがあるでしょう？ うちのクラス、出店をやる予定なのよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "それで今日は、クラスで何のお店にするか決める会議があるの！ 私はエッグタルトのお店を提案するつもりよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "みんなの考え抜いた案を聞きつつ、私の案への反応ももらえる！ とても建設的な催しだもの！　うきうきだってしちゃうわっ♪",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "そっか、応援してるよ！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "いい報告ができるよう、力を尽くすつもりよ！ ま、期待して待っててね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "（俺も、できることを考えておくか）",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "（私も、できることを考えておこうかな）",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "それじゃあ……あなたは学内外問わず、エッグタルトの宣伝を お願いしてもいいかしら？　できる範囲で構わないから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "はーい。とりあえずウマスタとかで宣伝しとくねー。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "あなたには……チラシのデザインをお願いしてもいい？ お得意のポップなデザインで、注目を集めてほしいのよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "それはお安いご用だけどさ～、 どっちかって言ったら、看板とかのほうが得意だよ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 6,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "チラシは印刷に時間がかかるから、早めに作っておきたいのよ。 看板もあとでお願いするつもり！　アイディア練っておいてねっ♪",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "……予算に関しては、他クラスも出店をすると言っていたし……。 大量発注で発送をまとめてもらえれば、抑えられる……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "あとは、業者を探すだけ……とはいえ、食品関係の 伝手はないから……いちから探すしかないか――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "業者、いくつか探しておいたよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "え、トレーナー……！？　真係！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
