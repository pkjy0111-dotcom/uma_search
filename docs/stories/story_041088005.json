      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1088,
          "speaker_name": "サトノクラウン",
          "text": "あ……そうね！　しばらくは変則的になると思うから……。 いつもより1時間くらい余裕をもって組んでもらえると助かるかも！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
