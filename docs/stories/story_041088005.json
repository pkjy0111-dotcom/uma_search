      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "メニュー完成～！　あとは看板かぁ～。 暗くなる前には、終わらせたいね～！",
