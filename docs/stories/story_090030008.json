      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "（台本のままは難しくて、ちょっと変えちゃったけど…… わたしらしくていいねって言ってもらえた演技！）",
          "flag_type": "",
          "flag": "",
          "choices": []
