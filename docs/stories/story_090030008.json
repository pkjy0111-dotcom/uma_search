      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "『わははっ、楽しや、楽しや、なほ舞うは楽しきことかな！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
