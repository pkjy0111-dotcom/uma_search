      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "おお。紹介されている本を読めば、 “流行”になりそうな理論をいち早く チェックできるわけですね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
