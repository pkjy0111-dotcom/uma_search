      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "フリー誌『読書家がすすめる楽しい本♪』が ついに完成しました……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
