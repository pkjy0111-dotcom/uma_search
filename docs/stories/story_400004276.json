      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "次は私です。今回はテーマを“夢”に 設定させていただきまして……。",
