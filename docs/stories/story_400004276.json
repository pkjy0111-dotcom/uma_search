      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "こうして1冊の本になったのは、 お2人の協力のおかげです……！ 図書委員として、こ、心より感謝いたします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
