      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "読書の習慣が無い方に本を薦めるため、 苦労に苦労を重ねた1冊……！",
          "flag_type": "",
          "flag": "",
          "choices": []
