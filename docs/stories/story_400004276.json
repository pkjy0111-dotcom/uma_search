    "7": {
      "frame_id": 7,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "さ、早速中身をチェックしてみましょう。 まずはハヤヒデさんの担当ページから……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "私が掲げたテーマは“流行”。 イマドキの学生が読みたくなる本の紹介だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "なにせ学会で発表されたばかりの、 最先端レース理論が掲載されている 書籍ばかりだからな！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
