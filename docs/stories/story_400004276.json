      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "“流行”に敏感なイマドキの方たちにも 興味を持っていただけると思います……！",
