      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "“夢”への道標となるマネジメント術。 それを身につけられる数々の本を、 コラム形式で紹介してみました。",
          "flag_type": "",
          "flag": "",
          "choices": []
