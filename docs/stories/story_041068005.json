      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "相変わらず、当然のように人助けをしているキタサンブラック。\r その上で、トレーニングも問題なくこなしている。",
          "flag_type": "",
          "flag": "",
          "choices": []
