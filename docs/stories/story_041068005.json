    "6": {
      "frame_id": 6,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "そっかぁ！　やっぱり軸が大事なんですね。\r 軸脚と上半身のバランスを意識して、重心を……ふむふむ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "あの、あと少しだけ走ってきてもいいですか？\r トレーナーさんに教わったこと、早く実践してみたくてっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "じゃあ、あと少しだけ",
              "flag_type": "",
              "flag": ""
            }
