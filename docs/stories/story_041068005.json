      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "走り始めたキタサンブラックの横顔は、\r 美しく精悍で、目を奪われるものだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
