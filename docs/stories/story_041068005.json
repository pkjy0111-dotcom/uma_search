    "5": {
      "frame_id": 5,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "あっ、そうだ！　トレーナーさんに聞いておきたいことが\r あって……踏み込みの時にですね、えっと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
