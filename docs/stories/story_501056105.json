      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "夏来たるー、岩をも砕く、セミの声ー。\r いやあ、風流でいいですねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "なんか余裕だな",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "なんか余裕だね",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
