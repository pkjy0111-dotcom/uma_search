      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『デビューした喜びが、この先の 競技人生でも永遠に続きますように』 ……そんな想いを込めてくれたようです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
