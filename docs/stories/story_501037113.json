      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "箱を開けた途端、ほんのり香るスパイス…… すぐに母の手作りだとわかりました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "手作り！？",
              "flag_type": "",
              "flag": ""
