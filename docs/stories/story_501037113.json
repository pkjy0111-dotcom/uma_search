      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "はい。母は以前からリース作りを 趣味にしているんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "趣味であんなにすごいものを……？",
              "flag_type": "",
              "flag": ""
