          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "君の爪のケア、手伝わせてくれないか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "貴方の爪のケア、手伝わせてくれない？",
