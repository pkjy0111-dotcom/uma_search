      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "あれっ、トレーナーさん！？ どうしたんスか、また目が覚めちゃったんスか？",
