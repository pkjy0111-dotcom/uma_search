      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "言葉の端々や態度から、 こちらへの気遣いがうかがえる。 どう返答すべきか――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "確かに、コンディションは大切ですね！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 23,
              "loop_ends_at_frame": -1,
              "text": "どうせなら、栄養をしっかり摂りたいです",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 47,
              "loop_ends_at_frame": -1,
              "text": "やはり今は仕事を優先したいので……",
              "flag_type": "",
              "flag": ""
