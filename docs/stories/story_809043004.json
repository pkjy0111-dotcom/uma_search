      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "だろう？　近場にいい店があるんだ。 ついてきてくれ。",
