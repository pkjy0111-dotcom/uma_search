      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "光明が見え、はやる気持ちは理解できるが、 体調を崩しては元も子もないだろ？",
          "flag_type": "",
          "flag": "",
          "choices": []
