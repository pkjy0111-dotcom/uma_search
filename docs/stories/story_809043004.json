      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "熱心なのは結構だが、区切りがつくまで ずっと軽い食事で済ませる気かい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "それは……",
