      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "こんな時はランチだよ、君！ 美味いご飯でコンディションを回復だ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
