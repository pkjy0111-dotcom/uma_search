      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "『校門前の元気な挨拶を聞いていると、 活力が湧いてきます！ これからも応援しています！』",
          "flag_type": "",
          "flag": "",
          "choices": []
