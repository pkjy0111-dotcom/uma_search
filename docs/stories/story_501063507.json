          "speaker_name": "",
          "text": "図書室に行くと、 悩み顔のイクノディクタスがいた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "何か困りごと？",
              "flag_type": "",
              "flag": ""
            }
