      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "はぁ……せっかく相談いただいたのに、 不正確な内容を伝えるわけには――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "イクノが思う回答で、いいんじゃないかな",
