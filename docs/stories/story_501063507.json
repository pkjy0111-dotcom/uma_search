      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ただ、幼児の心理状態はわかりません。 こうして調べても、一向に……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
