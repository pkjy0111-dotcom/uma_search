    "2": {
      "frame_id": 2,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "難しいものですね。 ――子育てというのは。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "子育て！？なんで！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ああ。ファンレターでいただいた 相談への返事を考えていまして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "前に、ファンレターのちょっとした質問に 答えてみたのです。 そしてそれを続けているうちに――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "お悩み相談レターっぽくなっている、と",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "いつも応援してくださる皆さんの お力になれるのは、私も嬉しいですから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ただ…… 『駆けっこのコツ』や『仲直りの方法』 ならまだしも――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "『小さな子が生まれて、 上の子が言うことを聞いてくれなくなった』 といった、子育ての話となると……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "赤ん坊と他の兄弟を平等に扱うのは、困難。 私も大家族ですからわかります。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
