      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その後、雨の中をのびのびと走る ミスターシービーを見守りながら、 彼女がケガをしないようにサポートした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
