      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "ふふふっ！ ありがとう、トレーナー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
