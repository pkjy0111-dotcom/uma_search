      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "どうして？ アタシの心は走りたがってるのに。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "君の自由を守るためだよ",
              "flag_type": "",
              "flag": "male_trainer"
