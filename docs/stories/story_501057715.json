        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーニングに励んでいると、 ポツポツと小雨が降り始めた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "シービー、そろそろ上がろう！",
              "flag_type": "",
              "flag": ""
            }
