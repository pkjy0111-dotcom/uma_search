      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "私は漫画本を。休日ですので、 自室でゆっくり読もうかと思いまして。",
