      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "……トレーナーさんでも、急には難しいですよね。 お渡ししておきますので、ご検討お願いします。私はこれで……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "あ！ごめん、最後に1つ……！",
              "flag_type": "",
              "flag": ""
