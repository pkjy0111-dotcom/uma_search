    "12": {
      "frame_id": 12,
      "background_id": 223,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "今日は1つ、絶対に確認したいことがあった。それは、 トレーナーとウマ娘が互いに知っておくべきもの――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "君の夢を聞かせてほしい",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "貴方の夢を聞かせてほしい",
