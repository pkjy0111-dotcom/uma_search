    "14": {
      "frame_id": 14,
      "background_id": 273,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "それを見て、私も両親のような 大人になりたいと思ったのです。",
