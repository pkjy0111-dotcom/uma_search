      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "ウマ娘として生まれたからには、 世界一の速さで駆けたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
