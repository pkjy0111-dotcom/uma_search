    "16": {
      "frame_id": 16,
      "background_id": 273,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "楽しそうに働いている姿です。 父は検察官、母は研究者です。 2人とも目を輝かせて仕事をしています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 273,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "父は言い逃れできない証拠を持って 裁判所へ向かう時。母は研究予算を 勝ち取った時が、特に生き生きしていますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
