      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90004,
          "speaker_name": "ナレーション",
          "text": "これは、ひたすらに速さだけを極め続ける、 信念のウマ娘の物語である。",
