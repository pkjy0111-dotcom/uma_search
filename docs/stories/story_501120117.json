      "background_id": 23,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90004,
          "speaker_name": "ナレーション",
          "text": "主軸となるのはレース。ライブのセンターは、 レースの勝者だけに立つことが許される。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
