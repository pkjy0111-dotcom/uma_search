      "frame_id": 18,
      "background_id": 273,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90004,
          "speaker_name": "ナレーション",
          "text": "父の、ためらわず真実を追究し正義を貫く姿。 母の、未知の解明というロマンに挑む姿。 そんな両親のようになりたいと彼女は思った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
      "background_id": 273,
