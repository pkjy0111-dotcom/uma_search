      "background_id": 273,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "両親です。これと決めた1つを どこまでも追求し、貫いていました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
