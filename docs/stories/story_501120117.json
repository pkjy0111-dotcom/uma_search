      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "　　　　　　　最高速への挑戦  　　　～ウマ娘スプリント超決戦・前夜～",
