      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 9046,
          "speaker_name": "セントライト",
          "text": "おかげで、取り戻した――とは言いませんが。 愛し子たちと並べる、わたくしになれました。",
          "flag_type": "",
          "flag": "",
          "choices": []
