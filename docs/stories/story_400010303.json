      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "コースに向かうため、地下バ道を通る。 ふと、抱いていた疑問が口をついて出た。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "……どうして、あんな“王命”を？",
