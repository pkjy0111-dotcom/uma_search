      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "今日はレジェンドたちと共に、 東京レース場に来ていた。というのも――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
