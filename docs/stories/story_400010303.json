      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "王命である。 セントライトらの復帰を助力せよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "数か月前。『Dream Fest』への参戦を レジェンドたちが決めてすぐ、 そんな指示を彼女から受けたからだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "温い。なんだこの、 腑抜けたトレーニングメニューは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "彼奴等への顧慮をやめろ。 ――疾く、修正せよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "……わかった！もう少し、考えてみる！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "……わかった！もう少し、考えてみるね！",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "先頃、彼奴等の記録が 伸び悩んでいると聞いた。訳を申せ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "みんな萎縮して、併走してくれなくて……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1115,
          "speaker_name": "オルフェーヴル",
          "text": "……ふむ。 であれば、余が相手をする。用意せよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "え……オルフェ自ら！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "どうやら、今回の“王命”はいつにも増して 本気らしい。そうして、彼女と共に、 レジェンドたちの復帰に尽力した結果――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 23,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 9046,
          "speaker_name": "セントライト",
          "text": "オルフェーヴル。トレーナー。 度重なるご助力、ありがとうございました。",
