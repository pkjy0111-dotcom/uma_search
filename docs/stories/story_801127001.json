      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ああ……！ その組み合わせは……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "目を輝かせ、近づいてきたのは 風紀委員のフェノーメノだ。 きびきびと歩くさまは、警官を彷彿とさせた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "トレーナーさん！ もしや、これから張り込みですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "本官もであります！　敷地内の植え込みを 無断でにんじん型に剪定するという 愚行を犯したホシを追っております！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "……ふっ、やはり張り込みは、 あんぱんと牛乳に限りますね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "刑事ドラマの金字塔、『満月に叫べ！』の 張り込みシーンでは いつも胸を高鳴らせておりました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ホシがいつ現れるかわからない、 ハラハラとした緊張感、 現れてからの、手に汗握る逮捕劇――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "それは目が離せなそうだ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "それは目が離せなそうだね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "そうでしょう、そうでしょう！ 幼い頃から何度も繰り返し見ては、 本官も彼らのようにと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "――ああっ……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "見つけたぞ、犯人！！ 確゛保゛お゛お゛お゛お゛お゛お゛っ！！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "怒濤の勢いで疾走していくフェノーメノ。 その姿は犯人を追い詰める警官というより――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "（悪を許さない“怪物”――みたいな……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
