      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "じゃあね～～……今日は高鬼にしようっ！\r いくよー！　ぱっぱか、たったか、じゃんけんぴょい！",
          "flag_type": "",
          "flag": "",
