      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "あ、私が鬼だ。よーし、みんな捕まえちゃうぞー！\r 逃げろ逃げろー！",
