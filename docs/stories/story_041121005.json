      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "（ふふ……であれば示さなければいけませんね。 より輝きを増した私の力――『聖剣』の一閃を！）",
          "flag_type": "",
          "flag": "",
