      "frame_id": 17,
      "background_id": 1,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "（何だろう……今日はよりトレーナー殿の視線が熱いような？ 私の騎士っぷりに磨きがかかったのかしら？）",
          "flag_type": "",
          "flag": "",
          "choices": []
