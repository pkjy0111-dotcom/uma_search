    "16": {
      "frame_id": 16,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90415,
          "speaker_name": "先輩トレーナー",
          "text": "トレーナーとは、ウマ娘たちの夢の支柱だ。 “頼るべき君主”である自分を、忘れないようにしていかないとね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "……はいっ！",
              "flag_type": "",
              "flag": ""
