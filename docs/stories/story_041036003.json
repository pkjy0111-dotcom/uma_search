      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（……最高の気分だった。うるさいヤツらも全員、勝者には口を 閉ざし、ノイズは消えた。なら、もっと上がっていけば……）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
