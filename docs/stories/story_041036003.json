    "6": {
      "frame_id": 6,
      "background_id": 194,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（聞こえるのは、ノイズばかり……同じ言葉を使っているのに、 誰とも話が通じなかった。そのうち、オレは対話を諦めた）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 194,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（そンな時……PCと出会った。初めて、声が聞こえた気がした。 プログラミングこそが、意思疎通可能な唯一の言語だったンだ）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（相棒がいれば、ノイズを搔き消せる。そして最も効果的に、 自分を証明できる場所として――オレはトレセン学園に目をつけた）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（入学してすぐ完成させた『Parcae』は、最高の相棒になった。 データを食わせるほどに、ヤツの導く答えの精度は高まり――）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90415,
          "speaker_name": "モノローグ",
          "text": "『――エアシャカール、一体何を考えて走ってるんだ……。 教官の話もまともに聞かない、協調性もない、独善的……でも』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90103,
          "speaker_name": "モノローグ",
          "text": "『シャカール先輩、一緒に走りたくないんだよねぇ……。 ちょっと違う世界の人みたいで、わかんなくて怖い……でも』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
