      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 93998,
          "speaker_name": "モノローグ",
          "text": "『――“勝つ”から、やっぱすごい……』",
          "flag_type": "",
          "flag": "",
          "choices": []
