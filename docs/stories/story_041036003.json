      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "（――もっともっと、勝っていけば。そう、例えば……本物の強者 しか手にできない称号を……三冠を、この頭に戴けば……！）",
          "flag_type": "",
          "flag": "",
          "choices": []
