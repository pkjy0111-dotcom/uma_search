      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "アタシは今年、自分の得意な距離で 最強サイコーなウマ娘になることを 目指してますよね。だけどそれって――",
          "flag_type": "",
          "flag": "",
