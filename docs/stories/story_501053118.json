      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "トレーナーさん、何か思いつきませんか！？ 『これこそ最強サイコーの証明！！』 って感じのヤツ……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
