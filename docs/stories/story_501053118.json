      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "ただGⅠに挑戦して、勝って、 『強いッ！！』って言われるだけじゃあ、 なんとなく足りないと思うんスよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
