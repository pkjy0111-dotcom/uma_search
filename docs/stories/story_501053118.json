      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "なんというかこう、目に見えてわかりやすい 形というか、証明みたいなものが あるんじゃあないかって……！",
          "flag_type": "",
          "flag": "",
          "choices": []
