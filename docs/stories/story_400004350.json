    "5": {
      "frame_id": 5,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "うーん、慈善事業？\r でも、王族がそんなメールを送るかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
