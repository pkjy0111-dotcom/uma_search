      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "ええ～、今朝方わたくし宛に届きまして、\r 『ある国の王子をしています』と。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
