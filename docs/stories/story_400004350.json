      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "はい、見ず知らずの方からお金を受け取る\r わけにはいかないので、『結構です』と。",
          "flag_type": "",
          "flag": "",
          "choices": []
