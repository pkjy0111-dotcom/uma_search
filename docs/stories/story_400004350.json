    "4": {
      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "なんでも、お金がたくさん余っていて\r 有効に使ってくれる人を探しているとか～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
