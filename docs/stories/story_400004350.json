      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "それで、返信はしたの？",
          "flag_type": "",
          "flag": "",
          "choices": []
