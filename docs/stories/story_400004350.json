      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "するとまたメールが届きまして、\r 王子が事故を起こしてしまったと……。",
