        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『ジャパンカップ』に集った 綺羅星のような同期たちに、世界の強豪。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
