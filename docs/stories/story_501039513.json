      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "プリファイも姫として参加した“舞闘会”！\r 今からテンションがぶち上がってしまって！",
