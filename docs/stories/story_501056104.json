      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "長距離不向きにくわえて、私の才能のなさ！\r これだけそろえば、鬼に金棒の弱さです！",
          "flag_type": "",
          "flag": "",
          "choices": []
