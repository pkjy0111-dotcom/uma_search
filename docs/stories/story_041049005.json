      "frame_id": 6,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90106,
          "speaker_name": "ナカヤマの同級生B",
          "text": "あ、はい……ランニング中の友だちが見たらしいんですけど、 河川敷で、何か警察の人と言い争ってたって……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 119,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90107,
          "speaker_name": "ガラの悪そうなウマ娘",
          "text": "はあ……知るわけないっしょ。あいつがどこいるかなんて。 ったく、どいつもこいつもナカヤマナカヤマってさあ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "え、俺以外に誰か彼女を……？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "え、私以外に誰か彼女を……？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 119,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90722,
          "speaker_name": "ガラの悪そうな男子",
          "text": "この前もあいつのこと捜してるって奴が来てよお。 は、どうせあんたも、ナカヤマに一杯食わされたクチだろ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "！？その話、詳しく教えてくれ……！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "！？その話、詳しく教えて……！",
              "flag_type": "",
