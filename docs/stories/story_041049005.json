      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90105,
          "speaker_name": "ナカヤマの同級生A",
          "text": "ナカヤマさんの居場所？　さあ……あの子と全然交流なくて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
