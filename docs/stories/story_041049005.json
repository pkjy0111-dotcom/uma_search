      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90565,
          "speaker_name": "担任の教師",
          "text": "そうですか……ナカヤマさん、ここにも来ていないんですね。 お仕事中お騒がせしてすみません。",
          "flag_type": "",
          "flag": "",
          "choices": []
