      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90107,
          "speaker_name": "ガラの悪そうなウマ娘",
          "text": "ポーカーにイカサマ仕込んで、バカ勝ちしたって話。 で、ダマされた奴らに追われてるんだってさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 119,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90107,
          "speaker_name": "ガラの悪そうなウマ娘",
          "text": "連中、目の色変えてそこら中捜し回ってるぜ。 あーあー、どうなっちまうのかねえ、あいつ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "（おかしい。そんなはずは……）",
