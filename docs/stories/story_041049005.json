      "background_id": 28,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "代わりに知ったのは、ナカヤマフェスタに関する よからぬ噂だった。彼女がそんなことをするはずは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "あ、ナカヤマ！！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 28,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "ん……？　ああ、トレーナーか。 なんだか青い顔してんなァ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "いや、実は……",
