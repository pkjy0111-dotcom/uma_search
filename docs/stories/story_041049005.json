      "background_id": 95,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "警察からの補導に、勝負でのイカサマ――学園内外に、 彼女に関する様々な噂が飛び交っていることを。",
          "flag_type": "",
          "flag": "",
          "choices": []
