      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90106,
          "speaker_name": "ナカヤマの同級生B",
          "text": "ナカヤマさん、なんかちょっと近寄りがたいっていうか。 ほら、この前も補導されたって聞いたし……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "補導……？",
