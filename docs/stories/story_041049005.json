      "background_id": 28,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1049,
          "speaker_name": "ナカヤマフェスタ",
          "text": "ハハハハッ……知らねえ間に随分おもしれえ話が 広まってるじゃねえか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
