      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90565,
          "speaker_name": "担任の教師",
          "text": "はあ、困ったわ……今週中に課題を出してもらわないと、 成績がつけられないのに……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "さ、捜すの手伝います……！！",
              "flag_type": "",
              "flag": ""
