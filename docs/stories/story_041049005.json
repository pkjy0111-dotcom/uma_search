      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 119,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90107,
          "speaker_name": "ガラの悪そうなウマ娘",
          "text": "ん？　あんたはあのトラブル絡みじゃないのか。 ほら、ナカヤマが――",
