      "background_id": 11,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "東京レース場で行われるエキシビションレース『スプラウト記念』。\r 伝説となったウマ娘の多くは、ここで頭角を現すとも言われている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 22,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そんなレースでの“怪物”の活躍に誰もが期待し、\r 目を輝かせていた――そんな、ある日のことだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
