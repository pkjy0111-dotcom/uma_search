      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "風水が趣味のリッキーと違って、 みんながみんなおまじないを 信じるわけじゃないんだから。",
          "flag_type": "",
          "flag": "",
          "choices": []
