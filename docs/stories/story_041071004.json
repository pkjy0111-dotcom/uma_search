    "9": {
      "frame_id": 9,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "すると、現在行われていると思しき企画展のタイトルが目に入った。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "……『刹那展』？",
              "flag_type": "",
              "flag": ""
            }
