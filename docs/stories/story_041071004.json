      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "しかしそれらは、ただ1つの共通項を持っています。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "……共通項って？",
