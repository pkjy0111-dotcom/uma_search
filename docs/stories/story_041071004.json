      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "はい。……もともと興味があり、足を運ぶつもりでいましたが、\r ぜひトレーナーさんにもお付き合いいただきたいと思いまして。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "どういう展示なんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "どういう展示なの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……種別は、多岐にわたります。\r 絵画、彫刻、写真、映像、工芸――",
          "flag_type": "",
          "flag": "",
          "choices": []
