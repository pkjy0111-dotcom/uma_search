      "frame_id": 6,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……メジロ家の『お車』に乗せられて、\r 美術館までやって来たのだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
