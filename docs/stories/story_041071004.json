    "7": {
      "frame_id": 7,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ふふっ、トレーナーさん、どうぞそう硬くならずに。\r ばあや、お見送りありがとう。行って参りますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "先ほどまでの緊張が残っているせいか、\r つい落ち着かない気分であたりを見回してしまう……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
