      "frame_id": 12,
      "background_id": 330,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "ええ。起承転結！ 最速4コマのスピード感、最高です！ そして整然としたコマ並び。直線しか勝たん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
