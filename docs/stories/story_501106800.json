      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "昼食まで持ち込んで、動く気が一切ないな。 怠惰の極みです、ミラ子さん。いただきます！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
