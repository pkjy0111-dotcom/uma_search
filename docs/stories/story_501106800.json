    "11": {
      "frame_id": 11,
      "background_id": 330,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "ふふ～ん、味もよりどりみどりだよ。 どう？　その漫画、おもしろい？",
