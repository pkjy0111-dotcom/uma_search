      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "どうぞどうぞ～。テーブルの上のお菓子も、 好きにつまんじゃっていいからね。",
