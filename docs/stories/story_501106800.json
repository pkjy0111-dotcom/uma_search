      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "……あ、そろそろお昼だね。 カップ麺でいいなら、二人のもあるよ～。",
