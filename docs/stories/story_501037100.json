      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "『皐月賞』のあとは『日本ダービー』―― 長い歴史と伝統を誇り、 ダービーウマ娘の称号は栄誉そのもの。",
          "flag_type": "",
          "flag": "",
          "choices": []
