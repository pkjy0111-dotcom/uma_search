      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "またあえて1月という早い時期を選ぶことで、 仮にトラブルが起きても、本番までに 対策の時間を取ることができます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
