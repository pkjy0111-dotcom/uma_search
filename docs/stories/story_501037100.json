      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "デビューからの3年間はとても重要ですし、 無駄なく計画的に走るべきです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
