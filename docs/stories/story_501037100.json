      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "あのシンボリルドルフ生徒会長も、 たびたびダービーへの想いを語っています。 ……必ず挑みたいです。",
          "flag_type": "",
          "flag": "",
          "choices": []
