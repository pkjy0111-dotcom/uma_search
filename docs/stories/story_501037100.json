      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "レース日程や挑む条件が決まっている以上、 先の見通しを立てるのは当然のことです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "そもそも私にはレース選びについて 明確な基準が存在します。 それに従えば、自ずと道は決まっていました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "基準？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
