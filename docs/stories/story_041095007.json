      "frame_id": 8,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "しばらく前。彼女の祖父母の家に行った際、 彼女が祖父から教わっていたモルタル造形の一輪挿し作り――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
