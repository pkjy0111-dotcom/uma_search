      "frame_id": 9,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "あれにすっかりはまり込んでいるようなのだ。このところ彼女は ずっと試作を繰り返しているらしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
