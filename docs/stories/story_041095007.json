      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1095,
          "speaker_name": "ビリーヴ",
          "text": "（丸みを帯びた形って難しいんだよな……。 でも、立方体はだいぶ整ってきたし、そろそろ……）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
