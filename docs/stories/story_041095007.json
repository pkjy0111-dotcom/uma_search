      "frame_id": 5,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そうしてリフレッシュすることで、 トレーニングにもより身が入るもの。だが――",
          "flag_type": "",
          "flag": "",
          "choices": []
