      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1095,
          "speaker_name": "ビリーヴ",
          "text": "今日は……ふたつは作れるか。時間的にも、速乾モルタルで さっと試作する程度かな。台座つきのと、四角柱と……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
