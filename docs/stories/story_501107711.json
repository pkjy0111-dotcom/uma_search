        {
          "next_frame": 2,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "……アタシの夢見た航海は、 もっと眩く輝いていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "2": {
      "frame_id": 2,
