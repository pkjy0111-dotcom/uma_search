      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "なのに、空には暗雲が垂れ込め、 荒波や嵐で阻まれてばかり――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "なぁ、航海士殿。 アタシはどうしたら宝島に辿り着ける？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "今までの努力を継続しよう",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "これまで以上に厳しくトレーニングしよう！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "What's!? それで通用するのか！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "無理をすれば、夢も遠のくよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "それは、一理あるが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "しっかり航路を引くから、信じてほしい",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "……そうだな。仲間を信じない奴が、 財宝を手に入れられるわけがない！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "困難なんかに負けるものか！ いつか絶対、宝島に辿り着いてやる！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 25,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "ああ！　今日の悔しさを思えば、 どんな試練も浅瀬の遊びに過ぎない！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
