      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "なんと……よろしいのですか？ たいへん貴重な本なのに――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "俺のことを思うなら受け取ってほしい",
