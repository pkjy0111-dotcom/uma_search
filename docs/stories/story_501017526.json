      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "トレーナー君。少々、失礼する。\r ブライアンの話を聞いてくる。",
