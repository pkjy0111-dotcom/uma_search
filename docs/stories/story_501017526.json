      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "本来であれば1日がかりの内容だが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
