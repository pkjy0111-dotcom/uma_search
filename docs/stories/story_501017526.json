      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "そこで相談なのだが――\r 本件の解決に、少し時間をもらいたい。",
