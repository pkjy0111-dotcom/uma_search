    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女が渡してくれたのは、 ざっくり切られたかぼちゃだった。 蒸してあるのか、熱々の湯気が立っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "ただ蒸しただけだけどさ、 熱いうちに食うとうまいぜ～？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "ありがとう、いただきます！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "かぼちゃを口に運び、頬張ると―― ほどよい甘さと、ほくほくとした食感に、 心も体も満たされていった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "おいしい……！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
