      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "あたしからの差し入れだ！ これ食ってちょっと元気つけてくれよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
