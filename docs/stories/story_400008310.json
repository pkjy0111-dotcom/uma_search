    },
    "10": {
      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "へへっ！　だろ～？ あたしが手塩にかけて育てたからな。 うまいのはお墨付きってわけだ！",
