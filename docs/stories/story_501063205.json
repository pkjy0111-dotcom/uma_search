      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "マックイーンさんのこれまでの努力が 必ず結果に繋がると、信じています。",
