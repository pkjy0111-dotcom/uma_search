      "frame_id": 13,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 0,
          "speaker_name": "雑誌の編集者",
          "text": "うう、どうしてあんなに突然……。 気まぐれな方とは聞いていたけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
