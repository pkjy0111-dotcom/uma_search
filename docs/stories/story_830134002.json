    "11": {
      "frame_id": 11,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "雑誌の編集者",
          "text": "急に断られちゃったんですよ～！ それも丁重に、丁寧に、礼儀正しく！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 0,
          "speaker_name": "雑誌の編集者",
          "text": "しかもその後、1度も連絡がとれず…… もうこうなったらと学園まで来てみたら、 ちょうど妹さんがいらして！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "なるほど……",
              "flag_type": "",
              "flag": ""
