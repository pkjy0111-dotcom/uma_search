      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ある日、学園に戻ろうとしたところ、 メジロアルダンが男性に迫られていた。 明らかに困っている様子なので――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "大丈夫？守衛を呼ぼうか？",
              "flag_type": "",
              "flag": ""
