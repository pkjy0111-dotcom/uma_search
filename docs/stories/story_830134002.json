      "frame_id": 9,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……あの？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "！そ、それで？",
              "flag_type": "",
              "flag": ""
            }
          ]
