    "5": {
      "frame_id": 5,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "こちらの方は雑誌の編集者さんなのですが…… 実は、姉様のことで 大変心を痛めていらっしゃるようで。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "姉様？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
