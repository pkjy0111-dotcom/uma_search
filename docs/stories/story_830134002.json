      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……まず改めて、姉に変わり 非礼をお詫び申し上げます。",
