      "frame_id": 6,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "はい。私の姉―― メジロラモーヌのことです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
