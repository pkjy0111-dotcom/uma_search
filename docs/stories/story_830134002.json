      "frame_id": 10,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "それで……こちらの方が企画なさった特集に、 姉様が出ることになったらしいのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
