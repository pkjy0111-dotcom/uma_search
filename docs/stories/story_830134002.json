    },
    "4": {
      "frame_id": 4,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "ああ、いえ……ご心配ありがとうございます。 ですが、その必要はございません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
