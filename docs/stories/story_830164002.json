      "frame_id": 4,
      "background_id": 186,
      "sub_background_id": 3,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "アタシのおふくろ、海外レースで走っててさ。 彼女とGⅠでやり合ったことがあるんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
