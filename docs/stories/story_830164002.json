      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "ああ、今日の講演者は ある意味アタシの思い出のウマ娘だからねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "思い出のウマ娘？",
