      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "後続集団を大きく引き離してデッドヒート！ そりゃあもうアツいタイマンだったよ！！",
