      "sub_background_id": 3,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1012,
          "speaker_name": "ヒシアマゾン",
          "text": "子どもの頃、そん時の動画を 見せてもらったんだけどさ。 2人ともすごかったんだぜ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
