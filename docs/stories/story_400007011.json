    "13": {
      "frame_id": 13,
      "background_id": 250,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『ありがとうございます。国内じゃ まだまだ認知度低いんですけどね。でも いずれは日本を代表する選手になりたくて』",
          "flag_type": "",
          "flag": "",
          "choices": []
