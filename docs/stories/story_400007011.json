      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『“U.A.F.”はそういう、まだまだ ウマ娘がするには知名度が低いスポーツの アスリートが、多く出てるじゃないですか』",
          "flag_type": "",
          "flag": "",
          "choices": []
