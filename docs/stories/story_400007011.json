      "frame_id": 12,
      "background_id": 250,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 2007,
          "speaker_name": "ソノンエルフィー",
          "text": "『ですがそこから、思いきってMMAに転向！ この前の試合も見ましたよ～！ 超かっこよかったです！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
