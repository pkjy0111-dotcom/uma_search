      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『パワーにはすごく自信あったんですよ。 でもまぁ、レースは力だけじゃ ダメじゃあないですか。限界感じちゃって』",
          "flag_type": "",
          "flag": "",
          "choices": []
