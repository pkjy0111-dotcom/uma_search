      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90103,
          "speaker_name": "U.A.F.出場ウマ娘A",
          "text": "『ええ。重賞に挑むところまでは いけたんですけどね～。どうにも勝てなくて』",
