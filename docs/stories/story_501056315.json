      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "才能なんか全然なくて、ただ幸運だけで\r 成り上がろうとしていた、強欲無双な女の子。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
