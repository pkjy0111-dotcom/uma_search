    "3": {
      "frame_id": 3,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "でも、そんな彼女が有馬記念を制したのです。\r そう――全てと引き換えに。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "その娘は神様になったのです。ウマを越えて、\r 神の域へ。そして白い光に――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "って、ちょっと待ってください！　私、\r 生きてますっ。むしろ前より元気ですっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "いつまでもお達者が売り物なんですから\r 成仏させないで！　ハッピー全開、 グランプリピース！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "トレーナーさん、やりましたよ！\r 幸せですか？　私を育てて、よかったですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "もちろん！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "ううっ、その一言が聞きたかったんです！\r よーし、この勢いで勝利者インタビューを！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "えー、全国のみなさん、おめでたい日を\r さらにおめでたくしてしまいました！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "今日私は、この場で言わざるをえません。\r 『菊の舞台に次いで、中山にも福が来た』と！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "望めば福は来るのです。\r 幸せは、願えば必ずやってきます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
