      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1109,
          "speaker_name": "ラインクラフト",
          "text": "だって本当に、強いんですから。 彼女たちは。……わたしたちは！",
