      "frame_id": 20,
      "background_id": 174,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1109,
          "speaker_name": "ラインクラフト",
          "text": "――必ず、勝って！ ティアラウマ娘たちの強さを、証明してみせますっ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "……！！",
              "flag_type": "",
              "flag": ""
