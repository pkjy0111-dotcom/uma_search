    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90420,
          "speaker_name": "中堅トレーナーA",
          "text": "……惜しいレースも何度もあった。けど、あと一歩がどうしても 届かなかった。そこを、私が埋めてあげたかったんだけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
