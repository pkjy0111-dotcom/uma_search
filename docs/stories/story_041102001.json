    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "つい先日のことだ。 先輩が担当していたウマ娘が、レースの世界を去った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
