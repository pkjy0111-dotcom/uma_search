    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90420,
          "speaker_name": "中堅トレーナーA",
          "text": "未勝利戦に、何度も挑んだけど……結局、勝ち上がれなかったから。 気持ちを切り替えて、進学しますって……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
