    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90420,
          "speaker_name": "中堅トレーナーA",
          "text": "……担当した子がこうやって去っていくのは、やっぱ慣れないな。 だって、何度経験しても…………くっ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90420,
          "speaker_name": "中堅トレーナーA",
          "text": "当然、悔いは残さなかった。あの子と全力で走ってきたつもり。 けれどね、つい思っちゃうのよ……もっと、できただろって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
