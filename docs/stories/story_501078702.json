    "5": {
      "frame_id": 5,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "お願い、ゼファっち！ ウチらにサイキョー美肌ケア教えて！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
