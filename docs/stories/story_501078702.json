      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "ってことは、それがゼファっちの バブいタマゴ肌のヒミツ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
