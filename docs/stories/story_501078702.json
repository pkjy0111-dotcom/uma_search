    "7": {
      "frame_id": 7,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "ゼファーの肌って、超～キレイじゃん！ ありえんくらいきめ細かくてモチモチ！ 何使ってるか、前から気になってたんだよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "私はこちらの洗顔料を愛用しています。 母が勤める会社の製品で、オーガニックの――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "おおっ、オーガニック！！ バズりネタ押さえてんねぇ☆",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "風見鶏というわけではありませんが。 ……私は自然と共に育ってきましたから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "――幼い頃の私は体が弱く、 空気のよい緑豊かな地で 日々を過ごしていました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "豊穣の風に育まれた食物をいただき、 植物や動物と戯れ……長らく 自然の恵みに与ってきたのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1064,
          "speaker_name": "メジロパーマー",
          "text": "なるほど！　じゃあ、オーガニックなものを 食べてきたおかげで、健康になれたんだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "オーガニック、神！ リスペクトするしかなくね！？　美肌的に！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
