    "6": {
      "frame_id": 6,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "メジロパーマーたちの話によると、どうやら 夜更かしして肌が荒れてしまったらしい……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
