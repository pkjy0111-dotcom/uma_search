      "frame_id": 17,
      "background_id": 40,
      "sub_background_id": 2130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1025,
          "speaker_name": "マンハッタンカフェ",
          "text": "――そういった記憶が、沈んだ私を引き上げることが…… ごくたまにですが確かに、あるんです……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
