      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "それこそが我が―― “華麗なる一族”の玉条。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
