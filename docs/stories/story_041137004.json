      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1137,
          "speaker_name": "キセキ",
          "text": "そんな圧倒的な強さを秘めた相手にも。 あたしは――",
