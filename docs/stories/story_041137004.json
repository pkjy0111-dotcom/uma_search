      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90415,
          "speaker_name": "ベテラントレーナー",
          "text": "まあ、キセキが判断したことだ。……なあ、トレーナー君？ 君には、俺たちにはない、彼女を勝たせられる案があるんだろう？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "はい、俺たちはそう思っています",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "はい、私たちはそう思っています",
              "flag_type": "",
              "flag": "female_trainer"
