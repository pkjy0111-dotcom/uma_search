      "background_id": 29,
      "sub_background_id": 411,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1137,
          "speaker_name": "キセキ",
          "text": "そしてあたしは、あなたのその一言をきっかけに新しい―― 最強の案を用意することにした。そこまでは間違いないですね？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "……ああ",
              "flag_type": "",
              "flag": "male_trainer"
            },
