      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1137,
          "speaker_name": "キセキ",
          "text": "……っ、トレーナーさん！　最後に作戦タイムいいですか！ あのっ、ちょっとだけ！　2コマでいいんで！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "ああ、もちろん。一緒に緊張をほぐそう",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "うん、もちろん。一緒に緊張をほぐそう",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
