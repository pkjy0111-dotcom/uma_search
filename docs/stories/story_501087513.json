      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "邪魔のつもりはありませんよ？　勝負服の マーちゃんを撮ってほしかっただけなので。",
