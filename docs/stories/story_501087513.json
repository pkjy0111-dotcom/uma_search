      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "どうしましたか、トレーナーさん。 マーちゃんに何かご用でしょうか。",
