      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90620,
          "speaker_name": "カメラマンA",
          "text": "ダメというか、今日のテーマは日常風景 なので、勝負服姿は目立ちすぎちゃって――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "ちょっとすみません！",
              "flag_type": "",
              "flag": ""
            }
