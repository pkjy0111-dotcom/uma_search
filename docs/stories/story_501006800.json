    "5": {
      "frame_id": 5,
      "background_id": 51,
      "sub_background_id": 310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "サンタさんのトナカイと、併走したいんだ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "サンタさんの、トナカイと！？",
              "flag_type": "",
              "flag": ""
