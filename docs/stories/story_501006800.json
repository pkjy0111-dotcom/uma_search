      "frame_id": 16,
      "background_id": 156,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "うぅ……少し走って来たが、雪道だと\r 足を取られて上手く走れないな……。",
