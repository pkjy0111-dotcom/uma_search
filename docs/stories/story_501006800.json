    "12": {
      "frame_id": 12,
      "background_id": 51,
      "sub_background_id": 310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "うん……！\r トレーナーも楽しそうだとは思わないか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "（とてもまっすぐな目だ……）",
