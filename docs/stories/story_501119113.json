      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "まとめていただき、ありがとうございます。 残る期間ではこのシミュレーションに沿って トレーニングさせてもらえませんか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ああ、わかった……！",
