      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "――……なるほど。 そして第4コーナー時点では ここまで進出しておく必要がある、と。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ああ。…………参考になったか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "うん。…………参考になったかな？",
              "flag_type": "",
              "flag": "female_trainer"
