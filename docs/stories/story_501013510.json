      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "知っていることを話した。バラエティ記事 『お嬢様・無人島サバイバル』のためだと。",
          "flag_type": "",
          "flag": "",
          "choices": []
