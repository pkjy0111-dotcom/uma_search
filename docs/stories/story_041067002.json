      "frame_id": 35,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 36,
          "character_id": 90001,
          "speaker_name": "",
          "text": "走りの感想を伝えないのは失礼だと思った。まずは素直に\r 才能を褒め称え――次に見えた課題を伝えることにした。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 36,
              "loop_ends_at_frame": -1,
              "text": "まず、本気の競り合い経験が足りないと思う",
              "flag_type": "",
              "flag": ""
            }
          ]
