    "18": {
      "frame_id": 18,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90001,
          "speaker_name": "",
          "text": "たまらず打ち明けた。素質は素晴らしいと思ったが、自分は\r 一般のトレーナーで、高い垣根を越えられるような者ではないと。",
          "flag_type": "",
          "flag": "",
          "choices": []
