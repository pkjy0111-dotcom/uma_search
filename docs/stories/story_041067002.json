    "14": {
      "frame_id": 14,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "あの……少しよろしいでしょうか？\r 前にお見かけした方ですよね？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "サトノダイヤモンド……？",
