      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90435,
          "speaker_name": "新人トレーナーA",
          "text": "――誰も出なかったらしいな。\r これから、どうするんだろ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そしてパーティー当日。会場でパーティーゲームに興じる\r サトノダイヤモンドとキタサンブラックの姿を見かけた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "えええっ、リバーシって端っこ取られたら負けちゃう\r ゲームでしょ？　なのにそこに置くの、ダイヤちゃん！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "うん。四隅を取られたら負け。\r そんなふうに言われているなら――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ジンクスは破らないとね！\r 破って勝ってみせるから！",
          "flag_type": "",
          "flag": "",
          "choices": []
