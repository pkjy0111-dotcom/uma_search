    "15": {
      "frame_id": 15,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "模擬レースの時、私のことを見てくださっていましたよね？\r その……お聞きしたいことが～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
