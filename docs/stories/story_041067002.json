    "11": {
      "frame_id": 11,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "うわっ、出た！　ダイヤちゃんのジンクス破り！\r このモードに入った時は――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
