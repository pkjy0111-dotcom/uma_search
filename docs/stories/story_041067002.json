      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90435,
          "speaker_name": "新人トレーナーA",
          "text": "じゃあ会場でな。あ、そういえば知ってるか？　例のサトノ\r ダイヤモンドのトレーナートライアルだけど、合格者は……。",
          "flag_type": "",
          "flag": "",
          "choices": []
