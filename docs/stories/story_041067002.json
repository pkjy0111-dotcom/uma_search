      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "トレーナートライアル、どうしてお越しいただけなかったの\r でしょう？　関心を持っていただけていると思ったのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
