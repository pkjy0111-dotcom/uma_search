    "17": {
      "frame_id": 17,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "お眼鏡にかなわなかった理由はなんでしょう？\r ……やはり素質が足りなかったとか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "そんなことはないよ。だって……！",
              "flag_type": "",
              "flag": ""
