    "12": {
      "frame_id": 12,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "結果は――サトノダイヤモンドの圧勝だった。\r ただのおとなしいお嬢様とは違う、面白い一面を見た。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 136,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ジンクス――迷信のようなものを見かけると打ち破りたくなる\r ようだ。どんな人柄なのか、はかりかねていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
