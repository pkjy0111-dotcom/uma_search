      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "在学生の皆さん。そして普段お世話になっている\r トレーナーの皆さん。メジロ家からお伝えしたいことがございます。",
          "flag_type": "",
          "flag": "",
          "choices": []
