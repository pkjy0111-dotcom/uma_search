      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90435,
          "speaker_name": "新人トレーナーA",
          "text": "へー、メジロ家のカジュアルパーティーか。いろいろ企画して\r くれて、いつもありがたいな。君も行くだろ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "うん、まあ……",
              "flag_type": "",
              "flag": "male_trainer"
            },
