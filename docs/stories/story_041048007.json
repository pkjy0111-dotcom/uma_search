      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1048,
          "speaker_name": "トーセンジョーダン",
          "text": "あたしちょ～楽しみにしてて～。マジ絶対行くから！\r 今めっちゃ頑張るし、延長ナシだかんね？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "わかった！",
              "flag_type": "",
              "flag": ""
