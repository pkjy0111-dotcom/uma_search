      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1048,
          "speaker_name": "トーセンジョーダン",
          "text": "ふ～ん……合宿まで来て、よーやんね。\r まーいいけど。とりま次の準備してくる～。",
