      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1048,
          "speaker_name": "トーセンジョーダン",
          "text": "え、知らんの！？　今夜さ～、近くの神社で\r けっこーデカめな夏祭りあんの！",
          "flag_type": "",
          "flag": "",
          "choices": []
