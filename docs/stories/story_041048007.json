      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1048,
          "speaker_name": "トーセンジョーダン",
          "text": "……つかアンタさ～、知らなかったってことは、\r 夏祭り行かないんだ？　夜は何すんの？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "トレーニング資料の整理かな",
