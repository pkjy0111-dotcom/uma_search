      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9042,
          "speaker_name": "バイアリーターク",
          "text": "学生らしい、夢想的な噂…… 初めの頃は、そう考えていたのだがな。",
