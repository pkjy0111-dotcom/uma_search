      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9042,
          "speaker_name": "バイアリーターク",
          "text": "多くのウマ娘たちの成長を見てきた、今。 “想い”の意味がわかる気がするのだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "“想い”の意味……？",
              "flag_type": "",
              "flag": ""
