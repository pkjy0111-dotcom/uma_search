      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "なるほど。それは心強いですね。 ではトレーナーも、今日はよろしくお願いします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
