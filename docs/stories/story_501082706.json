      "frame_id": 15,
      "background_id": 218,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "ふふ……。 実はここ最近、トレーナーさんが前よりも キラキラしてるなぁって思ってたんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
