      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "いっぱい努力してるんですよね。 その姿勢、本当にステキです♪",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
