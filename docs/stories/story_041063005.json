      "frame_id": 30,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "私は、最善の方法を選んだと自負しています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "31": {
      "frame_id": 31,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "イクノディクタスの目が物語っている。 本心からの言葉。それを覆す術は……持っていない。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 32,
              "loop_ends_at_frame": -1,
              "text": "……わかった。けど、無理はしないように",
