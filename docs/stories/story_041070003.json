    "6": {
      "frame_id": 6,
      "background_id": 2,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "いいや、意志を否定したいわけではない。 だが今後走れなくなっても、という部分には賛成しかねる。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "（え、今後走れなくなっても……！？）",
              "flag_type": "",
              "flag": ""
