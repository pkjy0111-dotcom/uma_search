      "background_id": 2,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90046,
          "speaker_name": "シリウスの後輩",
          "text": "私は、弱いから……寄りかかってしまったら、 ダメだった時……きっと全部、そのせいにしてしまう。",
          "flag_type": "",
          "flag": "",
          "choices": []
