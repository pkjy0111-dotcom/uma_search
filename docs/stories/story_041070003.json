      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "……しかし――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
