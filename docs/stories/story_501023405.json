      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "相手の強さを知るほど、焦り、差し詰まる。 睡眠時間すら削るトレーナーの疲労は、 すでにピークに達していた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
