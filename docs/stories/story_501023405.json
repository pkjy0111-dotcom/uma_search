      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんなトレーナーの元へ、ビワハヤヒデが 戻ってくる。彼女は1人で無理をする トレーナーに気づき、怒っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
