      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "そう締めくくるのが、みんなに1番希望を\r 与えられると思っています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
