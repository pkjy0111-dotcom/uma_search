      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "そんな風に、記憶に残したら負け……。\r 選ばれた人が勝つだけになるから。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "選ばれない者が勝つ？",
              "flag_type": "",
              "flag": ""
