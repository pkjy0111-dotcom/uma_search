      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "トレーナーさん、あたし、このレースを\r オグリさんのラストランにはしないつもり です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
