      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "はい。オグリさんを追う立場のあたしが、\r 正々堂々グランプリ二冠を達成する！！",
          "flag_type": "",
          "flag": "",
          "choices": []
