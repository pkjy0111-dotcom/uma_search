      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "ふふ、本心を言っているだけさ。 今日はいつにも増して美しい、 グラツィオーソなフォームだったから……。",
          "flag_type": "",
          "flag": "",
          "choices": []
