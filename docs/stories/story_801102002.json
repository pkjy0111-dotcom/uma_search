      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "やあ、アモーレ！ 今日も畑仕事を頑張っているのかい？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "アースさん！？ み、見ないでください～っ！ 私いま、土で汚いので……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "……手を貸して、アモーレ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "……この手のおかげで、畑の野菜たちは いつもアニマートに育っているんだね。 土がついているからこそ、キレイな手だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "どうか、汚いなんて言わないで。 いつだって生命力にあふれる、 優しくて美しい君が――私は好きなんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90103,
          "speaker_name": "ウマ娘A",
          "text": "う……！ あ、アースさんってば……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
