    "9": {
      "frame_id": 9,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "放課後。 昼はすごいものを見たなあと思いながら コースの片づけをしていると――",
