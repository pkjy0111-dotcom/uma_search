      "frame_id": 11,
      "background_id": 74,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90104,
          "speaker_name": "ウマ娘B",
          "text": "……はあ。 また貴方は、歯の浮くようなことばかり……。",
