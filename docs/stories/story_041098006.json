      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "とはいえ特に急ぎの予定はなかったので、 コパノリッキーの案内についていくと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 173,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "ってわけでこちら、動物園ですっ！　えへへ、お出かけって 氣を動かすことになるから絶好の開運チャンスになるんだよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 173,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "そこで大事なのは、吉方位に行くこと！　この動物園はキミの 吉方位で、いつか絶対連れて行きたいなって思って、て……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 173,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "……泣き声？ ごめんトレーナー、ちょっと行ってくる！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
