      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "と、そうだ！　帰り道は真ん中の大きな道を通って帰ってね！ 幸運の通り道になっているから、きっと無事に帰れるよ！",
          "flag_type": "",
          "flag": "",
