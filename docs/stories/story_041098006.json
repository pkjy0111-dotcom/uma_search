      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんなある日。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
