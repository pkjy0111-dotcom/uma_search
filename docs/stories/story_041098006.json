      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "ううん、私は一緒にいただけだよ！ 見つけられてよかった、気をつけて帰ってね！",
          "flag_type": "",
          "flag": "",
