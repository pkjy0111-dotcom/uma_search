      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "と言って、その論文の内容を軽く説明する。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
