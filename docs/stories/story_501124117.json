      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "今年引退したワルツオブシャドウが、 この神馬駆に自身のこれまでの想いを 捧げることにしたのだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
