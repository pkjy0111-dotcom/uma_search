      "background_id": 151,
      "sub_background_id": 131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "自分たちの目的は、 この『駿大祭』の中で行われる神事、 『神馬駆』に参加することにあった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 206,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『神馬駆』は、レースを引退した ウマ娘たちの想いを三女神の前で称え慰める、 古より伝わる由緒正しき神事であり――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 206,
