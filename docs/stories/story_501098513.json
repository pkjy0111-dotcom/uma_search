      "background_id": 199,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "その誤解で、風水を試してくれた人も パパみたいな風水師も、みんなアンハッピー。 それがずっと悲しかったんだよね……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
