      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "やったー！ リッキー☆　ラッキー☆　効果バッチリっ！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "効果って？",
              "flag_type": "",
              "flag": ""
