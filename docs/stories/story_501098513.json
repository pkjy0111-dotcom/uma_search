      "frame_id": 11,
      "background_id": 199,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "けどそういうのって、たいてい方法が 間違ってたり、効果が出る前に すぐやめちゃってるからなんだよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
