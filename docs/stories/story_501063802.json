      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "残念ながら、ここの草むらに ラーメンはいないようですね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "ラーメンは草むらに生息していないんじゃ？",
              "flag_type": "",
              "flag": ""
            }
