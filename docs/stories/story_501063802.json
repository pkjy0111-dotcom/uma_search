      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "少し時間もあるので、 ふたりのキーホルダー捜しに 協力することにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
