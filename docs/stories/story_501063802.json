    "5": {
      "frame_id": 5,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "お話を聞き、 手分けして捜していた所だったのですが…… トレーナーさん、見覚えはありませんか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "う～ん、特にないけど……",
              "flag_type": "",
              "flag": ""
