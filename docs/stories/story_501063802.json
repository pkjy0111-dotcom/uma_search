      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "授業が終わったあとは、 こちらで食事をとり…… 済んだのは13時10分、ですか。",
