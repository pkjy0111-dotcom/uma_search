      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "ええ。期間限定黒豚骨ラーメンをいただいて、 キーホルダーと見比べたのを覚えているよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
