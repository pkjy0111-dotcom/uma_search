      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "しかし……実際に経験してみることで\r 見えてくるものは多かったな。これでは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "タキオン？",
