      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ああ。トレーナー君か。\r いやなんでもないよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
