      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "まぁ、君はそう言うだろう。\r そんなことはわかっている。しかしね――",
