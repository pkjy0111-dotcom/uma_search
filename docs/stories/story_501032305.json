      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "それより、『弥生賞』と『皐月賞』を\r 走り切った。約束は果たした。",
          "flag_type": "",
          "flag": "",
          "choices": []
