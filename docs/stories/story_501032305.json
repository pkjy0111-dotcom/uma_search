      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "そのうえで、今後のクラシックレースに\r ついてだが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "次は、『日本ダービー』だな！",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
