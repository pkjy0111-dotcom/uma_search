      "background_id": 119,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90001,
          "speaker_name": "",
          "text": "アストンマーチャンは冷静だった。 涙の跡もなく、淡々と墓石に向かって語りかけている。",
          "flag_type": "",
          "flag": "",
          "choices": []
