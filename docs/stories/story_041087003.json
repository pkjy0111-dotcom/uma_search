      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "お母さんがお医者さんですから、 マーちゃんの公園は病院だったのです。病院は命が流れています。",
          "flag_type": "",
          "flag": "",
          "choices": []
