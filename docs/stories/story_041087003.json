      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "なるほど、それで『強い』。 たしかに多くのヒトは、亡くなると悲しんでいました。",
          "flag_type": "",
          "flag": "",
          "choices": []
