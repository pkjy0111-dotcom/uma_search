      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……ともかく、ショックを受けていることには違いないだろう。 捜索に協力し、学外を見て回ることにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
