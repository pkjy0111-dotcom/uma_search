      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "あの子って、いつか急に手の届かないどこかへ行っちゃいそうな 雰囲気があって、だから不安になっちゃったっていうか……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "手の届かないどこかへ……？",
