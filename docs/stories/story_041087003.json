      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "変な子ではあるんです。フラフラどこかに行くのもいつもの事だし。 1人で埋葬に行ったのかもしれないですけど、でも――",
          "flag_type": "",
          "flag": "",
          "choices": []
