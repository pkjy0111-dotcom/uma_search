      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ここ数日、アストンマーチャンと会話する機会が度々あったが、 そんな雰囲気はあっただろうか？",
          "flag_type": "",
          "flag": "",
          "choices": []
