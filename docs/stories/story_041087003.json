      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "でもマーちゃんは知っています。命は流れるものなのです。 病院が教えてくれました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 27,
