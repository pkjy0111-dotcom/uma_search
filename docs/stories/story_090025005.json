      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "――私はその間、相手の死角を捉える方法を模索しようと思います。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
