      "frame_id": 2,
      "background_id": 126,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1087,
          "speaker_name": "？？？",
          "text": "――ピョンヌ・ピエールさん、もっち郎、 熊ノ森道夫くん、にゃこにゃこ之介……。",
          "flag_type": "",
          "flag": "",
          "choices": []
