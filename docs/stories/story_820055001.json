      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "そして、わふもふ玉三郎、 ここまでお疲れさまでした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 126,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "アストンマーチャンがなにやら 机の上のマスコットたちに話しかけていた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……何してるんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……何してるの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 126,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "労いとお別れを。 新天地を見つけられなかった子は、 海――遠くへ行くので。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "海？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 126,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "海は、役目を終えたものたちが いずれたどり着く場所です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
