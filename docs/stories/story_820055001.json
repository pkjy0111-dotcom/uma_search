      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1087,
          "speaker_name": "アストンマーチャン",
          "text": "ゆえに、この子たちは今日でサヨナラ―― 実行委員さんにさっき言われました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
