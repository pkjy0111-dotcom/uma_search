      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "もったいないなぁ……おまんじゅうなんて、 食べ歩きにもお土産にも最適なのに。",
