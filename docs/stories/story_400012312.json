      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90715,
          "speaker_name": "地元のおばあさん",
          "text": "何十年も前は、そんなお客さんで 賑わっていたんだけどねぇ……。 この街もずいぶん雰囲気が変わっちまった。",
          "flag_type": "",
          "flag": "",
          "choices": []
