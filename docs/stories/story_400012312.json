      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "いいですね。少しずつだけど、 人の流れができつつある――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 362,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "でも、これで満足してはいられません！ お客さんを定着させ、新たなお客さんを 呼び込む――そのために！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "そのために……！？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 362,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――は、まだアイディアがないので。 まずは一緒に街を回ってみましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
