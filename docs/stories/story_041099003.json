      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "『そんなふうに、過ごしているうちに、 町並みが少しずつ変わっていっていることに気づきました』",
