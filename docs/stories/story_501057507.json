      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『朝も夜も　瞼の裏に　浮かぶのは 戯れのように　駆け抜ける君』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
