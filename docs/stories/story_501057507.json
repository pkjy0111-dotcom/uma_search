    "4": {
      "frame_id": 4,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『……なんて、ヘタクソな短歌ですみません！ けど、ターフを駆けるあなたの姿を見た瞬間、 この短歌が頭に浮かびました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "ファンレター",
          "text": "『今度はいつ、あなたの走る姿を見られるか 胸が鳴り止みません。何処だって行きます。 小倉だって、札幌だって、海外だって！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "ねっ？　熱烈なファンレターでしょ。 まるでラブレターみたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
