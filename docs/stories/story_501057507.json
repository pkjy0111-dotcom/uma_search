      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "……誰かに期待されるのって、 アタシは苦手なんだけどね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "アタシは、アタシの心にしか従えない。 砂の感触が気持ちいいなって思えば、 ダートのレースだって走りたくなると思う。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "だからね、誰かの期待を裏切っちゃう ことだって、きっとあると思うんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
