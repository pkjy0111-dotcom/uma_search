      "frame_id": 10,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "でも、そんなアタシの走りを好きだって 言ってくれるなら……うん、嬉しいな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "そんな君をファンも応援してる",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "そんな貴方をファンも応援してる",
