      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "差し出されたお礼状を拝見する。 勝負服姿の写真に金色のサインが刻まれた だけのそれは、非常にシンプルであったが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ファンレターをくれた人全員に送るのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "ファンレターをくれた人全員に送るの？",
