      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "ありがとう存じます。 こちらこそ、ご挨拶を賜り大変恐縮です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
