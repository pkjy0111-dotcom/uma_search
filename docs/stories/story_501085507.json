      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90350,
          "speaker_name": "気品のある女性",
          "text": "その……私、ファンレターを毎年 出させていただいておりまして。 今年のお礼状も大っ……変、素敵でした！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "お礼状、ですか？",
              "flag_type": "",
              "flag": ""
            }
          ]
