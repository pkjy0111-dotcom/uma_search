      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90350,
          "speaker_name": "気品のある女性",
          "text": "ルビー様にファンレターをお送りすると、 お礼状をいただけるのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
