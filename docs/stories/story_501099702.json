      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "修行を嗜むのですわ。タルマエさんの 船酔い克服をお手伝いするのです。",
