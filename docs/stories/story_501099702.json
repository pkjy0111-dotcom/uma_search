      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "この前教室で、私が船酔いしやすい話を してたんです。それを聞いたカワカミさんが、 船酔い克服に協力してくださるって……！",
          "flag_type": "",
          "flag": "",
          "choices": [
