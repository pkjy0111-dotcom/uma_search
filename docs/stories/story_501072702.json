    "12": {
      "frame_id": 12,
      "background_id": 48,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "私は……知っているのです。 それと、同じ境地に達してしまった人物を。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 176,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "その方はウマ娘で、私の姉弟子。 高弟の中でも指折りの実力者であり、 若くして皆伝を允許された拳豪でありました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
