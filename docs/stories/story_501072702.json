      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "豪快にして心優しく、なにより一心に 武術を極めんとするその姿。私を含め、 多くの門弟が憧れていたものです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
