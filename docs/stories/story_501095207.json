      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そのうち日が落ちてきて、一言だけ挨拶を 交わし、互いの帰路をたどる……というのが、 ここしばらくの日常だったのだが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
