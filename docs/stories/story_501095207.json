      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "だから自分も、あまり気を使いすぎはしない。 こちらもこちらで、黙々と仕事を続ける。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
