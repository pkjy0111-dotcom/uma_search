      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "とはいえそれ自体は、見慣れてきた光景だ。 あの区画あたりはもはや、ほとんど ビリーヴのための作業スペースになっている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
