      "background_id": 121,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーナーの必死の呼びかけにより、\r アドマイヤベガはようやく目を覚ます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
