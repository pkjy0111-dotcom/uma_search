      "frame_id": 21,
      "background_id": 134,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "アタッカ！　言葉の届かない場所へ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "あ、ああ！もっと……！",
              "flag_type": "",
