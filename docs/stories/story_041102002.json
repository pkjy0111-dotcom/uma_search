      "background_id": 134,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 90001,
          "speaker_name": "",
          "text": "素人のつたない演奏を牽引し、それを音楽にしてくれた。 彼女の演奏技術は、自分でもわかるほど非凡だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
