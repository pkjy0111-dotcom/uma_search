      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "サウンズオブアースのヴァイオリンがあたりに響く。 ――瞬間、鳥肌が立った。",
          "flag_type": "",
