      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう尋ねられ、ふと疑問を抱いた。そういえば、 どうして彼女は自分を気にかけてくれたのだろう？",
          "flag_type": "",
          "flag": "",
          "choices": []
