      "frame_id": 30,
      "background_id": 134,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "君、ムジカの経験があるだろう？　でなければ説明がつかない！ もし未経験であの演奏をしたのなら、とんでもない原石だ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 31,
              "loop_ends_at_frame": -1,
              "text": "ありがとう……でも、すごいのは君だ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 31,
              "loop_ends_at_frame": -1,
              "text": "ありがとう……でも、すごいのは貴方だよ",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "31": {
