      "frame_id": 29,
      "background_id": 134,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "ブラヴィーーーーーッッッシモ！！！ 最高のセッションだった！　魂が震えたよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
