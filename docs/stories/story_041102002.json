      "background_id": 134,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "グラッツィエ！　私のブリッランテを感じてくれたんだね！ 君が笑顔を取り戻す、きっかけになれたかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
