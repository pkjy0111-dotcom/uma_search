      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1016,
          "speaker_name": "ナリタブライアン",
          "text": "私を強くするのはアンタの役目だ。\r どう舵取りするかはアンタに委ねる。",
