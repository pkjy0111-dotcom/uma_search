      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "年度代表ウマ娘とは最も評価の高い\r ウマ娘に贈られる栄誉ある称号の1つです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
