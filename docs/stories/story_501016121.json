      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "ふふっ、この式はナリタブライアンさん\r にとって大きな節目になることでしょうから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
