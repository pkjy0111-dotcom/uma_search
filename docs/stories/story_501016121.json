      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1016,
          "speaker_name": "ナリタブライアン",
          "text": "今はただ集中したい。\r 走ることだけに。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "……だが授賞式には過去受賞した\r 強豪ウマ娘も出席するらしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
