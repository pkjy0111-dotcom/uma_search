      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1016,
          "speaker_name": "ナリタブライアン",
          "text": "おい、本当に行かねばならんのか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "一応、欠席もできるけど……",
              "flag_type": "",
              "flag": ""
            }
