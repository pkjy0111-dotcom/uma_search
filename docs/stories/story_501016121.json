    "12": {
      "frame_id": 12,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女たちの姿や考え方を肌で感じることは、\r 走りにもいい影響を与えるはずだ。それに――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "立場は、強くあろうとする覚悟にもなる",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
