    "6": {
      "frame_id": 6,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "授賞式は後日となりますので、\r 出席用資料も目通しくださいね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
