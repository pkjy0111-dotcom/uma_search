      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1016,
          "speaker_name": "ナリタブライアン",
          "text": "私はそう選択すると決めた。\r 文句は言わない、私に必要な進路を取れ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
