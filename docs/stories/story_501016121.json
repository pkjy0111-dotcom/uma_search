      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "あ、ブライアンさんもちょうどよかった！\r このまま説明までさせてください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
