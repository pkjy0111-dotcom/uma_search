        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1016,
          "speaker_name": "ナリタブライアン",
          "text": "立場というものはいけ好かん。\r 余計な面倒を招く。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
