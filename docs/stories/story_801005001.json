      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "さあ、アシスタントの出番だよ！\r ポケットの中を探してみて。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "さっきこっそり入れ――",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "すごいマジックパワーだ……！",
              "flag_type": "",
