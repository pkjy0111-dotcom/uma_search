      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "そう身構えなくても大丈夫。\r さっ、ついてきて！",
          "flag_type": "",
