      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "――お待たせ、泣き虫なポニーちゃん。\r まばたきは禁止、どうぞご覧あれ！",
          "flag_type": "",
          "flag": "",
          "choices": []
