        {
          "next_frame": 3,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "――あっ、そこの君。\r 少しだけアシスタントを頼めないかな？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "アシスタント？",
