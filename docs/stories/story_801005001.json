      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1005,
          "speaker_name": "フジキセキ",
          "text": "しーっ。気づいたのはすごいけれど、\r 口にするのは厳禁だよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
