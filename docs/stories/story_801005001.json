      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "フジキセキに連れられ向かった先には、\r 膝をすりむいて泣きじゃくる女の子がいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
