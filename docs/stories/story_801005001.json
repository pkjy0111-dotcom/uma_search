      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その瞬間――\r フジキセキの手から、ぱっとコインが消えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
