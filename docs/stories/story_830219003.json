          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "もしかして、また劇をやるのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "もしかして、また劇をやるの？",
              "flag_type": "",
              "flag": "female_trainer"
