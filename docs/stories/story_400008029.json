      "background_id": 286,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "そして――やがては色も形も さまざまな実をつける。個性豊かで、 いずれも素晴らしい実を、な。",
          "flag_type": "",
          "flag": "",
          "choices": []
