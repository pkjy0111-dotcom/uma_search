    "5": {
      "frame_id": 5,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "……だがな。いつもどおりの仕事でも、 結果はさまざまに違うものだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 286,
      "sub_background_id": 113,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "始めは皆、同じ種。 豊かな場さえ整えてやれば、 いずれは立派な芽を吹いてくれる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
