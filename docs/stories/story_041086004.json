      "frame_id": 21,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "はぁっ……はぁっ……はぁっ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
