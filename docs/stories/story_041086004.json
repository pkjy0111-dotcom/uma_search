      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "ああ……ふふっ……ふふふ……っ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 23,
              "loop_ends_at_frame": -1,
              "text": "（あの時も、笑っていたっけ）",
