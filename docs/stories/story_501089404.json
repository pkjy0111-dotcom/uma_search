      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そこで、彼女はコンプレックスの根幹たる ヴィブロスとヴィルシーナの2人に、 合同トレーニングを申し込む。",
          "flag_type": "",
          "flag": "",
          "choices": []
