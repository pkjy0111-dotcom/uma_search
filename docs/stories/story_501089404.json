    "2": {
      "frame_id": 2,
      "background_id": 42,
      "sub_background_id": 2110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『ジャパンカップ』には実力者が目白押し。 スタミナや勝負勘など、 各々の強みを持ったウマ娘が出走する。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 246,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "読み切れない戦績の実力者も出走し、 油断できない熱戦になるはずだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
