    },
    "6": {
      "frame_id": 6,
      "background_id": 52,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1023,
          "speaker_name": "？？？",
          "text": "ああ！\r 申し訳ない、思索に耽っていたら不意に――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "そっちまで届けるよ！",
