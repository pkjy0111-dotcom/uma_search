    "5": {
      "frame_id": 5,
      "background_id": 52,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "頭上から響いた涼やかな声。見上げると、\r 教室の窓から顔を出すウマ娘がいた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "これ、君のか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "これ、貴方の？",
              "flag_type": "",
              "flag": "female_trainer"
