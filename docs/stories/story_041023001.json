    },
    "7": {
      "frame_id": 7,
      "background_id": 52,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1023,
          "speaker_name": "？？？",
          "text": "……いいのか？　感謝する！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
