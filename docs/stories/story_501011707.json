      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "グラスワンダーの言ったことを思い出しつつ、\r ウマ娘レース全体の繁栄を祈ることにした。",
          "flag_type": "chance_branch_root",
