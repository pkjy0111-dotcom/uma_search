      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "『情けは人のためならず』とも言いますし……\r 日本文化の素敵なところですよね♪",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "俺も試してみるよ",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "私も試してみるよ",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
