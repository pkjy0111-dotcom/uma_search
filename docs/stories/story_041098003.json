      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "このくらいは、毎日やってるんだから☆　風水は願いを 叶えてくれるものじゃない。努力の方向を教えてくれるもの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
