      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "え！？　違う違う。別にタルマエに『もっとトレーニングしろ』 って言われたから、やってるわけじゃなくて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
