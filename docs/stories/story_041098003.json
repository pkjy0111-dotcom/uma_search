      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1098,
          "speaker_name": "コパノリッキー",
          "text": "あ、トレーナー！　うん、不甲斐ないところ見せちゃった分、 トレーニングにも一段と氣を入れていかないとね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ホッコータルマエに言われたから？",
              "flag_type": "",
              "flag": ""
