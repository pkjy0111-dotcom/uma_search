      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "『――それでも折れそうな時は、 周りを頼ろう！　だってあたしたちは、 同じトレセン学園の生徒なんだから！！』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
