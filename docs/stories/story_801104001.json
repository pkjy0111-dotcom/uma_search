      "frame_id": 10,
      "background_id": 16,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "カツラギエースのスピーチは、 その後30分を経過しても 全く終わる気配がなかった……！",
          "flag_type": "",
          "flag": "",
          "choices": []
