      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "ハンデ？　ハンデか……。\r でもこれってハンデなんかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
