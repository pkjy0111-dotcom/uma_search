    "26": {
      "frame_id": 26,
      "background_id": 28,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "たとえ細っこくても、この体はお父ちゃん\r お母ちゃんにもらったもんや。この体で 堂々勝ったるわー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
