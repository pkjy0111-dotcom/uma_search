      "frame_id": 27,
      "background_id": 33,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "あの日、ウチの腹は決まった。いつか\r 見返したる。浪花一のウマ娘になったるって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
