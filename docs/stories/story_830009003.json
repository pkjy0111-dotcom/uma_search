      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "チキショウ！　チキショー！！\r スクールがナンボや。今に見とれよー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
