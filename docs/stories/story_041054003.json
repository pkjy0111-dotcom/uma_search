      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90034,
          "speaker_name": "キャロットマン",
          "text": "『君はまた走れる。その証拠に―― 私が、不可能を可能にしてみせる！！』",
