    "8": {
      "frame_id": 8,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1054,
          "speaker_name": "ビコーペガサス",
          "text": "すっごく嬉しいっ！！　だってアタシ―― 芝を走るキャロットマンに憧れて、トレセン学園に来たんだから！！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "芝を走るキャロットマン？",
              "flag_type": "",
              "flag": ""
