      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1054,
          "speaker_name": "ビコーペガサス",
          "text": "そう！　ほら、こないだ一緒に見たじゃんか！ アタシの大好きな回！　『キャロットマン　プリティデビュー！』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "ああ、112回！",
