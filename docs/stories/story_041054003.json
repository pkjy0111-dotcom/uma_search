      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 74,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1054,
          "speaker_name": "ビコーペガサス",
          "text": "……ホントに芝、走っていいんだ……！！ ありがとう、トレーナーっ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
