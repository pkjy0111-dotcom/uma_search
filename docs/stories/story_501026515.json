      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "サクラバクシンオーが\r ミホノブルボンを逃がすまいと 両手を広げている。",
