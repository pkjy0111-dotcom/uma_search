      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "わざとでないことは理解しました！\r しかし、原因についてはまだ議論の余地が――",
