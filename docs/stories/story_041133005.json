      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "――6番は先行二番手……。重バ場で入着した経験もあるから、 落ち着いた走り……。過去のレースから考えると……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 19,
              "loop_ends_at_frame": -1,
              "text": "クロノ、君も早く屋根の下へ……！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 19,
              "loop_ends_at_frame": -1,
              "text": "クロノ、貴方も早く屋根の下へ……！",
              "flag_type": "",
              "flag": "female_trainer"
            }
