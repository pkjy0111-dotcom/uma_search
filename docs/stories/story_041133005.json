      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "やがてウォーミングアップが終わり、 選手たちが続々とゲートインする。だが――",
