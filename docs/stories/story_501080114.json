      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "ボランティア主催",
          "text": "えー、ではリーダー役を決めます。 今回、清掃の指揮を取ってもらうのは……。",
          "flag_type": "",
          "flag": "",
          "choices": []
