      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "はい！　はいはい！　ウチ立候補します！ 今、絶賛主役期間中なんで。",
