      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "ひとーつ、ふたーつ、みーっつ……。 つい見逃しがちなところも隅々まで、と。",
