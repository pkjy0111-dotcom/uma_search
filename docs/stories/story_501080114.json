      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "いやぁ、いいな～。しみじみゴミを 拾い集めるのは。時を忘れて、没頭……。",
