      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "ボランティア先の友人",
          "text": "と、トランちゃん、大丈夫～？　いつも 目立つのは、あんまり好きじゃないって……。",
          "flag_type": "",
