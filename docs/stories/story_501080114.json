      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "えー、みなさん。 確かに今回の清掃は大変です。 ゴミの量もいつもの比じゃありません。",
