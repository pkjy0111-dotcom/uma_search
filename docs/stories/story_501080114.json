      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "まーかせて！　ふっふっふ、 キミの知らないウチを見せてあげよう。",
