      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "ふぅ……。軽めのトレーニングならば\r 大丈夫そうです。",
          "flag_type": "",
          "flag": "chance_branch_leaf",
          "choices": []
        }
