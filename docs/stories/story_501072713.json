      "frame_id": 16,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ヤエノムテキは無念そうに言って、\r 寮へ帰っていった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
