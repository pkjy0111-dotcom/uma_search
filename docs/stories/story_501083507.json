      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "――トレーナー。 ひと晩かけ――返事を、作成した。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "Check――頼む。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
