      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "――期待を――かけてくれて、いるんだな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 7,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "…………。 ――相応の――返事を――考えなくては。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
