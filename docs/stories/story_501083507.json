    "9": {
      "frame_id": 9,
      "background_id": 107,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "シンボリクリスエスが神妙な顔で、 手紙を差し出す。中身を読んでみると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 118,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "『応援ありがとうございます。 とてもとても嬉しいです。 私の心に、暖房がつきました。』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 118,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "『これからも見ていてくれれば、 もっとあたたかくなります。 そして、すごく真剣に頑張れます。』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 118,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "『日本のレース界のために、 びしばし活躍します。 ここで、指切りします。』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――恐ろしく素朴な文章に、 一瞬変な想像をしてしまった。 思わずシンボリクリスエスを見ると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "……元々――表現は、不得意だ。 まして――外国語では。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
