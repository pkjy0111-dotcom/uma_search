      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "だから……一切の――修辞を、排し。 なるべく――簡易に、書いた。",
