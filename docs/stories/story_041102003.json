      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そこから感じられるのは、焦燥、緊張、期待―― 何より、自分こそが勝者になるんだ、という強い気概だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
