      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "私たちの母親は2人とも、育成クラブの 指導者として、子どもたちと毎日 走ってるので……疲れを取れればなって。",
          "flag_type": "",
          "flag": "",
          "choices": []
