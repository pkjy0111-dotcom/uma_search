      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『“確率計算”……！ これこそが勝敗を左右する！！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『あたしの手札にあるカードからだけでも、 3人が揃えにくい役は理解る。 それを踏まえれば、多少の推測は可能ッ！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『彼女たちの性格は熟知している。 ポーカーとは高度な心理戦でもあるのだ！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 178,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『まずは彼女！　サトノクラウンだ。 いついかなるときもポーカーフェイスを 決めている、王道の強者……！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 178,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『だが、それだけに読みやすい。 おそらく、態度と手の強さは反比例するッ！ 自信のありそうなあの顔は十中八九ブラフ！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 178,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『そして、サトノダイヤモンド。 挑戦を好む彼女は、冒険の一手に出やすい。 付け入る隙があるとしたら、そこだ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『考えろ……！　あの口ぶり…… おそらく役なしではなかったはずだ』",
