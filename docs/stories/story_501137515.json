      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1137,
          "speaker_name": "キセキのモノローグ",
          "text": "『情報が足りない以上、 これ以上の推測は不可能。 だが、ここから導き出されるのは……！』",
          "flag_type": "",
          "flag": "",
          "choices": []
