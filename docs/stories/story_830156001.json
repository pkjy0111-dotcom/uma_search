      "frame_id": 9,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "……あら？　あらあらぁ？ 見て見て、あそこで座ってるお侍さん！ お久しぶりの方だよぉ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
