      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ええ、『お助け侍　徳次郎さん』よぉ。 長いこと放送してるドラマでねぇ。ここで 撮影があるって聞いて、楽しみにしてたの。",
          "flag_type": "",
          "flag": "",
          "choices": []
