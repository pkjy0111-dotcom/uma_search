    "7": {
      "frame_id": 7,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "時代劇といえば、歌劇同様、幾世代を超え、 大衆より愛され続けている伝統文化！ それを生で鑑賞できるとは、なんたる僥倖！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ふふっ。さっきの台詞のあとは、 悪者との決闘シーンなのよねぇ。 殺陣が派手で、そりゃあもう――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
