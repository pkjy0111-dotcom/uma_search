        {
          "next_frame": 11,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "3期前に、1度だけ出てきたお侍さんだわ！ あらぁ、また登場するのかしら。楽しみねぇ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "よく覚えてるな！？",
              "flag_type": "",
              "flag": "male_trainer"
