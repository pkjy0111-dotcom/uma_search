      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "あーん？　生きてるダイオウイカみてーな エンカウント率のくせして、よく言うぜ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
