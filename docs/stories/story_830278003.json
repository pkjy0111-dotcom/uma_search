      "frame_id": 13,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "ふむ、つまりどういうことだい？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "つまり……カブトムシの時代は終わった、 時代はタライで無人島ツアーだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
