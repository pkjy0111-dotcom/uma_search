      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1135,
          "speaker_name": "ステイゴールド",
          "text": "そりゃ、いるさ。 ここは私の部屋でもあるんだからな。",
          "flag_type": "",
          "flag": "",
