      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "マルゼンにも、ルドルフにも！ お前のできる精一杯で、喰らいついていってただろうが！",
