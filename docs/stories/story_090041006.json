      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "けど、アルダン！ 今ここにいんのは――ずっと頑張ってきたのは、お前だろ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
