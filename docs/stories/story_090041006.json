      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 93998,
          "speaker_name": "観客たち",
          "text": "――さすがはメジロラモーヌ。なんて美しさだ。 ――ええ、舞う姿もひときわ輝く宝玉のよう……。",
          "flag_type": "",
          "flag": "",
          "choices": []
