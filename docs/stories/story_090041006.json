      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "あたし、ちゃんと見てたぞ。 時間を見つけちゃ、コツコツ努力を積み上げてってさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
