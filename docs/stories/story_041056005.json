      "frame_id": 15,
      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
