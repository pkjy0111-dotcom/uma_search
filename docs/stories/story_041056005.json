      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "まま、待ってください！\r 助けてくださいよ～！",
