      "background_id": 17,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "って、ぎゃあああああ！　なに、しれっと罰当たりなこと\r 言ってるんですか！？　それを捨てるなんて、とんでもない！",
          "flag_type": "",
          "flag": "",
          "choices": []
