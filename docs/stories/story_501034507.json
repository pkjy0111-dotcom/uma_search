        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "イナリワンにファンレターが届いたので、 さっそく本人に渡した。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
