      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90731,
          "speaker_name": "品のある女性",
          "text": "ウマ娘として残した輝かしい歴史……一族の栄華。 ですから、ルビーさん。わたくしの言いたいこと、わかるでしょう？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
