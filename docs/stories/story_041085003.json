      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "…………。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
