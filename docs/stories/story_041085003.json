      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "どれも香り高く、 皆様に相応しい、気品溢れる味わいとなっています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
