      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "お招きいただき、感謝いたしますわ。 紅茶もさることながら、ティーフーズも素晴らしい色どりですわね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
