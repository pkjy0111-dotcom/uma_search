      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 90731,
          "speaker_name": "品のある女性",
          "text": "――まあ、そこに気づいたのね。嬉しいわ。 あれはまさに、貴方の御祖母様とお母様のことなのよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
