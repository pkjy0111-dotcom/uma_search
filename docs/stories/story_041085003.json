    "9": {
      "frame_id": 9,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "このお茶会はダイイチルビーが主催だ。 その場合、自分で注ぐのは……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "け、結構なお点前で……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……この言い方も間違っている気がするが、 自分で注ぐという行為は、確実にマナー違反だったはず。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "……ありがとう存じます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 37,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう言って、ダイイチルビーはお茶のおかわりを注いでくれる。 ……ホッとしたのも束の間――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 126,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90777,
          "speaker_name": "ルビーの執事",
          "text": "お嬢様がた、会食のお時間です。 迎えの車は、あちらに。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "か、会食？",
