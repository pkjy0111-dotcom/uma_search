      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "はい。次の試験会場でもあります。 ……では、参りましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
