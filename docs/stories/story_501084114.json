      "background_id": 74,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "――そう、もっとも警戒すべきは奴だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
