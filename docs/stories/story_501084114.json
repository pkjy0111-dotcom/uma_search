      "frame_id": 32,
      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 90001,
          "speaker_name": "",
          "text": "――その後、流れでタニノギムレットに 自分がまとめている同期のウマ娘たちの 調査記録を見てもらうことになった。",
          "flag_type": "",
          "flag": "",
          "choices": []
