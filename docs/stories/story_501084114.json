      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "オマエも引き続き、奴をよく観測することだ。 我が筋書を確たるものにするべくな。",
