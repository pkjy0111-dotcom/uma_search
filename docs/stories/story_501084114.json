      "background_id": 1,
      "sub_background_id": 210,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "ククッ。今日までの期間で、 これほどの調査記録をまとめるとは。 なかなかに狂気的だ……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 34,
              "loop_ends_at_frame": -1,
              "text": "……君のトレーナーだからね",
              "flag_type": "",
              "flag": "male_trainer"
