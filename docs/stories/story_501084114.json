      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "海外からの留学生である彼女は、 デビュー戦でもその静けさが際立っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
