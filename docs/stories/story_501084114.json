      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "不壊の志と、不変の歩調。 そこから成る絶対的な佇まいは、 厳しき自然の無慈悲さにも似ている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
