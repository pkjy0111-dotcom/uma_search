    "20": {
      "frame_id": 20,
      "background_id": 74,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "オマエも感じたのだろう？ クリスエスの冷厳なる魂の波を。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
