    "14": {
      "frame_id": 14,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "もちろん、ただ冷静なウマ娘というだけなら、 他にもたくさんいるだろう。 だが、シンボリクリスエスは――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
