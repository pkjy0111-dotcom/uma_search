      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 39,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その後、タニノギムレット視点での 同期ウマ娘たちの情報を新たに加え、 データの精度を更に高めることができた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
