      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "珍しい浮かれようのシーザリオを見て、ふと思いつく。 これは、彼女をより深く知るチャンスではないだろうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "あのさ、チケットってまだ買えるのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "あのさ、チケットってまだ買えるのかな？",
