      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "はい。ずっと観たかった公演で…… わくわくし過ぎて、昨日はなかなか寝付けなかったんですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
