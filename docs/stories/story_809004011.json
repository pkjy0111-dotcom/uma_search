    "8": {
      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "だからこそ……これまではいかに周囲が\r 持ち上げてくださっても、私の未熟な本質を 見抜き、精進せよとのみ仰っていたのですが。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "認めてもらえたんですね！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 39,
