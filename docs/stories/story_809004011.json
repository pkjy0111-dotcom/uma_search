      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "来てくださってありがとうございます、\r <username>トレーナー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 39,
