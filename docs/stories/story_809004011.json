      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "はい……！　ほんの少し、ですが……\r 名に恥じぬ姿を、見せられたように思います。",
