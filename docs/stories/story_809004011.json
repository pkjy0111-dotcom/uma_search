      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "大おば様はURAでも重役を務めてらっしゃる、\r 桐生院家の誇る敏腕トレーナーだったお方。",
          "flag_type": "",
          "flag": "",
          "choices": []
