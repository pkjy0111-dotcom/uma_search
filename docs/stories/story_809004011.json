      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "急なお声がけになってしまってすみません。\r ただ……どうしてもお伝えしたい話があって。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "話、ですか？",
              "flag_type": "",
              "flag": ""
