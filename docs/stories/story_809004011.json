      "frame_id": 6,
      "background_id": 39,
      "sub_background_id": 9111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "『よい指導者の顔になった』……と。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
