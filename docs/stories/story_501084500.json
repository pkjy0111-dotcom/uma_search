      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "言われた通りにやってたら、 こんな強くなったんすから！ やっぱ、ギム先輩はスゲーやっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
