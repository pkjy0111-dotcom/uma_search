      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "へ～！　そいつスゲーんすね！？ ギム先輩の……舎弟？　いや、ダチっすか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
