      "frame_id": 10,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 0,
          "speaker_name": "困っている青年",
          "text": "追いかけっこが好きで、 近づいては逃げての繰り返しなんです。 俺の足じゃ追いつけなくて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
