      "frame_id": 11,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "わっ、かわいいですね……。 もこもこしていて…… うわぁ、尻尾ごとお尻が動いちゃってる……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
