    "8": {
      "frame_id": 8,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 0,
          "speaker_name": "困っている青年",
          "text": "すみません、すみません、すみません！！ でもあの、今回は……落としたというか、 逃げられてしまって……こ、子犬に。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "……もしかして、あの子？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
