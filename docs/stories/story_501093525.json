      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ケイエスミラクルに言われ 視線を動かすとそこには、 何かを捜す青年が1人――",
          "flag_type": "",
          "flag": "",
          "choices": []
