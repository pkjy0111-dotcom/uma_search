      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……あ、あの、トレーナーさん！ ここはおれの出番……ですよね？ おれなら、追いつけると思うし。",
          "flag_type": "",
          "flag": "",
          "choices": []
