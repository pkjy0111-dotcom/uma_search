    },
    "9": {
      "frame_id": 9,
      "background_id": 119,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "少しだけ離れた場所から草の音がした。 その物陰で、子犬が尻尾を振っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
