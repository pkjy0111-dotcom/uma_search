      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "えっと……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "お説教からでいい？",
              "flag_type": "",
              "flag": ""
