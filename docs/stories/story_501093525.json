      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "落とし物……でしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 33,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "おれ、捜すの手伝ってきます！ あの辺りは土もぬかるんでいますし、 トレーナーさんはここで待ってて――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "2人で手伝った方が早いよ",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 33,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……そうですね、わかりました。 それじゃ、一緒に行きましょう！ 足元には、充分気をつけてくださいね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
