      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "もちろん――花だけでは、ありません。 ラッピングも、香りも、 様々な組み合わせが――",
          "flag_type": "",
          "flag": "",
          "choices": []
