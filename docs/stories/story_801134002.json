      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "花の――ブーケの、写真集です。",
          "flag_type": "",
          "flag": "",
