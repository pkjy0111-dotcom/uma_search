    "4": {
      "frame_id": 4,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "とても、勉強になるんです。 私も――ブーケを作るのが、 大好きなので。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
