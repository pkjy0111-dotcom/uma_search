    "5": {
      "frame_id": 5,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "贈りたい相手、状況―― 用途によって、選ぶ花も変わります。",
