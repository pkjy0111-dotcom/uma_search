      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "……諦めるのはまだ早いわ。お客さんが少ないということは、 この味を知らない人が多いということでしょう？",
          "flag_type": "",
          "flag": "",
          "choices": []
