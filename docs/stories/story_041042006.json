      "background_id": 191,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "ここは私に任せてちょうだい！ 必ずお店を復活させると約束するわ！",
