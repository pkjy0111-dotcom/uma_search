      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90720,
          "speaker_name": "弁当屋の店主",
          "text": "ごめんよ、パールちゃん……。 私もできれば続けたいけど……もう限界でね。",
