      "frame_id": 9,
      "background_id": 191,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1042,
          "speaker_name": "シーキングザパール",
          "text": "広めるのよ！　ブリリアントなお弁当の魅力を！！",
