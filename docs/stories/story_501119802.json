          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "いや、歩いてるだけで楽しくてさ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ううん、歩いてるだけで楽しくてね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "3": {
