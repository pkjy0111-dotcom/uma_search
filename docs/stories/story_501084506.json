      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "いつの間にか軒下から出ていた タニノギムレットは、 雨に打たれながら平然と言葉を続ける。",
          "flag_type": "",
          "flag": "",
          "choices": []
