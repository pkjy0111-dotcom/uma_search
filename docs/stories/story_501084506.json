      "frame_id": 5,
      "background_id": 30,
      "sub_background_id": 411,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "そんなもの――些末な事！！ 雨の冷たさを凌駕する情熱があればッ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
