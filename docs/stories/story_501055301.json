      "frame_id": 5,
      "background_id": 178,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "となれば目的に相応しい併走相手は、今現在 シニア級の中心になっているような子たち ということになるのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
