      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "はっきり言って、今のマーベラスサンデーは トゥインクル・シリーズを駆ける子たちの 中でも、相当の実力者だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
