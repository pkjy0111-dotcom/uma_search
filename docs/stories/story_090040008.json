      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "！　はい……よろしく、お願いしますっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
