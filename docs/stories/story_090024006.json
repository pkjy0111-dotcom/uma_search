      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "でも、誰よりも負けたくないウマ娘です……！",
          "flag_type": "",
          "flag": "",
          "choices": []
