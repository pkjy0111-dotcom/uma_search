    "8": {
      "frame_id": 8,
      "background_id": 17,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "……なったとしても！　やっぱり、諦めきれないんです！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "どこかに勝ち筋がないかって、きっと探します。 足りない部分を埋めたら結果が変わるかもと、努力してしまいます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "何度も落ち込んで、そのくせにもがいて。 普通のくせに往生際が悪い私ですけど――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
