      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "テイオーさんに言われました。誰かのコピーをしても勝てないって。 ……納得しました。コピーはオリジナルにはかなわない。",
          "flag_type": "",
          "flag": "",
          "choices": []
