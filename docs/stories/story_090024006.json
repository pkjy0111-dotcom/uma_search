    "7": {
      "frame_id": 7,
      "background_id": 17,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "けれど誰でもない素の状態の私が、天才や秀才に勝てるわけがない。 どうあがいたって無理、そういう結論に――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
