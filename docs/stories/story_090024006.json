      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "それどころか取り柄のなさに、自分で自分から目を背ける 情けないところまであって。そんな普通でしょうもない――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
