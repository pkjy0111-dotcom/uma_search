    "11": {
      "frame_id": 11,
      "background_id": 17,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "――私、この負けたくない気持ちだけは、誰にも負けませんから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 17,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "真似っ子をしても勝てないなら、真似っ子をやめます！ 私が、私として勝てるように！　一から可能性を探ります！",
          "flag_type": "",
          "flag": "",
          "choices": []
