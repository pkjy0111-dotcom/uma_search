      "frame_id": 13,
      "background_id": 123,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90774,
          "speaker_name": "ヤエノの姉弟子",
          "text": "へッ。己の道を見失った奴の背中なんて 追わせてどうなるってんだ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
