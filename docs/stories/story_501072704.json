      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90774,
          "speaker_name": "ヤエノの姉弟子",
          "text": "……お前もいつか、私の気持ちが わかるかもしれねぇ。レースの道を 極めたとして、結局は同じ虚無へ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
