    "18": {
      "frame_id": 18,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 0,
          "speaker_name": "ウマ娘の女の子",
          "text": "……ううん。 ありがとう、私もごめんね。おまわりさん。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "19": {
      "frame_id": 19,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "これにて事件解決！ トレーナーさん、ご協力感謝いたします！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
              "loop_ends_at_frame": -1,
              "text": "気持ちが伝わってよかったね",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "名演だったね、おまわりさん！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "20": {
      "frame_id": 20,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "はい。この顔に怯えられることも ありますが……子どもたちの 助けになりたい気持ちは、本物ですから！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "頑張って、応援しているよ！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "21": {
      "frame_id": 21,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ありがとうございます！　では早速、 治安維持のパトロールに行って参ります！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "22": {
      "frame_id": 22,
      "background_id": 31,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ありがとうございます！ 子どもに親しみを持ってもらうため、 この手は今後も活用できそうですね！",
          "flag_type": "",
          "flag": "",
          "choices": []
