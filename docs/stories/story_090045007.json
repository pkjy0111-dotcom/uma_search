    "10": {
      "frame_id": 10,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ふふ。私たち、なんだか難しく考えすぎちゃってたけど…… ラモーヌさんの言葉って、結構そのままの意味だったんですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1112,
          "speaker_name": "デアリングハート",
          "text": "『愛』を誓う――言葉通り、ラモーヌさんは今度の誓いの儀式で、 ただ自分の『レース愛』を誓うつもりなんだわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
