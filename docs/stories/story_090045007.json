      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1112,
          "speaker_name": "デアリングハート",
          "text": "まさしく恐怖映像よ。――あれだけやっても笑っていられるくらい、 ラモーヌさんにとっては軽いメニューなのかって、戦慄したわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
