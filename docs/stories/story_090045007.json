    "9": {
      "frame_id": 9,
      "background_id": 181,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1112,
          "speaker_name": "デアリングハート",
          "text": "そしてこの『愛』こそが、なによりの原動力―― 私たちを強くしてくれるものなんだわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
