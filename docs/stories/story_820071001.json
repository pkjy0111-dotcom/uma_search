      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "『ご、ごめんなさい、ぶつかってしまって！ ……私、転校してきたばかりで、 右も左も、わからなくって……』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
