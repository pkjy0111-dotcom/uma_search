    "4": {
      "frame_id": 4,
      "background_id": 52,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "『……よければ、案内してくれる？ この素敵な……トレセン学園をっ！』",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "…………前から、在籍していた気が……",
