      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "待機時間を含めて10分以上かかっている。 これだと数千……数万人のファンを掴むには 途方もない労力がかかるわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
