      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1103,
          "speaker_name": "ロイスアンドロイス",
          "text": "………………。 『突然の邂逅パターン』、終了。効果は微量。",
