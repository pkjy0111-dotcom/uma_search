      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90640,
          "speaker_name": "リポーター",
          "text": "おお～！　これは美味しそうですね！ ぜひご堪能下さい！",
