    },
    "5": {
      "frame_id": 5,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90640,
          "speaker_name": "リポーター",
          "text": "なるほど聖地巡礼ですか～！ すごいグッズの数ですねぇ！　――あ！ そちらの方もお伺いしてよろしいですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
