      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90640,
          "speaker_name": "リポーター",
          "text": "『タルマエブーム』によって、 ここ苫小牧市には 今日も多くの観光客が訪れています……！",
          "flag_type": "",
          "flag": "",
          "choices": []
