      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90350,
          "speaker_name": "タルマエのファンA",
          "text": "東京から来ました！ タルマエちゃんの故郷が見たくて……！ 聖地巡礼、みたいな感じです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
