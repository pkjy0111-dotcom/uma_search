      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90640,
          "speaker_name": "リポーター",
          "text": "え～このように、たくさんの方が ホッコータルマエさんのふるさとを 見たくて訪れているといった感じです！！",
