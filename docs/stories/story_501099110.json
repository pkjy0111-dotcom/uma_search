      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90360,
          "speaker_name": "タルマエのファンB",
          "text": "札幌から来ました。 今日はこれから港で海鮮丼食べるつもりです。 ウマスタでおススメされて気になってて！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
