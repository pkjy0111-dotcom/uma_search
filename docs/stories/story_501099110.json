      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90640,
          "speaker_name": "リポーター",
          "text": "早速ちょっとお話を伺ってみましょうか。 ――すみません、今日はどちらから？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
