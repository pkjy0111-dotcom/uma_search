      "frame_id": 9,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "そんな母様が、3時間も日本にいる。 ……久方ぶりの機会だ。",
