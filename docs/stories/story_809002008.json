      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9002,
          "speaker_name": "秋川理事長",
          "text": "今回、乗り継ぎの関係で 3時間ほど日本に滞在できるらしいのだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "えっ、たった3時間ですか！？",
              "flag_type": "",
              "flag": ""
