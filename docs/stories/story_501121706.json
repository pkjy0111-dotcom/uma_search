      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "ニャンコの声を聞きつけ、学園より参上！ その猫ちゃん、私が守護しよう！",
