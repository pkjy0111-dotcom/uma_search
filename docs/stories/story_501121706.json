      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1121,
          "speaker_name": "デュランダル",
          "text": "弱き者を守るのも騎士の務め。 見事、あの猫の守り役を 務めてみせましょう。",
          "flag_type": "",
          "flag": "",
          "choices": []
