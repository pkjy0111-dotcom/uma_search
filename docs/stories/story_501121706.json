      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "待ちなさい！　止まれという意味ではなく、 守り役について待ちなさいと言いました！",
