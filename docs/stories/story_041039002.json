      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "やだ、まさか、理想が高いなんて言いませんわよね？\r そんなの、冗談ポイポイですわよ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
