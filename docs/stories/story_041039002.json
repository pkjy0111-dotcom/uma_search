      "dialogue": [
        {
          "next_frame": 2,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "よし、ここの掲示板はこれでおしまい！\r さあ、じゃんじゃん参りますわよ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
