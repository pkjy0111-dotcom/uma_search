      "frame_id": 18,
      "background_id": 8,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "だって夢は自分で決めるもの、\r そして自力で勝ち取るものでしょう？",
