      "frame_id": 26,
      "background_id": 206,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "次の予定――夏祭りが始まるまで、まだ時間はあろう？ どれ。それまで、武将たちの格言を紹介してやろうぞ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "27": {
      "frame_id": 27,
      "background_id": 43,
      "sub_background_id": 2131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "にゃーはっはっは！　ワシの勝利じゃ！ ほれ、りんご飴を献上せい！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 28,
              "loop_ends_at_frame": -1,
              "text": "くっ……君は、射的が上手いんだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 28,
              "loop_ends_at_frame": -1,
              "text": "くっ……貴方は、射的が上手いんだね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "28": {
      "frame_id": 28,
      "background_id": 43,
      "sub_background_id": 2131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "当然よ！　幼少のみぎりより、火縄銃の模造品で 遊んでおったからのう。得意も得意、大得意じゃ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 29,
              "loop_ends_at_frame": -1,
              "text": "なるほど、そうだったのか……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 29,
              "loop_ends_at_frame": -1,
              "text": "なるほど、そうだったんだ……",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "29": {
      "frame_id": 29,
      "background_id": 43,
      "sub_background_id": 2131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "くく……情報戦に敗れれば、敗走は必定。 結果は戦の前に出ておったということじゃ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
