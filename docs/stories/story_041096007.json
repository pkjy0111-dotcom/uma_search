      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "さて。祭りも堪能したし、 そろそろ戻るとするかのう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
