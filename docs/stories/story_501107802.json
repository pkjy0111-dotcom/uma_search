      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "一体なんだってんだ？ この辺の花屋が軒並み空っぽなんて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
