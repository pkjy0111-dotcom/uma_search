      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "……ジャーニー。後ろのデカい車と…… その中の花は、まさか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 29,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "これはこれは……なるほど。 どうやら、申し訳ないことをしたようです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 29,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "ですが、ご容赦を。 なにぶん多くの花が必要になるものでして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 29,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1107,
          "speaker_name": "タップダンスシチー",
          "text": "へえ！　こっちは花束ひとつで十分なんだが、 知人のよしみで少し分けちゃくれないか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 29,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "お応えできず心苦しい限りですが、 これは以前から計画していたことでして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 29,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "店舗には事前に相談もさせていただきました。 そのうえで、すべて売り切れるのは こちらも予想外でしたが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
