      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1119,
          "speaker_name": "？？？",
          "text": "ご手配いただき、ありがとうございました。 では、失礼いたしますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
