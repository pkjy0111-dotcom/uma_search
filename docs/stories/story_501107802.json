      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 0,
          "speaker_name": "傍らに控えるウマ娘",
          "text": "ジャーニー様、そろそろ学園へ……。 花々のセットが間に合いません。",
