      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90103,
          "speaker_name": "オルフェの臣下A",
          "text": "いえ、負担など！　我々の尽くしたいという 望みを、我が王にお許し頂いているのです！",
