      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90103,
          "speaker_name": "オルフェの臣下A",
          "text": "私どもでスケジュールを照らし合わせ、 きちんと当番制にしておりますし。 ですからどうか、お手伝いさせてください。",
          "flag_type": "",
          "flag": "",
          "choices": []
