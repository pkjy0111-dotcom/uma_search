    "13": {
      "frame_id": 13,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "故にこそ、所用でオルフェーヴルの戻りが トレーニング直前になる日でも、それまでに 準備を整えておくことができていたのだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
