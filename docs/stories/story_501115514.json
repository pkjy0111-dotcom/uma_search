    "12": {
      "frame_id": 12,
      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "――このように、オルフェーヴルの場合は 臣下を名乗る生徒たちが常に2人以上随伴し、 率先して雑務を買って出てくれていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
