      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "勇気を与えられるトウカイテイオーの 凄さを改めて知り、ミホノブルボンは ますますトレーニングに励んだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
