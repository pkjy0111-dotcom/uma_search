      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "心配は無用だった。へこたれることなく 猛然と訓練する様を見て、逆に勇気をもらう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
