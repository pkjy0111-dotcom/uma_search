      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "ああ。みんなからのメッセージが\r 書かれているんだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
