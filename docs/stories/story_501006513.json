      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "改めて彼女が愛されていることを感じながら、\r オグリキャップにスカーフを返した。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "故郷の人たちから？",
              "flag_type": "",
              "flag": ""
