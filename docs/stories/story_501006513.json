      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "うまく説明はできないが……\r みんなが私を応援し、願いを託して くれているということだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
