      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "……なんだろう、とても胸が熱い。\r 1人ではない、そんな気がする。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "プレッシャーを感じるか？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
