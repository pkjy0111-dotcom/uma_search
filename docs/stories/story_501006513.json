      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "『いつも近くで応援してる』『一緒に走るよ』\r 『オグリはアタシたちの夢だから』と。",
          "flag_type": "",
          "flag": "",
          "choices": []
