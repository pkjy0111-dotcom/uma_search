      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "夢……これは、どういう意味なんだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
