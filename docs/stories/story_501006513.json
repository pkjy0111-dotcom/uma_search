      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "私が走ることで、喜んでもらえるのは\r わかっているが……私が夢というのは？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
