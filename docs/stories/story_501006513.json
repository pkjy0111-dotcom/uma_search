      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "憧れ、願い、様々な想いを形にしてくれる、\r そう信じているんじゃないか――と、 オグリキャップに伝えた。",
          "flag_type": "",
          "flag": "",
          "choices": []
