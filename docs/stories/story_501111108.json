      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "参加競技の志望をクラス内で募った際、 かなり偏りが出てしまいまして。",
