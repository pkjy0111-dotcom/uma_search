      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ファン感謝祭において、ウマ娘たちは 『必ず1つ以上の催事に参加する』 ということになっている。",
          "flag_type": "",
          "flag": "",
          "choices": []
