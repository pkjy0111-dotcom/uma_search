      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "スタンドに立てかけたタブレットで、 まとめておいた作業工程を改めて見返す。 ちょっと気が遠くなる作業量ではあるが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "いや、頑張ろう！メサイアのためにも……！",
