      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "特に自身での催事提案などは行わず、 学園側から提示されたいずれかの催事に 参加する手はずになっていたが――",
          "flag_type": "",
          "flag": "",
          "choices": []
