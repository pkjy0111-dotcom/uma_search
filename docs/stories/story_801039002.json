      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "さ、私を捕まえてごらんなさい！",
          "flag_type": "",
          "flag": "",
          "choices": []
