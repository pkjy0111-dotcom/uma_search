      "frame_id": 19,
      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "10まで数えた彼女は、猛然と子どもたちを\r 追い回し、1人また1人とひっ捕まえていく！",
          "flag_type": "",
          "flag": "",
          "choices": []
