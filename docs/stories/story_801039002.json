      "frame_id": 17,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "姫たるもの、王子様をとっ捕まえて\r ナンボですわね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "18": {
      "frame_id": 18,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "そうと決まれば一網打尽にして差し上げます。\r では数えますわよ。いーち、にーい――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
