        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "",
          "text": "休憩中。公園でたくさんの子どもたちに\r 追いかけられるウマ娘を見かけた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
