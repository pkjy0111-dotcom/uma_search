      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『触らなくても、影を踏んだら勝ち』という\r ルール追加を提案してみた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
