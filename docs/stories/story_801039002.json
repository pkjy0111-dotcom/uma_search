      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "あら……せめて姫の影に触れたいという\r 奥ゆかしさ！　素敵ですわね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
