      "frame_id": 22,
      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "いいでしょう。いつかはと姫を追い続ける\r 騎士のストーリーもあり寄りのありですわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
