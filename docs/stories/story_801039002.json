    "2": {
      "frame_id": 2,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "おーっほっほ！　私に追いつこうなんて\r 百億年早いですわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90215,
          "speaker_name": "男の子A",
          "text": "くそ～～、姫のねえちゃんもう1回！\r 次は負けね～！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90216,
          "speaker_name": "男の子B",
          "text": "今度は挟み撃ちにしようぜ。\r ぜったい捕まえてやるからな！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "もちろん、姫としてお相手しますわよ。\r 追いつけるものなら追いついてごらんなさい！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90215,
          "speaker_name": "男の子A",
          "text": "待て～～っ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "1対多数の変則鬼ごっこ。だが彼女は\r ブランコを飛び越え、ジャングルジムに 登りと縦横無尽だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "……はぁっ、はぁっ。\r ふふん、貴方たちまだまだですわね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90215,
          "speaker_name": "男の子A",
          "text": "ずっりぃよ～。姫のねえちゃん\r 年上なんだから、もっと手加減しろよな！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "姫たるもの、いかなる時も手は抜きません。\r 悔しければもっと頭を使わないと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "知恵と勇気。そして“剛腕”があれば！\r 世の中に解決できない問題はありませんのよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "そうでしょう？　そこのトレーナーさん！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "お、俺？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "わ、私？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "そうです、走りの指導がご専門の貴方！\r さ、子どもたちにアドバイスですわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そうは言っても、ウマ娘と走りで\r 勝負するのはあまりに無謀なので――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "次は鬼を交代！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 20,
              "loop_ends_at_frame": -1,
              "text": "もっとハンデをつけてあげたら？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "子どもたち全員で逃げれば、いくら彼女でも\r もう少し手こずるだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
