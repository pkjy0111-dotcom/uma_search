    },
    "16": {
      "frame_id": 16,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "私、姫ですので鬼と呼ばれるのは\r ちょっと……。でも――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
