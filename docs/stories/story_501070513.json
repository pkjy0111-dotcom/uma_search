      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "クク。無知は無力だろう、子犬？ だが……。",
