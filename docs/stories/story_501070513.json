    "9": {
      "frame_id": 9,
      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "んでもって、古臭いのはごめんだ。 スラックスは『ノータック』かつ 『ローライズ』で、フォルムをシンプルに。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
