      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "無知なりに足掻いたのは評価してやる。 ――特別レッスンだ。",
