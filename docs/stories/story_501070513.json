      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "何故？　ハッ！ 吊るし売りのスーツで私の隣に立つ気か？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "で、モデルはどうする。 ブリティッシュか、クラシコか…… 作るのはスリーピースだ、遊び放題だな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "く、くらし……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "わからない……スーツに関しては門外漢だ。 だがシリウスシンボリのトレーナーとして、 選択権は放棄したくない――ならば。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……君の勝負服を参考にしたい！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……貴方の勝負服を参考にしたい！",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女の勝負服もスーツに近い。 なにかヒントを得られるのではないだろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
