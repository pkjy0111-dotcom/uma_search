      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "ぴーんとですね……！ ちょっとあっちから歩いてくるンで 見てくださいっ！",
