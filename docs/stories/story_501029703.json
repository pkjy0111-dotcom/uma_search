      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ドラマの撮影を見て、俳優たちの放つ輝きに 魅せられたユキノビジン。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
