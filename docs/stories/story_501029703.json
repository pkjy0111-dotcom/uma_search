    "6": {
      "frame_id": 6,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "シチーさんにお願いして、 ちゃンと演技できるように、 特訓してもらうことになったンです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "やれることはなンでもやって、 本番でかっけぇ演技できるように なりたいですから……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "トレーナーとしては、ぜひとも見守りたい。 気合いみなぎるユキノビジンに頼み、 特訓を見学させてもらうことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
