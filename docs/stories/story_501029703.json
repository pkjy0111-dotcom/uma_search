      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "俳優さんのですっ！ ちょっとだけとはいえ、 あたしもその仲間入りするンで！",
