    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "勇気を出して端役のオーディションに 応募した彼女は、先日見事、 合格を果たしたのだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
