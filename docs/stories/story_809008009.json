      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "これからは、手を取ってもらうんじゃない。\r 肩を並べて一緒に走るんだって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
