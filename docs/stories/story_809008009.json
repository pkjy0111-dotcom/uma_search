    "26": {
      "frame_id": 26,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "あの人に出会わないままだったら、私……\r 夢を諦めてしまっていたかもしれません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
