      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "ふぅ～、ごちそうさまでした♪",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "あれ、ライトハローさん？",
