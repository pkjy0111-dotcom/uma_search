    "4": {
      "frame_id": 4,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "実はこれから大仕事があって、\r おいしいお料理で気合いを入れて来たんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ますますの活躍ぶりですね",
              "flag_type": "",
              "flag": ""
            }
          ]
