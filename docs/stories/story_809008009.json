    "22": {
      "frame_id": 22,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "ありがとうございます。……でもこれは、\r 私1人で見つけたものではなくて――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
