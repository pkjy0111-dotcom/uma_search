      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "だから、決めたんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
