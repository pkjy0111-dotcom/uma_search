      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "でも絶対に実績を積んで、追いついてみせる。\r それが私の目標で――理想なんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
