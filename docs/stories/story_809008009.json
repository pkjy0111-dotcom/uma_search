    "3": {
      "frame_id": 3,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "あ！　トレーナーさん！\r まさかこのタイミングでお会いできるなんて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
