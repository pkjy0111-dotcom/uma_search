    },
    "5": {
      "frame_id": 5,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "いえいえ、そんな！\r まだ目の前のことで精一杯で。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 29,
      "sub_background_id": 111,
