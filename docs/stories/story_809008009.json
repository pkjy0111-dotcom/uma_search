      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 9008,
          "speaker_name": "ライトハロー",
          "text": "まぁ……その間にその人、ますます活躍して\r 遠のくばかりなんですが。あはは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
