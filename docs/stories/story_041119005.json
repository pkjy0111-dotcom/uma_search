      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "こちらを気遣う言葉と声色。本当なら、 言葉のままに受け入れたいのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
