      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "問題があれば、私にも共有を。共に考えましょう。 私は、貴方の努力に報いたいと考えています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
