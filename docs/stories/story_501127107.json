      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな彼女の話を聞き、 ひとりよりふたりのほうがよいだろう ということで、付き合っていたのだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
