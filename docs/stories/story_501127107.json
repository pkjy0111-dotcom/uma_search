      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "門限破り、壁の損壊、果てにはDJ機材を 持ち込み夜通しズンチャカなど、過去に取り 締まった事態が再び起こらぬとは限りません！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "私は皆さんを信じております！ が、絶対はない！ 事件が起こってからでは遅いのです！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
