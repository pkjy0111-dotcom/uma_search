      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "よって当日は、 パトロールを申し出るつもりであります！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
