      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "楽しいイベントでこそ、 気のゆるみは起こり得るもの！",
          "flag_type": "",
          "flag": "",
          "choices": []
