      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ボス！ 現在、異常なしであります！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "お疲れ様、こちらも異常はないよ",
