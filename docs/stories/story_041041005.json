    "6": {
      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "ご安心を！　委員長たるもの、レースだけでなく\r トレセンフードバトルでもバク！　シン！　優！　勝！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
