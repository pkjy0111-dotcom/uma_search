      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "トレーナーさんに恥をかかせるような結果には致しません！！",
          "flag_type": "",
          "flag": "",
          "choices": []
