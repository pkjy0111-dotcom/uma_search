      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "であれば、悪くないタイミングです！\r ちょうど練習を開始するところでしたから！",
          "flag_type": "",
          "flag": "",
          "choices": []
