      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "規定のメニューは3種類！\r 大盛りオムライス、爆盛り唐揚げ、メガ盛りサラダ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
