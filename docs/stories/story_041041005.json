      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "サクラバクシンオーの話から推測するに――\r 学園内でフードバトルがあり、その練習をするようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
