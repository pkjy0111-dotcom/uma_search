    "5": {
      "frame_id": 5,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "せっかくですから、そこで私の勇姿を見ていてください！\r 準備はいいですか！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "えっ、えっ？",
              "flag_type": "",
              "flag": ""
