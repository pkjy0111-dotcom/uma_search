      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1041,
          "speaker_name": "サクラバクシンオー",
          "text": "その次は大盛りオムライス！\r お肉を食べたらご飯を食べるのが定番です！",
