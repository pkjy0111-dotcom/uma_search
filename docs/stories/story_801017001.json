      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ふむ、なるほど。不安を払拭する意味でも、\r しっかり走っておきたいということか……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
