      "frame_id": 15,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "正攻法ではないかもしれないが……\r 今の君たちには最適な調整だろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
