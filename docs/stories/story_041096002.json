      "background_id": 275,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "いずれも、天晴な生き方じゃ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "……ん？卑怯な手を使っていても……？",
              "flag_type": "",
              "flag": ""
