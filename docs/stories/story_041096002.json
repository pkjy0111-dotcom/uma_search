    },
    "5": {
      "frame_id": 5,
      "background_id": 275,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "やはり武将はよいのぉ……今日の祭事で、武将魂に触れた思いじゃ。 日々、その生き様を手本としていかねばな！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "手本？武将の生き様を……？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
