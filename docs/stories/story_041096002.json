    },
    "7": {
      "frame_id": 7,
      "background_id": 285,
      "sub_background_id": 113,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1096,
          "speaker_name": "ノーリーズン",
          "text": "勝利のために情を捨てた者、忠臣として信義に殉じた者。 強者に取り入って、上手く出世した者もおるのう。",
          "flag_type": "",
          "flag": "",
          "choices": []
