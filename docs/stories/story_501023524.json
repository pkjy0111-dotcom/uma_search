      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "やはり君にはわかるようだな。\r 見た目以上に髪質が改善されているのが！",
