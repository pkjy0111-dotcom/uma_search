    "5": {
      "frame_id": 5,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "関西に移転したと聞いた時は驚いたよ。\r だが、レースで勝利したこの脚で 私はついにたどり着いた……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "今から暴れ狂う髪の毛と決別してくる。\r ……君はここで待っていてくれ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
