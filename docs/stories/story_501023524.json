      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "お待たせしたね……トレーナー君。",
          "flag_type": "",
          "flag": "",
          "choices": []
