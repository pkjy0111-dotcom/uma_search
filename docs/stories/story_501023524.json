      "background_id": 29,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "いったい、どう生まれ変わるのだろうか？\r ドキドキしながら待っていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
