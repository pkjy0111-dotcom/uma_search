      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 9040,
          "speaker_name": "ダーレーアラビアン",
          "text": "そのウマ娘はさ、最初は強くなかったんだ。 デビュー戦では大きな差をつけられた。",
