    "13": {
      "frame_id": 13,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……よし、今日のメニューは全て終了だな。\r 予定時刻通り、過不足なし。うん、完璧だ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
