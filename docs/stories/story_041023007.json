      "frame_id": 17,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ふっ……君は私のことを、相当なトレーニングバカだと\r 思っているらしいな。まあそれも間違いではないが。",
          "flag_type": "",
          "flag": "",
          "choices": []
