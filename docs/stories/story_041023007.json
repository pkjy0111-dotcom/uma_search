    "18": {
      "frame_id": 18,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "今は字義通り、君の今晩の予定を尋ねたんだ。\r もし何もないのなら、付き合ってほしいところがある。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 19,
              "loop_ends_at_frame": -1,
              "text": "付き合ってほしいところ？",
              "flag_type": "",
              "flag": ""
