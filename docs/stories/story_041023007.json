    "16": {
      "frame_id": 16,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "そうだ、トレーナー君。今晩の予定はどうなっている？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "……追加トレーニングの相談？",
              "flag_type": "",
              "flag": ""
