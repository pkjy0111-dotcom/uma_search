    "15": {
      "frame_id": 15,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……彼女の落ち着いている様子を見ると、自分の心配は\r 杞憂なのかもしれない。そう考えていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
