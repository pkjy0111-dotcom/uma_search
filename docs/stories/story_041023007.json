    "19": {
      "frame_id": 19,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ああ。思えばこの合宿が始まってから、トレーニングの\r 擦り合わせ以外で君と会話をしていないことに気づいてね。",
          "flag_type": "",
          "flag": "",
          "choices": []
