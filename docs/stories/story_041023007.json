      "background_id": 43,
      "sub_background_id": 2131,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "……結局、どれほど完璧な式を組み上げても、\r 自身の力量が追いつかなければ、ただの数字の羅列に過ぎない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
