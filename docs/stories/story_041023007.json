      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ああ……そうらしいな。理論が完璧に仕上がっている分、\r 上手くいかないとどうにも焦って熱くなりすぎる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
