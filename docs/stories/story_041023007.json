    },
    "11": {
      "frame_id": 11,
      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ん？　……よくわからんが、問題はないよ。\r さあ、10分経ったぞ。トレーニングを続けよう。",
          "flag_type": "",
          "flag": "",
          "choices": []
