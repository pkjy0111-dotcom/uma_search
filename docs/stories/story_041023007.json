    "14": {
      "frame_id": 14,
      "background_id": 42,
      "sub_background_id": 2121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ああ、トレーナー君もお疲れ様。\r それでは、合宿所へ戻ろうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
