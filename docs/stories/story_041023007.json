      "background_id": 42,
      "sub_background_id": 2111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その言葉通り、ビワハヤヒデは計画に沿って愚直に\r 体幹トレーニングを続け――",
          "flag_type": "",
          "flag": "",
          "choices": []
