    "6": {
      "frame_id": 6,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "味のみならず、店内のインテリアや置いているケーキの種類、\r 立地や客層など……15項目を立ててチェックしています。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "なので、もちろんケーキは好きですが、\r より正しく言うと『ケーキ屋が好き』ですね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "なるほど……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "この後も、もう1軒見に行く予定です。\r と言ってもそこは、ケーキ屋ではなくカフェなのですが。",
          "flag_type": "",
          "flag": "",
          "choices": []
