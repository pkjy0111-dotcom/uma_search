      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "今日訪ねたケーキ屋の記録をつけていました。\r どこも個性的で素晴らしかったので、忘れないうちに。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ケーキが好きなんだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ケーキが好きなんだね",
              "flag_type": "",
              "flag": "female_trainer"
