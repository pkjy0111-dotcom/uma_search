    "5": {
      "frame_id": 5,
      "background_id": 28,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "……ご存じの通り、実家がケーキ屋なものですから。\r ドイツにいた頃から、研究がてらお店を回るのが趣味なんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
