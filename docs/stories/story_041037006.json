      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1037,
          "speaker_name": "エイシンフラッシュ",
          "text": "本日のメイン、『ムーンカフェ』。トレーナーさんの後ろの\r 2つ目の曲がり角から住宅街を抜け、15分ほどの場所にあります。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "一緒に行ってもいい？",
              "flag_type": "",
              "flag": ""
            }
