      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "普段は少女漫画なんて読みませんから！\r ほら、あたしには似合わないし！　あはは～！",
          "flag_type": "",
          "flag": "",
          "choices": []
