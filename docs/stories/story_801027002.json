      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "しょ、少女漫画なんですけど、\r これはえっと～……そう！ 友だちが薦めてくるから仕方なく！",
          "flag_type": "",
          "flag": "",
          "choices": []
