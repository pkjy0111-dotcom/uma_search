      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "って、うわぁっ！？\r い、いつの間にいたんですか！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "なに読んでたの？",
              "flag_type": "",
              "flag": ""
