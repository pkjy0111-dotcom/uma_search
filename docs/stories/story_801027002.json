      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1027,
          "speaker_name": "メジロライアン",
          "text": "やっぱこの先生の作品はいいなぁ……。\r よしっ、全巻集めて一気読みしよっと！！",
          "flag_type": "",
          "flag": "",
          "choices": []
