      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だがナリタブライアンは意に介していない。 ただ目の前のレースへと意識を向けていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
