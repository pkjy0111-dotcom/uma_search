      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ある日のこと。エアシャカールは突然、 大量の段ボールをトレーナー室に運んできた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "シャ、シャカールさん？これはいったい……",
