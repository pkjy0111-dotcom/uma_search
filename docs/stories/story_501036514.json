      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "身を寄せ合う2人の写真の裏には…… 『せめてどうしたらいいのか教えてほしい。 私の気持ちもわかって』と書かれていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
