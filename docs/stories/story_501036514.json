      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "やる。 いらねェモンは処分しとけ。",
          "flag_type": "",
          "flag": "",
          "choices": []
