    },
    "9": {
      "frame_id": 9,
      "background_id": 68,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "同じトレーナーとして感銘を受けたので、\r 彼女の“仕事”を手伝うことにした。",
          "flag_type": "",
          "flag": "",
          "choices": []
