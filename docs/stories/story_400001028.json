      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 9004,
          "speaker_name": "桐生院トレーナー",
          "text": "お、お疲れ様です。\r まさか、このような場所でお会いするなんて！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "入らないんですか？",
              "flag_type": "",
              "flag": ""
