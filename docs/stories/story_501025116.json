      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "何か思惑があるように語り出した。",
          "flag_type": "",
          "flag": "",
          "choices": []
