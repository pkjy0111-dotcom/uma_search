    "12": {
      "frame_id": 12,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "国内で走る分には問題ない。だがパワーを\r 要求される海外のバ場には合わない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "さらに環境の変化と長時間の旅程。\r 2つの悪条件によって、体重の急増減も 再発しかねない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
