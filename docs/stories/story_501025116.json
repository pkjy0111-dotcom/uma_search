    "11": {
      "frame_id": 11,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "前人未踏の領域にも到達しうる存在。\r が――足の爪は決して強くない。",
          "flag_type": "",
          "flag": "",
          "choices": []
