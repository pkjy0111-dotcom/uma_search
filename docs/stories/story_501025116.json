      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ウマ娘『マンハッタンカフェ』。\r その才能については今更言及するまでもない。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
