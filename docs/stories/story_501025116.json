      "frame_id": 14,
      "background_id": 63,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "条件は以上だ。\r さて、ここを踏まえた上でだ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
