      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "――『学園の許可を頂いて、オグリキャップさんの更なるご活躍を\r 願い、ファンの有志で様々な出し物や料理をご用意しました』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 18,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "料理……！　『次回のレースに向け、英気を養える会になれば\r 嬉しいです。間に合うようでしたら、勝負服もご着用いただき』――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 18,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1006,
          "speaker_name": "オグリキャップ",
          "text": "……。……『当日を楽しみにしております』。\r うん……楽しみだ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
