      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "……あたしの夢は、遠すぎるものだって言われてきた。 届かない、別世界の話だって。",
          "flag_type": "",
          "flag": "",
          "choices": []
