    "7": {
      "frame_id": 7,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ここ数日で、自分が感じてきたことをカツラギエースに伝える。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "",
          "text": "真っ直ぐに夢を追いかけている姿から目を離せなかったこと。 そして何よりも、胸を張り夢を口にするカツラギエースを――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "全力で支えたいと思った",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
