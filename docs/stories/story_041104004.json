      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "はっ、はぁ、はぁ……くっ、はっ……。 もっと、速く……速く、誰よりも前へ行くんだ……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "カツラギエース！",
              "flag_type": "",
              "flag": ""
