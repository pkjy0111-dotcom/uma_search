      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その姿を見ながら、心に決めたことを口にしようと、 カツラギエースのもとへと歩く。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
