      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "認められなかった。認めてやるもんかって思ってた。 もちろん今も……あたしは『別世界』を全力で否定してる。",
          "flag_type": "",
          "flag": "",
          "choices": []
