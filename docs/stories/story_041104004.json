    "4": {
      "frame_id": 4,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "……っ、はっ……あ？ トレーナーさん？　はぁ……はよっ、どうしたんだ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "緊張する胸を押さえ、真っ直ぐにカツラギエースを見つめる。 そして大きく深呼吸をしてから――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "君をスカウトしにきた",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "貴方をスカウトしにきた",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 119,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "おっ……おお……？　どうしたんだよ、いきなり。 その、スカウトって……マジか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
