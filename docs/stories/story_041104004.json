      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ウマ娘界のエース。そこに辿り着くまでには、どれだけの壁があり、 どれだけの道のりを進まなくてはならないのだろう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
