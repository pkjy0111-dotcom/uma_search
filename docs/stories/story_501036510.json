      "frame_id": 19,
      "background_id": 106,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "――時よ止まれ、ボクは美しい！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
