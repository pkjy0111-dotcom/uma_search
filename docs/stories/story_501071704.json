      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "もしかして、どこか公園にでも行って、 リンゴを食べるということだろうか。 そんなことを思いながらついていくと――",
          "flag_type": "",
          "flag": "",
          "choices": []
