      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――意外なことに、メジロアルダンの脚は そのまま学園へ向かい、ついには トレーナー室についてしまった。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "あれ……寄り道は？",
