      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1029,
          "speaker_name": "ユキノビジン",
          "text": "あれ、トレーナーさん？ どうしたンですか、不思議そうな顔して。",
          "flag_type": "",
          "flag": "",
          "choices": []
