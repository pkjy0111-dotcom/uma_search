    "3": {
      "frame_id": 3,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "――あ。こんにちは。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "いえ――人探しでは。実は、その。 種を埋められる場所がないかと―― 探していました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "種？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "はい――花の種、です。 実家から、持ってきたものなのですが―― 陽の当たる場所で、育ててあげたくて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "――けれど。残念ながら、 場所が――見つからないんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "部屋では育てられない？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "できれば――みんなの目に映るような、 そんな場所があれば、と。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "きっと、綺麗な花を咲かせますから――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "夢を持ち、懸命に日々を過ごしている、 多くのウマ娘さんたちの―― 癒しに、なれるような。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "トレーニング中、レース前―― ライバルが、身近にいる状況の中では、 気を張らずには、いられません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 96,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "でも、ほんの少し――相手が誰であれ、 同じ花を見て……ほんのひとときの、 安らぎを得られたら、と。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "その場面を思い浮かべているのか、 穏やかな表情に、柔らかさが増す。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
