      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1134,
          "speaker_name": "カレンブーケドール",
          "text": "もう少し、探してみます。 ――失礼しますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
