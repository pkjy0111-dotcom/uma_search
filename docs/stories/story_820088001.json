      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "息抜きに学園内を散歩していると、 キョロキョロと何かを探している カレンブーケドールを見つけた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "こんにちは。誰か、探してる？",
