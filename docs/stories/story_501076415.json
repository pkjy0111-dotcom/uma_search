      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "サクラローレルは、ナリタブライアンが 進もうとする未来を走りで見せると己に誓う。 それをかなえるため、レースへ赴くのだった。",
          "flag_type": "",
          "flag": "",
