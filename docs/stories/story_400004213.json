      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "贔屓にしている茶葉専門店が、 近くにあってね。 ようするに買い物の帰りさ。",
          "flag_type": "",
          "flag": "",
          "choices": []
