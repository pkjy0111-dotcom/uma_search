      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "君たちにもご馳走したいところだが、 まだ、調合前だからねぇ。",
