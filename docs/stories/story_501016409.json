      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "強豪との出会いを求め出席するか、否か。\r これは彼女の今後を決める重要な選択だ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "強豪たちとの戦いを望み『出席する』",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "まだその段階ではないので『出席しない』",
              "flag_type": "",
              "flag": ""
