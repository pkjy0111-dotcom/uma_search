      "frame_id": 20,
      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "へへっ、そっかそっか！ ……よっしゃ！　じゃあちょっと収穫すんの手伝ってくれるか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
