      "background_id": 57,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "ついでに色々持って帰ってくれよ！　いっぱいあるしよ！ ……今ハサミ持ってくる！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
