      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう言って笑顔で畑を去るカツラギエース。 ……だがその陰に、家族のことを寂し気に話す顔がちらつく。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
