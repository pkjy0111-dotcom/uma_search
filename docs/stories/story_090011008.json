      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "あ、あのぉ～……そろそろ始まるみたいですよ。",
