    "16": {
      "frame_id": 16,
      "background_id": 131,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1007,
          "speaker_name": "ゴールドシップ",
          "text": "おいスぺ、他人事みてぇな顔してんな！　なんだよオマエんとこの\r チーム名は！　少しは捻れ！！　いいか、ネーミングってのは――",
