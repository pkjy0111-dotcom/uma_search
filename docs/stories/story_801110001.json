      "frame_id": 21,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "そうか……大切なのは、 謙虚に学ぶ姿勢を忘れないことなのですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
