      "frame_id": 20,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "書き込み量が私の比じゃない……！ いまだに、これほどの勉強を……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "これを続けていけば、過信はなくなる",
              "flag_type": "",
              "flag": ""
            }
          ]
