      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "なるほど……驕りが大敵なのは、 レースを走る時と一緒ですね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
