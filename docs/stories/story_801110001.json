    "16": {
      "frame_id": 16,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "こちらも道半ばで、語れる言葉は多くない。 それでも、未来の指導者のために なにか一言残してみよう――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "自分を過信しないようにしてる、かな！",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 23,
              "loop_ends_at_frame": -1,
              "text": "自分を律することを心がけている",
              "flag_type": "",
              "flag": ""
            }
