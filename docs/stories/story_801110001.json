      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ただ自信のない言葉では、 教え子は不安になる……案配が難しいな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 19,
