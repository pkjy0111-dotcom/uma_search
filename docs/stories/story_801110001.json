      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "私も、ノートに書き溜め続けます。 貴方に負けないくらい、びっしりと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
