    "15": {
      "frame_id": 15,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "指導者としての考えをぜひお聞かせください。 お願いします！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
