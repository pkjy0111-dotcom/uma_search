    "2": {
      "frame_id": 2,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "――あの。 少々、よろしいでしょうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ん、何かな？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "突然すみません。 貴方がたのトレーニングを拝見し、 お伺いしたいことがありまして。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "<chrname>の偵察！？ いや、しかし堂々としている……。 とりあえず話を聞いてみよう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "答えられる範囲は広くないけど……いいかな",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "はい、もちろんです。 まずは覗き見るような形になり、 失礼いたしました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "トレーニングの姿勢に軌道の修正方法。 目が釘付けになるほどの、学びだったのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "なのでぜひ、ご教示ください――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ウマ娘の指導にあたる際、 どんなことを心がけていらっしゃいますか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "え、教え方のほうを知りたいのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "え、教え方のほうを知りたいの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "はい。将来、指導者を目指している 身として、ぜひ伺いたいのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "日々、このように知見を蓄積していまして――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "『指導要領』と書かれたノートから、 ビッシリ書き込まれた細かい文字が覗けた。 これだけで彼女の本気が伝わってくる……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "私は、私の学びと経験を、 後に続く者に残していきたいのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "そのためには、走るだけでは足りない。 真に伝える言葉を、 指導方法を学びたいのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ウマ娘たちの歴史を紡ぎ、 輝かしい未来を作るために。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
