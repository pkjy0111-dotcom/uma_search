      "frame_id": 19,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90001,
          "speaker_name": "",
          "text": "1冊のノートを取り出し、彼女に見せた。 これは先輩トレーナーから教わったこと、 教本で学んだことを記したノートだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
