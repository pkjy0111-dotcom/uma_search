      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "わあっ、素敵な写真！\r これは……うん、思い出クローバー3つだね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "思い出クローバー？",
