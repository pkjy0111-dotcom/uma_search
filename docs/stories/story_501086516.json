      "frame_id": 16,
      "background_id": 68,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "そう。一切の容赦なく、 肉体を増やし、削り、磨き上げる――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
