      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "新しいトレーニングプランを聞きながら、 メジロラモーヌは適量の食事をとった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
