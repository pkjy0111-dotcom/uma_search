      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1086,
          "speaker_name": "メジロラモーヌ",
          "text": "こちらのお料理、 取り分けていただける？　――2人分に。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
