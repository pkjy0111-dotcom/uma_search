    "20": {
      "frame_id": 20,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "トレーナーさんの最近の推しはッ！？\r あっすみません言い直します最近のって 言うと今までの推しを蔑ろにしている感が！",
          "flag_type": "",
          "flag": "",
          "choices": []
