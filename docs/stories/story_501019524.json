      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ああーッッ！！　残念！！\r 不正解です……。",
          "flag_type": "",
          "flag": "",
