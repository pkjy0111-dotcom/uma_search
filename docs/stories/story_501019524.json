    "18": {
      "frame_id": 18,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "さすがです、トレーナーさん！　お見事也！\r いやぁ、同志として鼻が高いッッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
