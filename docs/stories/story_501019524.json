      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "はわーーーーッッ！\r 大・正・解！　ですッッ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
