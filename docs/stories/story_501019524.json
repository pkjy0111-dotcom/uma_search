      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……突発的クイズの結果、\r アグネスデジタルがすさまじく早口になった。",
          "flag_type": "",
          "flag": "",
          "choices": []
