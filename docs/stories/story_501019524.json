    },
    "17": {
      "frame_id": 17,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ファル子さんに向けるフラッシュさんの\r やれやれ系微笑みは、同じ部屋という 密室で育まれたものなんですねぇ……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
