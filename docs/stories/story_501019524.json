    "19": {
      "frame_id": 19,
      "background_id": 10,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "じゃっ、その勢いでぜひ、\r 推し語りなんかどうでしょッッ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
