      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "幸運にすがる気持ちは誰にも負けません！\r むしろそれだけ！　運のみで、おいしい とこゲットしたい！",
          "flag_type": "",
          "flag": "",
