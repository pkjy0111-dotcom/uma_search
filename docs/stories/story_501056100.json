    "4": {
      "frame_id": 4,
      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "なんとっ！　紅茶のお茶っぱが立ってます！\r ティーバッグからはみ出た強者がピョコっと！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……本当だ",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "……本当だね",
              "flag_type": "female_trainer",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "なんと縁起のいい！　これは予感がしますよ。\r ついに運命の日が来たのかも。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "こうしちゃいられません。早速アソコに\r ご神託をもらいにいきましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "占い大好き娘マチカネフクキタルは、\r 一目散に“あの場所”へと駆けていく……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "神様、どうか教えてください。\r 今日はソノ日なのですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "ご意志を示したまえ！　ほんぎゃろ、\r ふんぎゃろ～。きえええええ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "で、出ました、大吉！　ってことは……\r ついに来ましたよ、トレーナーさん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "神様はこう告げています。『今日こそ\r トゥインクル・シリーズに向けて 活動を開始すべし』と！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "ついに始動の時です！　月は東に、日は西に。\r んでもって私はメイクデビュー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "これできっと、シラオキ様に\r ご恩返しできることでしょう！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "シラオキ様って確か……",
              "flag_type": "",
              "flag": ""
            }
