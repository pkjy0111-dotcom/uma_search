      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "はい！　トレーナーさん、私にはお姉ちゃん\r ほどの才能はありません。でも……。",
