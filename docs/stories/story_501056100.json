      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "どうしても走り出せなかった私に、\r シラオキ様は勇気をくれたんですよね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
