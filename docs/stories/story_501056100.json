      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "あのお告げによって、私はトレセン学園に\r 導かれたのでした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
