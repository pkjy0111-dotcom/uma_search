      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "でわでわ、この良き日に鈴を鳴らして、\r あらためて祈りを捧げましょう！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
