      "frame_id": 16,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "私には優秀な姉がいて、比べるとダメダメ\r でしたから。でもあの日、夢の中で……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
