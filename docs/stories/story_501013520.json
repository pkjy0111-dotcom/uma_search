      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1013,
          "speaker_name": "メジロマックイーン",
          "text": "カフェテリアのキッチンも借りられました。\r 準備は万全。さあ――",
