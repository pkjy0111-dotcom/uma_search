      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "トレーナー、気づいたことがあります。 このバス……。",
          "flag_type": "",
          "flag": "",
          "choices": []
