      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1120,
          "speaker_name": "カルストンライトオ",
          "text": "思ったより蛇行していませんか？ できれば道路などにこだわらず、 スパッと直行で……。",
