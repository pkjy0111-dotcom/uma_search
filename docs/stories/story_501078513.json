      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "かつて見たレースで浴びたような、 どこまでも自由で、純粋で、鮮烈な風。",
          "flag_type": "",
          "flag": "",
          "choices": []
