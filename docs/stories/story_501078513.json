      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90600,
          "speaker_name": "記者A",
          "text": "いやぁ、近くで見ると ますます可愛さが際立つ勝負服ですね～！ では、キュートなポーズいってみましょう！",
          "flag_type": "",
          "flag": "",
          "choices": []
