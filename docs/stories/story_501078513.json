      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "夢や理想……ならば私は、風です。",
          "flag_type": "",
          "flag": "",
          "choices": []
