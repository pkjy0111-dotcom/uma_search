    "16": {
      "frame_id": 16,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "契約を交わしたあの日が、ごく最近である ようにも、ずっと昔だったようにも思えます。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "ああ……なんとなく、わかります",
