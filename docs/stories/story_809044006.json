      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "ふふ。トレーナーさんにとっての、 担当ウマ娘さんのようなものでしょうか？",
