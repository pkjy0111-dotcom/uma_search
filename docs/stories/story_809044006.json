    "14": {
      "frame_id": 14,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "プロデュースとマネジメントを、 私が担当することになって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
