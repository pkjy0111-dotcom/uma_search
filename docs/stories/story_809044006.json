      "frame_id": 15,
      "background_id": 137,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "……不思議。それからは、 とても濃密な日々でしたから――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
