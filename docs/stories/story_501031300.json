      "frame_id": 8,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "相談乗ってもらうついでに。 キミが手持ち無沙汰になるんだったら、 内職手伝ってもらおうかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
