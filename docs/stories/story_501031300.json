      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "…………あー。なんだかその目、 いつもよりも頑固そうな感じするの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 41,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "わかった。あたしもトレーナーに レースのことで聞きたいことあったし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
