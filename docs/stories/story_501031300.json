      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "えっ。いいの、いいの。 トレーナーはトレーナーの仕事があるでしょ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ちょうど暇だったから",
