    "9": {
      "frame_id": 9,
      "background_id": 37,
      "sub_background_id": 530,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "アイネスフウジンの隣で、 封筒の指定された箇所にシールを貼る。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
