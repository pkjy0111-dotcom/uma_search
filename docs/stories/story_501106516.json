      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "そこ、食材にもすごくこだわってて。 仕入れ次第で、メニューが変わるんですよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
