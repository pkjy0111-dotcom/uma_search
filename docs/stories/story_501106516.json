      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "最近お気に入りのお店なんですよ。 生地がふわっふわで、具も大きいのが ゴロゴロ入ってるんです！",
          "flag_type": "",
          "flag": "",
          "choices": []
