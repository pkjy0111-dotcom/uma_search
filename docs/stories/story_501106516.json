      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "つまり、同じお好み焼きには、 2度と出会えないかもしれなくて～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
