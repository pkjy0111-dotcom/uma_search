      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1106,
          "speaker_name": "ヒシミラクル",
          "text": "えへへ～。次の休日、 お好み焼き屋さんに行くんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
