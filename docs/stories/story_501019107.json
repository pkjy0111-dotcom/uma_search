    "13": {
      "frame_id": 13,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "さ、というワケでトレーナーさん。\r 祈りは済んだので、後は2人で食べましょう！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "え、いいのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "え、いいの？",
              "flag_type": "",
              "flag": "female_trainer"
