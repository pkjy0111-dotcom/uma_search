    "16": {
      "frame_id": 16,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "さあトレーナーさん、今日は包み隠さず\r 心の扉を開いてください。 最近の最推しは……誰なんです？",
          "flag_type": "",
          "flag": "chance_branch_root",
          "choices": []
