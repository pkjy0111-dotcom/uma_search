      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ととと、トレーナーさん！\r さあ準備はいいですか？",
