      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "いいい、いきなりデジたんにですか？\r 急に祭り上げられても、そんな……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 24,
              "loop_ends_at_frame": -1,
              "text": "これ食べて、これからも頑張って",
