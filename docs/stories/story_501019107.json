    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ううう、あたしも恵まれたものです。\r まさかこうして……ウマ娘ちゃん愛を語れる 同志に巡り合えるなんてッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
