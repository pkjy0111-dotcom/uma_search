    "3": {
      "frame_id": 3,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "今日はカプの祭典、バレンタイン！\r このクソデカ感情が『エッ！？』というほど こもってしまった手作りチョコをッ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "誰にあげるんだ？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "誰にあげるの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 3,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ともにッ、祭壇にッ、捧げましょうッ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……誰かに渡す話にすらならなかった。\r アグネスデジタル自作の神棚が、 トレーナー室に持ち込まれ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "さあさあ、どうぞお召しあがりください、\r ウマ娘ちゃん方。あたしの想像の中で……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 118,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1019,
          "speaker_name": "アグネスデジタル",
          "text": "ああ、実際に食べていただくまでもなく、\r どんな表情をするか……見える……。 ン゛ン゛……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "直接渡さないのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "直接渡さないの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
