      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "……『有馬記念』で戦い、越えて―― 私の言葉の正しさを示してやる。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
