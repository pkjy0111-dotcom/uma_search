      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "……心配をかけたことは詫びておく。 が、『寂しい』だなんだはありえねえ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "でも――",
