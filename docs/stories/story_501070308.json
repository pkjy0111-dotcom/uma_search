      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女にとって、避けたいレースなのでは ないだろうか。と、思ったのだが――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
