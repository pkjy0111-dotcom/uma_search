      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "……。 アンタは私のトレーナーだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "だから、話しておくことがある。面貸せ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "アンタの頭ん中のことはわかってる。 何を考え、憂いているのか――その全てをだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
