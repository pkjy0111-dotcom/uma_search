      "background_id": 58,
      "sub_background_id": 11,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……やはり、あのノートは 彼女に見られていたようだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
