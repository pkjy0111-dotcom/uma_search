          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "え、今日の講師って、君のお母さん！？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "え、今日の講師って、貴方のお母さん！？",
