    "15": {
      "frame_id": 15,
      "background_id": 31,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "うん、わかった。\r アタシは帰るけど……ここで寝ないでね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
