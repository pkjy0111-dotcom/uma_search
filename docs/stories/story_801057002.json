      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "ふふっ、それじゃ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
