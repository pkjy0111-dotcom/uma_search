    },
    "6": {
      "frame_id": 6,
      "background_id": 183,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "時間も限られていますし、謎の解き方が間違っていないか\r もう1度確認したほうがいいかもしれませんね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
