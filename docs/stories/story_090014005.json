    "7": {
      "frame_id": 7,
      "background_id": 183,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1030,
          "speaker_name": "ライスシャワー",
          "text": "あ……じゃあライス、その間にウララちゃんをお迎えに\r いってもいいかなっ？　戻ってくるの遅いから、心配で……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
