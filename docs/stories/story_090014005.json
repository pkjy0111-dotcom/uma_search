      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "あれぇ、わたし……\r 展望台で見つけた棒、どこやっちゃったんだっけっ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
