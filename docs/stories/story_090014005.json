      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1022,
          "speaker_name": "ファインモーション",
          "text": "『全ての数を集めし部屋、仰ぎ見る場所に宝あり』\r 上のほうは全部見たつもりだけれど、どこか見落としているのかな？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
