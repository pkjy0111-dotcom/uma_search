      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "あれれ？　……なんか、ぽっけが軽い？\r あれ、こっちのぽっけだっけ？　んん……っ？",
          "flag_type": "",
          "flag": "",
          "choices": []
