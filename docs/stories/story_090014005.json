    "17": {
      "frame_id": 17,
      "background_id": 184,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "だから、すぐあの棒を見つけるねっ！ 歩いたとこにあるはずだから！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
