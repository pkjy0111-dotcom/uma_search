    "2": {
      "frame_id": 2,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90645,
          "speaker_name": "アナウンサーA",
          "text": "『――トゥインクル・シリーズトレンド速報、 続いての特集はこちらの方！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90645,
          "speaker_name": "アナウンサーA",
          "text": "『昨年のクラシック級での活躍が話題の、 メジロ家の令嬢メジロアルダンさーん！』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 76,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『いやぁ、日本ダービーでの彼女の走りは、 凄まじかったですね。以降の戦績もすごくて、 シニア級での活躍も期待したいです――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "……光栄ですが、少々照れますね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "メジロアルダンの世間での評価は、 クラシック級での戦いを経て、 大きく変化しつつあった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女を評するにあたり、偉大な姉の名前が 出ることはなくなった。『メジロアルダン』 としてその名を確立したように見える……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "しかし、その一方で――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『――注目はやはり、あのオグリキャップと 今後どれほど渡り合っていけるかでしょうね』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90645,
          "speaker_name": "アナウンサーA",
          "text": "『やはり、彼女は別格なのでしょうか？』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 55,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『そう表現するのが適切じゃないかと。\r 同じように強いウマ娘、人気のあるウマ娘は 今年もたくさん現れていますが――』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『これまでの彼女の走るレース、その蹄跡は、\r 常に事前の常識を覆すもので……私自身、 いちファンとしていつも心躍らされます』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『そうしたワクワクさせる、夢を見せるなど、\r スター性と言える点で、オグリキャップは、 これまでにないものがあるんじゃないかと』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90640,
          "speaker_name": "アナウンサーB",
          "text": "『彼女の走るレース、競う相手にファンも 注目せざるを得ない。正に時代を代表し、 時代を作るウマ娘と言えるでしょうね』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
