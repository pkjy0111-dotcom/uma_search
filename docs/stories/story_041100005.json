      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "そうだよぉ。朝ごはんとまとめてこしらえてねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ワンダーアキュートが作ったお弁当。いったい どんな中身なのだろうか、と想像を巡らせていると――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "トレーナーさん、お昼がまだなら、 よかったら一緒にこれ、食べないかい？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "いいのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "いいの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "もちろんだよ。今日は少し作りすぎちゃったし、 トレーナーさんが食べてくれるならあたしも嬉しいわぁ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "それじゃあお弁当、ごちそうになろうかな",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90425,
          "speaker_name": "先輩トレーナー",
          "text": "お、<username>！　今からみんなで昼いくけどどう？ 駅前にできたラーメン屋、けっこういい感じらしくてさ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "すみません、今日はちょっと",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "あら！　トレーナーさん、あたしのことならいいのよ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "お弁当なら、また一緒に食べる機会があるだろうから。 せっかくのお誘いだし、行っておいでよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そうはいかない。誘ってもらったことに感謝しつつ、 ワンダーアキュートと昼食を共にすることになったと伝え――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "すみませんが、またの機会に！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90425,
          "speaker_name": "先輩トレーナー",
          "text": "おお、そっかそっか。先約があったんだな。 それじゃあまた今度！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ベーコンとチーズのサンドウィッチにミートローフ―― ワンダーアキュートのお弁当は洋風の家庭料理だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "",
          "text": "目移りしてしまうが、まずサンドウィッチを頬張る。 厚切りのベーコンとトマトと、香ばしいライ麦パンがよく合う。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "おいしい！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "あらあら……そんなに嬉しそうな顔してくれて、ありがたいねぇ。 紅茶もお飲みね。魔法瓶に入れてきたから、ぽかぽかよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "14": {
      "frame_id": 14,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ゆっくりと紅茶をすする。 茶葉の香りがふわりと鼻の奥いっぱいに広がって――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "（落ち着くなあ……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 53,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "デザートにさくらんぼのパイも用意してるからね。 食べたいだけ取り分けてあげようか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 2,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "はぁ～、お腹いっぱいだねぇ。 それに……日差しも穏やかで……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
