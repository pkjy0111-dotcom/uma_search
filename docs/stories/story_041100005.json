      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "トレーナーさん、わざわざありがとねぇ。 お誘いを断らせちゃって、申し訳なかったけれど。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "いや、それは俺がそうしたかったからで……",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 21,
