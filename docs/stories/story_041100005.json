          "next_frame": 2,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "あぁ、トレーナーさん。 これからお昼かね？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 2,
              "loop_ends_at_frame": -1,
              "text": "アキュートは、お弁当？",
