      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 0,
          "speaker_name": "錫細工の職人",
          "text": "……ただの試作品ですよ。ガラクタです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "そんな……！すごく感動しました！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "22": {
      "frame_id": 22,
      "background_id": 156,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "触れなくとも、その滑らかさやあたたかみが 伝わってくるようだ、と一生懸命力説する。 職人は、小さく笑顔を見せてくれた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
