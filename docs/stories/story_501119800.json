      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 0,
          "speaker_name": "錫細工の職人",
          "text": "……どこかでご覧に？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
