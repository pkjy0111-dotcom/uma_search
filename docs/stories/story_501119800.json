      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1119,
          "speaker_name": "ドリームジャーニー",
          "text": "昔……東欧の市場で、 家族と一緒に見たんです。 私たちは、一目で心を奪われましたよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
