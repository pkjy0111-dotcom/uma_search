      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "宣伝といえば、やっぱり食べた人の 感想がトップ――1番です！ そこに気の利いたフレーズも加えたりして――",
          "flag_type": "",
          "flag": "",
          "choices": []
