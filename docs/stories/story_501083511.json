      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "カフェテリアの――新メニュー。 不人気ゆえ――早くも―― 削除が――検討されている、らしい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "くぅぅ！　食べれば、すごくおいしいのに…… 味噌エンジンプリズム炒め……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "――いや――プリズム……ではなく プラズマ――だった、はずだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "名前が覚えづらいからじゃ……",
              "flag_type": "",
              "flag": ""
            }
