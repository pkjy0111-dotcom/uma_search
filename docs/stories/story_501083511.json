      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "どうにかならないかなぁ…… こんなにおいしいのに……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "何の話？",
