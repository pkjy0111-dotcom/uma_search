      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "でも、この料理って食べてみると まさに味噌エンジンプラズマって名前が しっくりくる気がしなくもなくて……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "このままほとんど注文されずに 消えていくのは、作った人も お料理そのものもかわいそうで……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
