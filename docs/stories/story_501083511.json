      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "……そう――だな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "料理のよさを宣伝するとかは？",
              "flag_type": "",
              "flag": ""
