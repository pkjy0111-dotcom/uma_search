      "frame_id": 13,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90605,
          "speaker_name": "密着スタッフ",
          "text": "そしておふたりは、幼少期同じスクールに 通っていた旧知の仲……でしたよね？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
