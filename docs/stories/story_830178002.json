      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1108,
          "speaker_name": "ドゥラメンテ",
          "text": "はい。……この樹が花を咲かせる時期なので、 見に来ました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "私もだ。所用で近くまで来たのでな。 ……今年も見事に咲いているぞ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
