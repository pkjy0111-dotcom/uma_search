    "12": {
      "frame_id": 12,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90605,
          "speaker_name": "密着スタッフ",
          "text": "この樹はたしか――世界的アスリートとして 功績を残したドゥラメンテさんの一族に、 国から贈られた記念樹……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
