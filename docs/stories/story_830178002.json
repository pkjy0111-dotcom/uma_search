      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "ん？　おや、ドゥラじゃないか。 奇遇だな……密着取材中か？",
          "flag_type": "",
