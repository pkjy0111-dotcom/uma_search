      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1018,
          "speaker_name": "エアグルーヴ",
          "text": "ええ。私の母が運営する育成スクールで、 共に。 記念樹が贈られたのも、ちょうどその頃かと。",
          "flag_type": "",
          "flag": "",
          "choices": []
