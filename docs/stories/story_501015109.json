          "next_frame": 4,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "世界中の王侯貴族を集めてね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "どの辺がささやか！？",
