      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "なに、至ってシンプルな推察だよ。\r 浮かない顔をして体重計に 乗っているのを見ればね。",
          "flag_type": "",
          "flag": "",
          "choices": []
