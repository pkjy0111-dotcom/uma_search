      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1010,
          "speaker_name": "タイキシャトル",
          "text": "ワタシ、走るのも食べるのも自信ありマス！\r 優勝目指して頑張りますネ！",
