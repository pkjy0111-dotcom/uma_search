      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 9001,
          "speaker_name": "駿川たづな",
          "text": "芝もダートも好成績のタイキシャトルさんが、 ここでどんな走りをするのか楽しみです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 11,
