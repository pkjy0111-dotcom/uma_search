      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1010,
          "speaker_name": "タイキシャトル",
          "text": "コースに置かれたにんじんを食べながら\r ゴールを目指す、『にんじん食い競走』！",
          "flag_type": "",
          "flag": "",
          "choices": []
