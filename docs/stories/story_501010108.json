      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 0,
          "speaker_name": "イベント実況",
          "text": "各ウマ娘、綺麗なスタートを切りました！ 最強の脚と胃袋を持つのは、どのウマ娘か！？",
          "flag_type": "",
          "flag": "",
          "choices": []
