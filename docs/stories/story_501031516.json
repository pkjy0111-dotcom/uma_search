    "3": {
      "frame_id": 3,
      "background_id": 11,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "うん、実は今日バイトだった\r たい焼き屋さんでね――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
