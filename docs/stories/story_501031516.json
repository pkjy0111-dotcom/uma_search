      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "あ、トレーナー。えへへ、今ひとりで\r たい焼きパーティーしてるところなの♪",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "たい焼きパーティー？",
