      "frame_id": 5,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "あちゃー……。でも、ピンチはチャンス！\r その材料を使って、店長さんの焼き方を バイトのみんなに伝授するの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 30,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1031,
          "speaker_name": "アイネスフウジン",
          "text": "材料を無駄にしなくて済むし、\r バイトの練習にもなるし、一石二鳥なの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
