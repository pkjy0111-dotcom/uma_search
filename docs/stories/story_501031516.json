      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "たい焼き屋の店長",
          "text": "……しまった。間違って材料を\r 発注しすぎちまったよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
