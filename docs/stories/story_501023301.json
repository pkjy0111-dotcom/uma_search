      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そしてビワハヤヒデもバスへと乗り込む。\r それに続こうと足を踏み出すと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
