      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ふふっ、トレーナー君。君も少し眠そうだな。\r 帰りのバスの中ではゆっくり休むといい。",
          "flag_type": "",
          "flag": "",
          "choices": []
