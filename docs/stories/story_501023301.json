      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 0,
          "speaker_name": "ベテラントレーナー",
          "text": "……見たか、お前。夏合宿中の\r ナリタブライアンのトレーニング。",
