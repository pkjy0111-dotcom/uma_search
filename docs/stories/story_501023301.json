      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "ふぁ……私も、隣で休憩をいただく予定だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
