      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "そのたくさんの思い出は、 今も変わらず私を走らせてくれる 大切な宝物なんです！",
