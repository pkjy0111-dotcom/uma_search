      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1001,
          "speaker_name": "スペシャルウィーク",
          "text": "悩んだ時も、挫けそうになった時も、 いつも誰かが傍にいて、 勇気づけてくれたんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
