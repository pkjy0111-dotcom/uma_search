      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "通りすがりのゲームセンターから、凄まじい\r 音が届いた。興味本位で覗いてみると――",
          "flag_type": "",
          "flag": "",
