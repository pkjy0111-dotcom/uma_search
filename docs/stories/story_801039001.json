      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "あぁ……ダメですわ。\r どうしても1位の点数に敵いませんの。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 36,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "けれど、諦めるわけにまいりません。\r この困難はきっと、高貴なるものの使命――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 36,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "私、必ず頂点に君臨してみせます。\r この『ウマ娘専用パンチングマシーン』で！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "すごい気迫だな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "すごい気迫だね",
              "flag_type": "",
              "flag": "female_trainer"
