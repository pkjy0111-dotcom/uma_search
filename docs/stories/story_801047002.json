      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "でしたら、ぜひ本を読んで頂き、\r 一緒に感想など語り合えると嬉しいです！",
