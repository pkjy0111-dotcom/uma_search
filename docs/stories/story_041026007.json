    },
    "12": {
      "frame_id": 12,
      "background_id": 37,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "先日、ゲームセンターで獲得したうさぎの人形だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
