        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ナイスネイチャに促され、\r 書いた願い事は……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "ナイスネイチャが健康でありますように",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ナイスネイチャがもっと強くなれますように",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "ナイスネイチャの特技が増えますように",
