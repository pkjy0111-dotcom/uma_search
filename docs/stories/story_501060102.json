      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "お願いごとは、文字にしとくのも大事なの。\r 言った言わないで神様と揉めたかないでしょ。",
          "flag_type": "",
          "flag": "",
          "choices": []
