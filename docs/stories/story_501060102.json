      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "ねえねえ。\r アレ書こうよ、お願いぶら下げるヤツ。",
          "flag_type": "",
