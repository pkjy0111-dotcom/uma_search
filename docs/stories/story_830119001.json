      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1065,
          "speaker_name": "ダイタクヘリオス",
          "text": "怪しまれんよーにってことは マサカリ隠したほうがよき？",
          "flag_type": "",
          "flag": "",
