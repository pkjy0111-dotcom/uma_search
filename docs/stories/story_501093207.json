      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう、先日ダイイチルビーに頼まれたのは、 自分とケイエスミラクルの パーティーへの出席だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
