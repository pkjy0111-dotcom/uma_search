      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "これまで短距離路線は、王道・中長距離路線 に比べ、注目度の面では劣っておりました。",
          "flag_type": "",
          "flag": "",
          "choices": []
