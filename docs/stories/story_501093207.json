      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1085,
          "speaker_name": "ダイイチルビー",
          "text": "『高松宮記念』への注目度をより高めるべく、 私から一族にお願いし、 開催いただいたものとなります。",
          "flag_type": "",
          "flag": "",
          "choices": []
