      "dialogue": [
        {
          "next_frame": -1,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "こうしてフェノーメノは熱心に もうひと汗流すのだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
