      "frame_id": 7,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "それをこの身をもって 証明したいのであります！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "わかった、試してみるといいよ",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 11,
              "loop_ends_at_frame": -1,
              "text": "それは許可できない",
              "flag_type": "",
              "flag": ""
            }
