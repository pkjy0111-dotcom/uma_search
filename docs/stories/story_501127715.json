        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "フェノーメノはこの日予定されていた トレーニングを全てこなした。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
