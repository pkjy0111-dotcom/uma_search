      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "これより自主トレーニングに 入らせていただいてよろしいでしょうか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "え、じ、自主トレ？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "はい。ボス考案のトレーニングメニューに 不満などありません。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "しかしそこへいくらかの鍛錬を追加し、 日々120％のメニューをこなすのです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 9,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "鍛錬の積み重ねはいずれ大きな結果を生み、 よりよい未来へと繋がるはず。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
