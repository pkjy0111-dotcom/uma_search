      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ボスの承認を得たからには、 全力で遂行させていただきます！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
