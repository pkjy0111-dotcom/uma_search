      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "本日もご指導ありがとうございました！ 疲労の中に確かな手応えを感じるであります。 では――",
          "flag_type": "",
          "flag": "",
          "choices": []
