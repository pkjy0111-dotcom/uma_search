      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "ボスならそう言ってくれると 思っていました！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
