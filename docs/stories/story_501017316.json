      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ちゃちゃっと茶を頂こう。\r ……どうだ？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 27,
              "loop_ends_at_frame": -1,
              "text": "どうかな……",
