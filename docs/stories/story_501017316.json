      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "さあ、トレーナー君。\r とにもかくにもお茶だよ、お茶。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
