      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90953,
          "speaker_name": "バンブーの父親",
          "text": "こうやってコツコツ努力していれば――必ず！！ 俺に負けない、熱き戦いの場に立つことができるはずだ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
