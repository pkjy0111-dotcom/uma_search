      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90953,
          "speaker_name": "バンブーの父親",
          "text": "うむ、話を聞きたい気持ちを抑え、よく頑張った！！ その我慢も、トレーニングのうちだからな！！",
          "flag_type": "",
          "flag": "",
          "choices": []
