      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "よかったら、朝練を見てもらえないっスか！ いきなりで申し訳ないんスけど、ぜひッ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
