      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1053,
          "speaker_name": "幼いバンブー",
          "text": "うんっ！！　ガマンするし、コツコツやる！！ アタシ――父さんみたいに強くなるから！！",
