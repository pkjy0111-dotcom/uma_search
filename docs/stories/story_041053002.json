      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1053,
          "speaker_name": "幼いバンブー",
          "text": "燃えろ熱血トレーニング、その十……おわりっ！ はあっ……はあっ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
