      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "弾め筋肉！！　ギャラクティカ・腿上げ！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 70,
      "sub_background_id": 150,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1053,
          "speaker_name": "バンブーメモリー",
          "text": "どりゃあああ！！　燃えろ熱血トレーニングッ！！ その十五――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "その『熱血トレーニング』って何……？",
              "flag_type": "",
              "flag": ""
