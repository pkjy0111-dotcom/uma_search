      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "いえ、どうしたと言われると、 どうもしないって答えになるんですけど……。",
