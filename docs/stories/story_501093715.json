      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "……でも、おれの“どうもしない”は すごく貴重で恵まれたものなんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
