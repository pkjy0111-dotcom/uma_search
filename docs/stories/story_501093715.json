      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "長く体を動かせない時期もありました。 やりたくてもできないこと、 たくさんありました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
