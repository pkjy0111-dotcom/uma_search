        {
          "next_frame": 3,
          "character_id": 1093,
          "speaker_name": "ケイエスミラクル",
          "text": "あの、トレーナーさん……もう少しだけ トレーニング、やらせてくれませんか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "……急にどうしたの？",
              "flag_type": "",
              "flag": ""
