      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "ウチの勝利を信じて、鍛えて、GⅠで勝たせまくってッ！\r 日本一にしたるってぐらいの覚悟が！　ホンマにあるんかッ！？",
          "flag_type": "",
          "flag": "",
          "choices": []
