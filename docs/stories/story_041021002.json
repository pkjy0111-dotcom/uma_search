      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "……アンタ、新人トレーナーやろ。\r 経験も実績もロクにあれへんのに、覚悟あるんか？",
          "flag_type": "",
          "flag": "",
          "choices": []
