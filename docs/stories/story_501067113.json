      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "サトノ一族が考える“名ウマ娘”とは、 ウマ娘界の発展を支えるウマ娘のことです。",
          "flag_type": "",
          "flag": "",
