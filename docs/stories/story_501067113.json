      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "はい。\r いかがでしょうか？",
