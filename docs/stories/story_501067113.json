      "frame_id": 6,
      "background_id": 95,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1067,
          "speaker_name": "サトノダイヤモンド",
          "text": "ご存じの通り、サトノ一族は ウマ娘界に貢献したいと考え、 運営協力や慈善活動などをしてまいりました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
