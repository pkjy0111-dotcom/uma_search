      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "大地を揺らし、雨をはねのけて、風のように駆けるウマ娘。 気づけば息をするのも忘れるほどに、彼女は鮮烈だった――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 9,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1057,
          "speaker_name": "？？？",
          "text": "……はぁ、はぁ……ふー！ あー！　やっぱり相手がいないと物足りないなぁ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
