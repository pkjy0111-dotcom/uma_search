      "background_id": 37,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "",
          "text": "目を見張るようなスピードが、 さらに、さらに、さらに、加速する。常識を置いていく。",
          "flag_type": "",
          "flag": "",
          "choices": []
