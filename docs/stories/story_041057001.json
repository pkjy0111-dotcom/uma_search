      "background_id": 9,
      "sub_background_id": 410,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1057,
          "speaker_name": "？？？",
          "text": "でも、こんな日にグラウンドに来る子なんて――ん？",
          "flag_type": "",
          "flag": "",
          "choices": []
