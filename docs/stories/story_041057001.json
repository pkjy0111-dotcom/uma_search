      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そんな、どこか行き詰まった空気を打ち破るかのように―― 雨の中、一心不乱に走るウマ娘がいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
