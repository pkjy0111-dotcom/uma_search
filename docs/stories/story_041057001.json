      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……あまりの衝撃に、胸は高鳴り、足が震えて 立ち去ることができずにいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
