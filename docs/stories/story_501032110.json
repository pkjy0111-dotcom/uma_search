      "frame_id": 14,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "実は最近、『熱狂』や『情熱』から\r 発生するパワーにとても興味があってね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
