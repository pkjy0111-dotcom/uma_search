    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "",
          "text": "思い出してみれば、彼女はずいぶん前から、\r 『感情』について興味を示していた。",
