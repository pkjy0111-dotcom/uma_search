      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "",
          "text": "一時的なものかと思っていたが、\r どうやらそういうわけでもないらしい。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "どうしてそんなに興味があるんだ？",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "どうしてそんなに興味があるの？",
