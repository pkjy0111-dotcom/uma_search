      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "アグネスタキオンが、心理テストをしたい\r と言って、トレーナー室へやってきた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
