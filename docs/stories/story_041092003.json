      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1092,
          "speaker_name": "ダンツフレーム",
          "text": "必死に頑張ってトレセン学園に入れた時は、本当に嬉しかった。 これでわたしも、大舞台で“主役”になれるって。",
          "flag_type": "",
          "flag": "",
          "choices": []
