      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "次は、と切り出す前だった。 すでに彼女の中では確定事項のようだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
