      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女はシンボリルドルフを毛嫌いしている。 偶然にすぎないだろう。",
