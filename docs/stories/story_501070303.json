      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1070,
          "speaker_name": "シリウスシンボリ",
          "text": "まず、ジュニア級のゴールはそこだ。 いいな。",
          "flag_type": "",
          "flag": "",
