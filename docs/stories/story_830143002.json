      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "はい、何かご用でしょうか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "蹄鉄整備の器具を借りたくて",
              "flag_type": "",
              "flag": ""
