      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "",
          "text": "いつになく柔らかい笑みを浮かべ、 備品の整備をしているイクノディクタス。 穏やかな光景に、少し頬が緩んだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "あの、ちょっといいかな",
