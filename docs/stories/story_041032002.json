      "frame_id": 32,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "まさか自ら被験体となりに来てくれたのか！？\r なんたる素晴らしいモルモット精神！　感服の一言に尽きるね！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 33,
              "loop_ends_at_frame": -1,
              "text": "違う違う違う違う！",
              "flag_type": "",
              "flag": ""
            }
