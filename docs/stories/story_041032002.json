      "frame_id": 30,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ふぅン。『知るか』『好き勝手やれ』と言いつつも忠告していく……\r ククッ、相変わらず愉快だなシャカール君は。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "31": {
      "frame_id": 31,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "と、おや……？\r 君は――先日の新人トレーナー君じゃないか！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
