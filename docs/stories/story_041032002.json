      "frame_id": 28,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "……けどな、実験実験で授業にもまともに出ねェ、選抜レースにも\r 出ねェ、スカウトも受けねェ、当然デビューもしねェ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "29": {
      "frame_id": 29,
      "background_id": 2,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1036,
          "speaker_name": "エアシャカール",
          "text": "――てめェの学園での立場、そろそろヤベェぞ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
