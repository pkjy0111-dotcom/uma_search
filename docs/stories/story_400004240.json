      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ネイチャさん、チェックアウト後の 予定は決めていますか？ 新幹線の時間まで結構ありますが。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "ん～？　特には考えてなかったなぁ。 お土産買うくらい。イクノは？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "観光スポットを回るコースを いくつか見繕っています。 よろしければご一緒にどうですか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1060,
          "speaker_name": "ナイスネイチャ",
          "text": "おお～、ウワサの『イクノツアーズ』！ ぜひご一緒させていただこうじゃないですか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 67,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "イクノツアーズ……？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
