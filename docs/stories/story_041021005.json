      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "せや、またチビたちにお菓子つけて送ったろ。\r トレーナーも購買に付き合わへん？",
          "flag_type": "",
          "flag": "",
          "choices": []
