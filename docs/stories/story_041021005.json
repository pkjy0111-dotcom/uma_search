      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "ああ。お父ちゃん、トレーナーに感心しとったやろ。\r アンタのおかげでウチがごっつ成長したってな。",
          "flag_type": "",
          "flag": "",
          "choices": []
