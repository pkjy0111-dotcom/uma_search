      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "はははっ、なら、返事にそう書いとくわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
