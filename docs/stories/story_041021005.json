      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "家族にお菓子選んだってや。\r 手紙でそう伝えるし、挨拶代わりにちょうどええやろ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "ぜひ選ばせて！！",
