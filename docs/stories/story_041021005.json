      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1021,
          "speaker_name": "タマモクロス",
          "text": "『トレーナーと“白い稲妻”を天下に轟かせぇ』、やって！\r お父ちゃんの期待、裏切るんやないで！",
          "flag_type": "",
          "flag": "",
          "choices": [
