      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして合宿所に着くと―― キタサンブラックは海を見つめながら、 考え込んでいた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
