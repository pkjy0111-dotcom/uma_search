    "9": {
      "frame_id": 9,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "人気投票、第1位……。第1位か……。 トレーナーさん、人気って不思議ですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "人気は、強さや実績とは少し違う……。 強ければあるわけでも、 弱ければないわけでもない……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "でもあたし、まだまだ第1位に ふさわしい走りはできてないと思います！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
