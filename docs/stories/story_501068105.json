      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "でも浮かれてはいられませんよね！ ズンドコサと頑張らなきゃ、です！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ああ、その意気だ！",
              "flag_type": "",
