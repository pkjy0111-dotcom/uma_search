      "background_id": 60,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1068,
          "speaker_name": "キタサンブラック",
          "text": "まだまだ上があるのはわかるのに、みんなが ここまで持ち上げてくれるのは、なんか少し 落ち着かなくて……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
