      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "",
          "text": "今回彼女と見たのは、今話題の探偵映画。 観客を助手に見立てた構成が話題の作品だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
