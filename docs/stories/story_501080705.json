      "background_id": 224,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1080,
          "speaker_name": "トランセンド",
          "text": "『手詰まりかと思ったが…… どうやら透明人間も、 匂いまでは消せなかったようだ』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
