      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "あ、移動がご面倒ですかしら！？ 私がお姫様抱っこにてお運びしますわ！！",
