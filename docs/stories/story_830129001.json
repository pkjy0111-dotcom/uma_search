      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1011,
          "speaker_name": "グラスワンダー",
          "text": "いえいえ～。ちょうど息抜きしたい 気分でしたし、私でお役に立てるのならば。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
