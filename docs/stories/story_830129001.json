      "frame_id": 7,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "がってんですわっ！ ではグラスさん、奥の席へおかけになって！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
