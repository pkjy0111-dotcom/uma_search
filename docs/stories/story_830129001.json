      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "スコーンは自分でちぎってもらえばいいのよ！ ここは『お好きなジャムは』とか、 そういうことを伺うほうがいいわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
