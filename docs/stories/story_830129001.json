      "frame_id": 9,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "ま、待ちなさいっ！　気づかいはいいけれど、 そんなことをしたら埃が立つでしょ！",
