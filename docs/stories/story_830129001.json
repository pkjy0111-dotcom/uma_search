      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "そこは 『お越しいただきありがとうございます』 でしょっ！",
          "flag_type": "",
          "flag": "",
