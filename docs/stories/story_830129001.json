      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "グラスさん、少々お待ちあそばせ！！ スコーン、食べやすいように引きちぎって――",
