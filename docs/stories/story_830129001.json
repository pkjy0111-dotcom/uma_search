    "6": {
      "frame_id": 6,
      "background_id": 11,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1061,
          "speaker_name": "キングヘイロー",
          "text": "いーい、カワカミさん？　一流のお茶会は、 ご招待した方への配慮が重要よ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
