      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9043,
          "speaker_name": "佐岳メイ",
          "text": "最後までよく走り切った。 どんな結果であれ、 君たちへの注目度合いは上がったはずだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
