      "sub_background_id": 20,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "トレーナー室に来た<chrname>に 説明する。『プロジェクトL'Arc』で いかに彼女の能力が開花しているかも含め。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
