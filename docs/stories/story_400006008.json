      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "",
          "text": "佐岳メイからの労いに一礼する。 だが目指すべき地点はまだ先だ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "これからも頑張ります",
              "flag_type": "",
              "flag": ""
