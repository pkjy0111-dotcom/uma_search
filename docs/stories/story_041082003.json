    "26": {
      "frame_id": 26,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "……ああ、わたし！　はい、大丈夫ですよ。 少し貧血気味になっちゃっただけで。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
