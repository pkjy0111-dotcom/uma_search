      "frame_id": 25,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "――え、トレーナーさん……？ どうかなさったんですか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 26,
              "loop_ends_at_frame": -1,
              "text": "大丈夫！？",
              "flag_type": "",
              "flag": ""
