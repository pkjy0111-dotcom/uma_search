      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "思考、実践、失敗。思考、実践、失敗。思考、実践、失敗―― ただひたすらに、徹底的に、己の体と向き合う日々が綴られていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
