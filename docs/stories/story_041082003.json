      "frame_id": 27,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "今出せる、スピードの限界値を探っていたんですけど…… 根を詰めすぎたみたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "28": {
      "frame_id": 28,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "気をつけていたんですけどねぇ。走っていると、難しいです。 全力を出さないって、本来であれば全く逆の発想ですし。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "29": {
      "frame_id": 29,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "三階級制覇！　なぁんて、言っている場合じゃないですね。 もちろん、諦めるつもりもないですけどっ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "30": {
      "frame_id": 30,
      "background_id": 19,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1082,
          "speaker_name": "ノースフライト",
          "text": "というわけで、ご心配おかけしましたっ！ もう大丈夫です。ありがとうございます――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "31": {
      "frame_id": 31,
      "background_id": 7,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "明るく笑っていたノースフライト。彼女からはなんの気負いも 感じなかった。次からはより慎重に体調を見極めて走るのだろう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 32,
              "loop_ends_at_frame": -1,
              "text": "（帰るか――ん？何か落ちてる……）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "32": {
      "frame_id": 32,
