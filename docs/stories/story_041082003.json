      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "それは1冊のノートだった。表紙には、丁寧かつ繊細な字で、 『フーちゃんのレース研究』と書いてある。ナンバリングは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 33,
              "loop_ends_at_frame": -1,
              "text": "12冊目！？",
