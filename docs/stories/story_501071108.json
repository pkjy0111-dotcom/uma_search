      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1071,
          "speaker_name": "メジロアルダン",
          "text": "これまでの私は、“今”の自分を刻むべく、\r レースを全力で走ることに集中し、 ライバルに勝つことを意識してきました。",
          "flag_type": "",
          "flag": "",
          "choices": []
