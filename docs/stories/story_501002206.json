      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 90001,
          "speaker_name": "",
          "text": "――しかし期待が高まる観客席とは対照的に、\r 控室は重い空気に包まれていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
