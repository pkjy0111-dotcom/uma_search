      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 0,
          "speaker_name": "観客A",
          "text": "ね！　レースの勝敗よりも、\r もう彼女を見るのが楽しみ……！",
