      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "だがこれでも、大事なテストだけはしっかり\r 受けているからね。問題ないとも。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "小テストもちゃんとやろう",
              "flag_type": "",
              "flag": ""
