      "frame_id": 6,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "だからこそ、学問を身につけることは、\r 趣味の範囲でいいと思っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
