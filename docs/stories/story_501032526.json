      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "飲むことで脳神経細胞を活性化させ、\r あらゆる現象を即座に認識し――",
