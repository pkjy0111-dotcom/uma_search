      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "君に見てもらわねばならないほど、\r 私の成績は悪くないんだがねぇ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
