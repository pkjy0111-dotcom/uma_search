      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "ふっふっふっ……\r 実は、今朝できたばかりの薬なんだがね！？",
