    },
    "5": {
      "frame_id": 5,
      "background_id": 27,
      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "私には必要ないものさ。\r 学びは確かに人生を豊かにするものだが――",
