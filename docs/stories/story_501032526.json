      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "それに私にはテストを受けるよりも、\r やりたいことがあるんだ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "いつもの研究？",
