    "27": {
      "frame_id": 27,
      "background_id": 28,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1046,
          "speaker_name": "スマートファルコン",
          "text": "まだ誰にも知られてないファル子じゃ、みんなを引っ張ってけない。\r まずは知ってもらうことから始めなくちゃ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 28,
              "loop_ends_at_frame": -1,
              "text": "今まで通り諦めなければチャンスは来る",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 28,
              "loop_ends_at_frame": -1,
              "text": "今まで通り諦めなければチャンスは来るよ",
              "flag_type": "",
              "flag": "female_trainer"
