      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1046,
          "speaker_name": "スマートファルコン",
          "text": "え～っ、今まで通りじゃなにも変えられないよ！\r もっと売り込み方もがっちり☆考えていかないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
