      "frame_id": 10,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "そこに小分けかつ大袋のきなこ棒が 売っています。値段も手ごろです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
