      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "……値段も手ごろ、手に入りやすく、\r 小分けで、高タンパク質なおやつ、ですか。",
          "flag_type": "",
          "flag": "",
          "choices": []
