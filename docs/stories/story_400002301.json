      "frame_id": 8,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "たしかに、きなこはタンパク質を多く含む。 サイズ的にもケータリングにぴったりだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
