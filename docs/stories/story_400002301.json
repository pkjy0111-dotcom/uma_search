      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "商店街の真ん中で大の大人が立ち尽くして どうしました。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
