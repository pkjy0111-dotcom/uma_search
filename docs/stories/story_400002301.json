      "frame_id": 11,
      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "ただ、きなこは口の水分を吸いますので 水分の用意も必須です。気を付けてください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
