      "background_id": 30,
      "sub_background_id": 121,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "少々遠いですが、ここから3つ目の交差点を 右折したところに個人商店があります。",
          "flag_type": "",
          "flag": "",
          "choices": []
