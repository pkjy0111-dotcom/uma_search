    "34": {
      "frame_id": 34,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 35,
          "character_id": 90001,
          "speaker_name": "",
          "text": "息の上がったクロノジェネシスに『お疲れ様』と声をかける。 そのあとで、さっきの走りについて感想を伝えた。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 35,
              "loop_ends_at_frame": -1,
              "text": "大逃げは、さすがに自由すぎるかも……！",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "35": {
      "frame_id": 35,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 36,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "す、すみませんっ……！　とにかく自由に走ろうと考えていたら、 いつの間にか、こんな……うう……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "36": {
      "frame_id": 36,
      "background_id": 9,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 37,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
