      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 9006,
          "speaker_name": "樫本理子",
          "text": "……やはり、そうなりますか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
