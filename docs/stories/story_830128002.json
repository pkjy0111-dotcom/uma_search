      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "着物が届いたのよ。 ほら、グランマに送るお礼の写真で着る用の！",
