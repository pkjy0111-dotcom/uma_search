      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "かつての街の復活と、新風の導入。 それが、私たちの計画の概要となります。",
