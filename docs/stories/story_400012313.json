      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "さらに――現役ウマ娘など、レース関係の お客さんが楽しめる新たなコンテンツの充実。",
          "flag_type": "",
          "flag": "",
          "choices": []
