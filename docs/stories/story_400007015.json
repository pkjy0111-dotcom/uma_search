      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90630,
          "speaker_name": "キャスター",
          "text": "『そんな盛り上がる“U.A.F.”を、 当番組ではこれから毎週 特集していくこととなりました！』",
          "flag_type": "",
          "flag": "",
          "choices": []
