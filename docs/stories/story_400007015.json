      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90630,
          "speaker_name": "キャスター",
          "text": "『今日はまず、実際に観戦した人たちの 推し選手が誰かアンケートを――――』",
          "flag_type": "",
          "flag": "",
          "choices": []
