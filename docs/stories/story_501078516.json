      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "あら、トレーナーさん。奇遇ですね。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ご飯食べてきたのか？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "ご飯食べてきたの？",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
