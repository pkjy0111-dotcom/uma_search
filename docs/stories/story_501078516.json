      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1078,
          "speaker_name": "ヤマニンゼファー",
          "text": "気づいてしまうと、とってもお腹が 空くものでして、さらなる食事をと……。",
          "flag_type": "",
          "flag": "male_trainer",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "食べ過ぎは注意してな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "食べ過ぎは注意してね",
              "flag_type": "",
              "flag": "female_trainer"
            },
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "空腹じゃ走れないからな",
