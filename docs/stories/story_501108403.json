      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "トレーナーの手記",
          "text": "『皐月賞』で呼びかけに反応して 視野が広がったように、感覚を広げる…… 世界と『つながる』訓練が。",
          "flag_type": "",
          "flag": "",
          "choices": []
