      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "だから『先生』と、敬意を込めて呼んでいるんです。 ウマ娘にしかできない、特別な役割として。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
