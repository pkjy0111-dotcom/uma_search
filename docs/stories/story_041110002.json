      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1110,
          "speaker_name": "シーザリオ",
          "text": "ウマ娘を導くウマ娘――母親から直に教わる子もいれば、 育成クラブのコーチから運命的なものを感じ、師事する子もいます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
