      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "いや。 そういった類の話ではないよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
