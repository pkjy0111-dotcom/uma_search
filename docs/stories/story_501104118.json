    "15": {
      "frame_id": 15,
      "background_id": 22,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "んー……色々あるんだけど……―― 早い話、『導く』って ガラじゃあないんだよな。",
