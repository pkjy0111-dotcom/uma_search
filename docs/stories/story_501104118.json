    "16": {
      "frame_id": 16,
      "background_id": 76,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "……あたしは、これまでもこれからも、 ただ“前”に“進む”つもりだ。 ていうか多分……それしかできない。",
          "flag_type": "",
          "flag": "",
          "choices": []
