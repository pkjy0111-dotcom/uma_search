    "17": {
      "frame_id": 17,
      "background_id": 76,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1104,
          "speaker_name": "カツラギエース",
          "text": "そんでもって“前”に“進む”時、 あたしはいつだって、自分と、 自分の抱えた物と進むことしか考えてない。",
          "flag_type": "",
          "flag": "",
          "choices": []
