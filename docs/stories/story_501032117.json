      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 34,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ということはつまり、\r 彼女は1度走ることを諦めようと……！？",
