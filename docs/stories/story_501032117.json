      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "プランＢとは、彼女自身の脚ではなく、\r 他のウマ娘で限界への到達を目指すもの――",
          "flag_type": "",
          "flag": "",
          "choices": []
