      "frame_id": 21,
      "background_id": 1,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "君は私の脚に期待してくれていた。\r クラシックレースに出ようと意気込んだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
