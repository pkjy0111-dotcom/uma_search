      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "フ、期待を寄せるのは君の勝手だ。\r 否定はしないよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
