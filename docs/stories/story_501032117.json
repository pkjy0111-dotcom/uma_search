      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "アグネスタキオンは、\r 懐かしむように目を閉じた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
