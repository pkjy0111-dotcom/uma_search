      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1032,
          "speaker_name": "アグネスタキオン",
          "text": "だから、プランＢに舵を切った。",
          "flag_type": "",
          "flag": "",
          "choices": []
