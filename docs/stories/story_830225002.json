      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "そして……濡れた文字を組み合わせれば、 お告げの文章が完成！　というわけです！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
              "loop_ends_at_frame": -1,
              "text": "…………なるほど？",
              "flag_type": "",
              "flag": ""
