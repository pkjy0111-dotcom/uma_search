      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1056,
          "speaker_name": "マチカネフクキタル",
          "text": "むふふ、これは私が考案した 最先端のお告げ受信方法でして！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
