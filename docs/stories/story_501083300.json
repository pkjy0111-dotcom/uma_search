      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90655,
          "speaker_name": "記者B",
          "text": "そ、それはつまり、もしかしなくても―― 長期の休養に入られるということですか！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
