      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1084,
          "speaker_name": "タニノギムレット",
          "text": "永く、深く――されど近くにて、 刻の流れを、杯を携え、眺めさせてもらおう。",
          "flag_type": "",
          "flag": "",
          "choices": []
