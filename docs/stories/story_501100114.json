      "frame_id": 27,
      "background_id": 16,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "君のやり方“ハードトレーニング”と いうのは、ひとつの正しい答えだ。 計算し尽くした上でやるならね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
