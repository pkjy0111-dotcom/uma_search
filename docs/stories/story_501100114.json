      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 90001,
          "speaker_name": "",
          "text": "テイエムオペラオーは一呼吸すると――",
          "flag_type": "",
          "flag": "",
          "choices": []
