      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "しっかりと見ろ。自分の行いを、信念を。 そしてワンダーアキュートというウマ娘を。",
