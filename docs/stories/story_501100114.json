    "28": {
      "frame_id": 28,
      "background_id": 16,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "アキュートさんもそれは認めている。 なのになぜ、自分から見限る？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
