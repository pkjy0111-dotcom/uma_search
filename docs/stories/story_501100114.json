      "frame_id": 32,
      "background_id": 16,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 33,
          "character_id": 90001,
          "speaker_name": "",
          "text": "そう簡単に自信など持てるはずもない。 トレセン学園での生活は…… うまくいかないことばかりだった。",
          "flag_type": "",
          "flag": "",
          "choices": []
