      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 32,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "目を――開くんだ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 32,
              "loop_ends_at_frame": -1,
              "text": "でも……！",
