      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "なに、この山……ゴミがいっぱい……。 壊れた時計に、ゴルフクラブに……。",
