      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "私、こういうの許せません！ こんなに豊かな環境、感謝して 守っていかなきゃいけないはずなのに――",
          "flag_type": "",
          "flag": "",
          "choices": []
