      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "ふぅ……昨日はさんざんだったな……。 今日こそ後れを取り戻さないと――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
