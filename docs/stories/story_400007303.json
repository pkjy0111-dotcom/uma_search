      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "リッキーは木々の氣を浴びろ―― なんて言ってたけど……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
