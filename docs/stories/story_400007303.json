      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "不法投棄っちゅうやつかねえ？ せっかくの綺麗な景色がもったいない。",
