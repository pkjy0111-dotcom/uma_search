      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "汚れたまま放っておくなんてだめ！ こうなったら――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
