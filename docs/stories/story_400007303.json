      "background_id": 128,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "私、ゴミ拾いします！ この山に綺麗さを取り戻さないと！",
