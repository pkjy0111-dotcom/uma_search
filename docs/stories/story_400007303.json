      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1100,
          "speaker_name": "ワンダーアキュート",
          "text": "ふふっ、ふたりともいい子いい子じゃねぇ。 なあに、みんなでやれば きっとあっという間よ～。",
          "flag_type": "",
          "flag": "",
          "choices": []
