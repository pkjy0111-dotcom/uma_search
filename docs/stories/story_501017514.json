      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "トレーナー君はどう思う？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "ギャグが高度すぎるかも",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 25,
              "loop_ends_at_frame": -1,
              "text": "皆が驚きそう……",
