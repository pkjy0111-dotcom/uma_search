        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ある日、シンボリルドルフが真剣な表情で\r 相談を持ちかけてきた――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
