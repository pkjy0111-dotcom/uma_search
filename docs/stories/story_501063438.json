      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "その強さと管理能力、そして、今回も 無事に走り切ったこと。それらに敬意を込め、 観客たちは称賛を送った。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
