      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "ここまで走り続けてきたイクノディクタス。 彼女が積み上げてきた経験に挑みたい、と――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
