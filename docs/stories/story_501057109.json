      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "人、少ないね。 クリスマスにファミレスにくる人は あんまりいないのかな？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "夜はもっと混むかも",
