      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1057,
          "speaker_name": "ミスターシービー",
          "text": "じゃ、行こ。 あったかいもの飲みたいな。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
