      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "な、なるほど、参考になります……！",
          "flag_type": "",
          "flag": "",
          "choices": []
