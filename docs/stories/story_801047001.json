      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "ほ、本当ですか……！　よろしければ、\r 感想とか聞いてもかまいませんか？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
