      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "やはり映画には、スピードや迫力も\r 重要ということなんでしょうか……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "盛り上がるよね",
              "flag_type": "",
              "flag": ""
