      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "いきなりすみません。実はこの映画、\r 私の好きな小説が原作なんです……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "原作ファンとしては、\r 映像化したのが嬉しくて、つい……！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "ラストのカーチェイスが凄かった",
              "flag_type": "",
              "flag": ""
            },
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "映像が綺麗だったよね",
              "flag_type": "",
              "flag": ""
