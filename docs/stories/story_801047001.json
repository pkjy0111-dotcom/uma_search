    "10": {
      "frame_id": 10,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "カーチェイスに変更したことで、\r たしかに臨場感が増したと思います。 ドライビングテクニックも凄かったですし！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
