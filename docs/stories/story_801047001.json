      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "そうですね、あそこのシーンは\r 私も手に汗握りました……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
