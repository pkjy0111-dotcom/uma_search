      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1047,
          "speaker_name": "ゼンノロブロイ",
          "text": "って、す、すみません長々と……っ。\r 1人で白熱してしまいました……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
