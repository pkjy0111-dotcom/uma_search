      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "ふふ、壮観だろう。\r ここのシェフが考案した新作料理たちだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
