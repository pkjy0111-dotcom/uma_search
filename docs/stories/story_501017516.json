      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "おや、トレーナー君。\r 奇遇だな。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "この料理たちは……？",
