      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "カフェテリアのメニュー改革もその一環だ。\r ……本当に、頭が下がる思いだよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
