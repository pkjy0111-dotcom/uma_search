      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1017,
          "speaker_name": "シンボリルドルフ",
          "text": "トレセン学園で働く人々は皆、常日頃から\r 我々によりよい生活を提供しようと 心を砕いてくださっている。",
          "flag_type": "",
          "flag": "",
          "choices": []
