      "sub_background_id": 101,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1022,
          "speaker_name": "どこか上品なウマ娘",
          "text": "こっちまでお湯が飛んできそうな迫力に、店内を満たす熱気……\r 出来上がりを待つときめきも全部、想像以上！",
          "flag_type": "",
          "flag": "",
          "choices": []
