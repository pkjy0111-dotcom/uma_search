      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1022,
          "speaker_name": "どこか上品なウマ娘",
          "text": "『麺屋極極』まで、エスコートしてくださるかしら？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "えっ",
              "flag_type": "",
              "flag": ""
