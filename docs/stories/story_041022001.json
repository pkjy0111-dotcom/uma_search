      "frame_id": 14,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……確かにそうなのだが、先程までの世間知らずぶりといい、\r ちょっとこのまま放っておくのはためらわれた。そこで――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 15,
