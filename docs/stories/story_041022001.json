      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1022,
          "speaker_name": "どこか上品なウマ娘",
          "text": "ここまで連れて来てくれて、本当に本当にありがとうっ。\r キミには感謝してもしきれないよ！",
          "flag_type": "",
          "flag": "",
          "choices": [
