      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1022,
          "speaker_name": "どこか上品なウマ娘",
          "text": "……あっ、ごめんなさい！　えへへ、私ったらつい。\r 『行き先は自分で調べる』のが普通、だったよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
