      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1022,
          "speaker_name": "？？？",
          "text": "う～ん、はてな……？\r ――あっ！　すみません、そちらの方！　少しよろしいかしら？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……俺？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……私？",
              "flag_type": "",
              "flag": "female_trainer"
