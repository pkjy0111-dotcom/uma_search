      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1022,
          "speaker_name": "どこか上品なウマ娘",
          "text": "ネットで記事を見た時から、ずっと自分の目で見てみたかったんだ！\r はぁ～……この瞬間を43日間待ったよ～……！",
          "flag_type": "",
          "flag": "",
          "choices": []
