      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "戦意があるのはいいことだが、 ありすぎて前のめりになっては意味がない。 少し冷静にさせる必要があるだろう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "アース、今日の作戦だけど――",
