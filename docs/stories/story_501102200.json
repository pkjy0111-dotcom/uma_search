        {
          "next_frame": 4,
          "character_id": 1102,
          "speaker_name": "サウンズオブアース",
          "text": "ああ……！　体が歓喜ではちきれそうだ！ 早く皆とセッションしたい……！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
