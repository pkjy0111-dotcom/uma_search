      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "前にクラスメイトが、『酔いやすい』って言っていたのが 気になりまして……前もって調べておいたほうがいいかと！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 30,
