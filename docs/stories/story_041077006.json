      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "ダンサーさんも、音楽も、きらきらで…… あっ、ダンスに参加できるみたいですよ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 25,
