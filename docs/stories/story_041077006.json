      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "やっぱりお昼時は、1番人気のジェットコースターも空いてますね。 ――うん、パレードの後はビッグカミナリ山で決まりですっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
