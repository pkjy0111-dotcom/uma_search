      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "パレードを存分に楽しんだ後。 真っ先に、1番人気のジェットコースターへと移動した。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
