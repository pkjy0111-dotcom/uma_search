      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 1077,
          "speaker_name": "ナリタトップロード",
          "text": "ぐるぐるぐるぐる……ふふっ！ トレーナーさんが踊っているって、なんか珍しいですね！　ふふふ！",
          "flag_type": "",
          "flag": "",
