      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "っと、取っ組み合いは五分五分か。 なら〆は野良レースでいこうじゃねえか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "無断のレースはよくないぞ",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "無断のレースはよくないよ",
