      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "おうおうおう、威勢がいいねえ！ どっちも負けるな、意地見せてやんな！",
          "flag_type": "",
          "flag": "",
          "choices": []
