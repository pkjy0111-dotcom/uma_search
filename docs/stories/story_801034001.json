    "6": {
      "frame_id": 6,
      "background_id": 74,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "うおっつ、トレーナーさん！？　意地の ぶつかり合いに水かけるたあ野暮じゃねえか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 74,
      "sub_background_id": 112,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1034,
          "speaker_name": "イナリワン",
          "text": "ん、そうだ！　野良レースでも、お前さんが 認めりゃ、立派な模擬レースじゃねえか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "いや、そんな無茶な……",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
