      "dialogue": [
        {
          "next_frame": 2,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "最近不調気味のエアメサイアと共に、 神社へお祈りに来たところ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
