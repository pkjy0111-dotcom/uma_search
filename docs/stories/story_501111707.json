    "2": {
      "frame_id": 2,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "ゴミが散らかっているようですね。 ……私、神主さんのところに行ってきます。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "許可を頂いてきましたので、 掃除して参ります。トレーナーさんは――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "俺も手伝う",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "私も手伝うよ",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 39,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "……ありがとうございます。 それでは、一緒に。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "……そういえば。 幼い頃も、似たようなことをしていました。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "手つかずの大きな物置があって……。 埃だらけになっているのが気になったので、 掃除したのです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "もしかして……1人で？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "はい、1人で。汚れていたので掃除をする、 というだけでしたので…… 特にわざわざ言うようなことでもないかなと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "でも、ある日の掃除を終えて、外に出たら―― 母が待っていてくれて。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "私、埃まみれでしたのに……ためらいなく 抱きしめてくださったんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 39,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "……褒められたかったわけではありません。 けれど、母は見ていてくれた。 とても――嬉しかったです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
