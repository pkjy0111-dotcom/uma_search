    "11": {
      "frame_id": 11,
      "background_id": 39,
      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "神社は明らかに綺麗になっていた。 頬を撫でる風もどこか爽やかに感じる。",
          "flag_type": "",
          "flag": "",
          "choices": []
