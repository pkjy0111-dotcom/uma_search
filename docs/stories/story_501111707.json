      "sub_background_id": 130,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1111,
          "speaker_name": "エアメサイア",
          "text": "結局、1日かかってしまいました。 もっと早くできればよかったのですが。",
