      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "むふふ、『正審統一』『一休入魂』！ 我ながらなかなかいい字が書けました！",
