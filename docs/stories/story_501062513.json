          "flag": "",
          "choices": [
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
              "text": "……勝負服も集中法の一環か？",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 15,
              "loop_ends_at_frame": -1,
