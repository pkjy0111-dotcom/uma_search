      "frame_id": 9,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "さ～て、次の集中法は……書道っ！！ 墨汁と紙、取ってきますね！",
