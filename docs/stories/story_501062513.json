      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "むむむ……来た来た！ 集中パワーが体の内から ぐんぐん高まってきてますよ～！",
