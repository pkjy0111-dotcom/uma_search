      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "2人の視線を一身に浴びながら、\r お店を眺めているのだが……。",
