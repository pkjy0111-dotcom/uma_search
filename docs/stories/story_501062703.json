      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "よーし、トレーナーを探っちゃうぞ～！",
          "flag_type": "",
          "flag": "",
          "choices": []
