    "9": {
      "frame_id": 9,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "到着っ！\r ここで“おもてなし”するんだね！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "はい。ですが、その前に\r “おもてなし”について1つ――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "そもそも“おもてなし”とは何か？\r 私が学びえた答えはズバリ、 『相手の心に寄り添う』行為です。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "なるなる～！　それでそれで！？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "まずはトレーナーさんの嗜好を探りましょう。\r 好きに買い物をしてもらえますか？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 14,
              "loop_ends_at_frame": -1,
              "text": "了解！",
              "flag_type": "",
              "flag": ""
