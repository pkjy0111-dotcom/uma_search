    "5": {
      "frame_id": 5,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "それでは、どうぞっ！\r じゃっばば～～ん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "こんにちは、イクノディクタスです。\r 期待に応えられるよう努めますので、 よろしくお願いいたします。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "イクノ先生、本日はよろしくお願いします！\r 最高のおもてなしを、トレーナーに！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "承知しました。“おもてなし”プランも\r 考えてきましたので、お任せください。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
