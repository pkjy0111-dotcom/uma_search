      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "ちょちょちょ！　帰るのはお待ちで！\r 今日はまだ終わりじゃないんですっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
