      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "あっ、あのぬいぐるみはどうですか？\r 目がクリ！　脚もいっぱい！ トレーナーにピッタンですよっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 17,
              "loop_ends_at_frame": -1,
              "text": "すぐには決められないよ",
