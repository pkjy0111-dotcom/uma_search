    "4": {
      "frame_id": 4,
      "background_id": 29,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "はい！　それにビシっともてなせるよう、\r アドバイザーもお呼びしたんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
