      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1062,
          "speaker_name": "マチカネタンホイザ",
          "text": "実は、いつもトレーナーにお世話されて\r ばっかりだから、お礼におもてなしを しようと考えてまして。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "おもてなし……？",
              "flag_type": "",
              "flag": ""
