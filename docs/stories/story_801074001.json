      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "まずはウォーミングアップをして\r 体を温めているところですわ～。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 5,
