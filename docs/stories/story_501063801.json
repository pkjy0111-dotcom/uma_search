      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1063,
          "speaker_name": "イクノディクタス",
          "text": "ここまで修練を重ねてきたんです。 明日の本番、必ずや勝ってみせます。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 19,
