      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして、ふたりでイベントに向けた 練習を重ね――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
