      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "はっ、はっ、はっ、はっ……\r はぁっ、はぁーっ……暑くて汗が……。",
