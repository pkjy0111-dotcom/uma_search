              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "その分、鍛えられるぞ",
              "flag_type": "male_trainer",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "その分、鍛えられるよ",
