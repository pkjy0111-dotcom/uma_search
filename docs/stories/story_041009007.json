      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 1009,
          "speaker_name": "ダイワスカーレット",
          "text": "ちょっと休憩……！\r ふぅー……砂浜ってほんと疲れる……。",
