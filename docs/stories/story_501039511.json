      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "――いい？　ルールは単純よ！\r 今から空にポップコーンを投げて、 先にキャッチできたほうが勝ちっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
