    "16": {
      "frame_id": 16,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "と、トレーナーさんっ！\r そこまでやってくださるなんて……！ 姫、感激で涙ちょちょ切れますわ……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "17": {
      "frame_id": 17,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "……とは言ってもアタシのブンブンみたく\r 空に投げたポップコーンをキャッチとか できないでしょ？",
          "flag_type": "",
          "flag": "",
          "choices": []
