    "6": {
      "frame_id": 6,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "今はアタシの忠実なしもべ――名付けて\r 『ブンブン』よ！　ゆくゆくは、 新しい使い魔にしてあげようと思ってるのっ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "ふ、ふ～ん……そうなんですのね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "ふふん――カワカミも使い魔が欲しいなって\r 思っちゃったでしょ？",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "べ、べ、べ、べ、別に～？\r うらやましくなんかありませんわ！ 少しいいなあって思うだけで――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "やっぱうらやましいんじゃない。\r でも簡単なことじゃないわ。 アタシみたいに偉大な魔法少女じゃないと！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "ぐぬぬ～……ですわぁ～……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "12": {
      "frame_id": 12,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "――その時、ふと思いついた。\r そういえば、プリファイにも マスコットキャラが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "――いえ、姫には私がいますワン",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "は……？　な、なによ、アンタ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
