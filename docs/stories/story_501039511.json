    "20": {
      "frame_id": 20,
      "background_id": 5,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "ふんっ！　おもしろいじゃない！\r そんなに言うなら見せてもらおうじゃないの！",
          "flag_type": "",
          "flag": "",
          "choices": []
