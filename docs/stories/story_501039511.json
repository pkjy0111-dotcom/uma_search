      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1044,
          "speaker_name": "スイープトウショウ",
          "text": "ふっふーん、すごいでしょ。\r おやつをあげてるうちに、言うこと 聞いてくれるようになったのよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
