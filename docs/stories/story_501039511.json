      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1039,
          "speaker_name": "カワカミプリンセス",
          "text": "トレーナーさんに不可能はありませんの！\r きっと、ブンブンちゃんよりも華麗に ポップコーンをキャッチできますわっ！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 20,
              "loop_ends_at_frame": -1,
              "text": "え……！？",
              "flag_type": "",
              "flag": ""
            }
