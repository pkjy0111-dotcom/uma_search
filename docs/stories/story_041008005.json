      "background_id": 70,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "同じウマ娘なら――格上も格下もねぇ。\r 正々堂々、ハンデなしで勝負だっ、先輩！！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
