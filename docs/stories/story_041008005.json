      "frame_id": 18,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 19,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "あ、あー、テステスっと……。いくぜ。\r 『ハッ、今日のところは負けたぜ。認めるよ。じゃあな』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
