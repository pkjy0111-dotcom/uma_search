    },
    "17": {
      "frame_id": 17,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "そっか。負けたら、あんましゃべらないようにすれば……。\r よし、練習しとこう。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
