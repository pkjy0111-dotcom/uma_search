    "19": {
      "frame_id": 19,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "……。…………。……………………。だけどなー、\r 次に勝つのはこの俺だ！　テメエなんかブチ抜く！！",
