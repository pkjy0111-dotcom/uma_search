      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 1008,
          "speaker_name": "ウオッカ",
          "text": "うおおおおっ、ムリだぁぁぁ。これ、ハードル高すぎんだろ。\r 素直に負けを認めて、引き下がるなんて！",
          "flag_type": "",
          "flag": "",
          "choices": []
