      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1043,
          "speaker_name": "シンコウウインディ",
          "text": "『驚かせてすみません！　よかったら、 いたずらの参考にしてください！ ウインディちゃんのファンより』",
          "flag_type": "",
          "flag": "",
