    "7": {
      "frame_id": 7,
      "background_id": 192,
      "sub_background_id": 120,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "近所だったから……とは言うが、昨日は結局あの後も豪雨が続いた。 『忍びなかった』で支払えるような手間ではないと思うが――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "君は……本当に、真面目なんだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 8,
              "loop_ends_at_frame": -1,
              "text": "貴方は……本当に、真面目なんだね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
        }
      ]
    },
    "8": {
      "frame_id": 8,
