          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "（ここで待ってる……のでいいんだよな？）",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
