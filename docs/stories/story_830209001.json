      "frame_id": 6,
      "background_id": 31,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1092,
          "speaker_name": "ダンツフレーム",
          "text": "だから……ちょっとお手伝いしたくて。 それでお友だち役を探してたんです！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "なるほど。そういうことなら協力するよ",
              "flag_type": "",
              "flag": ""
