      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1092,
          "speaker_name": "ダンツフレーム",
          "text": "でも……転校ってドキドキするよなぁって。 上手くやっていけるかな～って…… わたしもそうでしたから。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
