    "16": {
      "frame_id": 16,
      "background_id": 74,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そんな彼女が目指している『目標』とは、果たして何なのか。 その関心が、自分の中で日に日に高まりつつある中――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
