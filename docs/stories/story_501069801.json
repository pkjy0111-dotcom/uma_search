      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "なんとかしなきゃって考えたときに―― テイオーさんに言われた言葉を 思い出したんです！",
