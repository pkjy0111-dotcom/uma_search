      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "ふふっ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "どうしたんです？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
