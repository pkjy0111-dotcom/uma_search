      "frame_id": 14,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "変ですよね。 私が関わった作品でもなんでもないのに。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "けれど、こんなふうにエンタメ作品で、 皆が笑顔になって。私も笑顔になって――",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "16": {
      "frame_id": 16,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "そうやって 幸せがどんどん広がっていくのは…… とても素敵だなと、改めて思ったんです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
