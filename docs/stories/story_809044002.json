      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90310,
          "speaker_name": "観客A",
          "text": "あの俳優、あんなドギツい演技できんだな！ ホント驚いたわ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90316,
          "speaker_name": "観客B",
          "text": "わかる！　イケメンイメージだったのに、 悪～い顔マジしびれた！　悪役もいいよねー！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "観客の満足度はかなり高いようね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "6": {
      "frame_id": 6,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "キャスティングとその意外性……。 クチコミで観客動員数が伸びそうだわ。 動画でも、同じ効果を狙えたら――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 7,
              "loop_ends_at_frame": -1,
              "text": "こんにちは、都留岐さん。お仕事ですか？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "7": {
      "frame_id": 7,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "あっ！　トレーナーさん。 ええ、市場調査を少々。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "8": {
      "frame_id": 8,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "トレーナーさんは、 映画を見終えたところですか？ どうでした？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 9,
              "loop_ends_at_frame": -1,
              "text": "アクションがド派手で非常に楽しめました",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "映画の感想を熱く語っていると、 他の観客たちの声も、 あちらこちらから聞こえてきた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "10": {
      "frame_id": 10,
      "background_id": 51,
      "sub_background_id": 310,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90311,
          "speaker_name": "観客C",
          "text": "ラスト、雪山での一騎打ちは、 マジでハラハラしたわ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
