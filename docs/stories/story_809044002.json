    "13": {
      "frame_id": 13,
      "background_id": 35,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 14,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "いえ、お客さんの感想を聞いていたら―― 何だか私まで嬉しくなってしまって。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
