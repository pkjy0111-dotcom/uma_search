      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 9044,
          "speaker_name": "都留岐涼花",
          "text": "だから、こういう素晴らしい作品に出合うと、 やる気が湧いてくるんですよね！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
