      "frame_id": 28,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 29,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "あっ！　あそこ、野鳥の巣が……ふふふっ。 あそこで飛んでる小鳥も、少し前まで雛だったんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 29,
              "loop_ends_at_frame": -1,
              "text": "エサを捕まえる練習とかしてるのかな？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
