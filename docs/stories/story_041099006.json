      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 25,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "えっと……これだけあれば、 初心者向けの山なら登れると思います……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
