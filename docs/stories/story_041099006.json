      "frame_id": 26,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 27,
          "character_id": 90001,
          "speaker_name": "",
          "text": "それに、先ほどから少し彼女の様子がおかしい気もする。 ……ここで、先程の失敗を挽回するとしよう。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 27,
              "loop_ends_at_frame": -1,
              "text": "タルマエ、よかったらなんだけど……！",
              "flag_type": "",
              "flag": ""
