    "29": {
      "frame_id": 29,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 30,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "そうかもしれませんね！　ふふ、こういう自然の営みを ゆっくり観察できるのが、登山の醍醐味ですよね。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "30": {
      "frame_id": 30,
      "background_id": 51,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 31,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "地元の山も……何度もこうやって登ったっけ。 樽前山にはビンズイっていう鳥がいて、すごくカワイイんです。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 31,
              "loop_ends_at_frame": -1,
              "text": "（いつものタルマエだ……誘ってよかった）",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
