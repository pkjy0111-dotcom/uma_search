      "frame_id": 19,
      "background_id": 46,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 20,
          "character_id": 90001,
          "speaker_name": "",
          "text": "……気のせいだろうか、勧めてくれる内容が彼女らしくないように 感じる。やけにこちらの顔色を窺っているふうなのも妙だ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
