      "frame_id": 25,
      "background_id": 32,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 90001,
          "speaker_name": "",
          "text": "ホッコータルマエに選んでもらったおかげで、 ハイキングの装備が揃った。彼女には何か、お礼をしたい。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
