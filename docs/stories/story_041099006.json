      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 28,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――ふぅ～……空気がおいしいですね……。 ここのコースはかなり舗装されてるので、歩きやすいんですよ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
