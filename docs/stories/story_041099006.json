      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 21,
          "character_id": 90001,
          "speaker_name": "",
          "text": "こういう時は――！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 21,
              "loop_ends_at_frame": -1,
              "text": "向こうの店のトレンチコートも気になるな！",
              "flag_type": "",
              "flag": ""
            }
          ]
