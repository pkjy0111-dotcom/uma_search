      "dialogue": [
        {
          "next_frame": 23,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "――あ、い、いいと思います！ おしゃれだし……向こうのお店、覗いてみましょうか！",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 23,
              "loop_ends_at_frame": -1,
              "text": "タルマエ！？待って、ごめん冗談……！",
              "flag_type": "",
