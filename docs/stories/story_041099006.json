    "21": {
      "frame_id": 21,
      "background_id": 168,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 22,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "えっ……ええ！？ ト、ト、トレンチコートですか……！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 22,
              "loop_ends_at_frame": -1,
              "text": "なんて、冗談――",
              "flag_type": "",
              "flag": ""
            }
