      "frame_id": 23,
      "background_id": 46,
      "sub_background_id": 1,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 24,
          "character_id": 90001,
          "speaker_name": "",
          "text": "和ませようと思っての発言だったが、真に受けられてしまう。 慌てて引き留めつつ、やはり彼女の態度が気にかかった……。",
          "flag_type": "",
          "flag": "",
          "choices": []
