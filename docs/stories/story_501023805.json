      "sub_background_id": 10,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 1023,
          "speaker_name": "ビワハヤヒデ",
          "text": "参ったな。目当ての教本が見当たらない。 貸出の履歴も残っていないということは……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 3,
              "loop_ends_at_frame": -1,
              "text": "誰かがここで読んでいるのかも？",
              "flag_type": "",
              "flag": ""
