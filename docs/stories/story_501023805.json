      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1083,
          "speaker_name": "シンボリクリスエス",
          "text": "……――とても、助かる。 今――ここで、詰まっているのだが……。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
