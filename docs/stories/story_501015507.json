      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1015,
          "speaker_name": "テイエムオペラオー",
          "text": "ボクの魅力をここまで忠実に再現するなんて、 世の中にはとんでもない鬼才がいたものだ！",
          "flag_type": "",
          "flag": "",
          "choices": []
