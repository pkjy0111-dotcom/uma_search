      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "だがシンボリクリスエスはトレーナーに語る。 不安を隠さなくていいと。自分たちは根幹に、 誰かの願いを自分の願いにする共通点がある。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
