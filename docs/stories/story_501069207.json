      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "マルゼンさんは言っていました。 『たとえ自分が欠けたとしても。 あたしは今のあたしとして走り続ける』と。",
          "flag_type": "",
          "flag": "",
          "choices": []
