      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1069,
          "speaker_name": "サクラチヨノオー",
          "text": "夏合宿から考えていました。 私は、欠けていく私としてどう戦うべきかと。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
