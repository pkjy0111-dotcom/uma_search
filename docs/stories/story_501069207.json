      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "『天皇賞（秋）』の控室。\r サクラチヨノオーは静かに 深呼吸した後、目を開いた。",
          "flag_type": "",
          "flag": "",
          "choices": []
