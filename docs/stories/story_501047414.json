      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 0,
          "speaker_name": "ゼンノロブロイ英雄譚",
          "text": "“秋シニア三冠”――『天皇賞（秋）』 『ジャパンカップ』『有馬記念』全てを 制した者にのみ、贈られる称号。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
      "background_id": 56,
      "sub_background_id": 111,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 0,
          "speaker_name": "ゼンノロブロイ英雄譚",
          "text": "英傑は数々居れど、 その称号を手にしたのはただ1人―― “世紀末覇王”、テイエムオペラオーのみだ。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 54,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 0,
          "speaker_name": "ゼンノロブロイ英雄譚",
          "text": "『私は私の剣を――英雄の証を、 高く高く掲げよう！　強く鋭く圧倒的に！ 誰もの目に留まるほど――高く！』",
          "flag_type": "",
          "flag": "",
          "choices": []
