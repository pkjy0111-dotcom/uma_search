      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 0,
          "speaker_name": "テレビ",
          "text": "『――大学の襷が渡ります。先頭との差は 2分半。走者は4年生、最後の箱根！』",
          "flag_type": "",
          "flag": "",
          "choices": []
