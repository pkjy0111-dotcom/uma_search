      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 17,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "テレビには、走る道を確保する警察車両と、 襷を渡すランナーたちが映っている。",
          "flag_type": "",
          "flag": "",
          "choices": []
