      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 18,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "もしも、ランナーを導いているのが 周りにいる警察官だけではない…… そう考えるようになったなら――",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 18,
              "loop_ends_at_frame": -1,
              "text": "一度俯瞰して、考えてみようか",
