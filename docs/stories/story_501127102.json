      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "――ボス。私はこれから、 どのように在るべきなのでしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "15": {
      "frame_id": 15,
      "background_id": 1,
      "sub_background_id": 9110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 16,
          "character_id": 1127,
          "speaker_name": "フェノーメノ",
          "text": "子どもたちの未来を守り、 成長させる者として、 私に何ができるのでしょうか。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "……なるほど、それが次の課題なんだな",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 16,
              "loop_ends_at_frame": -1,
              "text": "……なるほど、それが次の課題なんだね",
              "flag_type": "",
              "flag": "female_trainer"
            }
          ]
