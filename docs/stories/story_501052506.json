      "frame_id": 6,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "ほかにもね、学園に入る日も、おっきい布に 『がんばれ！』って書いてくれたの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
