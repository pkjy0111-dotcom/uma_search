      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "わたしのライブは、それ持って みんなで見に来るんだって！　約束したの！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
