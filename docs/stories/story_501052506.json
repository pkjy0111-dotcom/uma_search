      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "あのね、小さいころはよく、 おうちのお仕事小屋で踊ってたんだ～。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
      "background_id": 14,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "えへへ、踊ってるうちに思い出しちゃった！ みんなが木でマイク作ってくれたんだよ！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
