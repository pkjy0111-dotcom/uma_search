      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "だから、ライブもちゃんと練習しなきゃ！ がっかりさせちゃダメだもん！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
