      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 1052,
          "speaker_name": "ハルウララ",
          "text": "ねえねえ、どうだった！？ みんな、喜んでくれるかな！？",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 12,
              "loop_ends_at_frame": -1,
              "text": "きっと喜んでくれる！",
