      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "そうですか……！ お口にあったようで、なによりです。",
          "flag_type": "",
          "flag": "",
          "choices": []
