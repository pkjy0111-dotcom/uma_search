      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "どうか本日はゆるりとお過ごしいただきたく。 近頃、少々気が張っているご様子でしたので。",
          "flag_type": "",
          "flag": "",
          "choices": []
