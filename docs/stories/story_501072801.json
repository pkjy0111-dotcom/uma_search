      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女の誘いを受け、最近、根を詰めて 仕事をしていたため、周囲の景色に 目を向けていなかったことに気づく。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……ああ、ぜひ行こう！",
              "flag_type": "",
              "flag": "male_trainer"
            },
            {
              "next_frame": 4,
              "loop_ends_at_frame": -1,
              "text": "……うん、ぜひ行こう！",
              "flag_type": "",
              "flag": "female_trainer"
