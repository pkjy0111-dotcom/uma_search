    "5": {
      "frame_id": 5,
      "background_id": 10,
      "sub_background_id": 5110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "花見らしく、弁当の用意をして参りました。 お口にあえば、よいのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 6,
              "loop_ends_at_frame": -1,
              "text": "美味しい！",
              "flag_type": "",
              "flag": ""
