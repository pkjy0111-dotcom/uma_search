      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "そして迎えた翌日―― ヤエノムテキが言っていた通り、 野外ステージの桜は鮮やかに咲き誇っていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
