      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1072,
          "speaker_name": "ヤエノムテキ",
          "text": "実は、祖母のレシピを参考にしたのです。 昔、修行を兼ねたハイキングをする時に 持たせてくれていたものでして。",
          "flag_type": "",
          "flag": "",
          "choices": []
