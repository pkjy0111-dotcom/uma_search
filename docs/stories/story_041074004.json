      "frame_id": 9,
      "background_id": 69,
      "sub_background_id": 4321,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "……あら？　葉っぱが……すべて、落ちてしまったのですね～。\r 頑張って、しがみついていたのですが……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 10,
              "loop_ends_at_frame": -1,
              "text": "でも、よく見てごらん",
