      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "そうですか～。\r もうすぐ……春が来るのですわね。",
          "flag_type": "",
