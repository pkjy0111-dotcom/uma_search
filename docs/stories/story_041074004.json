      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "ここは……あなたと、初めてお会いした……。\r ふふ……そう、2人で並んで、あの木を見上げていて――",
          "flag_type": "",
          "flag": "",
          "choices": []
