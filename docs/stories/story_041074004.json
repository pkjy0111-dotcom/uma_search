      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1074,
          "speaker_name": "メジロブライト",
          "text": "……選抜レースでは、必ず結果を残さなければなりませんわね。\r 皆さまに、ご期待いただける走りをしなければ……。",
          "flag_type": "",
          "flag": "",
          "choices": []
