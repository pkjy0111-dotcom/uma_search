      "frame_id": 5,
      "background_id": 174,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 6,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "それが、苫小牧市。 ……私の大切なふるさとです。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
