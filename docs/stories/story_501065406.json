      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "でっけぇパーティー、ブチかませばじゃね！？ レースでパすればダイイチルビーと ケイエスミラクルも元気になるはずだと。",
          "flag_type": "",
          "flag": "",
          "choices": []
