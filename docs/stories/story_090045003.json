      "dialogue": [
        {
          "next_frame": 26,
          "character_id": 1112,
          "speaker_name": "デアリングハート",
          "text": "（ライバルたちと、ハートとハートをぶつけあう真剣勝負！ The passion...The thrill!　あの瞬間に夢中だった）",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
