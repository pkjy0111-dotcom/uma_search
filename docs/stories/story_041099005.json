      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 15,
          "character_id": 1099,
          "speaker_name": "ホッコータルマエ",
          "text": "うーん……今日はこの辺にして、また考え直そうかな。 相談に乗っていただいてありがとうございました！",
          "flag_type": "",
          "flag": "",
          "choices": []
