    "10": {
      "frame_id": 10,
      "background_id": 349,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 11,
          "character_id": 90001,
          "speaker_name": "",
          "text": "コラムは、トウカイテイオーが 最初にケガから復帰した『大阪杯』に触れ、 『有馬記念』へと至る軌跡をまとめていた。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "11": {
      "frame_id": 11,
      "background_id": 349,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 12,
          "character_id": 90001,
          "speaker_name": "",
          "text": "彼女の再起を願ったファンの心情が、 臨場感たっぷりに綴られている……！",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
