      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 7,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "レースドラマの面白さを知っていただく…… それが、グランプリのファン投票の 動機に繋がるのではないかと……。",
          "flag_type": "",
          "flag": "",
          "choices": []
