    "12": {
      "frame_id": 12,
      "background_id": 1,
      "sub_background_id": 1110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 13,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "ファンは思いを込めた一票を投じ、 あの奇跡の感動を分かち合った…… その魅力を伝えられればと思って……。",
          "flag_type": "",
          "flag": "",
          "choices": [
            {
              "next_frame": 13,
              "loop_ends_at_frame": -1,
              "text": "すごくいいよ！外部にも発信してみたら？",
              "flag_type": "",
              "flag": ""
            }
          ]
        }
      ]
    },
    "13": {
      "frame_id": 13,
      "background_id": 1,
