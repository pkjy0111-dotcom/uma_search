      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 1133,
          "speaker_name": "クロノジェネシス",
          "text": "お、お忙しい中、申し訳ございません……！ ですが、日頃ご愛読いただいている トレーナーさんに……ご意見を賜りたく……！",
          "flag_type": "",
          "flag": "",
          "choices": []
