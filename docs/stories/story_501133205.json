    "8": {
      "frame_id": 8,
      "background_id": 349,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 9,
          "character_id": 1133,
          "speaker_name": "ライターC",
          "text": "『レースキャリアを象徴するような復活劇。 不死鳥の如く何度も甦った 彼女の最初の復活劇は――大阪杯』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "9": {
      "frame_id": 9,
      "background_id": 349,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 10,
          "character_id": 1133,
          "speaker_name": "ライターC",
          "text": "『地の果てまで走って行けちゃう気分！―― かの名言が飛び出したのは大阪杯の前だった』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
