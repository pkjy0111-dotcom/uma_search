    "7": {
      "frame_id": 7,
      "background_id": 349,
      "sub_background_id": 0,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 8,
          "character_id": 1133,
          "speaker_name": "ライターC",
          "text": "『トウカイテイオーの有馬記念は、 あまりにも有名な伝説のレースである』",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
