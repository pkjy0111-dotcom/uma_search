      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 4,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "サクラローレルは、倶楽部が考えた負担の 少ないトレーニングが助けになったと語る。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "4": {
      "frame_id": 4,
      "background_id": 21,
      "sub_background_id": 30,
