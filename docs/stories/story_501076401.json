      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 5,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "後日、トレーナーはケガの予防の参考に、 ヴィクトリー倶楽部から資料を借り受ける。 それはウマ娘たちの壮絶な闘病の記録だった。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "5": {
      "frame_id": 5,
