    "2": {
      "frame_id": 2,
      "background_id": 24,
      "sub_background_id": 110,
      "cg": "",
      "animated_cg": null,
      "dialogue": [
        {
          "next_frame": 3,
          "character_id": 90001,
          "speaker_name": "モノローグ",
          "text": "彼女たちによると、ヴィクトリー倶楽部には 色々な先駆者がいるそうだ。その中には、 ケガから立ち上がったウマ娘もいたという。",
          "flag_type": "",
          "flag": "",
          "choices": []
        }
      ]
    },
    "3": {
      "frame_id": 3,
